        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0428\u043e\u04b3 \u0414\u043e\u0440\u043e \u04b3\u0443\u043a\u043c\u0440\u043e\u043d\u043b\u0438\u0433\u0438\u043d\u0438\u043d\u0433 \u0438\u043a\u043a\u0438\u043d\u0447\u0438 \u0439\u0438\u043b\u0438 \u043e\u043b\u0442\u0438\u043d\u0447\u0438 \u043e\u0439\u0438\u043d\u0438\u043d\u0433 \u0431\u0438\u0440\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438 \u044d\u0434\u0438. \u042d\u0433\u0430\u043c\u0438\u0437 \u0425\u0430\u0433\u0433\u0435\u0439 \u043f\u0430\u0439\u0493\u0430\u043c\u0431\u0430\u0440 \u043e\u0440\u049b\u0430\u043b\u0438 \u042f\u04b3\u0443\u0434\u043e \u04b3\u043e\u043a\u0438\u043c\u0438 \u0428\u0430\u043b\u0442\u0438\u0451\u043b \u045e\u0493\u043b\u0438 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b\u0433\u0430 \u0432\u0430 \u043e\u043b\u0438\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439 \u0401\u04b3\u0443\u0437\u0430\u0434\u0430\u0445 \u045e\u0493\u043b\u0438 \u0401\u0448\u0443\u0430\u0433\u0430 \u049b\u0443\u0439\u0438\u0434\u0430\u0433\u0438 \u0441\u045e\u0437\u043b\u0430\u0440\u0438\u043d\u0438 \u0430\u0451\u043d \u049b\u0438\u043b\u0434\u0438"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0448\u0443\u043d\u0434\u0430\u0439 \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u201c\u0411\u0443 \u0445\u0430\u043b\u049b: \u00ab\u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u0443\u0439\u0438\u043d\u0438 \u049b\u0443\u0440\u0438\u0448 \u0432\u0430\u049b\u0442\u0438 \u04b3\u0430\u043b\u0438 \u043a\u0435\u043b\u0433\u0430\u043d\u0438 \u0439\u045e\u049b\u00bb, \u0434\u0435\u0431 \u0430\u0439\u0442\u0430\u0434\u0438."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0448\u0443\u043d\u0434\u0430\u0439 \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u201c\u0411\u0443 \u0445\u0430\u043b\u049b: \u00ab\u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u0443\u0439\u0438\u043d\u0438 \u049b\u0443\u0440\u0438\u0448 \u0432\u0430\u049b\u0442\u0438 \u04b3\u0430\u043b\u0438 \u043a\u0435\u043b\u0433\u0430\u043d\u0438 \u0439\u045e\u049b\u00bb, \u0434\u0435\u0431 \u0430\u0439\u0442\u0430\u0434\u0438."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u042d\u0439 \u0418\u0441\u0440\u043e\u0438\u043b \u0445\u0430\u043b\u049b\u0438, \u041c\u0435\u043d\u0438\u043d\u0433 \u0443\u0439\u0438\u043c \u0432\u0430\u0439\u0440\u043e\u043d \u0431\u045e\u043b\u0438\u0431 \u0451\u0442\u0433\u0430\u043d\u0434\u0430, \u0441\u0438\u0437\u043b\u0430\u0440 \u04b3\u0430\u0448\u0430\u043c\u0430\u0442\u043b\u0438 \u0443\u0439\u043b\u0430\u0440\u0434\u0430 \u044f\u0448\u0430\u0448\u0438\u043d\u0433\u0438\u0437\u043d\u0438\u043d\u0433 \u0432\u0430\u049b\u0442\u0438\u043c\u0438?!\u201d"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u041c\u0430\u043d\u0430 \u044d\u043d\u0434\u0438 \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u201c\u040e\u0437 \u0430\u04b3\u0432\u043e\u043b\u0438\u043d\u0433\u0438\u0437\u0433\u0430 \u049b\u0430\u0440\u0430\u0431, \u044f\u0445\u0448\u0438\u043b\u0430\u0431 \u045e\u0439\u043b\u0430\u0431 \u043a\u045e\u0440\u0438\u043d\u0433\u043b\u0430\u0440\u2013\u0447\u0438!"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u041a\u045e\u043f \u044d\u043a\u0438\u0431, \u043a\u0430\u043c \u045e\u0440\u0430\u0441\u0438\u0437. \u041e\u0432\u049b\u0430\u0442 \u0435\u0439\u0441\u0438\u0437\u2013\u0443, \u0442\u045e\u0439\u043c\u0430\u0439\u0441\u0438\u0437. \u0418\u0447\u0430\u0441\u0438\u0437, \u0430\u043c\u043c\u043e \u0447\u0430\u043d\u049b\u043e\u0493\u0438\u043d\u0433\u0438\u0437 \u049b\u043e\u043d\u043c\u0430\u0439\u0434\u0438, \u0443\u0441\u0442\u0438\u043d\u0433\u0438\u0437\u0433\u0430 \u043a\u0438\u044f\u0441\u0438\u0437\u2013\u0443, \u0438\u0441\u0438\u043d\u043e\u043b\u043c\u0430\u0439\u0441\u0438\u0437. \u041a\u0438\u043c \u043f\u0443\u043b \u0438\u0448\u043b\u0430\u0441\u0430, \u0443\u043d\u0438\u043d\u0433 \u043f\u0443\u043b\u0438 \u0439\u0438\u0440\u0442\u0438\u049b \u04b3\u0430\u043c\u0451\u043d\u0433\u0430 \u0442\u0443\u0448\u0430\u0434\u0438.\u201d"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u201c\u040e\u0437 \u0430\u04b3\u0432\u043e\u043b\u0438\u043d\u0433\u0438\u0437\u0433\u0430 \u049b\u0430\u0440\u0430\u0431, \u044f\u0445\u0448\u0438\u043b\u0430\u0431 \u045e\u0439\u043b\u0430\u0431 \u043a\u045e\u0440\u0438\u043d\u0433, \u0430\u0445\u0438\u0440!"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0422\u043e\u0493\u043b\u0430\u0440\u0433\u0430 \u0447\u0438\u049b\u0438\u0431, \u0451\u0493\u043e\u0447 \u043e\u043b\u0438\u0431 \u0442\u0443\u0448\u0438\u043d\u0433\u043b\u0430\u0440\u2013\u0434\u0430, \u041c\u0435\u043d\u0438\u043d\u0433 \u0443\u0439\u0438\u043c\u043d\u0438 \u049b\u0443\u0440\u0438\u043d\u0433\u043b\u0430\u0440. \u041c\u0435\u043d \u0443\u043d\u0438 \u043a\u045e\u0440\u0438\u0431 \u0437\u0430\u0432\u049b\u043b\u0430\u043d\u0430\u043c\u0430\u043d, \u0443 \u0435\u0440\u0434\u0430 \u0443\u043b\u0443\u0493\u043b\u0430\u043d\u0430\u043c\u0430\u043d, \u2014 \u0434\u0435\u0439\u0434\u0438 \u042d\u0433\u0430\u043c\u0438\u0437."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u041a\u045e\u043f \u04b3\u043e\u0441\u0438\u043b \u043e\u043b\u0438\u0448\u043d\u0438 \u0443\u043c\u0438\u0434 \u049b\u0438\u043b\u0433\u0430\u043d \u044d\u0434\u0438\u043d\u0433\u0438\u0437, \u043c\u0430\u043d\u0430 \u043a\u0430\u043c \u043e\u043b\u0434\u0438\u043d\u0433\u0438\u0437. \u0423\u0439\u0433\u0430 \u043e\u043b\u0438\u0431 \u043a\u0435\u043b\u0433\u0430\u043d\u0438\u043d\u0433\u0438\u0437\u043d\u0438 \u0441\u043e\u0447\u0438\u0431 \u044e\u0431\u043e\u0440\u0434\u0438\u043c. \u041d\u0438\u043c\u0430 \u0443\u0447\u0443\u043d?\u201d \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0448\u0443\u043d\u0434\u0430\u0439 \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u201c\u041c\u0435\u043d\u0438\u043d\u0433 \u0443\u0439\u0438\u043c \u0432\u0430\u0439\u0440\u043e\u043d \u0431\u045e\u043b\u0438\u0431 \u0451\u0442\u0433\u0430\u043d \u043f\u0430\u0439\u0442\u0434\u0430, \u0441\u0438\u0437\u043b\u0430\u0440 \u0444\u0430\u049b\u0430\u0442 \u045e\u0437 \u0443\u0439\u0438\u043d\u0433\u0438\u0437 \u04b3\u0430\u049b\u0438\u0434\u0430 \u045e\u0439\u043b\u0430\u0433\u0430\u043d\u0438\u043d\u0433\u0438\u0437 \u0443\u0447\u0443\u043d \u0448\u0443\u043d\u0434\u0430\u0439 \u049b\u0438\u043b\u0434\u0438\u043c."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0421\u0438\u0437\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u0434\u0430\u0441\u0442\u0438\u043d\u0433\u0438\u0437\u0434\u0430\u043d \u043e\u0441\u043c\u043e\u043d\u0434\u0430\u043d \u0448\u0443\u0434\u0440\u0438\u043d\u0433 \u0442\u0443\u0448\u043c\u0430\u0434\u0438, \u0435\u0440 \u04b3\u0430\u043c \u04b3\u043e\u0441\u0438\u043b \u0431\u0435\u0440\u043c\u0430\u0434\u0438."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u041c\u0435\u043d \u0434\u0430\u043b\u0430\u043b\u0430\u0440\u0433\u0430, \u049b\u0438\u0440\u043b\u0438 \u0436\u043e\u0439\u043b\u0430\u0440\u0433\u0430 \u049b\u0443\u0440\u0493\u043e\u049b\u0447\u0438\u043b\u0438\u043a \u043a\u0435\u043b\u0442\u0438\u0440\u0438\u0431, \u0434\u043e\u043d, \u0443\u0437\u0443\u043c \u0448\u0430\u0440\u0431\u0430\u0442\u0438 \u0432\u0430 \u0437\u0430\u0439\u0442\u0443\u043d \u043c\u043e\u0439\u0438\u043d\u0438 \u049b\u0443\u0440\u0438\u0442\u0434\u0438\u043c. \u0415\u0440\u0434\u0430\u043d \u0447\u0438\u049b\u049b\u0430\u043d \u0431\u0430\u0440\u0447\u0430 \u04b3\u043e\u0441\u0438\u043b\u043d\u0438 \u0439\u045e\u049b \u049b\u0438\u043b\u0434\u0438\u043c, \u043e\u0434\u0430\u043c\u043b\u0430\u0440 \u0432\u0430 \u04b3\u0430\u0439\u0432\u043e\u043d\u043b\u0430\u0440\u043d\u0438 \u049b\u0438\u0439\u0438\u043d\u0447\u0438\u043b\u0438\u043a\u043b\u0430\u0440\u0433\u0430 \u0434\u0443\u0447\u043e\u0440 \u049b\u0438\u043b\u0434\u0438\u043c. \u0418\u043d\u0441\u043e\u043d\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u045e\u0437 \u049b\u045e\u043b\u043b\u0430\u0440\u0438 \u0431\u0438\u043b\u0430\u043d \u049b\u0438\u043b\u0433\u0430\u043d \u043c\u0435\u04b3\u043d\u0430\u0442\u043b\u0430\u0440\u0438\u043d\u0438 \u043f\u0443\u0447\u0433\u0430 \u0447\u0438\u049b\u0430\u0440\u0434\u0438\u043c.\u201d"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0428\u0443\u043d\u0434\u0430\u043d \u043a\u0435\u0439\u0438\u043d \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b, \u043e\u043b\u0438\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439 \u0401\u0448\u0443\u0430 \u0432\u0430 \u0441\u0443\u0440\u0433\u0443\u043d\u0434\u0430\u043d \u049b\u0430\u0439\u0442\u0438\u0431 \u043a\u0435\u043b\u0433\u0430\u043d \u0431\u0430\u0440\u0447\u0430 \u0445\u0430\u043b\u049b \u045e\u0437\u043b\u0430\u0440\u0438\u043d\u0438\u043d\u0433 \u042d\u0433\u0430\u0441\u0438 \u0425\u0443\u0434\u043e\u0433\u0430 \u0438\u0442\u043e\u0430\u0442 \u049b\u0438\u043b\u0438\u0448\u0434\u0438. \u040e\u0437\u043b\u0430\u0440\u0438\u043d\u0438\u043d\u0433 \u042d\u0433\u0430\u0441\u0438 \u0425\u0443\u0434\u043e \u044e\u0431\u043e\u0440\u0433\u0430\u043d \u0425\u0430\u0433\u0433\u0435\u0439 \u043f\u0430\u0439\u0493\u0430\u043c\u0431\u0430\u0440\u043d\u0438\u043d\u0433 \u0441\u045e\u0437\u043b\u0430\u0440\u0438\u0433\u0430 \u049b\u0443\u043b\u043e\u049b \u0441\u043e\u043b\u0438\u0448\u0434\u0438, \u0447\u0443\u043d\u043a\u0438 \u0443\u043b\u0430\u0440 \u042d\u0433\u0430\u043c\u0438\u0437\u0434\u0430\u043d \u049b\u045e\u0440\u049b\u0430\u0440\u0434\u0438."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u044d\u043b\u0447\u0438\u0441\u0438 \u0425\u0430\u0433\u0433\u0435\u0439 \u0423\u043d\u0438\u043d\u0433 \u049b\u0443\u0439\u0438\u0434\u0430\u0433\u0438 \u0441\u045e\u0437\u043b\u0430\u0440\u0438\u043d\u0438 \u0445\u0430\u043b\u049b\u049b\u0430 \u0435\u0442\u043a\u0430\u0437\u0434\u0438:\n\u2014 \u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u201c\u041c\u0435\u043d \u0441\u0438\u0437\u043b\u0430\u0440 \u0431\u0438\u043b\u0430\u043d \u0431\u0438\u0440\u0433\u0430\u043c\u0430\u043d.\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u042f\u04b3\u0443\u0434\u043e \u04b3\u043e\u043a\u0438\u043c\u0438 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b\u043d\u0438, \u043e\u043b\u0438\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439 \u0401\u0448\u0443\u0430\u043d\u0438 \u0432\u0430 \u0441\u0443\u0440\u0433\u0443\u043d\u0434\u0430\u043d \u049b\u0430\u0439\u0442\u0438\u0431 \u043a\u0435\u043b\u0433\u0430\u043d \u0431\u0443\u0442\u0443\u043d \u0445\u0430\u043b\u049b\u043d\u0438 \u042d\u0433\u0430\u043c\u0438\u0437 \u0440\u0443\u04b3\u043b\u0430\u043d\u0442\u0438\u0440\u0434\u0438. \u0428\u0443\u043d\u0434\u0430 \u0443\u043b\u0430\u0440 \u04b3\u0430\u043c\u043c\u0430\u043b\u0430\u0440\u0438 \u043a\u0435\u043b\u0438\u0431, \u045e\u0437\u043b\u0430\u0440\u0438\u043d\u0438\u043d\u0433 \u0425\u0443\u0434\u043e\u0441\u0438, \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c\u043d\u0438\u043d\u0433 \u0443\u0439\u0438\u043d\u0438 \u0442\u0438\u043a\u043b\u0430\u0448\u043d\u0438 \u0431\u043e\u0448\u043b\u0430\u0434\u0438\u043b\u0430\u0440."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0428\u043e\u04b3 \u0414\u043e\u0440\u043e \u04b3\u0443\u043a\u043c\u0440\u043e\u043d\u043b\u0438\u0433\u0438\u043d\u0438\u043d\u0433 \u0438\u043a\u043a\u0438\u043d\u0447\u0438 \u0439\u0438\u043b\u0438 \u043e\u043b\u0442\u0438\u043d\u0447\u0438 \u043e\u0439\u0438\u043d\u0438\u043d\u0433 \u0439\u0438\u0433\u0438\u0440\u043c\u0430 \u0442\u045e\u0440\u0442\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438\u0434\u0430 \u041c\u0430\u044a\u0431\u0430\u0434\u043d\u0438\u043d\u0433 \u049b\u0430\u0439\u0442\u0430 \u049b\u0443\u0440\u0438\u043b\u0438\u0448\u0438 \u0431\u043e\u0448\u043b\u0430\u043d\u0434\u0438."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u040e\u0448\u0430 \u0439\u0438\u043b\u043d\u0438\u043d\u0433 \u0435\u0442\u0442\u0438\u043d\u0447\u0438 \u043e\u0439\u0438, \u0439\u0438\u0433\u0438\u0440\u043c\u0430 \u0431\u0438\u0440\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438\u0434\u0430 \u042d\u0433\u0430\u043c\u0438\u0437 \u044f\u043d\u0430 \u0445\u0430\u0431\u0430\u0440 \u044e\u0431\u043e\u0440\u0438\u0431, \u049b\u0443\u0439\u0438\u0434\u0430\u0433\u0438 \u0441\u045e\u0437\u043b\u0430\u0440\u043d\u0438 \u0425\u0430\u0433\u0433\u0435\u0439 \u043f\u0430\u0439\u0493\u0430\u043c\u0431\u0430\u0440\u0433\u0430 \u0430\u0451\u043d \u049b\u0438\u043b\u0434\u0438"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u042f\u04b3\u0443\u0434\u043e \u04b3\u043e\u043a\u0438\u043c\u0438 \u0428\u0430\u043b\u0442\u0438\u0451\u043b \u045e\u0493\u043b\u0438 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b\u0433\u0430, \u043e\u043b\u0438\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439 \u0401\u04b3\u0443\u0437\u0430\u0434\u0430\u0445 \u045e\u0493\u043b\u0438 \u0401\u0448\u0443\u0430\u0433\u0430 \u0432\u0430 \u0441\u0443\u0440\u0433\u0443\u043d\u0434\u0430\u043d \u049b\u0430\u0439\u0442\u0438\u0431 \u043a\u0435\u043b\u0433\u0430\u043d \u0431\u0443\u0442\u0443\u043d \u0445\u0430\u043b\u049b\u049b\u0430 \u0448\u0443 \u0433\u0430\u043f\u043b\u0430\u0440\u043d\u0438 \u0435\u0442\u043a\u0430\u0437"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u201c\u041e\u0440\u0430\u043d\u0433\u0438\u0437\u0434\u0430 \u043e\u043b\u0434\u0438\u043d\u0433\u0438 \u041c\u0430\u044a\u0431\u0430\u0434\u043d\u0438\u043d\u0433 \u0448\u0443\u04b3\u0440\u0430\u0442\u0438\u043d\u0438 \u043a\u045e\u0440\u0433\u0430\u043d\u043b\u0430\u0440 \u0431\u043e\u0440\u043c\u0438? \u04b2\u043e\u0437\u0438\u0440\u0433\u0438 \u043a\u045e\u0440\u0438\u043d\u0438\u0448\u0438 \u049b\u0430\u043d\u0434\u0430\u0439? \u041e\u043b\u0434\u0438\u043d\u0433\u0438\u0441\u0438 \u0431\u0438\u043b\u0430\u043d \u0441\u043e\u043b\u0438\u0448\u0442\u0438\u0440\u0433\u0430\u043d\u0434\u0430 \u0431\u0443\u043d\u0438\u0441\u0438 \u0430\u0440\u0437\u0438\u043c\u0430\u0433\u0430\u043d \u0431\u0438\u0440 \u043d\u0430\u0440\u0441\u0430\u0434\u0430\u0439\u2013\u043a\u0443, \u0430\u0445\u0438\u0440!"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u00ab\u042d\u043d\u0434\u0438 \u0434\u0430\u0434\u0438\u043b \u0431\u045e\u043b, \u044d\u0439 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b! \u0414\u0430\u0434\u0438\u043b \u0431\u045e\u043b, \u044d\u0439 \u043e\u043b\u0438\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439 \u0401\u0448\u0443\u0430! \u042d\u0439 \u0441\u0443\u0440\u0433\u0443\u043d\u0434\u0430\u043d \u049b\u0430\u0439\u0442\u0438\u0431 \u043a\u0435\u043b\u0433\u0430\u043d \u0448\u0443 \u044e\u0440\u0442 \u0430\u04b3\u043b\u0438, \u044d\u043d\u0434\u0438 \u0434\u0430\u0434\u0438\u043b \u0431\u045e\u043b\u0438\u043d\u0433\u043b\u0430\u0440!\u00bb \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0448\u0443\u043d\u0434\u0430\u0439 \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u00ab\u041c\u0435\u043d\u0438\u043d\u0433 \u0420\u0443\u04b3\u0438\u043c \u04b3\u0430\u043b\u0438 \u04b3\u0430\u043c \u0441\u0438\u0437\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u043e\u0440\u0430\u043d\u0433\u0438\u0437\u0434\u0430, \u049b\u045e\u0440\u049b\u043c\u0430\u043d\u0433\u043b\u0430\u0440. \u0418\u0448\u0433\u0430 \u043a\u0438\u0440\u0438\u0448\u0438\u043d\u0433\u043b\u0430\u0440, \u0447\u0443\u043d\u043a\u0438 \u041c\u0438\u0441\u0440\u0434\u0430\u043d \u0447\u0438\u049b\u049b\u0430\u043d\u0438\u043d\u0433\u0438\u0437\u0434\u0430 \u0441\u0438\u0437\u043b\u0430\u0440\u0433\u0430 \u0431\u0435\u0440\u0433\u0430\u043d \u0432\u0430\u044a\u0434\u0430\u043c\u0433\u0430 \u0431\u0438\u043d\u043e\u0430\u043d \u041c\u0435\u043d \u0441\u0438\u0437\u043b\u0430\u0440 \u0431\u0438\u043b\u0430\u043d \u0431\u0438\u0440\u0433\u0430\u043c\u0430\u043d.\u00bb"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u00ab\u042d\u043d\u0434\u0438 \u0434\u0430\u0434\u0438\u043b \u0431\u045e\u043b, \u044d\u0439 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b! \u0414\u0430\u0434\u0438\u043b \u0431\u045e\u043b, \u044d\u0439 \u043e\u043b\u0438\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439 \u0401\u0448\u0443\u0430! \u042d\u0439 \u0441\u0443\u0440\u0433\u0443\u043d\u0434\u0430\u043d \u049b\u0430\u0439\u0442\u0438\u0431 \u043a\u0435\u043b\u0433\u0430\u043d \u0448\u0443 \u044e\u0440\u0442 \u0430\u04b3\u043b\u0438, \u044d\u043d\u0434\u0438 \u0434\u0430\u0434\u0438\u043b \u0431\u045e\u043b\u0438\u043d\u0433\u043b\u0430\u0440!\u00bb \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0448\u0443\u043d\u0434\u0430\u0439 \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u00ab\u041c\u0435\u043d\u0438\u043d\u0433 \u0420\u0443\u04b3\u0438\u043c \u04b3\u0430\u043b\u0438 \u04b3\u0430\u043c \u0441\u0438\u0437\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u043e\u0440\u0430\u043d\u0433\u0438\u0437\u0434\u0430, \u049b\u045e\u0440\u049b\u043c\u0430\u043d\u0433\u043b\u0430\u0440. \u0418\u0448\u0433\u0430 \u043a\u0438\u0440\u0438\u0448\u0438\u043d\u0433\u043b\u0430\u0440, \u0447\u0443\u043d\u043a\u0438 \u041c\u0438\u0441\u0440\u0434\u0430\u043d \u0447\u0438\u049b\u049b\u0430\u043d\u0438\u043d\u0433\u0438\u0437\u0434\u0430 \u0441\u0438\u0437\u043b\u0430\u0440\u0433\u0430 \u0431\u0435\u0440\u0433\u0430\u043d \u0432\u0430\u044a\u0434\u0430\u043c\u0433\u0430 \u0431\u0438\u043d\u043e\u0430\u043d \u041c\u0435\u043d \u0441\u0438\u0437\u043b\u0430\u0440 \u0431\u0438\u043b\u0430\u043d \u0431\u0438\u0440\u0433\u0430\u043c\u0430\u043d.\u00bb"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0410\u0445\u0438\u0440, \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u00ab\u0411\u0438\u0440\u043f\u0430\u0441\u0434\u0430\u043d \u043a\u0435\u0439\u0438\u043d \u044f\u043d\u0430 \u0435\u0440\u0443 \u043e\u0441\u043c\u043e\u043d\u043d\u0438, \u0434\u0435\u043d\u0433\u0438\u0437 \u0432\u0430 \u049b\u0443\u0440\u0443\u049b\u043b\u0438\u043a\u043d\u0438 \u043b\u0430\u0440\u0437\u0430\u0433\u0430 \u0441\u043e\u043b\u0430\u043c\u0430\u043d."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0411\u0430\u0440\u0447\u0430 \u0445\u0430\u043b\u049b\u043b\u0430\u0440\u043d\u0438 \u0442\u0438\u0442\u0440\u0430\u0442\u0430\u043c\u0430\u043d, \u0443\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u0445\u0430\u0437\u0438\u043d\u0430\u0441\u0438 \u0448\u0443 \u0435\u0440\u0433\u0430 \u043a\u0435\u043b\u0442\u0438\u0440\u0438\u043b\u0430\u0434\u0438.\u00bb \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u00ab\u040e\u0448\u0430\u043d\u0434\u0430 \u041c\u0435\u043d \u043c\u0430\u043d\u0430 \u0448\u0443 \u041c\u0430\u044a\u0431\u0430\u0434\u043d\u0438 \u04b3\u0430\u0448\u0430\u043c\u0430\u0442 \u0431\u0438\u043b\u0430\u043d \u0431\u043e\u0439 \u049b\u0438\u043b\u0430\u043c\u0430\u043d.\u00bb"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u00ab\u041a\u0443\u043c\u0443\u0448 \u041c\u0435\u043d\u0438\u043a\u0438, \u043e\u043b\u0442\u0438\u043d \u04b3\u0430\u043c \u041c\u0435\u043d\u0438\u043a\u0438\u0434\u0438\u0440.\u00bb"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0430\u0439\u0442\u043c\u043e\u049b\u0434\u0430: \u00ab\u0411\u0443 \u041c\u0430\u044a\u0431\u0430\u0434\u043d\u0438\u043d\u0433 \u043a\u0435\u0439\u0438\u043d\u0433\u0438 \u0448\u0443\u04b3\u0440\u0430\u0442\u0438 \u043e\u043b\u0434\u0438\u043d\u0433\u0438\u0441\u0438\u0434\u0430\u043d \u04b3\u0430\u043c \u0443\u043b\u0443\u0493\u0440\u043e\u049b \u0431\u045e\u043b\u0430\u0434\u0438.\u00bb \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u00ab\u041c\u0430\u043d\u0430 \u0448\u0443 \u0435\u0440\u0433\u0430 \u0442\u0438\u043d\u0447\u043b\u0438\u043a \u0430\u0442\u043e \u049b\u0438\u043b\u0430\u043c\u0430\u043d.\u00bb\u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0428\u043e\u04b3 \u0414\u043e\u0440\u043e \u04b3\u0443\u043a\u043c\u0440\u043e\u043d\u043b\u0438\u0433\u0438\u043d\u0438\u043d\u0433 \u0438\u043a\u043a\u0438\u043d\u0447\u0438 \u0439\u0438\u043b\u0438, \u0442\u045e\u049b\u049b\u0438\u0437\u0438\u043d\u0447\u0438 \u043e\u0439\u043d\u0438\u043d\u0433 \u0439\u0438\u0433\u0438\u0440\u043c\u0430 \u0442\u045e\u0440\u0442\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438\u0434\u0430 \u0425\u0430\u0433\u0433\u0435\u0439 \u043f\u0430\u0439\u0493\u0430\u043c\u0431\u0430\u0440 \u043e\u0440\u049b\u0430\u043b\u0438 \u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u049b\u0443\u0439\u0438\u0434\u0430\u0433\u0438 \u0441\u045e\u0437\u043b\u0430\u0440\u0438 \u0430\u0451\u043d \u0431\u045e\u043b\u0434\u0438."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0425\u0430\u0433\u0433\u0435\u0439 \u0440\u0443\u04b3\u043e\u043d\u0438\u0439\u043b\u0430\u0440\u0433\u0430 \u0434\u0435\u0434\u0438:\n\u2014 \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c \u0448\u0443\u043d\u0434\u0430\u0439 \u0430\u0439\u0442\u043c\u043e\u049b\u0434\u0430: \u201c\u0420\u0443\u04b3\u043e\u043d\u0438\u0439\u043b\u0430\u0440\u0434\u0430\u043d \u049b\u043e\u043d\u0443\u043d \u0431\u045e\u0439\u0438\u0447\u0430 \u049b\u0443\u0439\u0438\u0434\u0430\u0433\u0438 \u04b3\u043e\u043b\u0430\u0442\u043b\u0430\u0440\u043d\u0438 \u0430\u0436\u0440\u0438\u043c \u049b\u0438\u043b\u0438\u0448\u043d\u0438 \u0441\u045e\u0440\u0430"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0430\u0433\u0430\u0440 \u0431\u0438\u0440\u043e\u0440\u0442\u0430\u0441\u0438 \u043a\u0438\u0439\u0438\u043c\u0438\u043d\u0438\u043d\u0433 \u044d\u0442\u0430\u0433\u0438\u0434\u0430 \u049b\u0443\u0440\u0431\u043e\u043d\u043b\u0438\u043a\u0434\u0430\u043d \u043e\u043b\u0438\u043d\u0433\u0430\u043d \u043c\u0443\u049b\u0430\u0434\u0434\u0430\u0441 \u0433\u045e\u0448\u0442\u043d\u0438 \u043e\u043b\u0438\u0431 \u043a\u0435\u0442\u0430\u0451\u0442\u0433\u0430\u043d \u0431\u045e\u043b\u0441\u0430, \u045e\u0448\u0430 \u043a\u0438\u0439\u0438\u043c\u043d\u0438\u043d\u0433 \u044d\u0442\u0430\u0433\u0438 \u0431\u0438\u043b\u0430\u043d \u043d\u043e\u043d, \u0448\u045e\u0440\u0432\u0430, \u0448\u0430\u0440\u043e\u0431, \u0437\u0430\u0439\u0442\u0443\u043d \u043c\u043e\u0439\u0438 \u0451\u043a\u0438 \u0431\u043e\u0448\u049b\u0430 \u0435\u0433\u0443\u043b\u0438\u043a\u043b\u0430\u0440\u0433\u0430 \u0442\u0435\u0433\u0438\u0431 \u043a\u0435\u0442\u0441\u0430, \u045e\u0448\u0430 \u043d\u0430\u0440\u0441\u0430\u043b\u0430\u0440 \u04b3\u0430\u043c \u043c\u0443\u049b\u0430\u0434\u0434\u0430\u0441 \u04b3\u0438\u0441\u043e\u0431\u043b\u0430\u043d\u0430\u0434\u0438\u043c\u0438?\u201d\n\u0420\u0443\u04b3\u043e\u043d\u0438\u0439\u043b\u0430\u0440 \u0425\u0430\u0433\u0433\u0435\u0439\u0433\u0430:\n\u2014 \u0419\u045e\u049b, \u2014 \u0434\u0435\u0431 \u0436\u0430\u0432\u043e\u0431 \u0431\u0435\u0440\u0434\u0438\u043b\u0430\u0440."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0425\u0430\u0433\u0433\u0435\u0439 \u044f\u043d\u0430 \u0441\u045e\u0440\u0430\u0434\u0438:\n\u2014 \u0410\u0433\u0430\u0440 \u0431\u0438\u0440\u043e\u0440\u0442\u0430\u0441\u0438 \u0438\u043d\u0441\u043e\u043d \u0436\u0430\u0441\u0430\u0434\u0438\u0433\u0430 \u0442\u0435\u043a\u043a\u0430\u043d\u0438 \u0443\u0447\u0443\u043d \u04b3\u0430\u0440\u043e\u043c \u0431\u045e\u043b\u0441\u0430\u2013\u044e, \u044e\u049b\u043e\u0440\u0438\u0434\u0430 \u0430\u0439\u0442\u0438\u0431 \u045e\u0442\u0438\u043b\u0433\u0430\u043d \u0435\u0433\u0443\u043b\u0438\u043a\u043b\u0430\u0440\u0434\u0430\u043d \u0431\u0438\u0440\u0438\u0433\u0430 \u0442\u0435\u0433\u0441\u0430, \u045e\u0448\u0430 \u0435\u0433\u0443\u043b\u0438\u043a\u043b\u0430\u0440 \u04b3\u0430\u0440\u043e\u043c \u04b3\u0438\u0441\u043e\u0431\u043b\u0430\u043d\u0430\u0434\u0438\u043c\u0438?\n\u0420\u0443\u04b3\u043e\u043d\u0438\u0439\u043b\u0430\u0440:\n\u2014 \u04b2\u0430, \u2014 \u0434\u0435\u0431 \u0436\u0430\u0432\u043e\u0431 \u0431\u0435\u0440\u0434\u0438\u043b\u0430\u0440."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0428\u0443\u043d\u0434\u0430 \u0425\u0430\u0433\u0433\u0435\u0439 \u0434\u0435\u0434\u0438:\n\u2014 \u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u201c\u041c\u0430\u043d\u0430 \u0431\u0443 \u043e\u0434\u0430\u043c\u043b\u0430\u0440 \u04b3\u0430\u043c, \u0431\u0443 \u0445\u0430\u043b\u049b \u04b3\u0430\u043c \u043e\u043b\u0434\u0438\u043c\u0434\u0430 \u0445\u0443\u0434\u0434\u0438 \u0448\u0443\u043d\u0434\u0430\u0439\u0434\u0438\u0440. \u0423\u043b\u0430\u0440 \u049b\u0438\u043b\u0433\u0430\u043d \u0438\u0448\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u04b3\u0430\u043c\u043c\u0430\u0441\u0438, \u0443\u043b\u0430\u0440 \u043a\u0435\u043b\u0442\u0438\u0440\u0433\u0430\u043d \u04b3\u0430\u0440 \u0431\u0438\u0440 \u043d\u0430\u0437\u0440 \u04b3\u0430\u043c \u04b3\u0430\u0440\u043e\u043c\u0434\u0438\u0440."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u042d\u043d\u0434\u0438 \u0441\u0438\u0437\u0433\u0430 \u043d\u0438\u043c\u0430 \u0431\u045e\u043b\u0433\u0430\u043d\u0438 \u04b3\u0430\u049b\u0438\u0434\u0430 \u045e\u0439\u043b\u0430\u0431 \u043a\u045e\u0440\u0438\u043d\u0433. \u041c\u0435\u043d\u0438\u043d\u0433 \u041c\u0430\u044a\u0431\u0430\u0434\u0438\u043c\u043d\u0438 \u049b\u0430\u0439\u0442\u0430 \u049b\u0443\u0440\u0438\u0448\u043d\u0438 \u0431\u043e\u0448\u043b\u0430\u043c\u0430\u0441\u0438\u043d\u0433\u0438\u0437\u0434\u0430\u043d \u043e\u043b\u0434\u0438\u043d \u043d\u0438\u043c\u0430\u043b\u0430\u0440 \u0431\u045e\u043b\u0433\u0430\u043d\u0438 \u04b3\u0430\u049b\u0438\u0434\u0430 \u045e\u0439\u043b\u0430\u0431 \u043a\u045e\u0440\u0438\u043d\u0433."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "20 \u0447\u043e\u0440\u0430 \u0434\u043e\u043d \u043e\u043b\u0430\u043c\u0430\u043d \u0434\u0435\u0431, \u0445\u0438\u0440\u043c\u043e\u043d\u0433\u0430 \u0431\u043e\u0440\u0433\u0430\u043d\u0438\u043d\u0433\u0438\u0437\u0434\u0430, \u0443 \u0435\u0440\u0434\u0430 \u0444\u0430\u049b\u0430\u0442 10 \u0447\u043e\u0440\u0430 \u0434\u043e\u043d \u0431\u045e\u043b\u0430\u0440\u0434\u0438. \u0425\u0443\u043c\u0434\u0430\u043d 50 \u043a\u045e\u0437\u0430 \u0448\u0430\u0440\u043e\u0431 \u043e\u043b\u0430\u043c\u0430\u043d \u0434\u0435\u0431, \u0431\u043e\u0440\u0433\u0430\u043d\u0438\u043d\u0433\u0438\u0437\u0434\u0430 \u044d\u0441\u0430 \u0444\u0430\u049b\u0430\u0442 20 \u043a\u045e\u0437\u0430 \u0448\u0430\u0440\u043e\u0431 \u0442\u043e\u043f\u0430\u0440\u0434\u0438\u043d\u0433\u0438\u0437.\u201d"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u042d\u0433\u0430\u043c\u0438\u0437 \u0434\u0435\u043c\u043e\u049b\u0434\u0430: \u201c\u041c\u0435\u04b3\u043d\u0430\u0442\u0438\u043d\u0433\u0438\u0437\u043d\u0438\u043d\u0433 \u0431\u0430\u0440\u0447\u0430 \u0441\u0430\u043c\u0430\u0440\u0430\u0441\u0438\u043d\u0438 \u0439\u045e\u049b \u049b\u0438\u043b\u0430\u0439 \u0434\u0435\u0431 \u0433\u0430\u0440\u043c\u0441\u0435\u043b, \u0448\u0438\u0440\u0430 \u0432\u0430 \u0434\u045e\u043b \u044e\u0431\u043e\u0440\u0434\u0438\u043c. \u0428\u0443\u043d\u0434\u0430 \u04b3\u0430\u043c \u041c\u0435\u043d\u0433\u0430 \u049b\u0430\u0439\u0442\u0438\u0448\u043d\u0438 \u0440\u0430\u0434 \u049b\u0438\u043b\u0434\u0438\u043d\u0433\u0438\u0437."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0410\u043c\u043c\u043e \u0431\u0443\u0433\u0443\u043d, \u0442\u045e\u049b\u049b\u0438\u0437\u0438\u043d\u0447\u0438 \u043e\u0439\u043d\u0438\u043d\u0433 \u0439\u0438\u0433\u0438\u0440\u043c\u0430 \u0442\u045e\u0440\u0442\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438\u0434\u0430\u043d \u0431\u043e\u0448\u043b\u0430\u0431, \u0441\u0438\u0437\u043b\u0430\u0440\u0433\u0430 \u043d\u0438\u043c\u0430 \u0431\u045e\u043b\u0438\u0448\u0438\u0433\u0430 \u044d\u044a\u0442\u0438\u0431\u043e\u0440 \u0431\u0435\u0440\u0438\u043d\u0433\u043b\u0430\u0440. \u041c\u0430\u044a\u0431\u0430\u0434\u0438\u043c\u043d\u0438\u043d\u0433 \u043f\u043e\u0439\u0434\u0435\u0432\u043e\u0440\u0438 \u0451\u0442\u049b\u0438\u0437\u0438\u043b\u0433\u0430\u043d\u0438\u0433\u0430 \u0430\u043d\u0447\u0430 \u0431\u045e\u043b\u0434\u0438. \u040e\u0448\u0430\u043d\u0434\u0430\u043d \u0431\u0435\u0440\u0438, \u044d\u043a\u043a\u0430\u043d \u0443\u0440\u0443\u0493\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437 \u0434\u0435\u044f\u0440\u043b\u0438 \u04b3\u043e\u0441\u0438\u043b \u0431\u0435\u0440\u043c\u0430\u0441 \u044d\u0434\u0438, \u0442\u043e\u043a\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437, \u0430\u043d\u0436\u0438\u0440\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437, \u0430\u043d\u043e\u0440\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437 \u0432\u0430 \u0437\u0430\u0439\u0442\u0443\u043d \u0434\u0430\u0440\u0430\u0445\u0442\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437\u043d\u0438\u043d\u0433 \u04b3\u043e\u0441\u0438\u043b\u0438 \u0431\u0435\u0431\u0430\u0440\u0430\u043a\u0430 \u044d\u0434\u0438. \u0410\u043c\u043c\u043e \u0431\u0443\u0433\u0443\u043d\u0434\u0430\u043d \u0431\u043e\u0448\u043b\u0430\u0431, \u041c\u0435\u043d \u0441\u0438\u0437\u043b\u0430\u0440\u0433\u0430 \u0431\u0430\u0440\u0430\u043a\u0430 \u0431\u0435\u0440\u0430\u043c\u0430\u043d.\u201d"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0410\u043c\u043c\u043e \u0431\u0443\u0433\u0443\u043d, \u0442\u045e\u049b\u049b\u0438\u0437\u0438\u043d\u0447\u0438 \u043e\u0439\u043d\u0438\u043d\u0433 \u0439\u0438\u0433\u0438\u0440\u043c\u0430 \u0442\u045e\u0440\u0442\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438\u0434\u0430\u043d \u0431\u043e\u0448\u043b\u0430\u0431, \u0441\u0438\u0437\u043b\u0430\u0440\u0433\u0430 \u043d\u0438\u043c\u0430 \u0431\u045e\u043b\u0438\u0448\u0438\u0433\u0430 \u044d\u044a\u0442\u0438\u0431\u043e\u0440 \u0431\u0435\u0440\u0438\u043d\u0433\u043b\u0430\u0440. \u041c\u0430\u044a\u0431\u0430\u0434\u0438\u043c\u043d\u0438\u043d\u0433 \u043f\u043e\u0439\u0434\u0435\u0432\u043e\u0440\u0438 \u0451\u0442\u049b\u0438\u0437\u0438\u043b\u0433\u0430\u043d\u0438\u0433\u0430 \u0430\u043d\u0447\u0430 \u0431\u045e\u043b\u0434\u0438. \u040e\u0448\u0430\u043d\u0434\u0430\u043d \u0431\u0435\u0440\u0438, \u044d\u043a\u043a\u0430\u043d \u0443\u0440\u0443\u0493\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437 \u0434\u0435\u044f\u0440\u043b\u0438 \u04b3\u043e\u0441\u0438\u043b \u0431\u0435\u0440\u043c\u0430\u0441 \u044d\u0434\u0438, \u0442\u043e\u043a\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437, \u0430\u043d\u0436\u0438\u0440\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437, \u0430\u043d\u043e\u0440\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437 \u0432\u0430 \u0437\u0430\u0439\u0442\u0443\u043d \u0434\u0430\u0440\u0430\u0445\u0442\u043b\u0430\u0440\u0438\u043d\u0433\u0438\u0437\u043d\u0438\u043d\u0433 \u04b3\u043e\u0441\u0438\u043b\u0438 \u0431\u0435\u0431\u0430\u0440\u0430\u043a\u0430 \u044d\u0434\u0438. \u0410\u043c\u043c\u043e \u0431\u0443\u0433\u0443\u043d\u0434\u0430\u043d \u0431\u043e\u0448\u043b\u0430\u0431, \u041c\u0435\u043d \u0441\u0438\u0437\u043b\u0430\u0440\u0433\u0430 \u0431\u0430\u0440\u0430\u043a\u0430 \u0431\u0435\u0440\u0430\u043c\u0430\u043d.\u201d"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u040e\u0448\u0430 \u043a\u0443\u043d\u0438, \u043e\u0439\u043d\u0438\u043d\u0433 \u0439\u0438\u0433\u0438\u0440\u043c\u0430 \u0442\u045e\u0440\u0442\u0438\u043d\u0447\u0438 \u043a\u0443\u043d\u0438\u0434\u0430 \u042d\u0433\u0430\u043c\u0438\u0437 \u044f\u043d\u0430 \u0445\u0430\u0431\u0430\u0440 \u044e\u0431\u043e\u0440\u0438\u0431, \u049b\u0443\u0439\u0438\u0434\u0430\u0433\u0438 \u0441\u045e\u0437\u043b\u0430\u0440\u043d\u0438 \u0425\u0430\u0433\u0433\u0435\u0439 \u043f\u0430\u0439\u0493\u0430\u043c\u0431\u0430\u0440\u0433\u0430 \u0430\u0451\u043d \u049b\u0438\u043b\u0434\u0438"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u042f\u04b3\u0443\u0434\u043e \u04b3\u043e\u043a\u0438\u043c\u0438 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b\u0433\u0430 \u0430\u0439\u0442: \u201c\u041e\u0441\u043c\u043e\u043d \u0432\u0430 \u0435\u0440\u043d\u0438 \u043b\u0430\u0440\u0437\u0430\u0433\u0430 \u0441\u043e\u043b\u0430\u043c\u0430\u043d."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u0428\u043e\u04b3\u043b\u0438\u043a\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u0442\u0430\u0445\u0442\u043b\u0430\u0440\u0438\u043d\u0438 \u0430\u0493\u0434\u0430\u0440\u0430\u043c\u0430\u043d, \u0445\u0430\u043b\u049b\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u0448\u043e\u04b3\u043b\u0438\u043a\u043b\u0430\u0440\u0438\u0433\u0430 \u0431\u0430\u0440\u04b3\u0430\u043c \u0431\u0435\u0440\u0430\u043c\u0430\u043d. \u0416\u0430\u043d\u0433 \u0430\u0440\u0430\u0432\u0430\u043b\u0430\u0440\u0438 \u0432\u0430 \u0443\u043b\u0430\u0440\u043d\u0438\u043d\u0433 \u0443\u0441\u0442\u0438\u0434\u0430\u0433\u0438\u043b\u0430\u0440\u043d\u0438 \u0430\u0493\u0434\u0430\u0440\u0430\u043c\u0430\u043d. \u041e\u0442\u043b\u0430\u0440, \u0447\u0430\u0432\u0430\u043d\u0434\u043e\u0437\u043b\u0430\u0440 \u0439\u0438\u049b\u0438\u043b\u0430\u0434\u0438, \u04b3\u0430\u043c\u043c\u0430 \u0431\u0438\u0440\u2013\u0431\u0438\u0440\u0438\u043d\u0438\u043d\u0433 \u049b\u0438\u043b\u0438\u0447\u0438\u0434\u0430\u043d \u04b3\u0430\u043b\u043e\u043a \u0431\u045e\u043b\u0430\u0434\u0438."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u042d\u0433\u0430\u043c\u0438\u0437\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440: \u00ab\u042d\u0439 \u049b\u0443\u043b\u0438\u043c \u0428\u0430\u043b\u0442\u0438\u0451\u043b \u045e\u0493\u043b\u0438 \u0417\u0430\u0440\u0443\u0431\u0430\u0431\u0435\u043b, \u045e\u0448\u0430 \u043a\u0443\u043d\u0438, \u2014 \u0434\u0435\u043c\u043e\u049b\u0434\u0430 \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c, \u2014 \u0441\u0435\u043d\u0438 \u040e\u0437 \u043c\u0443\u04b3\u0440 \u0443\u0437\u0443\u0433\u0438\u043c\u0434\u0430\u0439 \u049b\u0438\u043b\u0430\u043c\u0430\u043d, \u0447\u0443\u043d\u043a\u0438 \u041c\u0435\u043d \u0441\u0435\u043d\u0438 \u0442\u0430\u043d\u043b\u0430\u0433\u0430\u043d\u043c\u0430\u043d.\u00bb \u0421\u0430\u0440\u0432\u0430\u0440\u0438 \u041e\u043b\u0430\u043c\u043d\u0438\u043d\u0433 \u043a\u0430\u043b\u043e\u043c\u0438 \u0448\u0443\u0434\u0438\u0440.\u201d"
                }
            }
        }
    }
}