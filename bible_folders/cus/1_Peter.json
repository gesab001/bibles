                "6": {
                    "verse_nr": 6,
                    "verse": "\u56e0 \u6b64 \uff0c \u4f60 \u4eec \u662f \u5927 \u6709 \u559c \u4e50 \uff1b \u4f46 \u5982 \u4eca \uff0c \u5728 \u767e \u822c \u7684 \u8bd5 \u70bc \u4e2d \u6682 \u65f6 \u5fe7 \u6101 \uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u53eb \u4f60 \u4eec \u7684 \u4fe1 \u5fc3 \u65e2 \u88ab \u8bd5 \u9a8c \uff0c \u5c31 \u6bd4 \u90a3 \u88ab \u706b \u8bd5 \u9a8c \u4ecd \u7136 \u80fd \u574f \u7684 \u91d1 \u5b50 \u66f4 \u663e \u5b9d \u8d35 \uff0c \u53ef \u4ee5 \u5728 \u8036 \u7a23 \u57fa \u7763 \u663e \u73b0 \u7684 \u65f6 \u5019 \u5f97 \u7740 \u79f0 \u8d5e \u3001 \u8363 \u8000 \u3001 \u5c0a \u8d35 \u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60 \u4eec \u867d \u7136 \u6ca1 \u6709 \u89c1 \u8fc7 \u4ed6 \uff0c \u5374 \u662f \u7231 \u4ed6 \uff1b \u5982 \u4eca \u867d \u4e0d \u5f97 \u770b \u89c1 \uff0c \u5374 \u56e0 \u4fe1 \u4ed6 \u5c31 \u6709 \u8bf4 \u4e0d \u51fa \u6765 \u3001 \u6ee1 \u6709 \u8363 \u5149 \u7684 \u5927 \u559c \u4e50 \uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5e76 \u4e14 \u5f97 \u7740 \u4f60 \u4eec \u4fe1 \u5fc3 \u7684 \u679c \u6548 \uff0c \u5c31 \u662f \u7075 \u9b42 \u7684 \u6551 \u6069 \u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8bba \u5230 \u8fd9 \u6551 \u6069 \uff0c \u90a3 \u9884 \u5148 \u8bf4 \u4f60 \u4eec \u8981 \u5f97 \u6069 \u5178 \u7684 \u4f17 \u5148 \u77e5 \u65e9 \u5df2 \u8be6 \u7ec6 \u7684 \u5bfb \u6c42 \u8003 \u5bdf \uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5c31 \u662f \u8003 \u5bdf \u5728 \u4ed6 \u4eec \u5fc3 \u91cc \u57fa \u7763 \u7684 \u7075 \uff0c \u9884 \u5148 \u8bc1 \u660e \u57fa \u7763 \u53d7 \u82e6 \u96be \uff0c \u5f8c \u6765 \u5f97 \u8363 \u8000 \uff0c \u662f \u6307 \u7740 \u751a \u9ebd \u65f6 \u5019 \uff0c \u5e76 \u600e \u6837 \u7684 \u65f6 \u5019 \u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4ed6 \u4eec \u5f97 \u4e86 \u542f \u793a \uff0c \u77e5 \u9053 \u4ed6 \u4eec \u6240 \u4f20 \u8bb2 \uff08 \u539f \u6587 \u662f \u670d \u4e8b \uff09 \u7684 \u4e00 \u5207 \u4e8b \uff0c \u4e0d \u662f \u4e3a \u81ea \u5df1 \uff0c \u4e43 \u662f \u4e3a \u4f60 \u4eec \u3002 \u90a3 \u9760 \u7740 \u4ece \u5929 \u4e0a \u5dee \u6765 \u7684 \u5723 \u7075 \u4f20 \u798f \u97f3 \u7ed9 \u4f60 \u4eec \u7684 \u4eba \uff0c \u73b0 \u5728 \u5c06 \u8fd9 \u4e9b \u4e8b \u62a5 \u7ed9 \u4f60 \u4eec \uff1b \u5929 \u4f7f \u4e5f \u613f \u610f \u8be6 \u7ec6 \u5bdf \u770b \u8fd9 \u4e9b \u4e8b \u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6240 \u4ee5 \u8981 \u7ea6 \u675f \u4f60 \u4eec \u7684 \u5fc3 \uff0c \uff08 \u539f \u6587 \u662f \u675f \u4e0a \u4f60 \u4eec \u5fc3 \u4e2d \u7684 \u8170 \uff09 \uff0c \u8c28 \u614e \u81ea \u5b88 \uff0c \u4e13 \u5fc3 \u76fc \u671b \u8036 \u7a23 \u57fa \u7763 \u663e \u73b0 \u7684 \u65f6 \u5019 \u6240 \u5e26 \u6765 \u7ed9 \u4f60 \u4eec \u7684 \u6069 \u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60 \u4eec \u65e2 \u4f5c \u987a \u547d \u7684 \u513f \u5973 \uff0c \u5c31 \u4e0d \u8981 \u6548 \u6cd5 \u4ece \u524d \u8499 \u6627 \u65e0 \u77e5 \u7684 \u65f6 \u5019 \u90a3 \u653e \u7eb5 \u79c1 \u6b32 \u7684 \u6837 \u5b50 \u3002\n"
