    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4e3a \u57fa \u7763 \u8036 \u7a23 \u88ab \u56da \u7684 \u4fdd \u7f57 \uff0c \u540c \u5144 \u5f1f \u63d0 \u6469 \u592a \u5199 \u4fe1 \u7ed9 \u6211 \u4eec \u6240 \u4eb2 \u7231 \u7684 \u540c \u5de5 \u8153 \u5229 \u95e8 \uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u548c \u59b9 \u5b50 \u4e9a \u8153 \u4e9a \u5e76 \u4e0e \u6211 \u4eec \u540c \u5f53 \u5175 \u7684 \u4e9a \u57fa \u5e03 \uff0c \u4ee5 \u53ca \u5728 \u4f60 \u5bb6 \u7684 \u6559 \u4f1a \u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u613f \u6069 \u60e0 \u3001 \u5e73 \u5b89 \u4ece \u795e \u6211 \u4eec \u7684 \u7236 \u548c \u4e3b \u8036 \u7a23 \u57fa \u7763 \u5f52 \u4e0e \u4f60 \u4eec \uff01\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211 \u7977 \u544a \u7684 \u65f6 \u5019 \u63d0 \u5230 \u4f60 \uff0c \u5e38 \u4e3a \u4f60 \u611f \u8c22 \u6211 \u7684 \u795e \uff1b\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0 \u542c \u8bf4 \u4f60 \u7684 \u7231 \u5fc3 \u5e76 \u4f60 \u5411 \u4e3b \u8036 \u7a23 \u548c \u4f17 \u5723 \u5f92 \u7684 \u4fe1 \u5fc3 \uff08 \u6216 \u4f5c \uff1a \u56e0 \u542c \u8bf4 \u4f60 \u5411 \u4e3b \u8036 \u7a23 \u548c \u4f17 \u5723 \u5f92 \u6709 \u7231 \u5fc3 \u6709 \u4fe1 \u5fc3 \uff09 \u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u613f \u4f60 \u4e0e \u4eba \u6240 \u540c \u6709 \u7684 \u4fe1 \u5fc3 \u663e \u51fa \u529f \u6548 \uff0c \u4f7f \u4eba \u77e5 \u9053 \u4f60 \u4eec \u5404 \u6837 \u5584 \u4e8b \u90fd \u662f \u4e3a \u57fa \u7763 \u505a \u7684 \u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5144 \u5f1f \u963f \uff0c \u6211 \u4e3a \u4f60 \u7684 \u7231 \u5fc3 \uff0c \u5927 \u6709 \u5feb \u4e50 \uff0c \u5927 \u5f97 \u5b89 \u6170 \uff0c \u56e0 \u4f17 \u5723 \u5f92 \u7684 \u5fc3 \u4ece \u4f60 \u5f97 \u4e86 \u7545 \u5feb \u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211 \u867d \u7136 \u9760 \u7740 \u57fa \u7763 \u80fd \u653e \u80c6 \u5429 \u5490 \u4f60 \u5408 \u5b9c \u7684 \u4e8b \uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7136 \u800c \u50cf \u6211 \u8fd9 \u6709 \u5e74 \u7eaa \u7684 \u4fdd \u7f57 \uff0c \u73b0 \u5728 \u53c8 \u662f \u4e3a \u57fa \u7763 \u8036 \u7a23 \u88ab \u56da \u7684 \uff0c \u5b81 \u53ef \u51ed \u7740 \u7231 \u5fc3 \u6c42 \u4f60 \uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5c31 \u662f \u4e3a \u6211 \u5728 \u6346 \u9501 \u4e2d \u6240 \u751f \u7684 \u513f \u5b50 \u963f \u5c3c \u897f \u6bcd \uff08 \u5c31 \u662f \u6709 \u76ca \u5904 \u7684 \u610f \u601d \uff09 \u6c42 \u4f60 \u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6 \u4ece \u524d \u4e0e \u4f60 \u6ca1 \u6709 \u76ca \u5904 \uff0c \u4f46 \u5982 \u4eca \u4e0e \u4f60 \u6211 \u90fd \u6709 \u76ca \u5904 \u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211 \u73b0 \u5728 \u6253 \u53d1 \u4ed6 \u4eb2 \u81ea \u56de \u4f60 \u90a3 \u91cc \u53bb \uff0c \u4ed6 \u662f \u6211 \u5fc3 \u4e0a \u7684 \u4eba \u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211 \u672c \u6765 \u6709 \u610f \u5c06 \u4ed6 \u7559 \u4e0b \uff0c \u5728 \u6211 \u4e3a \u798f \u97f3 \u6240 \u53d7 \u7684 \u6346 \u9501 \u4e2d \u66ff \u4f60 \u4f3a \u5019 \u6211 \u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f46 \u4e0d \u77e5 \u9053 \u4f60 \u7684 \u610f \u601d \uff0c \u6211 \u5c31 \u4e0d \u613f \u610f \u8fd9 \u6837 \u884c \uff0c \u53eb \u4f60 \u7684 \u5584 \u884c \u4e0d \u662f \u51fa \u65bc \u52c9 \u5f3a \uff0c \u4e43 \u662f \u51fa \u65bc \u7518 \u5fc3 \u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4ed6 \u6682 \u65f6 \u79bb \u5f00 \u4f60 \uff0c \u6216 \u8005 \u662f \u53eb \u4f60 \u6c38 \u8fdc \u5f97 \u7740 \u4ed6 \uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4e0d \u518d \u662f \u5974 \u4ec6 \uff0c \u4e43 \u662f \u9ad8 \u8fc7 \u5974 \u4ec6 \uff0c \u662f \u4eb2 \u7231 \u7684 \u5144 \u5f1f \u3002 \u5728 \u6211 \u5b9e \u5728 \u662f \u5982 \u6b64 \uff0c \u4f55 \u51b5 \u5728 \u4f60 \u5462 \uff01 \u8fd9 \u4e5f \u4e0d \u62d8 \u662f \u6309 \u8089 \u4f53 \u8bf4 \uff0c \u662f \u6309 \u4e3b \u8bf4 \uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60 \u82e5 \u4ee5 \u6211 \u4e3a \u540c \u4f34 \uff0c \u5c31 \u6536 \u7eb3 \u4ed6 \uff0c \u5982 \u540c \u6536 \u7eb3 \u6211 \u4e00 \u6837 \u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6 \u82e5 \u4e8f \u8d1f \u4f60 \uff0c \u6216 \u6b20 \u4f60 \u751a \u9ebd \uff0c \u90fd \u5f52 \u5728 \u6211 \u7684 \u8d26 \u4e0a \uff0c\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6211 \u5fc5 \u507f \u8fd8 \u3002 \u8fd9 \u662f \u6211 \u4fdd \u7f57 \u4eb2 \u7b14 \u5199 \u7684 \u3002 \u6211 \u5e76 \u4e0d \u7528 \u5bf9 \u4f60 \u8bf4 \uff0c \u8fde \u4f60 \u81ea \u5df1 \u4e5f \u662f \u4e8f \u6b20 \u65bc \u6211 \u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5144 \u5f1f \u963f \uff0c \u671b \u4f60 \u4f7f \u6211 \u5728 \u4e3b \u91cc \u56e0 \u4f60 \u5f97 \u5feb \u4e50 \uff08 \u6216 \u4f5c \uff1a \u76ca \u5904 \uff09 \u5e76 \u671b \u4f60 \u4f7f \u6211 \u7684 \u5fc3 \u5728 \u57fa \u7763 \u91cc \u5f97 \u7545 \u5feb \u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211 \u5199 \u4fe1 \u7ed9 \u4f60 \uff0c \u6df1 \u4fe1 \u4f60 \u5fc5 \u987a \u670d \uff0c \u77e5 \u9053 \u4f60 \u6240 \u8981 \u884c \u7684 \uff0c \u5fc5 \u8fc7 \u65bc \u6211 \u6240 \u8bf4 \u7684 \u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u6b64 \u5916 \u4f60 \u8fd8 \u8981 \u7ed9 \u6211 \u9884 \u5907 \u4f4f \u5904 \uff1b \u56e0 \u4e3a \u6211 \u76fc \u671b \u85c9 \u7740 \u4f60 \u4eec \u7684 \u7977 \u544a \uff0c \u5fc5 \u8499 \u6069 \u5230 \u4f60 \u4eec \u90a3 \u91cc \u53bb \u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u4e3a \u57fa \u7763 \u8036 \u7a23 \u4e0e \u6211 \u540c \u5750 \u76d1 \u7684 \u4ee5 \u5df4 \u5f17 \u95ee \u4f60 \u5b89 \u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u4e0e \u6211 \u540c \u5de5 \u7684 \u9a6c \u53ef \u3001 \u4e9a \u91cc \u8fbe \u53e4 \u3001 \u5e95 \u9a6c \u3001 \u8def \u52a0 \u3001 \u4e5f \u90fd \u95ee \u4f60 \u5b89 \u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u613f \u6211 \u4eec \u4e3b \u8036 \u7a23 \u57fa \u7763 \u7684 \u6069 \u5e38 \u5728 \u4f60 \u7684 \u5fc3 \u91cc \u3002 \u963f \u4eec \uff01\n"
                }
            }
        }
    }
}