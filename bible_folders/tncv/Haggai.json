                "18": {
                    "verse_nr": "18",
                    "verse": "\u2018\u0e19\u0e31\u0e1a\u0e41\u0e15\u0e48\u0e27\u0e31\u0e19\u0e19\u0e35\u0e49\u0e40\u0e1b\u0e47\u0e19\u0e15\u0e49\u0e19\u0e44\u0e1b \u0e15\u0e31\u0e49\u0e07\u0e41\u0e15\u0e48\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e22\u0e35\u0e48\u0e2a\u0e34\u0e1a\u0e2a\u0e35\u0e48\u0e40\u0e14\u0e37\u0e2d\u0e19\u0e17\u0e35\u0e48\u0e40\u0e01\u0e49\u0e32\u0e19\u0e35\u0e49 \u0e08\u0e07\u0e15\u0e23\u0e36\u0e01\u0e15\u0e23\u0e2d\u0e07\u0e16\u0e36\u0e07\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e27\u0e32\u0e07\u0e10\u0e32\u0e19\u0e23\u0e32\u0e01\u0e02\u0e2d\u0e07\u0e1e\u0e23\u0e30\u0e27\u0e34\u0e2b\u0e32\u0e23\u0e02\u0e2d\u0e07\u0e2d\u0e07\u0e04\u0e4c\u0e1e\u0e23\u0e30\u0e1c\u0e39\u0e49\u0e40\u0e1b\u0e47\u0e19\u0e40\u0e08\u0e49\u0e32 \u0e08\u0e07\u0e43\u0e04\u0e23\u0e48\u0e04\u0e23\u0e27\u0e0d\u0e27\u0e48\u0e32"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0e21\u0e35\u0e40\u0e21\u0e25\u0e47\u0e14\u0e1e\u0e37\u0e0a\u0e43\u0e14\u0e46 \u0e40\u0e2b\u0e25\u0e37\u0e2d\u0e2d\u0e22\u0e39\u0e48\u0e43\u0e19\u0e22\u0e38\u0e49\u0e07\u0e09\u0e32\u0e07\u0e1a\u0e49\u0e32\u0e07? \u0e08\u0e27\u0e1a\u0e08\u0e19\u0e1a\u0e31\u0e14\u0e19\u0e35\u0e49\u0e40\u0e16\u0e32\u0e2d\u0e07\u0e38\u0e48\u0e19 \u0e21\u0e30\u0e40\u0e14\u0e37\u0e48\u0e2d \u0e17\u0e31\u0e1a\u0e17\u0e34\u0e21 \u0e41\u0e25\u0e30\u0e15\u0e49\u0e19\u0e21\u0e30\u0e01\u0e2d\u0e01\u0e44\u0e21\u0e48\u0e44\u0e14\u0e49\u0e1c\u0e25\u0e34\u0e1c\u0e25\u0e40\u0e25\u0e22 \u201c\u2006\u2018\u0e19\u0e31\u0e1a\u0e15\u0e31\u0e49\u0e07\u0e41\u0e15\u0e48\u0e27\u0e31\u0e19\u0e19\u0e35\u0e49\u0e44\u0e1b\u0e40\u0e23\u0e32\u0e08\u0e30\u0e2d\u0e27\u0e22\u0e1e\u0e23\u0e40\u0e08\u0e49\u0e32\u2019\u2006\u201d"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0e40\u0e28\u0e23\u0e38\u0e1a\u0e1a\u0e32\u0e40\u0e1a\u0e25\u0e40\u0e1b\u0e47\u0e19\u0e41\u0e2b\u0e27\u0e19\u0e15\u0e23\u0e32\u0e02\u0e2d\u0e07\u0e2d\u0e07\u0e04\u0e4c\u0e1e\u0e23\u0e30\u0e1c\u0e39\u0e49\u0e40\u0e1b\u0e47\u0e19\u0e40\u0e08\u0e49\u0e32 - \u0e1e\u0e23\u0e30\u0e14\u0e33\u0e23\u0e31\u0e2a\u0e02\u0e2d\u0e07\u0e2d\u0e07\u0e04\u0e4c\u0e1e\u0e23\u0e30\u0e1c\u0e39\u0e49\u0e40\u0e1b\u0e47\u0e19\u0e40\u0e08\u0e49\u0e32\u0e21\u0e32\u0e16\u0e36\u0e07\u0e2e\u0e31\u0e01\u0e01\u0e31\u0e22\u0e40\u0e1b\u0e47\u0e19\u0e04\u0e23\u0e31\u0e49\u0e07\u0e17\u0e35\u0e48\u0e2a\u0e2d\u0e07\u0e43\u0e19\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e22\u0e35\u0e48\u0e2a\u0e34\u0e1a\u0e2a\u0e35\u0e48\u0e02\u0e2d\u0e07\u0e40\u0e14\u0e37\u0e2d\u0e19\u0e19\u0e31\u0e49\u0e19\u0e27\u0e48\u0e32"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u201c\u0e08\u0e07\u0e1a\u0e2d\u0e01\u0e40\u0e28\u0e23\u0e38\u0e1a\u0e1a\u0e32\u0e40\u0e1a\u0e25\u0e1c\u0e39\u0e49\u0e27\u0e48\u0e32\u0e01\u0e32\u0e23\u0e02\u0e2d\u0e07\u0e22\u0e39\u0e14\u0e32\u0e2b\u0e4c\u0e27\u0e48\u0e32 \u0e40\u0e23\u0e32\u0e08\u0e30\u0e40\u0e02\u0e22\u0e48\u0e32\u0e1f\u0e49\u0e32\u0e2a\u0e27\u0e23\u0e23\u0e04\u0e4c\u0e41\u0e25\u0e30\u0e41\u0e1c\u0e48\u0e19\u0e14\u0e34\u0e19\u0e42\u0e25\u0e01"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u0e40\u0e23\u0e32\u0e08\u0e30\u0e04\u0e27\u0e48\u0e33\u0e23\u0e32\u0e0a\u0e1a\u0e31\u0e25\u0e25\u0e31\u0e07\u0e01\u0e4c\u0e41\u0e25\u0e30\u0e02\u0e22\u0e35\u0e49\u0e2d\u0e33\u0e19\u0e32\u0e08\u0e02\u0e2d\u0e07\u0e1a\u0e23\u0e23\u0e14\u0e32\u0e2d\u0e32\u0e13\u0e32\u0e08\u0e31\u0e01\u0e23\u0e15\u0e48\u0e32\u0e07\u0e0a\u0e32\u0e15\u0e34 \u0e40\u0e23\u0e32\u0e08\u0e30\u0e04\u0e27\u0e48\u0e33\u0e23\u0e16\u0e21\u0e49\u0e32\u0e28\u0e36\u0e01\u0e41\u0e25\u0e30\u0e1e\u0e25\u0e02\u0e31\u0e1a \u0e21\u0e49\u0e32\u0e28\u0e36\u0e01\u0e41\u0e25\u0e30\u0e04\u0e19\u0e02\u0e35\u0e48\u0e21\u0e49\u0e32\u0e02\u0e2d\u0e07\u0e1e\u0e27\u0e01\u0e40\u0e02\u0e32\u0e08\u0e30\u0e25\u0e49\u0e21\u0e25\u0e07 \u0e15\u0e48\u0e32\u0e07\u0e25\u0e49\u0e21\u0e25\u0e07\u0e14\u0e49\u0e27\u0e22\u0e14\u0e32\u0e1a\u0e02\u0e2d\u0e07\u0e1e\u0e35\u0e48\u0e19\u0e49\u0e2d\u0e07\u0e02\u0e2d\u0e07\u0e40\u0e02\u0e32"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u201c\u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c\u0e1c\u0e39\u0e49\u0e17\u0e23\u0e07\u0e24\u0e17\u0e18\u0e34\u0e4c\u0e1b\u0e23\u0e30\u0e01\u0e32\u0e28\u0e27\u0e48\u0e32 \u2018\u0e43\u0e19\u0e27\u0e31\u0e19\u0e19\u0e31\u0e49\u0e19\u0e40\u0e23\u0e32\u0e08\u0e30\u0e23\u0e31\u0e1a\u0e40\u0e08\u0e49\u0e32 \u0e40\u0e28\u0e23\u0e38\u0e1a\u0e1a\u0e32\u0e40\u0e1a\u0e25\u0e1a\u0e38\u0e15\u0e23\u0e40\u0e0a\u0e2d\u0e31\u0e25\u0e17\u0e34\u0e40\u0e2d\u0e25\u0e1c\u0e39\u0e49\u0e23\u0e31\u0e1a\u0e43\u0e0a\u0e49\u0e02\u0e2d\u0e07\u0e40\u0e23\u0e32\u2019 \u0e2d\u0e07\u0e04\u0e4c\u0e1e\u0e23\u0e30\u0e1c\u0e39\u0e49\u0e40\u0e1b\u0e47\u0e19\u0e40\u0e08\u0e49\u0e32\u0e1b\u0e23\u0e30\u0e01\u0e32\u0e28\u0e14\u0e31\u0e07\u0e19\u0e31\u0e49\u0e19 \u2018\u0e41\u0e25\u0e30\u0e40\u0e23\u0e32\u0e08\u0e30\u0e17\u0e33\u0e43\u0e2b\u0e49\u0e40\u0e08\u0e49\u0e32\u0e40\u0e1b\u0e47\u0e19\u0e14\u0e31\u0e48\u0e07\u0e41\u0e2b\u0e27\u0e19\u0e15\u0e23\u0e32\u0e02\u0e2d\u0e07\u0e40\u0e23\u0e32\u0e40\u0e1e\u0e23\u0e32\u0e30\u0e40\u0e23\u0e32\u0e44\u0e14\u0e49\u0e40\u0e25\u0e37\u0e2d\u0e01\u0e2a\u0e23\u0e23\u0e40\u0e08\u0e49\u0e32\u2019 \u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c\u0e1c\u0e39\u0e49\u0e17\u0e23\u0e07\u0e24\u0e17\u0e18\u0e34\u0e4c\u0e1b\u0e23\u0e30\u0e01\u0e32\u0e28\u0e14\u0e31\u0e07\u0e19\u0e31\u0e49\u0e19\u201d"
                }
            }
        }
