    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u062f\u0631 \u0631\u0648\u0632 \u0627\u0648\u0644 \u0645\u0627\u0647 \u0634\u0634\u0645 \u0627\u0632 \u0633\u0627\u0644 \u062f\u0648\u0645 \u062f\u0627\u0631\u06cc\u0648\u0634 \u067e\u0627\u062f\u0634\u0627\u0647\u060c \u06a9\u0644\u0627\u0645 \u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0647 \u0648\u0627\u0633\u0637\u0647 \u062d\u062c\u06cc \u0646\u0628\u06cc \u0648\u0628\u0647 \u0632\u0631\u0628\u0627\u0628\u0644 \u0628\u0646 \u0634\u0627\u0644\u062a\u06cc\u0626\u06cc\u0644 \u0648\u0627\u0644\u06cc \u06cc\u0647\u0648\u062f\u0627 \u0648 \u0628\u0647 \u06cc\u0647\u0648\u0634\u0639 \u0628\u0646 \u06cc\u0647\u0648\u0635\u0627\u062f\u0642 \u0631\u0626\u06cc\u0633 \u06a9\u0647\u0646\u0647 \u0631\u0633\u06cc\u062f\u0647\u060c \u06af\u0641\u062a:"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u06cc\u0647\u0648\u0647 \u0635\u0628\u0627\u06cc\u0648\u062a \u062a\u06a9\u0644\u0645 \u0646\u0645\u0648\u062f\u0647\u060c \u0686\u0646\u06cc\u0646 \u0645\u06cc\u200c\u0641\u0631\u0645\u0627\u06cc\u062f: \u0627\u06cc\u0646 \u0642\u0648\u0645 \u0645\u06cc\u200c\u06af\u0648\u06cc\u0646\u062f \u0648\u0642\u062a \u0622\u0645\u062f\u0646 \u0645\u0627 \u06cc\u0639\u0646\u06cc \u0648\u0642\u062a \u0628\u0646\u0627 \u0646\u0645\u0648\u062f\u0646 \u062e\u0627\u0646\u0647 \u062e\u062f\u0627\u0648\u0646\u062f \u0646\u0631\u0633\u06cc\u062f\u0647 \u0627\u0633\u062a."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u067e\u0633 \u06a9\u0644\u0627\u0645 \u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0647 \u0648\u0627\u0633\u0637\u0647 \u062d\u062c\u06cc \u0646\u0628\u06cc \u0646\u0627\u0632\u0644 \u0634\u062f\u0647\u060c \u06af\u0641\u062a:"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0622\u06cc\u0627 \u0648\u0642\u062a \u0634\u0645\u0627\u0633\u062a \u06a9\u0647 \u0634\u0645\u0627 \u062f\u0631 \u062e\u0627\u0646\u0647 \u0647\u0627\u06cc \u0645\u0633\u0642\u0641 \u062e\u0648\u062f \u0633\u0627\u06a9\u0646 \u0634\u0648\u06cc\u062f \u0648 \u0627\u06cc\u0646 \u062e\u0627\u0646\u0647 \u062e\u0631\u0627\u0628 \u0628\u0645\u0627\u0646\u062f\u061f"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u067e\u0633 \u062d\u0627\u0644 \u06cc\u0647\u0648\u0647 \u0635\u0628\u0627\u06cc\u0648\u062a \u0686\u0646\u06cc\u0646 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u062f\u0644 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u0631\u0627\u0647\u0647\u0627\u06cc \u062e\u0648\u06cc\u0634 \u0645\u0634\u063a\u0648\u0644 \u0633\u0627\u0632\u06cc\u062f."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0628\u0633\u06cc\u0627\u0631 \u06a9\u0627\u0634\u062a\u0647\u200c\u0627\u06cc\u062f \u0648 \u0627\u0646\u062f\u06a9 \u062d\u0627\u0635\u0644 \u0645\u06cc\u200c\u06a9\u0646\u06cc\u062f. \u0645\u06cc\u200c\u062e\u0648\u0631\u06cc\u062f \u0627\u0645\u0627 \u0633\u06cc\u0631 \u0646\u0645\u06cc \u0634\u0648\u06cc\u062f \u0648\u0645\u06cc \u0646\u0648\u0634\u06cc\u062f \u0644\u06cc\u06a9\u0646 \u0633\u06cc\u0631\u0627\u0628 \u0646\u0645\u06cc \u06af\u0631\u062f\u06cc\u062f. (\u0631\u062e\u062a )\u0645\u06cc \u067e\u0648\u0634\u06cc\u062f \u0627\u0645\u0627 \u06af\u0631\u0645 \u0646\u0645\u06cc \u0634\u0648\u06cc\u062f \u0648 \u0622\u0646\u06a9\u0647 \u0645\u0632\u062f \u0645\u06cc\u200c\u06af\u06cc\u0631\u062f\u060c \u0645\u0632\u062f \u062e\u0648\u06cc\u0634 \u0631\u0627 \u062f\u0631 \u06a9\u06cc\u0633\u0647 \u0633\u0648\u0631\u0627\u062e \u062f\u0627\u0631 \u0645\u06cc\u200c\u06af\u0630\u0627\u0631\u062f."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u067e\u0633 \u06cc\u0647\u0648\u0647 \u0635\u0628\u0627\u06cc\u0648\u062a \u0686\u0646\u06cc\u0646 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f: \u062f\u0644 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u0631\u0627\u0647\u0647\u0627\u06cc \u062e\u0648\u062f \u0645\u0634\u063a\u0648\u0644 \u0633\u0627\u0632\u06cc\u062f."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0628\u0647 \u06a9\u0648\u0647 \u0628\u0631\u0622\u0645\u062f\u0647 \u0648\u0686\u0648\u0628 \u0622\u0648\u0631\u062f\u0647\u060c \u062e\u0627\u0646\u0647 \u0631\u0627 \u0628\u0646\u0627 \u0646\u0645\u0627\u06cc\u06cc\u062f \u0648 \u062e\u062f\u0627\u0648\u0646\u062f\u0645\u06cc \u06af\u0648\u06cc\u062f: \u0627\u0632 \u0622\u0646 \u0631\u0627\u0636\u06cc \u0634\u062f\u0647\u060c \u062c\u0644\u0627\u0644 \u062e\u0648\u0627\u0647\u0645 \u06cc\u0627\u0641\u062a."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0645\u0646\u062a\u0638\u0631 \u0628\u0633\u06cc\u0627\u0631 \u0628\u0648\u062f\u06cc\u062f \u0648 \u0627\u06cc\u0646\u06a9 \u06a9\u0645 \u0634\u062f \u0648 \u0686\u0648\u0646 \u0622\u0646 \u0631\u0627\u0628\u0647 \u062e\u0627\u0646\u0647 \u0622\u0648\u0631\u062f\u06cc\u062f \u0645\u0646 \u0628\u0631 \u0622\u0646 \u062f\u0645\u06cc\u062f\u0645. \u06cc\u0647\u0648\u0647 \u0635\u0628\u0627\u06cc\u0648\u062a \u0645\u06cc\u200c\u067e\u0631\u0633\u062f \u06a9\u0647 \u0633\u0628\u0628 \u0627\u06cc\u0646 \u0686\u06cc\u0633\u062a\u061f \u0633\u0628\u0628 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u062e\u0627\u0646\u0647 \u0645\u0646 \u062e\u0631\u0627\u0628 \u0645\u06cc\u200c\u0645\u0627\u0646\u062f \u0648 \u0647\u0631\u06a9\u062f\u0627\u0645 \u0627\u0632 \u0634\u0645\u0627 \u0628\u0647 \u062e\u0627\u0646\u0647 \u062e\u0648\u06cc\u0634 \u0645\u06cc\u200c\u0634\u062a\u0627\u0628\u06cc\u062f."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0627\u0632 \u0627\u06cc\u0646 \u0633\u0628\u0628\u060c \u0622\u0633\u0645\u0627\u0646\u0647\u0627 \u0628\u062e\u0627\u0637\u0631 \u0634\u0645\u0627 \u0627\u0632 \u0634\u0628\u0646\u0645 \u0628\u0627\u0632 \u062f\u0627\u0634\u062a\u0647 \u0645\u06cc\u200c\u0634\u0648\u062f \u0648 \u0632\u0645\u06cc\u0646 \u0627\u0632\u0645\u062d\u0635\u0648\u0644\u0634 \u0628\u0627\u0632\u062f\u0627\u0634\u062a\u0647 \u0645\u06cc\u200c\u06af\u0631\u062f\u062f."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0648 \u0645\u0646 \u0628\u0631 \u0632\u0645\u06cc\u0646 \u0648\u0628\u0631 \u06a9\u0648\u0647\u0647\u0627 \u0648 \u0628\u0631 \u063a\u0644\u0647 \u0648 \u0639\u0635\u06cc\u0631 \u0627\u0646\u06af\u0648\u0631 \u0648 \u0631\u0648\u063a\u0646 \u0632\u06cc\u062a\u0648\u0646 \u0648\u0628\u0631 \u0647\u0631\u200c\u0622\u0646\u0686\u0647 \u0632\u0645\u06cc\u0646 \u0645\u06cc\u200c\u0631\u0648\u06cc\u0627\u0646\u062f \u0648 \u0628\u0631 \u0627\u0646\u0633\u0627\u0646 \u0648 \u0628\u0647\u0627\u06cc\u0645 \u0648\u062a\u0645\u0627\u0645\u06cc \u0645\u0634\u0642\u062a \u0647\u0627\u06cc \u062f\u0633\u062a\u0647\u0627 \u062e\u0634\u06a9\u0633\u0627\u0644\u06cc \u0631\u0627 \u062e\u0648\u0627\u0646\u062f\u0645.\u00bb"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0622\u0646\u06af\u0627\u0647 \u0632\u0631\u0628\u0627\u0628\u0644 \u0628\u0646 \u0634\u0627\u0644\u062a\u06cc\u0626\u06cc\u0644 \u0648 \u06cc\u0647\u0648\u0634\u0639 \u0628\u0646 \u06cc\u0647\u0648\u0635\u0627\u062f\u0642 \u0631\u0626\u06cc\u0633 \u06a9\u0647\u0646\u0647 \u0648 \u062a\u0645\u0627\u0645\u06cc \u0628\u0642\u06cc\u0647 \u0642\u0648\u0645 \u0628\u0647 \u0642\u0648\u0644 \u06cc\u0647\u0648\u0647 \u062e\u062f\u0627\u06cc \u062e\u0648\u062f \u0648 \u0628\u0647 \u06a9\u0644\u0627\u0645 \u062d\u062c\u06cc \u0646\u0628\u06cc \u0686\u0646\u0627\u0646\u06a9\u0647 \u06cc\u0647\u0648\u0647 \u062e\u062f\u0627\u06cc \u0627\u06cc\u0634\u0627\u0646 \u0627\u0648 \u0631\u0627 \u0641\u0631\u0633\u062a\u0627\u062f\u0647 \u0628\u0648\u062f \u06af\u0648\u0634 \u062f\u0627\u062f\u0646\u062f\u060c \u0648 \u0642\u0648\u0645 \u0627\u0632 \u062e\u062f\u0627\u0648\u0646\u062f \u062a\u0631\u0633\u06cc\u062f\u0646\u062f."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0648 \u062d\u062c\u06cc\u060c \u0631\u0633\u0648\u0644 \u062e\u062f\u0627\u0648\u0646\u062f\u060c \u067e\u06cc\u063a\u0627\u0645 \u062e\u062f\u0627\u0648\u0646\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u0642\u0648\u0645 \u0628\u06cc\u0627\u0646 \u06a9\u0631\u062f\u0647\u060c \u06af\u0641\u062a: \u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u06a9\u0647 \u0645\u0646 \u0628\u0627 \u0634\u0645\u0627 \u0647\u0633\u062a\u0645."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0648\u062e\u062f\u0627\u0648\u0646\u062f \u0631\u0648\u062d \u0632\u0631\u0628\u0627\u0628\u0644 \u0628\u0646 \u0634\u0627\u0644\u062a\u06cc\u0626\u06cc\u0644 \u0648\u0627\u0644\u06cc \u06cc\u0647\u0648\u062f\u0627 \u0648\u0631\u0648\u062d \u06cc\u0647\u0648\u0634\u0639 \u0628\u0646 \u06cc\u0647\u0648\u0635\u0627\u062f\u0642\u060c \u0631\u0626\u06cc\u0633 \u06a9\u0647\u0646\u0647\u060c \u0648 \u0631\u0648\u062d \u062a\u0645\u0627\u0645\u06cc \u0628\u0642\u06cc\u0647 \u0642\u0648\u0645 \u0631\u0627 \u0628\u0631\u0627\u0646\u06af\u06cc\u0632\u0627\u0646\u06cc\u062f \u062a\u0627 \u0628\u0631\u0648\u0646\u062f \u0648 \u062f\u0631 \u062e\u0627\u0646\u0647 \u06cc\u0647\u0648\u0647 \u0635\u0628\u0627\u06cc\u0648\u062a \u062e\u062f\u0627\u06cc \u062e\u0648\u062f \u0628\u0647\u200c\u06a9\u0627\u0631 \u067e\u0631\u062f\u0627\u0632\u0646\u062f."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u062f\u0631\u0631\u0648\u0632 \u0628\u06cc\u0633\u062a \u0648 \u0686\u0647\u0627\u0631\u0645 \u0645\u0627\u0647 \u0634\u0634\u0645 \u0627\u0632 \u0633\u0627\u0644 \u062f\u0648\u0645 \u062f\u0627\u0631\u06cc\u0648\u0634 \u067e\u0627\u062f\u0634\u0627\u0647\u060c \u0627\u06cc\u0646 \u0648\u0627\u0642\u0639 \u0634\u062f."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u062f\u0631 \u0631\u0648\u0632 \u0628\u06cc\u0633\u062a \u0648 \u06cc\u06a9\u0645 \u0645\u0627\u0647 \u0647\u0641\u062a\u0645\u060c \u06a9\u0644\u0627\u0645 \u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0648\u0627\u0633\u0637\u0647 \u062d\u062c\u06cc \u0646\u0628\u06cc \u0646\u0627\u0632\u0644 \u0634\u062f\u0647\u060c \u06af\u0641\u062a:"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0632\u0631\u0628\u0627\u0628\u0644 \u0628\u0646 \u0634\u0627\u0644\u062a\u06cc\u0626\u06cc\u0644 \u0648\u0627\u0644\u06cc \u06cc\u0647\u0648\u062f\u0627 \u0648 \u06cc\u0647\u0648\u0634\u0639 \u0628\u0646 \u06cc\u0647\u0648\u0635\u0627\u062f\u0642 \u0631\u0626\u06cc\u0633 \u06a9\u0647\u0646\u0647\u060c \u0648 \u0628\u0642\u06cc\u0647 \u0642\u0648\u0645 \u0631\u0627 \u062e\u0637\u0627\u0628 \u06a9\u0631\u062f\u0647\u060c \u0628\u06af\u0648:"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u06a9\u06cc\u0633\u062a \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0634\u0645\u0627 \u06a9\u0647 \u0628\u0627\u0642\u06cc\u200c\u0645\u0627\u0646\u062f\u0647 \u0648\u0627\u06cc\u0646 \u062e\u0627\u0646\u0647 \u0631\u0627 \u062f\u0631 \u062c\u0644\u0627\u0644 \u0646\u062e\u0633\u062a\u06cc\u0646\u0634 \u062f\u06cc\u062f\u0647 \u0628\u0627\u0634\u062f\u061f \u067e\u0633 \u0627\u0644\u0627\u0646 \u062f\u0631 \u0646\u0638\u0631 \u0634\u0645\u0627 \u0686\u06af\u0648\u0646\u0647 \u0645\u06cc\u200c\u0646\u0645\u0627\u06cc\u062f\u061f \u0622\u06cc\u0627 \u062f\u0631 \u0646\u0638\u0631 \u0634\u0645\u0627\u0646\u0627\u0686\u06cc\u0632 \u0646\u0645\u06cc \u0646\u0645\u0627\u06cc\u062f\u061f"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0627\u0645\u0627 \u0627\u0644\u0627\u0646 \u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f: \u0627\u06cc \u0632\u0631\u0628\u0627\u0628\u0644 \u0642\u0648\u06cc\u200c\u062f\u0644 \u0628\u0627\u0634 \u0648\u200c\u0627\u06cc \u06cc\u0647\u0648\u0634\u0639 \u0628\u0646 \u06cc\u0647\u0648\u0635\u0627\u062f\u0642 \u0631\u0626\u06cc\u0633 \u06a9\u0647\u0646\u0647 \u0642\u0648\u06cc\u200c\u062f\u0644 \u0628\u0627\u0634 \u0648\u200c\u0627\u06cc \u062a\u0645\u0627\u0645\u06cc \u0642\u0648\u0645 \u0632\u0645\u06cc\u0646 \u0642\u0648\u06cc\u200c\u062f\u0644 \u0628\u0627\u0634\u06cc\u062f \u0648 \u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u0641\u0631\u0645\u0627\u06cc\u062f \u06a9\u0647 \u0645\u0634\u063a\u0648\u0644 \u0628\u0634\u0648\u06cc\u062f \u0632\u06cc\u0631\u0627 \u06a9\u0647 \u0645\u0646 \u0628\u0627 \u0634\u0645\u0627 \u0647\u0633\u062a\u0645. \u0642\u0648\u0644 \u06cc\u0647\u0648\u0647 \u0635\u0628\u0627\u06cc\u0648\u062a \u0627\u06cc\u0646 \u0627\u0633\u062a."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0628\u0631\u062d\u0633\u0628 \u06a9\u0644\u0627\u0645 \u0622\u0646 \u0639\u0647\u062f\u06cc \u06a9\u0647 \u062f\u0631 \u062d\u06cc\u0646 \u0628\u06cc\u0631\u0648\u0646 \u0622\u0648\u0631\u062f\u0646 \u0634\u0645\u0627 \u0627\u0632 \u0645\u0635\u0631 \u0628\u0627 \u0634\u0645\u0627 \u0628\u0633\u062a\u0645 \u0648\u0686\u0648\u0646\u06a9\u0647 \u0631\u0648\u062d \u0645\u0646 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0634\u0645\u0627 \u0642\u0627\u06cc\u0645 \u0645\u06cc\u200c\u0628\u0627\u0634\u062f\u060c \u062a\u0631\u0633\u0627\u0646 \u0645\u0628\u0627\u0634\u06cc\u062f."
                },
                "6": {
                    "verse_nr": "6",
