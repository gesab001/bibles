            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0631\u0648\u06cc\u0627\u06cc \u0639\u0648\u0628\u062f\u06cc\u0627."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0647\u0627\u0646 \u0645\u0646 \u062a\u0648 \u0631\u0627\u06a9\u0648\u0686\u06a9\u062a\u0631\u06cc\u0646 \u0627\u0645\u062a\u200c\u0647\u0627 \u06af\u0631\u062f\u0627\u0646\u06cc\u062f\u0645 \u0648 \u062a\u0648 \u0628\u0633\u06cc\u0627\u0631 \u062e\u0648\u0627\u0631\u0647\u0633\u062a\u06cc."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u200c\u0627\u06cc \u06a9\u0647 \u062f\u0631 \u0634\u06a9\u0627\u0641\u0647\u0627\u06cc \u0635\u062e\u0631\u0647 \u0633\u0627\u06a9\u0646 \u0647\u0633\u062a\u06cc \u0648 \u0645\u0633\u06a9\u0646 \u062a\u0648 \u0628\u0644\u0646\u062f \u0645\u06cc\u200c\u0628\u0627\u0634\u062f \u0648 \u062f\u0631 \u062f\u0644 \u062e\u0648\u062f \u0645\u06cc\u200c\u06af\u0648\u06cc\u06cc \u06a9\u06cc\u0633\u062a \u06a9\u0647 \u0645\u0631\u0627 \u0628\u0647 \u0632\u0645\u06cc\u0646 \u0641\u0631\u0648\u062f \u0628\u06cc\u0627\u0648\u0631\u062f\u060c \u062a\u06a9\u0628\u0631 \u062f\u0644\u062a\u060c \u062a\u0648\u0631\u0627 \u0641\u0631\u06cc\u0628 \u062f\u0627\u062f\u0647 \u0627\u0633\u062a."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f: \u0627\u06af\u0631\u0686\u0647 \u062e\u0648\u06cc\u0634\u062a\u0646 \u0631\u0627 \u0645\u062b\u0644 \u0639\u0642\u0627\u0628 \u0628\u0644\u0646\u062f \u0633\u0627\u0632\u06cc \u0648 \u0622\u0634\u06cc\u0627\u0646\u0647 \u062e\u0648\u062f\u0631\u0627 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0633\u062a\u0627\u0631\u06af\u0627\u0646 \u0628\u06af\u0630\u0627\u0631\u06cc \u0645\u0646 \u062a\u0648 \u0631\u0627 \u0627\u0632 \u0622\u0646\u062c\u0627 \u0641\u0631\u0648\u062f\u062e\u0648\u0627\u0647\u0645 \u0622\u0648\u0631\u062f."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0627\u06af\u0631 \u062f\u0632\u062f\u0627\u0646 \u06cc\u0627 \u063a\u0627\u0631\u062a \u06a9\u0646\u0646\u062f\u06af\u0627\u0646 \u0634\u0628 \u0646\u0632\u062f \u062a\u0648 \u0622\u06cc\u0646\u062f\u060c (\u0686\u06af\u0648\u0646\u0647 \u0647\u0644\u0627\u06a9 \u0634\u062f\u06cc )\u061f \u0622\u06cc\u0627 \u0628\u0642\u062f\u0631\u06a9\u0641\u0627\u06cc\u062a \u063a\u0627\u0631\u062a \u0646\u0645\u06cc \u06a9\u0646\u0646\u062f\u061f \u0648 \u0627\u06af\u0631 \u0627\u0646\u06af\u0648\u0631\u0686\u06cc\u0646\u0627\u0646 \u0646\u0632\u062f \u062a\u0648\u0622\u06cc\u0646\u062f \u0622\u06cc\u0627 \u0628\u0639\u0636\u06cc \u062e\u0648\u0634\u0647\u200c\u0647\u0627 \u0631\u0627 \u0646\u0645\u06cc \u06af\u0630\u0627\u0631\u0646\u062f\u061f"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0686\u06cc\u0632\u0647\u0627\u06cc \u0639\u06cc\u0633\u0648 \u0686\u06af\u0648\u0646\u0647 \u062a\u0641\u062a\u06cc\u0634 \u0634\u062f\u0647 \u0648 \u0686\u06cc\u0632\u0647\u0627\u06cc \u0645\u062e\u0641\u06cc \u0627\u0648 \u0686\u06af\u0648\u0646\u0647 \u062a\u0641\u062d\u0635 \u06af\u0631\u062f\u06cc\u062f\u0647 \u0627\u0633\u062a\u061f"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0647\u0645\u0647 \u0622\u0646\u0627\u0646\u06cc \u06a9\u0647 \u0628\u0627 \u062a\u0648 \u0647\u0645\u0639\u0647\u062f \u0628\u0648\u062f\u0646\u062f \u062a\u0648 \u0631\u0627 \u0628\u0633\u0631\u062d\u062f\u0641\u0631\u0633\u062a\u0627\u062f\u0646\u062f \u0648 \u0635\u0644\u062d \u0627\u0646\u062f\u06cc\u0634\u0627\u0646 \u062a\u0648\u060c \u062a\u0648 \u0631\u0627 \u0641\u0631\u06cc\u0628 \u062f\u0627\u062f\u0647\u060c \u0628\u0631\u062a\u0648 \u063a\u0627\u0644\u0628 \u0622\u0645\u062f\u0646\u062f \u0648 \u062e\u0648\u0631\u0646\u062f\u06af\u0627\u0646 \u0646\u0627\u0646 \u062a\u0648 \u062f\u0627\u0645\u06cc \u0632\u06cc\u0631 \u062a\u0648\u06af\u0633\u062a\u0631\u062f\u0646\u062f. \u062f\u0631 \u0627\u06cc\u0634\u0627\u0646 \u0641\u0637\u0627\u0646\u062a\u06cc \u0646\u06cc\u0633\u062a."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f: \u0622\u06cc\u0627 \u062f\u0631 \u0622\u0646 \u0631\u0648\u0632 \u062d\u06a9\u06cc\u0645\u0627\u0646 \u0627\u062f\u0648\u0645 \u0631\u0627 \u0648 \u0641\u0637\u0627\u0646\u062a \u0631\u0627 \u0627\u0632 \u06a9\u0648\u0647 \u0639\u06cc\u0633\u0648 \u0646\u0627\u0628\u0648\u062f \u0646\u062e\u0648\u0627\u0647\u0645 \u06af\u0631\u062f\u0627\u0646\u06cc\u062f\u061f"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0648 \u062c\u0628\u0627\u0631\u0627\u0646 \u062a\u0648\u200c\u0627\u06cc \u062a\u06cc\u0645\u0627\u0646 \u0647\u0631\u0627\u0633\u0627\u0646 \u062e\u0648\u0627\u0647\u0646\u062f\u0634\u062f \u062a\u0627 \u0647\u0631 \u06a9\u0633 \u0627\u0632 \u06a9\u0648\u0647 \u0639\u06cc\u0633\u0648 \u0628\u0647 \u0642\u062a\u0644 \u0645\u0646\u0642\u0637\u0639 \u0634\u0648\u062f."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0628\u0647\u200c\u0633\u0628\u0628 \u0638\u0644\u0645\u06cc \u06a9\u0647 \u0628\u0631 \u0628\u0631\u0627\u062f\u0631\u062a \u06cc\u0639\u0642\u0648\u0628 \u0646\u0645\u0648\u062f\u06cc\u060c \u062e\u062c\u0627\u0644\u062a \u062a\u0648 \u0631\u0627 \u062e\u0648\u0627\u0647\u062f \u067e\u0648\u0634\u0627\u0646\u06cc\u062f \u0648 \u062a\u0627 \u0628\u0647 \u0627\u0628\u062f \u0645\u0646\u0642\u0637\u0639 \u062e\u0648\u0627\u0647\u06cc \u0634\u062f."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u062f\u0631 \u0631\u0648\u0632\u06cc \u06a9\u0647 \u0628\u0647 \u0645\u0642\u0627\u0628\u0644 \u0648\u06cc \u0627\u06cc\u0633\u062a\u0627\u062f\u0647 \u0628\u0648\u062f\u06cc\u060c \u0647\u0646\u06af\u0627\u0645\u06cc \u06a9\u0647 \u063a\u0631\u06cc\u0628\u0627\u0646 \u0627\u0645\u0648\u0627\u0644 \u0627\u0648 \u0631\u0627 \u063a\u0627\u0631\u062a \u0646\u0645\u0648\u062f\u0646\u062f \u0648\u0628\u06cc\u06af\u0627\u0646\u06af\u0627\u0646 \u0628\u0647 \u062f\u0631\u0648\u0627\u0632\u0647 \u0647\u0627\u06cc\u0634 \u062f\u0627\u062e\u0644 \u0634\u062f\u0646\u062f \u0648 \u0628\u0631\u0627\u0648\u0631\u0634\u0644\u06cc\u0645 \u0642\u0631\u0639\u0647 \u0627\u0646\u062f\u0627\u062e\u062a\u0646\u062f\u060c \u062a\u0648 \u0646\u06cc\u0632 \u0645\u062b\u0644 \u06cc\u06a9\u06cc \u0627\u0632 \u0622\u0646\u0647\u0627\u0628\u0648\u062f\u06cc."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0628\u0631 \u0631\u0648\u0632 \u0628\u0631\u0627\u062f\u0631 \u062e\u0648\u062f \u0647\u0646\u06af\u0627\u0645 \u0645\u0635\u06cc\u0628\u062a\u0634 \u0646\u06af\u0627\u0647 \u0645\u06a9\u0646 \u0648 \u0628\u0631 \u0628\u0646\u06cc \u06cc\u0647\u0648\u062f\u0627 \u062f\u0631 \u0631\u0648\u0632 \u0647\u0644\u0627\u06a9\u062a \u0627\u06cc\u0634\u0627\u0646 \u0634\u0627\u062f\u06cc \u0645\u0646\u0645\u0627 \u0648 \u062f\u0631 \u0631\u0648\u0632 \u062a\u0646\u06af\u06cc \u0627\u06cc\u0634\u0627\u0646 \u0644\u0627\u0641 \u0645\u0632\u0646."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0648 \u0628\u0647 \u062f\u0631\u0648\u0627\u0632\u0647 \u0647\u0627\u06cc \u0642\u0648\u0645 \u0645\u0646 \u062f\u0631 \u0631\u0648\u0632 \u0628\u0644\u0627\u06cc \u0627\u06cc\u0634\u0627\u0646 \u062f\u0627\u062e\u0644 \u0645\u0634\u0648 \u0648 \u062a\u0648 \u0646\u06cc\u0632 \u0628\u0631 \u0628\u062f\u06cc \u0627\u06cc\u0634\u0627\u0646 \u062f\u0631 \u0631\u0648\u0632 \u0628\u0644\u0627\u06cc \u0627\u06cc\u0634\u0627\u0646 \u0645\u0646\u06af\u0631 \u0648 \u062f\u0633\u062a \u062e\u0648\u062f \u0631\u0627 \u0628\u0631 \u0627\u0645\u0648\u0627\u0644 \u0627\u06cc\u0634\u0627\u0646 \u062f\u0631 \u0631\u0648\u0632 \u0628\u0644\u0627\u06cc \u0627\u06cc\u0634\u0627\u0646 \u062f\u0631\u0627\u0632 \u0645\u06a9\u0646."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0648 \u0628\u0631 \u0633\u0631 \u062f\u0648 \u0631\u0627\u0647 \u0645\u0627\u06cc\u0633\u062a \u062a\u0627\u0641\u0631\u0627\u0631\u06cc\u0627\u0646 \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627 \u0645\u0646\u0642\u0637\u0639 \u0633\u0627\u0632\u06cc \u0648 \u0628\u0627\u0642\u06cc \u0645\u0627\u0646\u062f\u06af\u0627\u0646 \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627 \u062f\u0631 \u0631\u0648\u0632 \u062a\u0646\u06af\u06cc \u062a\u0633\u0644\u06cc\u0645 \u0645\u0646\u0645\u0627."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0632\u06cc\u0631\u0627 \u06a9\u0647 \u0631\u0648\u0632\u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0631 \u062c\u0645\u06cc\u0639 \u0627\u0645\u062a\u200c\u0647\u0627 \u0646\u0632\u062f\u06cc\u06a9 \u0627\u0633\u062a \u0648 \u0686\u0646\u0627\u0646\u06a9\u0647 \u0639\u0645\u0644 \u0646\u0645\u0648\u062f\u06cc \u0647\u0645\u0686\u0646\u0627\u0646 \u0628\u0647 \u062a\u0648 \u0639\u0645\u0644 \u06a9\u0631\u062f\u0647 \u062e\u0648\u0627\u0647\u062f\u0634\u062f \u0648 \u0627\u0639\u0645\u0627\u0644\u062a \u0628\u0631 \u0633\u0631\u062a \u062e\u0648\u0627\u0647\u062f \u0628\u0631\u06af\u0634\u062a."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0632\u06cc\u0631\u0627\u0686\u0646\u0627\u0646\u06a9\u0647 \u0628\u0631 \u06a9\u0648\u0647 \u0645\u0642\u062f\u0633 \u0645\u0646 \u0646\u0648\u0634\u06cc\u062f\u06cc\u062f\u060c \u0647\u0645\u0686\u0646\u0627\u0646 \u062c\u0645\u06cc\u0639 \u0627\u0645\u062a\u200c\u0647\u0627 \u062e\u0648\u0627\u0647\u0646\u062f \u0646\u0648\u0634\u06cc\u062f \u0648 \u0622\u0634\u0627\u0645\u06cc\u062f\u0647\u060c \u062e\u0648\u0627\u0647\u0646\u062f\u0628\u0644\u0639\u06cc\u062f \u0648 \u0686\u0646\u0627\u0646 \u062e\u0648\u0627\u0647\u0646\u062f \u0634\u062f \u06a9\u0647 \u06af\u0648\u06cc\u0627 \u0646\u0628\u0648\u062f\u0647\u200c\u0627\u0646\u062f."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0627\u0645\u0627 \u0628\u0631 \u06a9\u0648\u0647 \u0635\u0647\u06cc\u0648\u0646 \u0646\u062c\u0627\u062a \u062e\u0648\u0627\u0647\u062f \u0628\u0648\u062f \u0648\u0645\u0642\u062f\u0633 \u062e\u0648\u0627\u0647\u062f \u0634\u062f \u0648 \u062e\u0627\u0646\u062f\u0627\u0646 \u06cc\u0639\u0642\u0648\u0628 \u0645\u06cc\u0631\u0627\u062b \u062e\u0648\u062f\u0631\u0627 \u0628\u0647 \u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u0646\u062f\u200c\u0622\u0648\u0631\u062f."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0648 \u062e\u0627\u0646\u062f\u0627\u0646 \u06cc\u0639\u0642\u0648\u0628 \u0622\u062a\u0634 \u0648 \u062e\u0627\u0646\u062f\u0627\u0646 \u06cc\u0648\u0633\u0641 \u0634\u0639\u0644\u0647 \u0648 \u062e\u0627\u0646\u062f\u0627\u0646 \u0639\u06cc\u0633\u0648 \u06a9\u0627\u0647 \u0639\u0648\u0628\u062f\u06cc\u0627 \u062e\u0648\u0627\u0647\u0646\u062f \u0628\u0648\u062f \u0648 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0627\u06cc\u0634\u0627\u0646 \u0645\u0634\u062a\u0639\u0644 \u0634\u062f\u0647\u060c \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627 \u062e\u0648\u0627\u0647\u062f \u0633\u0648\u0632\u0627\u0646\u06cc\u062f \u0648 \u0628\u0631\u0627\u06cc \u062e\u0627\u0646\u062f\u0627\u0646 \u0639\u06cc\u0633\u0648 \u0628\u0642\u06cc\u062a\u06cc \u0646\u062e\u0648\u0627\u0647\u062f \u0645\u0627\u0646\u062f \u0632\u06cc\u0631\u0627 \u062e\u062f\u0627\u0648\u0646\u062f \u062a\u06a9\u0644\u0645 \u0646\u0645\u0648\u062f\u0647 \u0627\u0633\u062a."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0648\u0627\u0647\u0644 \u062c\u0646\u0648\u0628 \u06a9\u0648\u0647 \u0639\u06cc\u0633\u0648 \u0631\u0627 \u0648 \u0627\u0647\u0644 \u0647\u0627\u0645\u0648\u0646 \u0641\u0644\u0633\u0637\u06cc\u0646\u06cc\u0627\u0646 \u0631\u0627 \u0628\u0647 \u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u0646\u062f\u200c\u0622\u0648\u0631\u062f \u0648 \u0635\u062d\u0631\u0627\u06cc \u0627\u0641\u0631\u0627\u06cc\u0645 \u0648\u0635\u062d\u0631\u0627\u06cc \u0633\u0627\u0645\u0631\u0647 \u0631\u0627 \u0628\u0647 \u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u0646\u062f\u200c\u0622\u0648\u0631\u062f \u0648\u0628\u0646\u06cc\u0627\u0645\u06cc\u0646 \u062c\u0644\u0639\u0627\u062f \u0631\u0627 (\u0645\u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u062f \u0634\u062f)."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0648 \u0627\u0633\u06cc\u0631\u0627\u0646 \u0627\u06cc\u0646 \u0644\u0634\u06a9\u0631 \u0628\u0646\u06cc\u200c\u0627\u0633\u0631\u0627\u0626\u06cc\u0644 \u0645\u0644\u06a9 \u06a9\u0646\u0639\u0627\u0646\u06cc\u0627\u0646 \u0631\u0627\u062a\u0627 \u0635\u0631\u0641\u0647 \u0628\u0647 \u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u0646\u062f\u200c\u0622\u0648\u0631\u062f \u0648 \u0627\u0633\u06cc\u0631\u0627\u0646 \u0627\u0648\u0631\u0634\u0644\u06cc\u0645 \u06a9\u0647 \u062f\u0631 \u0635\u0641\u0627\u0631\u062f \u0647\u0633\u062a\u0646\u062f \u0634\u0647\u0631\u0647\u0627\u06cc \u062c\u0646\u0648\u0628 \u0631\u0627 \u0628\u0647 \u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u0646\u062f\u200c\u0622\u0648\u0631\u062f."
                },
