        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u06cc\u0647\u0648\u062f\u0627\u060c \u063a\u0644\u0627\u0645 \u0639\u06cc\u0633\u06cc \u0645\u0633\u06cc\u062d \u0648 \u0628\u0631\u0627\u062f\u0631 \u06cc\u0639\u0642\u0648\u0628\u060c \u0628\u0647 \u062e\u0648\u0627\u0646\u062f\u0647\u200c\u0634\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u062e\u062f\u0627\u06cc \u067e\u062f\u0631 \u062d\u0628\u06cc\u0628 \u0648\u0628\u0631\u0627\u06cc \u0639\u06cc\u0633\u06cc \u0645\u0633\u06cc\u062d \u0645\u062d\u0641\u0648\u0638 \u0645\u06cc\u200c\u0628\u0627\u0634\u06cc\u062f."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0631\u062d\u0645\u062a \u0648 \u0633\u0644\u0627\u0645\u062a\u06cc \u0648 \u0645\u062d\u0628\u062a \u0628\u0631 \u0634\u0645\u0627 \u0627\u0641\u0632\u0648\u0646 \u0628\u0627\u062f."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u200c\u0627\u06cc \u062d\u0628\u06cc\u0628\u0627\u0646\u060c \u0686\u0648\u0646 \u0634\u0648\u0642 \u062a\u0645\u0627\u0645 \u062f\u0627\u0634\u062a\u0645 \u06a9\u0647 \u062f\u0631\u0628\u0627\u0631\u0647 \u0646\u062c\u0627\u062a \u0639\u0627\u0645 \u0628\u0647 \u0634\u0645\u0627 \u0628\u0646\u0648\u06cc\u0633\u0645\u060c \u0646\u0627\u0686\u0627\u0631 \u0634\u062f\u0645 \u06a9\u0647 \u0627\u0644\u0627\u0646 \u0628\u0647 \u0634\u0645\u0627 \u0628\u0646\u0648\u06cc\u0633\u0645 \u0648 \u0646\u0635\u06cc\u062d\u062a \u062f\u0647\u0645 \u062a\u0627 \u0634\u0645\u0627 \u0645\u062c\u0627\u0647\u062f\u0647 \u06a9\u0646\u06cc\u062f\u0628\u0631\u0627\u06cc \u0622\u0646 \u0627\u06cc\u0645\u0627\u0646\u06cc \u06a9\u0647 \u06cc\u06a9 \u0628\u0627\u0631 \u0628\u0647 \u0645\u0642\u062f\u0633\u06cc\u0646 \u0633\u067e\u0631\u062f\u0647 \u0634\u062f."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0632\u06cc\u0631\u0627 \u06a9\u0647 \u0628\u0639\u0636\u06cc \u0627\u0634\u062e\u0627\u0635 \u062f\u0631 \u062e\u0641\u0627 \u062f\u0631\u0622\u0645\u062f\u0647\u200c\u0627\u0646\u062f \u06a9\u0647 \u0627\u0632\u0642\u062f\u06cc\u0645 \u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u0642\u0635\u0627\u0635 \u0645\u0642\u0631\u0631 \u0634\u062f\u0647 \u0628\u0648\u062f\u0646\u062f\u061b \u0645\u0631\u062f\u0645\u0627\u0646 \u0628\u06cc\u200c\u062f\u06cc\u0646 \u06a9\u0647 \u0641\u06cc\u0636 \u062e\u062f\u0627\u06cc \u0645\u0627 \u0631\u0627 \u0628\u0647 \u0641\u062c\u0648\u0631 \u062a\u0628\u062f\u06cc\u0644 \u0646\u0645\u0648\u062f\u0647 \u0648 \u0639\u06cc\u0633\u06cc \u0645\u0633\u06cc\u062d \u0622\u0642\u0627\u06cc \u0648\u0627\u062d\u062f \u0648 \u062e\u062f\u0627\u0648\u0646\u062f \u0645\u0627 \u0631\u0627 \u0627\u0646\u06a9\u0627\u0631\u06a9\u0631\u062f\u0647\u200c\u0627\u0646\u062f."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u067e\u0633 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u0645 \u0634\u0645\u0627 \u0631\u0627 \u06cc\u0627\u062f \u062f\u0647\u0645\u060c \u0647\u0631\u0686\u0646\u062f \u0647\u0645\u0647\u200c\u0686\u06cc\u0632 \u0631\u0627 \u062f\u0641\u0639\u0647 \u0645\u06cc\u200c\u062f\u0627\u0646\u06cc\u062f \u06a9\u0647 \u0628\u0639\u062f \u0627\u0632 \u0622\u0646\u06a9\u0647 \u062e\u062f\u0627\u0648\u0646\u062f\u060c \u0642\u0648\u0645 \u0631\u0627 \u0627\u0632 \u0632\u0645\u06cc\u0646 \u0645\u0635\u0631 \u0631\u0647\u0627\u06cc\u06cc \u0628\u062e\u0634\u06cc\u062f\u0647 \u0628\u0648\u062f\u060c \u0628\u0627\u0631 \u062f\u06cc\u06af\u0631\u0628\u06cc \u0627\u06cc\u0645\u0627\u0646\u0627\u0646 \u0631\u0627 \u0647\u0644\u0627\u06a9 \u0641\u0631\u0645\u0648\u062f."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0648 \u0641\u0631\u0634\u062a\u06af\u0627\u0646\u06cc \u0631\u0627 \u06a9\u0647 \u0631\u06cc\u0627\u0633\u062a \u062e\u0648\u062f \u0631\u0627 \u062d\u0641\u0638 \u0646\u06a9\u0631\u062f\u0646\u062f \u0628\u0644\u06a9\u0647 \u0645\u0633\u06a9\u0646 \u062d\u0642\u06cc\u0642\u06cc \u062e\u0648\u062f \u0631\u0627 \u062a\u0631\u06a9 \u0646\u0645\u0648\u062f\u0646\u062f\u060c \u062f\u0631 \u0632\u0646\u062c\u06cc\u0631\u0647\u0627\u06cc \u0627\u0628\u062f\u06cc \u062f\u0631\u062a\u062d\u062a \u0638\u0644\u0645\u062a \u0628\u062c\u0647\u062a \u0642\u0635\u0627\u0635 \u06cc\u0648\u0645 \u0639\u0638\u06cc\u0645 \u0646\u06af\u0627\u0647 \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u0633\u062f\u0648\u0645 \u0648 \u063a\u0645\u0648\u0631\u0647 \u0648 \u0633\u0627\u06cc\u0631\u0628\u0644\u062f\u0627\u0646 \u0646\u0648\u0627\u062d\u06cc \u0622\u0646\u0647\u0627 \u0645\u062b\u0644 \u0627\u06cc\u0634\u0627\u0646 \u0686\u0648\u0646\u06a9\u0647 \u0632\u0646\u0627\u06a9\u0627\u0631 \u0634\u062f\u0646\u062f \u0648 \u062f\u0631\u200c\u067e\u06cc \u0628\u0634\u0631 \u062f\u06cc\u06af\u0631 \u0627\u0641\u062a\u0627\u062f\u0646\u062f\u060c \u062f\u0631 \u0639\u0642\u0648\u0628\u062a \u0622\u062a\u0634 \u0627\u0628\u062f\u06cc \u06af\u0631\u0641\u062a\u0627\u0631 \u0634\u062f\u0647\u060c \u0628\u062c\u0647\u062a \u0639\u0628\u0631\u062a \u0645\u0642\u0631\u0631 \u0634\u062f\u0646\u062f."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0644\u06cc\u06a9\u0646 \u0628\u0627\u0648\u062c\u0648\u062f \u0627\u06cc\u0646\u060c \u0647\u0645\u0647 \u0627\u06cc\u0646 \u062e\u0648\u0627\u0628 \u0628\u06cc\u0646\u0646\u062f\u06af\u0627\u0646 \u0646\u06cc\u0632 \u062c\u0633\u062f \u062e\u0648\u062f \u0631\u0627 \u0646\u062c\u0633 \u0645\u06cc\u200c\u0633\u0627\u0632\u0646\u062f \u0648 \u062e\u062f\u0627\u0648\u0646\u062f\u06cc \u0631\u0627\u062e\u0648\u0627\u0631 \u0645\u06cc\u200c\u0634\u0645\u0627\u0631\u0646\u062f \u0648 \u0628\u0631 \u0628\u0632\u0631\u06af\u0627\u0646 \u062a\u0647\u0645\u062a \u0645\u06cc\u200c\u0632\u0646\u0646\u062f."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0627\u0645\u0627\u0645\u06cc\u06a9\u0627\u0626\u06cc\u0644\u060c \u0631\u0626\u06cc\u0633 \u0645\u0644\u0627\u0626\u06a9\u0647\u060c \u0686\u0648\u0646 \u062f\u0631\u0628\u0627\u0631\u0647 \u062c\u0633\u062f\u0645\u0648\u0633\u06cc \u0628\u0627 \u0627\u0628\u0644\u06cc\u0633 \u0645\u0646\u0627\u0632\u0639\u0647 \u0645\u06cc\u200c\u06a9\u0631\u062f\u060c \u062c\u0631\u0627\u062a \u0646\u0646\u0645\u0648\u062f \u06a9\u0647 \u062d\u06a9\u0645 \u0627\u0641\u062a\u0631\u0627 \u0628\u0631 \u0627\u0648 \u0628\u0632\u0646\u062f \u0628\u0644\u06a9\u0647 \u06af\u0641\u062a: \u00ab\u062e\u062f\u0627\u0648\u0646\u062f \u062a\u0648 \u0631\u0627\u062a\u0648\u0628\u06cc\u062e \u0641\u0631\u0645\u0627\u06cc\u062f.\u00bb"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0644\u06a9\u0646 \u0627\u06cc\u0646 \u0627\u0634\u062e\u0627\u0635 \u0628\u0631 \u0622\u0646\u0686\u0647 \u0646\u0645\u06cc \u062f\u0627\u0646\u0646\u062f \u0627\u0641\u062a\u0631\u0627\u0621 \u0645\u06cc\u200c\u0632\u0646\u0646\u062f \u0648 \u062f\u0631 \u0622\u0646\u0686\u0647 \u0645\u062b\u0644 \u062d\u06cc\u0648\u0627\u0646 \u063a\u06cc\u0631\u0646\u0627\u0637\u0642 \u0628\u0627\u0644\u0637\u0628\u0639 \u0641\u0647\u0645\u06cc\u062f\u0647\u200c\u0627\u0646\u062f\u060c \u062e\u0648\u062f \u0631\u0627 \u0641\u0627\u0633\u062f\u0645\u06cc \u0633\u0627\u0632\u0646\u062f."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0648\u0627\u06cc \u0628\u0631 \u0627\u06cc\u0634\u0627\u0646 \u0632\u06cc\u0631\u0627 \u06a9\u0647 \u0628\u0647 \u0631\u0627\u0647 \u0642\u0627\u0626\u0646 \u0631\u0641\u062a\u0647\u200c\u0627\u0646\u062f \u0648\u062f\u0631 \u06af\u0645\u0631\u0627\u0647\u06cc \u0628\u0644\u0639\u0627\u0645 \u0628\u062c\u0647\u062a \u0627\u062c\u0631\u062a \u063a\u0631\u0642 \u0634\u062f\u0647\u200c\u0627\u0646\u062f \u0648 \u062f\u0631\u0645\u0634\u0627\u062c\u0631\u062a \u0642\u0648\u0631\u062d \u0647\u0644\u0627\u06a9 \u06af\u0634\u062a\u0647\u200c\u0627\u0646\u062f."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0627\u06cc\u0646\u0647\u0627 \u062f\u0631\u0636\u06cc\u0627\u0641\u062a \u0647\u0627\u06cc \u0645\u062d\u0628\u062a\u0627\u0646\u0647 \u0634\u0645\u0627 \u0635\u062e\u0631\u0647\u200c\u0647\u0627 \u0647\u0633\u062a\u0646\u062f \u0686\u0648\u0646 \u0628\u0627 \u0634\u0645\u0627 \u0634\u0627\u062f\u06cc \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f\u060c \u0648 \u0634\u0628\u0627\u0646\u0627\u0646\u06cc \u06a9\u0647 \u062e\u0648\u06cc\u0634\u062a\u0646 \u0631\u0627\u0628\u06cc \u062e\u0648\u0641 \u0645\u06cc\u200c\u067e\u0631\u0648\u0631\u0646\u062f \u0648 \u0627\u0628\u0631\u0647\u0627\u06cc \u0628\u06cc\u200c\u0622\u0628 \u0627\u0632 \u0628\u0627\u062f\u0647\u0627\u0631\u0627\u0646\u062f\u0647 \u0634\u062f\u0647 \u0648 \u062f\u0631 \u062e\u062a\u0627\u0646 \u0635\u06cc\u0641\u06cc \u0628\u06cc\u200c\u0645\u06cc\u0648\u0647\u060c \u062f\u0648\u0628\u0627\u0631\u0647 \u0645\u0631\u062f\u0647 \u0648 \u0627\u0632 \u0631\u06cc\u0634\u0647\u200c\u06a9\u0646\u062f\u0647 \u0634\u062f\u0647\u060c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0648 \u0627\u0645\u0648\u0627\u062c \u062c\u0648\u0634\u06cc\u062f\u0647 \u062f\u0631\u06cc\u0627 \u06a9\u0647 \u0631\u0633\u0648\u0627\u06cc\u06cc \u062e\u0648\u062f \u0631\u0627 \u0645\u062b\u0644 \u06a9\u0641 \u0628\u0631\u0645\u06cc \u0622\u0648\u0631\u0646\u062f \u0648 \u0633\u062a\u0627\u0631\u06af\u0627\u0646 \u0622\u0648\u0627\u0631\u0647 \u0647\u0633\u062a\u0646\u062f \u06a9\u0647 \u0628\u0631\u0627\u06cc \u0627\u06cc\u0634\u0627\u0646 \u062a\u0627\u0631\u06cc\u06a9\u06cc \u0638\u0644\u0645\u062a \u062c\u0627\u0648\u062f\u0627\u0646\u06cc \u0645\u0642\u0631\u0631 \u0627\u0633\u062a."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0644\u06a9\u0646 \u062e\u0646\u0648\u062e \u06a9\u0647 \u0647\u0641\u062a\u0645 \u0627\u0632\u0622\u062f\u0645 \u0628\u0648\u062f\u060c \u062f\u0631\u0628\u0627\u0631\u0647 \u0647\u0645\u06cc\u0646 \u0627\u0634\u062e\u0627\u0635 \u062e\u0628\u0631 \u062f\u0627\u062f\u0647\u060c \u06af\u0641\u062a: \u00ab\u0627\u06cc\u0646\u06a9 \u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0627 \u0647\u0632\u0627\u0631\u0627\u0646 \u0647\u0632\u0627\u0631 \u0627\u0632 \u0645\u0642\u062f\u0633\u06cc\u0646 \u062e\u0648\u062f \u06cc\u0647\u0648\u062f\u0627 \u0622\u0645\u062f"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u062a\u0627 \u0628\u0631\u0647\u0645\u0647 \u062f\u0627\u0648\u0631\u06cc \u0646\u0645\u0627\u06cc\u062f \u0648 \u062c\u0645\u06cc\u0639 \u0628\u06cc\u200c\u062f\u06cc\u0646\u0627\u0646 \u0631\u0627\u0645\u0644\u0632\u0645 \u0633\u0627\u0632\u062f\u060c \u0628\u0631 \u0647\u0645\u0647 \u06a9\u0627\u0631\u0647\u0627\u06cc \u0628\u06cc\u200c\u062f\u06cc\u0646\u06cc \u06a9\u0647 \u0627\u06cc\u0634\u0627\u0646 \u06a9\u0631\u062f\u0646\u062f \u0648 \u0628\u0631\u062a\u0645\u0627\u0645\u06cc \u0633\u062e\u0646\u0627\u0646 \u0632\u0634\u062a \u06a9\u0647 \u06af\u0646\u0627\u0647\u06a9\u0627\u0631\u0627\u0646 \u0628\u06cc\u200c\u062f\u06cc\u0646 \u0628\u0647 \u062e\u0644\u0627\u0641 \u0627\u0648 \u06af\u0641\u062a\u0646\u062f.\u00bb"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0627\u06cc\u0646\u0627\u0646\u0646\u062f \u0647\u0645\u0647\u0645\u0647\u200c\u06a9\u0646\u0627\u0646 \u0648 \u06af\u0644\u0647\u200c\u0645\u0646\u062f\u0627\u0646 \u06a9\u0647 \u0628\u0631\u062d\u0633\u0628 \u0634\u0647\u0648\u0627\u062a \u062e\u0648\u062f \u0633\u0644\u0648\u06a9 \u0645\u06cc\u200c\u0646\u0645\u0627\u06cc\u0646\u062f \u0648 \u0628\u0647 \u0632\u0628\u0627\u0646 \u062e\u0648\u062f \u0633\u062e\u0646\u0627\u0646 \u062a\u06a9\u0628\u0631\u0622\u0645\u06cc\u0632\u0645\u06cc \u06af\u0648\u06cc\u0646\u062f \u0648 \u0635\u0648\u0631\u062a\u0647\u0627\u06cc \u0645\u0631\u062f\u0645 \u0631\u0627 \u0628\u062c\u0647\u062a \u0633\u0648\u062f\u0645\u06cc \u067e\u0633\u0646\u062f\u0646\u062f."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0627\u0645\u0627 \u0634\u0645\u0627\u200c\u0627\u06cc \u062d\u0628\u06cc\u0628\u0627\u0646\u060c \u0628\u062e\u0627\u0637\u0631 \u0622\u0648\u0631\u06cc\u062f \u0622\u0646 \u0633\u062e\u0646\u0627\u0646\u06cc \u06a9\u0647 \u0631\u0633\u0648\u0644\u0627\u0646 \u062e\u062f\u0627\u0648\u0646\u062f \u0645\u0627 \u0639\u06cc\u0633\u06cc \u0645\u0633\u06cc\u062d \u067e\u06cc\u0634 \u06af\u0641\u062a\u0647\u200c\u0627\u0646\u062f\u060c"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0686\u0648\u0646 \u0628\u0647 \u0634\u0645\u0627 \u062e\u0628\u0631 \u062f\u0627\u062f\u0646\u062f \u06a9\u0647 \u062f\u0631 \u0632\u0645\u0627\u0646 \u0622\u062e\u0631 \u0645\u0633\u062a\u0647\u0632\u0626\u06cc\u0646 \u062e\u0648\u0627\u0647\u0646\u062f \u0622\u0645\u062f \u06a9\u0647 \u0628\u0631\u062d\u0633\u0628 \u0634\u0647\u0648\u0627\u062a \u0628\u06cc\u200c\u062f\u06cc\u0646\u06cc \u062e\u0648\u062f \u0631\u0641\u062a\u0627\u0631 \u062e\u0648\u0627\u0647\u0646\u062f \u06a9\u0631\u062f."
                },
                "19": {
                    "verse_nr": "19",
