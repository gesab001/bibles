{
    "version": "PES",
    "book_name": "3 John",
    "book_nr": "64",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0645\u0646 \u06a9\u0647 \u067e\u06cc\u0631\u0645\u060c \u0628\u0647 \u063a\u0627\u06cc\u0633 \u062d\u0628\u06cc\u0628 \u06a9\u0647 \u0627\u0648 \u0631\u0627 \u062f\u0631 \u0631\u0627\u0633\u062a\u06cc \u0645\u062d\u0628\u062a \u0645\u06cc\u200c\u0646\u0645\u0627\u06cc\u0645."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u200c\u0627\u06cc \u062d\u0628\u06cc\u0628\u060c \u062f\u0639\u0627 \u0645\u06cc\u200c\u06a9\u0646\u0645 \u06a9\u0647 \u062f\u0631 \u0647\u0631 \u0648\u062c\u0647 \u06a9\u0627\u0645\u06cc\u0627\u0628 \u0648 \u062a\u0646\u062f\u0631\u0633\u062a \u0628\u0648\u062f\u0647 \u0628\u0627\u0634\u06cc\u060c \u0686\u0646\u0627\u0646\u06a9\u0647 \u062c\u0627\u0646 \u062a\u0648\u06a9\u0627\u0645\u06cc\u0627\u0628 \u0627\u0633\u062a."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0632\u06cc\u0631\u0627 \u06a9\u0647 \u0628\u0633\u06cc\u0627\u0631 \u0634\u0627\u062f \u0634\u062f\u0645 \u0686\u0648\u0646 \u0628\u0631\u0627\u062f\u0631\u0627\u0646 \u0622\u0645\u062f\u0646\u062f \u0648 \u0628\u0631 \u0631\u0627\u0633\u062a\u06cc \u062a\u0648 \u0634\u0647\u0627\u062f\u062a \u062f\u0627\u062f\u0646\u062f\u060c \u0686\u0646\u0627\u0646\u06a9\u0647 \u062a\u0648 \u062f\u0631 \u0631\u0627\u0633\u062a\u06cc \u0633\u0644\u0648\u06a9 \u0645\u06cc\u200c\u0646\u0645\u0627\u06cc\u06cc."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0645\u0631\u0627 \u0628\u06cc\u0634 \u0627\u0632 \u0627\u06cc\u0646 \u0634\u0627\u062f\u06cc \u0646\u06cc\u0633\u062a \u06a9\u0647 \u0628\u0634\u0646\u0648\u0645 \u06a9\u0647 \u0641\u0631\u0632\u0646\u062f\u0627\u0646\u0645 \u062f\u0631\u0631\u0627\u0633\u062a\u06cc \u0633\u0644\u0648\u06a9 \u0645\u06cc\u200c\u0646\u0645\u0627\u06cc\u0646\u062f."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u200c\u0627\u06cc \u062d\u0628\u06cc\u0628\u060c \u0622\u0646\u0686\u0647 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0628\u0647 \u0628\u0631\u0627\u062f\u0631\u0627\u0646 \u062e\u0635\u0648\u0635 \u0628\u0647 \u063a\u0631\u06cc\u0628\u0627\u0646\u060c \u0628\u0647 \u0627\u0645\u0627\u0646\u062a \u0645\u06cc\u200c\u06a9\u0646\u06cc\u060c"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u06a9\u0647 \u062f\u0631 \u062d\u0636\u0648\u0631 \u06a9\u0644\u06cc\u0633\u0627 \u0628\u0631 \u0645\u062d\u0628\u062a \u062a\u0648\u0634\u0647\u0627\u062f\u062a \u062f\u0627\u062f\u0646\u062f \u0648 \u0647\u0631\u06af\u0627\u0647 \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627 \u0628\u0637\u0648\u0631 \u0634\u0627\u06cc\u0633\u062a\u0647 \u062e\u062f\u0627\u0628\u062f\u0631\u0642\u0647 \u06a9\u0646\u06cc\u060c \u0646\u06cc\u06a9\u0648\u06cc\u06cc \u0645\u06cc\u200c\u0646\u0645\u0627\u06cc\u06cc \u0632\u06cc\u0631\u0627 \u06a9\u0647 \u0628\u062c\u0647\u062a \u0627\u0633\u0645 \u0627\u0648 \u0628\u06cc\u0631\u0648\u0646 \u0631\u0641\u062a\u0646\u062f \u0648 \u0627\u0632 \u0627\u0645\u062a\u0647\u0627 \u0686\u06cc\u0632\u06cc \u0646\u0645\u06cc \u06af\u06cc\u0631\u0646\u062f."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u067e\u0633 \u0628\u0631 \u0645\u0627 \u0648\u0627\u062c\u0628 \u0627\u0633\u062a \u06a9\u0647 \u0686\u0646\u06cc\u0646 \u0627\u0634\u062e\u0627\u0635 \u0631\u0627 \u0628\u067e\u0630\u06cc\u0631\u06cc\u0645 \u062a\u0627\u0634\u0631\u06cc\u06a9 \u0631\u0627\u0633\u062a\u06cc \u0628\u0634\u0648\u06cc\u0645."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0628\u0647 \u06a9\u0644\u06cc\u0633\u0627 \u0686\u06cc\u0632\u06cc \u0646\u0648\u0634\u062a\u0645 \u0644\u06a9\u0646 \u062f\u06cc\u0648\u062a\u0631\u0641\u06cc\u0633 \u06a9\u0647 \u0633\u0631\u062f\u0627\u0631\u06cc \u0628\u0631 \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627 \u062f\u0648\u0633\u062a \u0645\u06cc\u200c\u062f\u0627\u0631\u062f \u0645\u0627 \u0631\u0627 \u0642\u0628\u0648\u0644 \u0646\u0645\u06cc \u06a9\u0646\u062f."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0644\u0647\u0630\u0627 \u0627\u06af\u0631 \u0622\u06cc\u0645 \u06a9\u0627\u0631\u0647\u0627\u06cc\u06cc \u0631\u0627 \u06a9\u0647 \u0627\u0648 \u0645\u06cc\u200c\u06a9\u0646\u062f\u0628\u0647 \u06cc\u0627\u062f \u062e\u0648\u0627\u0647\u0645 \u0622\u0648\u0631\u062f \u0632\u06cc\u0631\u0627 \u0628\u0647 \u0633\u062e\u0646\u0627\u0646 \u0646\u0627\u0634\u0627\u06cc\u0633\u062a\u0647 \u0628\u0631\u0645\u0627 \u06cc\u0627\u0648\u0647\u200c\u06af\u0648\u06cc\u06cc \u0645\u06cc\u200c\u06a9\u0646\u062f \u0648 \u0628\u0647 \u0627\u06cc\u0646 \u0642\u0627\u0646\u0639 \u0646\u0634\u062f\u0647\u060c \u0628\u0631\u0627\u062f\u0631\u0627\u0646 \u0631\u0627 \u062e\u0648\u062f \u0646\u0645\u06cc \u067e\u0630\u06cc\u0631\u062f \u0648 \u06a9\u0633\u0627\u0646\u06cc \u0631\u0627 \u0646\u06cc\u0632 \u06a9\u0647 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u0646\u062f\u060c \u0645\u0627\u0646\u0639 \u0627\u06cc\u0634\u0627\u0646 \u0645\u06cc\u200c\u0634\u0648\u062f \u0648 \u0627\u0632 \u06a9\u0644\u06cc\u0633\u0627 \u0628\u06cc\u0631\u0648\u0646 \u0645\u06cc\u200c\u06a9\u0646\u062f."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u200c\u0627\u06cc \u062d\u0628\u06cc\u0628 \u0628\u0647 \u0628\u062f\u06cc \u0627\u0642\u062a\u062f\u0627 \u0645\u0646\u0645\u0627 \u0628\u0644\u06a9\u0647 \u0628\u0647 \u0646\u06cc\u06a9\u0648\u06cc\u06cc \u0632\u06cc\u0631\u0627 \u0646\u06cc\u06a9\u0648\u200c\u06a9\u0631\u062f\u0627\u0631 \u0627\u0632 \u062e\u062f\u0627\u0633\u062a \u0648 \u0628\u062f\u06a9\u0631\u062f\u0627\u0631 \u062e\u062f\u0627 \u0631\u0627\u0646\u062f\u06cc\u062f\u0647 \u0627\u0633\u062a."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0647\u0645\u0647 \u0645\u0631\u062f\u0645 \u0648 \u062e\u0648\u062f \u0631\u0627\u0633\u062a\u06cc \u0646\u06cc\u0632 \u0628\u0631\u062f\u06cc\u0645\u062a\u0631\u06cc\u0648\u0633 \u0634\u0647\u0627\u062f\u062a \u0645\u06cc\u200c\u062f\u0647\u0646\u062f \u0648 \u0645\u0627 \u0647\u0645 \u0634\u0647\u0627\u062f\u062a \u0645\u06cc\u200c\u062f\u0647\u06cc\u0645 \u0648 \u0622\u06af\u0627\u0647\u06cc\u062f \u06a9\u0647 \u0634\u0647\u0627\u062f\u062a \u0645\u0627 \u0631\u0627\u0633\u062a \u0627\u0633\u062a."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0645\u0631\u0627 \u0686\u06cc\u0632\u0647\u0627\u06cc \u0628\u0633\u06cc\u0627\u0631 \u0628\u0648\u062f \u06a9\u0647 \u0628\u0647 \u062a\u0648 \u0628\u0646\u0648\u06cc\u0633\u0645\u060c \u0644\u06a9\u0646 \u0646\u0645\u06cc \u062e\u0648\u0627\u0647\u0645 \u0628\u0647 \u0645\u0631\u06a9\u0628 \u0648 \u0642\u0644\u0645 \u0628\u0647 \u062a\u0648 \u0628\u0646\u0648\u06cc\u0633\u0645."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0644\u06a9\u0646 \u0627\u0645\u06cc\u062f\u0648\u0627\u0631\u0645 \u06a9\u0647 \u0628\u0647 \u0632\u0648\u062f\u06cc \u062a\u0648 \u0631\u0627 \u062e\u0648\u0627\u0647\u0645 \u062f\u06cc\u062f \u0648\u0632\u0628\u0627\u0646\u06cc \u06af\u0641\u062a\u06af\u0648 \u06a9\u0646\u06cc\u0645."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0633\u0644\u0627\u0645 \u0628\u0631 \u062a\u0648 \u0628\u0627\u062f. \u062f\u0648\u0633\u062a\u0627\u0646 \u0628\u0647 \u062a\u0648 \u0633\u0644\u0627\u0645 \u0645\u06cc\u200c\u0631\u0633\u0627\u0646\u0646\u062f. \u0633\u0644\u0627\u0645 \u0645\u0631\u0627 \u0628\u0647 \u062f\u0648\u0633\u062a\u0627\u0646 \u0646\u0627\u0645 \u0628\u0647 \u0646\u0627\u0645 \u0628\u0631\u0633\u0627\u0646."
                }
            }
        }
    }
}