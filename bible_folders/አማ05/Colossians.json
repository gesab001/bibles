        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1348\u1243\u12f5 \u12e8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u1210\u12cb\u122d\u12eb \u12a8\u1206\u1290\u12cd \u12a8\u1333\u12cd\u120e\u1235\u1293 \u12a8\u12c8\u1295\u12f5\u121b\u127d\u1295 \u12a8\u1322\u121e\u1274\u12ce\u1235\u1365"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1260\u1248\u120b\u1235\u12ed\u1235 \u1208\u121a\u1296\u1229\u1365 \u1260\u12ad\u122d\u1235\u1276\u1235 \u1245\u12f1\u1233\u1295\u1293 \u1273\u121b\u129e\u127d \u1208\u1206\u1291 \u12a0\u121b\u129e\u127d\u1365\n\u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a8\u12a0\u1263\u1273\u127d\u1295 \u1338\u130b\u1293 \u1230\u120b\u121d \u12ed\u1201\u1295\u120b\u127d\u1201\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1208\u12a5\u1293\u1295\u1270 \u1260\u121d\u1295\u1338\u120d\u12ed\u1260\u1275 \u130a\u12dc \u12e8\u130c\u1273\u127d\u1295 \u12e8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235\u1295 \u12a0\u1263\u1275 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u12d8\u12c8\u1275\u122d \u12a5\u1293\u1218\u1230\u130d\u1293\u1208\u1295\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1260\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u12eb\u120b\u127d\u1201\u1295 \u12a5\u121d\u1290\u1275\u1293 \u1235\u1208 \u121d\u12a5\u1218\u1293\u1295 \u1201\u1209 \u12eb\u120b\u127d\u1201\u1295 \u134d\u1245\u122d \u1230\u121d\u1270\u1293\u120d\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12e8\u12a5\u12cd\u1290\u1275 \u1243\u120d \u12e8\u1206\u1290\u12cd \u12c8\u1295\u130c\u120d \u1260\u1218\u1300\u1218\u122a\u12eb \u12c8\u12f0 \u12a5\u1293\u1295\u1270 \u1260\u12f0\u1228\u1230 \u130a\u12dc \u1260\u12a5\u122d\u1231 \u12eb\u1208\u12cd\u1295 \u1270\u1235\u134b \u1230\u121d\u1273\u127d\u128b\u120d\u1364 \u1235\u1208\u12da\u1205 \u12a5\u121d\u1290\u1273\u127d\u1201\u1293 \u134d\u1245\u122b\u127d\u1201 \u12e8\u1270\u1218\u1220\u1228\u1270\u12cd \u1260\u1230\u121b\u12ed \u1270\u12d8\u130b\u1305\u1276 \u1260\u121a\u1248\u12eb\u127d\u1201 \u1260\u12da\u1205 \u1270\u1235\u134b \u120b\u12ed \u1290\u12cd\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12a5\u1293\u1295\u1270 \u12c8\u1295\u130c\u120d\u1295 \u12a8\u1230\u121b\u127d\u1201\u1260\u1275\u1293 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295\u121d \u1338\u130b \u12a5\u12cd\u1290\u1270\u129b\u1290\u1275 \u12a8\u1270\u1228\u12f3\u127d\u1201\u1260\u1275 \u1240\u1295 \u1300\u121d\u122e \u12ed\u1205 \u12c8\u1295\u130c\u120d \u1260\u12a5\u1293\u1295\u1270 \u1218\u12ab\u12a8\u120d \u12a5\u1295\u12f0\u1206\u1290\u12cd \u1260\u1218\u120b\u12cd \u12d3\u1208\u121d \u1260\u121b\u134d\u122b\u1275\u1293 \u1260\u121b\u12f0\u130d \u120b\u12ed \u1290\u12cd\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12ed\u1205\u1295\u1295\u121d \u1260\u12a5\u129b \u121d\u1275\u12ad \u12e8\u12ad\u122d\u1235\u1276\u1235 \u1273\u121b\u129d \u12a0\u1308\u120d\u130b\u12ed \u12e8\u1206\u1290\u12cd\u1293 \u12a8\u12a5\u129b\u121d \u130b\u122d \u12a0\u1265\u122e\u1295 \u12e8\u121a\u1220\u122b\u12cd \u12e8\u1270\u12c8\u12f0\u12f0\u12cd \u12a4\u1333\u134d\u122b \u1290\u130d\u122e\u12a0\u127d\u128b\u120d\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u12a5\u1295\u12f2\u1201\u121d \u12a5\u122d\u1231 \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1218\u1295\u1348\u1235 \u12a5\u122d\u1235 \u1260\u122d\u1233\u127d\u1201 \u1218\u12cb\u12f0\u12f3\u127d\u1201\u1295 \u1290\u130d\u122e\u1293\u120d\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1235\u1208\u12da\u1205 \u12ed\u1205\u1295 \u12a8\u1230\u121b\u1295\u1260\u1275 \u1240\u1295 \u1300\u121d\u122e \u12a5\u1293\u1295\u1270 \u1260\u1218\u1295\u1348\u1233\u12ca \u1325\u1260\u1265\u1293 \u121b\u1235\u1270\u12cb\u120d \u1201\u1209 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u1348\u1243\u12f5 \u12e8\u121d\u1275\u1308\u1290\u12d8\u1261\u1260\u1275 \u1219\u1209 \u12d5\u12cd\u1240\u1275 \u12a5\u1295\u12f2\u1296\u122b\u127d\u1201 \u1208\u12a5\u1293\u1295\u1270 \u12a8\u1218\u1338\u1208\u12ed\u1293 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u12a8\u1218\u1208\u1218\u1295 \u12a0\u120b\u124b\u1228\u1325\u1295\u121d\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u1295\u12f2\u1201\u121d \u1208\u130c\u1273 \u1270\u1308\u1262 \u1260\u1206\u1290 \u1218\u1295\u1308\u12f5 \u1260\u1218\u1296\u122d \u12a5\u122d\u1231\u1295 \u1260\u134d\u1339\u121d \u12a5\u1295\u12f5\u1273\u1235\u12f0\u1235\u1271 \u121b\u1293\u1278\u12cd\u1295\u121d \u1218\u120d\u12ab\u121d \u1225\u122b \u1260\u1218\u1225\u122b\u1275 \u134d\u122c \u12a5\u1295\u12f5\u1273\u1348\u1229\u1293 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295\u121d \u1260\u121b\u12c8\u1245 \u12a5\u12eb\u12f0\u130b\u127d\u1201 \u12a5\u1295\u12f5\u1275\u1204\u12f1 \u12a5\u1295\u1338\u120d\u12eb\u1208\u1295\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u130c\u1273 \u1260\u1308\u1293\u1293\u12cd \u1280\u12ed\u1209 \u1265\u122d\u1273\u1271\u1295 \u1201\u1209 \u12ed\u1235\u1323\u127d\u1201\u1364 \u1260\u1275\u12d5\u130d\u1225\u1275\u121d \u1201\u1209\u1295 \u1290\u1308\u122d \u1208\u1218\u127b\u120d \u1260\u12f0\u1235\u1273 \u12e8\u1270\u12d8\u130b\u1303\u127d\u1201 \u1208\u1218\u1206\u1295 \u12eb\u1265\u1243\u127d\u1201\u1364"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u12f0\u130d\u121e\u121d \u1260\u1265\u122d\u1203\u1295 \u1218\u1295\u130d\u1225\u1275 \u12cd\u1235\u1325 \u12a8\u1245\u12f1\u1233\u1295 \u130b\u122d \u122d\u1235\u1275 \u12a5\u1295\u12f5\u1275\u12ab\u1348\u1209 \u12eb\u1260\u1243\u127d\u1201\u1295 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a0\u1265\u1295 \u1260\u12f0\u1235\u1273 \u12a0\u1218\u1235\u130d\u1291\u1275\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u12a5\u122d\u1231 \u12a8\u1328\u1208\u121b \u1280\u12ed\u120d \u12a0\u12f5\u1296 \u12c8\u12f0 \u1270\u12c8\u12f0\u12f0\u12cd \u120d\u1301 \u1218\u1295\u130d\u1225\u1275 \u12a5\u1295\u12f5\u1295\u1308\u1263 \u12a0\u12f5\u122d\u130e\u1293\u120d\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u1260\u12a5\u122d\u1231\u121d \u1270\u12cb\u1305\u1270\u1295 \u12e8\u1283\u1322\u12a0\u1273\u127d\u1295\u1295 \u12ed\u1245\u122d\u1273 \u12a0\u130d\u129d\u1270\u1293\u120d\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12ad\u122d\u1235\u1276\u1235 \u1208\u121b\u12ed\u1273\u12e8\u12cd \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u12cd\u1290\u1270\u129b \u121d\u1233\u120c \u1290\u12cd\u1362 \u12a5\u122d\u1231 \u12a8\u134d\u1325\u1228\u1275 \u1201\u1209 \u1260\u120b\u12ed \u1273\u120b\u1245\u1293 \u1260\u12b2\u122d \u1290\u12cd\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u1260\u1230\u121b\u12ed\u1293 \u1260\u121d\u12f5\u122d \u12eb\u1209\u1275\u1365 \u12e8\u121a\u1273\u12e9\u1275\u1293 \u12e8\u121b\u12ed\u1273\u12e9\u1275\u1365 \u12e8\u1230\u121b\u12ed \u1280\u12ed\u120e\u127d\u1293 \u1308\u12e2\u12ce\u127d\u1365 \u12a0\u1208\u1246\u127d\u1293 \u1263\u1208\u1225\u120d\u1323\u1296\u127d \u1201\u1209 \u12e8\u1270\u1348\u1320\u1229\u1275 \u1260\u12a5\u122d\u1231 \u1290\u12cd\u1362 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1201\u1209\u1295 \u1290\u1308\u122d \u12e8\u1348\u1320\u1228\u12cd \u1260\u12a5\u122d\u1231\u1293 \u1208\u12a5\u122d\u1231 \u1290\u12cd\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u12a5\u122d\u1231 \u12a8\u1201\u1209 \u1290\u1308\u122d \u1260\u134a\u1275 \u1290\u1260\u1228\u1364 \u1201\u1209\u121d \u1290\u1308\u122d \u1270\u12eb\u12ed\u12de \u12e8\u1246\u1218\u12cd \u1260\u12a5\u122d\u1231 \u1290\u12cd\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u12a5\u122d\u1231 \u12a0\u12ab\u1209 \u12e8\u1206\u1290\u127d\u12cd \u12e8\u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1295 \u122b\u1235 \u1290\u12cd\u1364 \u1260\u1201\u1209 \u1290\u1308\u122d \u1240\u12f3\u121a \u12a5\u1295\u12f2\u1206\u1295\u1364 \u12a8\u121e\u1275 \u1260\u1218\u1290\u1223\u1275\u121d \u134a\u1270\u129b\u1293 \u1218\u1300\u1218\u122a\u12eb \u1290\u12cd\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12ed\u1205\u121d \u12e8\u1206\u1290\u12cd \u12e8\u1218\u1208\u12ae\u1275 \u1219\u120b\u1275 \u1260\u120d\u1301 \u12a5\u1295\u12f2\u1296\u122d \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a0\u1265 \u1348\u1243\u12f5 \u1235\u1208 \u1206\u1290 \u1290\u12cd\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u1260\u12a5\u122d\u1231\u121d \u12a0\u121b\u12ab\u12ed\u1290\u1275 \u1260\u1230\u121b\u12ed\u121d \u1206\u1290 \u1260\u121d\u12f5\u122d \u12eb\u1209\u1275\u1295 \u1290\u1308\u122e\u127d \u1201\u1209 \u12a8\u122b\u1231 \u130b\u122d \u12a0\u1235\u1273\u1228\u1240\u1364 \u1260\u1218\u1235\u1240\u120d \u120b\u12ed \u1260\u1348\u1230\u1230\u12cd \u1260\u120d\u1301 \u12f0\u121d\u121d \u1230\u120b\u121d\u1295 \u12a0\u12f0\u1228\u1308\u1362"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u1240\u12f5\u121e \u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u122d\u1243\u127d\u1201 \u1275\u1296\u1229 \u1290\u1260\u122d\u1364 \u1260\u1210\u1233\u1263\u127d\u1201\u1293 \u1260\u12ad\u1349 \u1225\u122b\u127d\u1201\u121d \u121d\u12ad\u1295\u12eb\u1275 \u1320\u120b\u1276\u1279 \u1290\u1260\u122b\u127d\u1201\u1362"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u12a0\u1201\u1295 \u130d\u1295 \u1245\u12f1\u1233\u1295\u1293 \u1295\u1339\u1213\u1295\u1365 \u1290\u1240\u134b\u121d \u12e8\u120c\u1208\u1263\u127d\u1201 \u12a0\u12f5\u122d\u130e \u1260\u134a\u1271 \u120a\u12eb\u1240\u122d\u1263\u127d\u1201\u1365 \u120d\u1301 \u1260\u1225\u130b \u1260\u1218\u121e\u1271 \u121d\u12ad\u1295\u12eb\u1275 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a8\u122b\u1231 \u130b\u122d \u12a0\u1235\u1273\u1228\u1243\u127d\u1201\u1362"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u12ed\u1205\u121d \u12e8\u121a\u1206\u1290\u12cd \u1270\u1218\u1225\u122d\u1273\u127d\u1201\u1293 \u1270\u12f0\u120b\u12f5\u120b\u127d\u1201 \u1260\u12c8\u1295\u130c\u1209 \u12a8\u1270\u1308\u1298\u12cd \u1270\u1235\u134b \u1233\u1275\u1293\u12c8\u1321 \u1260\u12a5\u121d\u1290\u1275 \u1338\u1295\u1273\u127d\u1201 \u1235\u1275\u1296\u1229 \u1265\u127b \u1290\u12cd\u1364 \u12ed\u1205\u121d \u12c8\u1295\u130c\u120d \u12a5\u1293\u1295\u1270 \u12e8\u1230\u121b\u127d\u1201\u1275\u1293 \u1260\u12d3\u1208\u121d \u120b\u12ed \u120b\u1209\u1275 \u1230\u12ce\u127d \u1201\u1209 \u12e8\u1270\u1230\u1260\u12a8 \u1290\u12cd\u1364 \u12a5\u1294 \u1333\u12cd\u120e\u1235\u121d \u12a0\u1308\u120d\u130b\u12ed \u12e8\u1206\u1295\u12a9\u1275 \u1208\u12da\u1201 \u12c8\u1295\u130c\u120d \u1290\u12cd\u1362"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u12a0\u1201\u1295 \u1235\u1208 \u12a5\u1293\u1295\u1270 \u1260\u121d\u1240\u1260\u1208\u12cd \u1218\u12a8\u122b \u12f0\u1235 \u12ed\u1208\u129b\u120d\u1364 \u12a0\u12ab\u1209 \u1235\u1208 \u1206\u1290\u127d\u12cd \u1235\u1208 \u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1295\u121d \u12a8\u12ad\u122d\u1235\u1276\u1235 \u1218\u12a8\u122b \u12a8\u1218\u12ab\u1348\u120d \u12e8\u1310\u12f0\u1208\u1265\u129d\u1295 \u1260\u1225\u130b\u12ec \u12a0\u121f\u120b\u1208\u1201\u1362"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u1243\u120d \u1208\u12a5\u1293\u1295\u1270 \u1260\u1219\u1209 \u12a5\u1308\u120d\u1325 \u12d8\u1295\u12f5 \u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1260\u1270\u1230\u1320\u129d \u1280\u120b\u134a\u1290\u1275 \u1218\u1220\u1228\u1275 \u12e8\u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1295 \u12a0\u1308\u120d\u130b\u12ed \u1206\u129c\u12a0\u1208\u1201\u1362"
                },
                "26": {
                    "verse_nr": "26",
                    "verse": "\u12ed\u1205\u121d \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1243\u120d \u1263\u1208\u1349\u1275 \u12d8\u1218\u1293\u1275 \u12a8\u1230\u12cd \u120d\u1306\u127d \u1270\u1230\u12cd\u122e \u12e8\u1248\u12e8\u12cd\u1293 \u12a0\u1201\u1295 \u130d\u1295 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1208\u121a\u12eb\u121d\u1291\u1275 \u1201\u1209 \u12e8\u1308\u1208\u1320\u12cd \u121d\u1225\u1322\u122d \u1290\u12cd\u1362"
                },
                "27": {
                    "verse_nr": "27",
