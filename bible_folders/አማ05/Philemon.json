        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1235\u1208 \u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u12a5\u1235\u1228\u129b \u12a8\u1206\u1295\u12a9 \u12a8\u12a5\u1294 \u12a8\u1333\u12cd\u120e\u1235\u1293 \u12a8\u12c8\u1295\u12f5\u121b\u127d\u1295 \u12a8\u1322\u121e\u1274\u12ce\u1235\u1365\n\u1208\u1270\u12c8\u12f0\u12f0\u12cd \u12c8\u1295\u12f5\u121b\u127d\u1295\u1293 \u12e8\u1225\u122b \u1313\u12f0\u129b\u127d\u1295 \u1208\u1206\u1290\u12cd \u1208\u134a\u120d\u121e\u1293\u1365"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1208\u12a5\u1285\u1273\u127d\u1295 \u1208\u12a0\u134d\u1265\u12eb\u1365 \u12a8\u12a5\u129b \u130b\u122d \u12e8\u12ad\u122d\u1235\u1276\u1235 \u12c8\u1273\u12f0\u122d \u1208\u1206\u1290\u12cd \u1208\u12a0\u122d\u12aa\u1333\u1235\u1365 \u12a5\u1295\u12f2\u1201\u121d \u1260\u1264\u1275\u1205 \u1208\u121a\u1230\u1260\u1230\u1261 \u121d\u12a5\u1218\u1293\u1295\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12a8\u12a0\u1263\u1273\u127d\u1295 \u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1365 \u12a8\u130c\u1273\u127d\u1295\u121d \u12a8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u1338\u130b\u1293 \u1230\u120b\u121d \u1208\u12a5\u1293\u1295\u1270 \u12ed\u1201\u1295\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1260\u1338\u120e\u1274 \u12a0\u1295\u1270\u1295 \u12a5\u12eb\u1230\u1265\u12a9 \u1201\u120d\u130a\u12dc \u12a0\u121d\u120b\u12ac\u1295 \u12a0\u1218\u1230\u130d\u1293\u1208\u1201\u1364"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1260\u130c\u1273 \u1260\u12a2\u12e8\u1231\u1235 \u12eb\u1208\u1205\u1295 \u12a5\u121d\u1290\u1275\u1293 \u1208\u121d\u12a5\u1218\u1293\u1295\u121d \u12eb\u1208\u1205\u1295 \u134d\u1245\u122d \u1230\u121d\u127c\u12a0\u1208\u1201\u1364"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12e8\u12ad\u122d\u1235\u1276\u1235 \u1260\u1218\u1206\u1293\u127d\u1295 \u12eb\u1208\u1295 \u1218\u120d\u12ab\u121d \u1290\u1308\u122d \u1201\u1209 \u1260\u12ed\u134b \u12ed\u1273\u12c8\u1245 \u12d8\u1295\u12f5 \u12a5\u121d\u1290\u1275\u1205\u1295 \u1208\u120c\u120e\u127d \u1208\u121b\u12ab\u1348\u120d \u12a5\u1295\u12f5\u1275\u1270\u130b \u12a5\u1338\u120d\u12eb\u1208\u1201\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12c8\u1295\u12f5\u121c \u1206\u12ed! \u1260\u12a0\u1295\u1270 \u121d\u12ad\u1295\u12eb\u1275 \u12e8\u121d\u12a5\u1218\u1293\u1295 \u120d\u1265 \u1235\u1208 \u1273\u12f0\u1230 \u134d\u1245\u122d\u1205 \u1273\u120b\u1245 \u12f0\u1235\u1273\u1295\u1293 \u1218\u133d\u1293\u1293\u1275\u1295 \u1230\u1325\u1276\u129b\u120d\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1235\u1208\u12da\u1205 \u1270\u130d\u1263\u122d\u1205\u1295 \u1348\u133d\u121d \u1265\u12ec \u12a0\u1295\u1270\u1295 \u1208\u121b\u12d8\u12dd \u1260\u12ad\u122d\u1235\u1276\u1235 \u12f5\u134d\u1228\u1275 \u1290\u1260\u1228\u129d\u1364"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1206\u1296\u121d \u12a5\u1295\u12f0\u12da\u12eb \u12a8\u121b\u12f5\u1228\u130d \u12ed\u120d\u1245 \u1235\u1208 \u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u12a5\u1235\u1228\u129b \u12e8\u1206\u1295\u12a9\u1275 \u12a5\u1294 \u123d\u121b\u130d\u120c\u12cd \u1333\u12cd\u120e\u1235 \u1260\u134d\u1245\u122d \u1218\u1208\u1218\u1295\u1295 \u12a5\u1218\u122d\u1323\u1208\u1201\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u1294 \u12a5\u1235\u122d \u1264\u1275 \u12a5\u12eb\u1208\u1201 \u120d\u1304 \u1235\u1208 \u1206\u1290\u12cd \u1235\u1208 \u12a6\u1294\u1232\u121e\u1235 \u12a0\u1295\u1270\u1295 \u12a5\u1208\u121d\u1295\u1203\u1208\u1201\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u12a0\u1293\u1232\u121e\u1235 \u12a8\u12da\u1205 \u1260\u134a\u1275 \u12a0\u12ed\u1320\u1245\u121d\u1205\u121d \u1290\u1260\u122d\u1364 \u12a0\u1201\u1295 \u130d\u1295 \u1208\u12a0\u1295\u1270\u121d \u1206\u1290 \u1208\u12a5\u1294 \u1320\u1243\u121a \u1290\u12cd\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u12a5\u1290\u1206 \u12a5\u122d\u1231\u1295 \u12c8\u12f0 \u12a0\u1295\u1270 \u1218\u120d\u123c \u120d\u12ac\u12cb\u1208\u1201\u1364 \u12a5\u122d\u1231\u1295 \u12c8\u12f0 \u12a0\u1295\u1270 \u1235\u120d\u12a8\u12cd\u121d \u12e8\u1308\u12db \u122b\u1234\u1295 \u120d\u1265 \u12a5\u1295\u12f0 \u120b\u12ad\u1201 \u12a0\u12f5\u122d\u130c \u12a5\u1248\u1325\u1228\u12cb\u1208\u1201\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1260\u12c8\u1295\u130c\u120d \u121d\u12ad\u1295\u12eb\u1275 \u12a5\u12da\u1205 \u1273\u1235\u122c \u1233\u1208\u1201 \u1260\u12a0\u1295\u1270 \u121d\u1275\u12ad \u1206\u1296 \u12a5\u1295\u12f2\u12eb\u1308\u1208\u130d\u1208\u129d \u12a5\u122d\u1231 \u12a8\u12a5\u1294 \u130b\u122d \u1262\u1206\u1295 \u12f0\u1235 \u1263\u1208\u129d \u1290\u1260\u122d\u1364"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u1290\u1308\u122d \u130d\u1295 \u12e8\u12a0\u1295\u1270 \u1218\u120d\u12ab\u121d \u1225\u122b \u1260\u130d\u12f4\u1273 \u1233\u12ed\u1206\u1295 \u1260\u12cd\u12f4\u1273 \u12a5\u1295\u12f2\u1206\u1295 \u1265\u12ec \u12a0\u1295\u1270\u1295 \u1233\u120b\u1235\u1348\u1245\u12f5 \u121d\u1295\u121d \u121b\u12f5\u1228\u130d \u12a0\u120d\u1348\u1208\u130d\u1201\u121d\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12a6\u1294\u1232\u121e\u1235 \u12a5\u1235\u12a8 \u12a0\u1201\u1295 \u1208\u1325\u1242\u1275 \u130a\u12dc \u12e8\u1270\u1208\u12e8\u1205 \u121d\u1293\u120d\u1263\u1275 \u12a8\u12a5\u1295\u130d\u12f2\u1205 \u12c8\u12f2\u1205 \u1208\u1201\u120d\u130a\u12dc \u12a8\u12a0\u1295\u1270 \u130b\u122d \u12a5\u1295\u12f2\u1296\u122d \u12ed\u1206\u1293\u120d\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u12a8\u12a5\u1295\u130d\u12f2\u1205 \u12c8\u12f2\u1205 \u12a8\u12a0\u1295\u1270 \u130b\u122d \u12e8\u121a\u1296\u1228\u12cd\u121d \u12a5\u1295\u12f0 \u1263\u122a\u12eb \u1233\u12ed\u1206\u1295 \u12a8\u1263\u122a\u12eb \u12e8\u1260\u1208\u1320 \u1206\u1296 \u12a5\u1295\u12f0 \u12cd\u12f5 \u12c8\u1295\u12f5\u121d \u1290\u12cd\u1364 \u12a5\u122d\u1231 \u1208\u12a5\u1294 \u12cd\u12f5 \u12c8\u1295\u12f5\u121d \u1290\u12cd\u1364 \u12ed\u1201\u1295 \u12a5\u1295\u1302 \u1260\u1225\u130b \u12a5\u122d\u1231 \u1260\u12a0\u1308\u120d\u130d\u120e\u1271 \u1235\u1208\u121a\u1320\u1245\u121d\u1205 \u1260\u1218\u1295\u1348\u1231 \u12f0\u130d\u121e \u1260\u12ad\u122d\u1235\u1276\u1235 \u12c8\u1295\u12f5\u121d\u1205 \u1235\u1208 \u1206\u1290 \u1208\u12a0\u1295\u1270 \u12ed\u1260\u120d\u1325 \u1270\u12c8\u12f3\u1305 \u1290\u12cd\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u12a5\u1295\u130d\u12f2\u1205 \u12a5\u1294\u1295 \u12a5\u1295\u12f0 \u12c8\u1295\u12f5\u121d \u12a0\u12f5\u122d\u1308\u1205 \u12a8\u1248\u1320\u122d\u12a8\u129d \u120d\u12ad \u12a5\u1295\u12f0 \u12a5\u1294 \u12a0\u12f5\u122d\u1308\u1205 \u1270\u1240\u1260\u1208\u12cd\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u12eb\u12f0\u1228\u1230\u1265\u1205 \u1260\u12f0\u120d \u1262\u1296\u122d \u12c8\u12ed\u121d \u12d5\u12f3 \u1262\u1296\u122d\u1260\u1275 \u12d5\u12f3\u12cd\u1295 \u1260\u12a5\u1294 \u120b\u12ed \u12a0\u12f5\u122d\u1308\u12cd\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12a5\u1290\u1206 \u12a5\u1294 \u1333\u12cd\u120e\u1235 \u201c\u12d5\u12f3\u12cd\u1295 \u12a5\u12a8\u134d\u120d\u1203\u1208\u1201\u201d \u1265\u12ec \u1260\u1308\u12db \u12a5\u1304 \u133d\u134c \u12a0\u1228\u130b\u130d\u1325\u120d\u1203\u1208\u1201\u1364 \u1206\u1296\u121d \u1235\u1208 \u1215\u12ed\u12c8\u1275\u1205 \u12a0\u1295\u1270 \u122b\u1235\u1205 \u12a5\u1295\u12b3 \u12e8\u12a5\u1294 \u1263\u1208\u12d5\u12f3 \u1218\u1206\u1295\u12ad\u1295 \u12a5\u1294 \u120b\u1235\u1273\u12cd\u1235\u1205 \u12a0\u12eb\u1235\u1348\u120d\u130d\u121d\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u12a5\u1295\u130d\u12f2\u1205 \u12c8\u1295\u12f5\u121c \u1206\u12ed! \u12ed\u1205\u1295 \u1218\u120d\u12ab\u121d \u1290\u1308\u122d \u12a5\u1295\u12f5\u1273\u12f0\u122d\u130d\u120d\u129d \u1260\u130c\u1273 \u12a5\u1208\u121d\u1295\u1203\u1208\u1201\u1364 \u12a5\u1263\u12ad\u1205 \u120d\u1264\u1295 \u1260\u12ad\u122d\u1235\u1276\u1235 \u12a0\u12f5\u1235\u120d\u129d\u1362"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u12ed\u1205\u1295 \u12e8\u133b\u134d\u12a9\u120d\u1205 \u12a5\u1295\u12f0\u121d\u1275\u1273\u12d8\u12d8\u129d \u1260\u1218\u1270\u121b\u1218\u1295 \u1290\u12cd\u1364 \u12a5\u1295\u12f2\u12eb\u12cd\u121d \u12a5\u1294 \u12a8\u121d\u1320\u12ed\u1245\u1205 \u1260\u120b\u12ed \u12a5\u1295\u12f0\u121d\u1273\u12f0\u122d\u130d \u12d0\u12cd\u1243\u1208\u1201\u1362"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u1260\u12da\u12eb\u12cd\u121d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1338\u120e\u1273\u127d\u1201\u1295 \u1230\u121d\u1276 \u12c8\u12f0 \u12a5\u1293\u1295\u1270 \u12a5\u1295\u12f5\u1218\u1323 \u12a5\u1295\u12f0\u121a\u12eb\u12f0\u122d\u1308\u129d \u1270\u1235\u134b \u1235\u120b\u1208\u129d \u12e8\u121b\u122d\u134d\u1260\u1275\u1295 \u1264\u1275 \u12a0\u12d8\u130b\u1305\u120d\u129d\u1362"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u1260\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u121d\u12ad\u1295\u12eb\u1275 \u12a8\u12a5\u1294 \u130b\u122d \u12e8\u1273\u1230\u1228\u12cd \u12a4\u1333\u134d\u122b \u1230\u120b\u121d\u1273 \u12eb\u1240\u122d\u1265\u120d\u1203\u120d\u1364"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u12a5\u1295\u12f2\u1201\u121d \u12e8\u1225\u122b \u1313\u12f0\u129e\u127c \u12e8\u1206\u1291\u1275 \u121b\u122d\u1246\u1235\u1365 \u12a0\u122d\u1235\u1325\u122e\u12ae\u1235\u1365 \u12f4\u121b\u1235\u1293\u1365 \u1209\u1243\u1235 \u1230\u120b\u121d\u1273 \u12eb\u1240\u122d\u1261\u120d\u1203\u120d\u1362"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u12e8\u130c\u1273 \u12e8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u1338\u130b \u12a8\u12a5\u1293\u1295\u1270 \u130b\u122d \u12ed\u1201\u1295\u1362"
                }
            }
        }
    }
}