        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1293 \u12a5\u1295\u12f2\u1201\u121d \u1218\u1295\u130d\u1225\u1271\u1295 \u1260\u121a\u12eb\u124b\u1241\u121d\u1260\u1275 \u130a\u12dc\u1365 \u1260\u1215\u12eb\u12cb\u1295\u1293 \u1260\u1219\u1273\u1295 \u120b\u12ed \u1260\u121a\u1348\u122d\u12f0\u12cd \u1260\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u134a\u1275 \u12d0\u12f0\u122b \u12a5\u120d\u1203\u1208\u1201\u1364"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1243\u1209\u1295 \u1235\u1260\u12ad\u1364 \u1262\u1218\u127d\u121d \u1263\u12ed\u1218\u127d\u121d \u1260\u121b\u1295\u129b\u12cd\u121d \u130a\u12dc \u1270\u130d\u1270\u1205 \u1225\u122b\u1364 \u12a5\u12eb\u1235\u1228\u12f3\u1205 \u12a5\u12e8\u1308\u1220\u133d\u12ad\u1293 \u12a5\u12e8\u1218\u12a8\u122d\u12ad\u1365 \u1260\u1275\u12d5\u130d\u1225\u1275\u1293 \u1260\u121b\u1235\u1270\u121b\u122d \u1275\u130b\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1230\u12ce\u127d \u1215\u12ed\u12c8\u1275 \u12e8\u121a\u1308\u129d\u1260\u1275\u1295 \u1275\u121d\u1205\u122d\u1275 \u1208\u1218\u1235\u121b\u1275 \u12e8\u121b\u12ed\u1348\u120d\u1309\u1260\u1275 \u130a\u12dc \u12ed\u1218\u1323\u120d\u1364 \u1290\u1308\u122d \u130d\u1295 \u1208\u12c8\u122c \u1260\u1218\u1310\u121d\u1300\u1275 \u12a5\u1290\u122d\u1231 \u122b\u1233\u1278\u12cd \u12e8\u121a\u12c8\u12f1\u1275\u1295 \u1290\u1308\u122d \u12e8\u121a\u1290\u130d\u1229\u12a0\u1278\u12cd\u1295 \u12a0\u1235\u1270\u121b\u122a\u12ce\u127d \u12ed\u1230\u1260\u1235\u1263\u1209\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1235\u1208\u12da\u1205 \u12a5\u12cd\u1290\u1275\u1295 \u1218\u1235\u121b\u1275 \u1275\u1270\u12cd \u1270\u1228\u1275\u1295 \u1218\u1235\u121b\u1275 \u12ed\u12c8\u12f3\u1209\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12a0\u1295\u1270 \u130d\u1295 \u1260\u1201\u1209 \u1290\u1308\u122d \u1325\u1295\u1241\u1245 \u1201\u1295\u1364 \u1218\u12a8\u122b\u1295 \u1260\u1218\u1240\u1260\u120d \u133d\u1293\u1364 \u12e8\u12c8\u1295\u130c\u120d \u1230\u1263\u12aa\u1290\u1275\u1295 \u1225\u122b \u12a0\u12a8\u1293\u12cd\u1295\u1364 \u12a0\u1308\u120d\u130d\u120e\u1275\u1205\u1295\u121d \u1201\u1209 \u1348\u133d\u121d\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12a5\u1294 \u130d\u1295 \u1218\u1225\u12cb\u12d5\u1275 \u1208\u1218\u1206\u1295 \u1270\u1243\u122d\u1264\u12a0\u1208\u1201\u1364 \u12a8\u12da\u1205 \u12d3\u1208\u121d \u1270\u1208\u12ed\u127c \u12e8\u121d\u1204\u12f5\u1260\u1275 \u130a\u12dc\u121d \u12f0\u122d\u1236\u12a0\u120d\u1364"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u1326\u122d\u1290\u1275\u1295 \u1260\u1218\u120d\u12ab\u121d \u1270\u12cb\u130d\u127b\u1208\u1201\u1364 \u12e8\u12a5\u123d\u1245\u12f5\u12f5\u121d \u1229\u132b\u12ec\u1295 \u12a5\u1235\u12a8 \u1218\u1328\u1228\u123b\u12cd \u122e\u132b\u1208\u1201\u1364 \u12a5\u121d\u1290\u1275\u1295 \u1320\u1265\u1244\u12a0\u1208\u1201\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u12a8\u12a5\u1295\u130d\u12f2\u1205 \u12c8\u12f2\u1205 \u12e8\u133d\u12f5\u1245 \u12a0\u12ad\u120a\u120d \u1270\u12d8\u130b\u1305\u1276 \u12ed\u1320\u1265\u1240\u129b\u120d\u1364 \u12ed\u1205\u1295\u1295\u121d \u12a0\u12ad\u120a\u120d \u12eb \u1275\u12ad\u12ad\u1208\u129b \u134d\u122d\u12f5\u1295 \u1348\u122b\u1301 \u130c\u1273 \u1260\u12da\u12eb\u1295 \u1240\u1295 \u12ed\u1230\u1320\u129b\u120d\u1364 \u12e8\u121a\u1230\u1320\u12cd\u121d \u1208\u12a5\u1294 \u1265\u127b \u1233\u12ed\u1206\u1295 \u12e8\u12a5\u122d\u1231\u1295 \u1218\u1308\u1208\u1325 \u1208\u121a\u12c8\u12f5\u12f1 \u1201\u1209 \u1290\u12cd\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1260\u134d\u1325\u1290\u1275 \u12c8\u12f0 \u12a5\u1294 \u12a5\u1295\u12f5\u1275\u1218\u1323 \u12ed\u1201\u1295\u1364"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12f4\u121b\u1235 \u12ed\u1205\u1295 \u12d3\u1208\u121d \u12c8\u12f6 \u1270\u1208\u12ed\u1276\u129b\u120d\u1364 \u12c8\u12f0 \u1270\u1230\u120e\u1295\u1244\u121d \u1204\u12f6\u12a0\u120d\u1364 \u1244\u122d\u1242\u1235 \u12c8\u12f0 \u1308\u120b\u1275\u12eb\u1365 \u1272\u1276\u121d \u12c8\u12f0 \u12f5\u120d\u121b\u1325\u12eb \u1204\u12f0\u12cb\u120d\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u12a8\u12a5\u1294 \u130b\u122d \u12eb\u1208\u12cd \u1209\u1243\u1235 \u1265\u127b \u1290\u12cd\u1364 \u121b\u122d\u1246\u1235 \u1208\u12a0\u1308\u120d\u130d\u120e\u1275 \u1235\u1208\u121a\u1320\u1245\u1218\u129d \u1348\u120d\u1308\u1205 \u12ed\u12d8\u12b8\u12cd \u1293\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u1272\u12aa\u1246\u1235\u1295 \u12c8\u12f0 \u12a4\u134c\u1236\u1295 \u120d\u12ac\u12cb\u1208\u1201\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1235\u1275\u1218\u1323 \u1260\u1322\u122e\u12a0\u12f3 \u1260\u12ab\u122d\u1351\u1235 \u12d8\u1295\u12f5 \u12e8\u1270\u12cd\u12a9\u1275\u1295 \u12ab\u1263 \u12ed\u12d8\u1205\u120d\u129d \u1293\u1364 \u1218\u133b\u1215\u134d\u1271\u1295\u121d \u12ed\u120d\u1241\u1295\u121d \u12e8\u1265\u122b\u1293 \u1218\u133b\u1215\u134d\u1271\u1295 \u12a0\u121d\u1323\u120d\u129d\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u12e8\u1293\u1235 \u12a0\u1295\u1325\u1228\u129b\u12cd \u12a5\u1235\u12ad\u1295\u12f5\u122d \u12a8\u134d\u1270\u129b \u1312\u12f3\u1275 \u12a0\u12f0\u1228\u1230\u1265\u129d\u1364 \u130c\u1273 \u12a5\u1295\u12f0 \u1225\u122b\u12cd \u12ed\u12a8\u134d\u1208\u12cb\u120d\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12a5\u122d\u1231 \u12a5\u129b \u12e8\u121d\u1295\u1293\u1308\u1228\u12cd\u1295 \u1260\u1265\u122d\u1271 \u1235\u1208\u121a\u1243\u12c8\u121d \u12a0\u1295\u1270\u121d \u122b\u1235\u1205 \u12a8\u12a5\u122d\u1231 \u1270\u1320\u1295\u1240\u1245\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u1260\u1270\u12a8\u1230\u1235\u12a9\u1260\u1275 \u1290\u1308\u122d \u12e8\u1218\u1300\u1218\u122a\u12eb \u1218\u12a8\u120b\u12a8\u12eb\u12ec\u1295 \u1263\u1240\u1228\u1265\u12a9 \u130a\u12dc \u1201\u1209\u121d \u1270\u1208\u12e9\u129d \u12a5\u1295\u1302 \u12a8\u12a5\u1294 \u130b\u122d \u1206\u1296 \u12e8\u1228\u12f3\u129d \u121b\u1295\u121d \u12a0\u120d\u1290\u1260\u1228\u121d\u1364 \u12ed\u1205\u1295\u1295\u121d \u1290\u1308\u122d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u1295\u12f0 \u1260\u12f0\u120d \u12a0\u12f5\u122d\u130e \u12a0\u12ed\u124a\u1320\u122d\u1263\u1278\u12cd\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u1290\u1308\u122d \u130d\u1295 \u1218\u120d\u12a5\u12ad\u1271 \u1260\u12a5\u1294 \u12a0\u121b\u12ab\u12ed\u1290\u1275 \u1260\u1219\u1209 \u12a5\u1295\u12f2\u1290\u1308\u122d\u1293 \u12a0\u1215\u12db\u1265 \u1201\u1209 \u12a5\u1295\u12f2\u1230\u1219\u1275 \u130c\u1273 \u12a8\u12a5\u1294 \u130b\u122d \u1206\u1296 \u12a0\u1260\u1228\u1273\u129d\u1364 \u12a8\u12a0\u1295\u1260\u1233\u121d \u12a0\u134d \u12f5\u129b\u1208\u1201\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u130c\u1273 \u12a8\u121b\u1295\u129b\u12cd\u121d \u12ad\u1349 \u1290\u1308\u122d \u12eb\u12f5\u1290\u129b\u120d\u1364 \u1260\u1230\u121b\u12ed \u120b\u1208\u12cd \u1218\u1295\u130d\u1225\u1271\u121d \u12eb\u1260\u1243\u129b\u120d\u1364 \u1208\u12a5\u122d\u1231 \u1208\u12d8\u1208\u12d3\u1208\u121d \u12a5\u1235\u12a8 \u12d8\u1208\u12d3\u1208\u121d \u12ad\u1265\u122d \u12ed\u1201\u1295! \u12a0\u121c\u1295\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u1208\u1335\u122d\u1235\u1245\u120b\u1293 \u1208\u12a0\u1242\u120b\u1365 \u1208\u12a6\u1294\u1232\u134e\u122d\u121d \u1264\u1270\u1230\u1266\u127d \u1230\u120b\u121d\u1273 \u12a0\u1245\u122d\u1265\u120d\u129d\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u12a4\u122b\u1235\u1326\u1235 \u1260\u1246\u122e\u1295\u1276\u1235 \u1240\u122d\u1276\u12a0\u120d\u1364 \u1325\u122e\u134a\u121e\u1235 \u1235\u1208 \u1273\u1218\u1218 \u1260\u121a\u120a\u1322 \u1275\u127c\u12cb\u1208\u1201\u1362"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u12ad\u1228\u121d\u1275 \u1233\u12ed\u1308\u1263 \u12c8\u12f0 \u12a5\u1294 \u12a5\u1295\u12f5\u1275\u1218\u1323 \u12ed\u1201\u1295\u1362\n\u12a4\u12cd\u1261\u1209\u1235\u1365 \u1331\u12f4\u1235\u1365 \u120a\u1296\u1235\u1365 \u1245\u120b\u12cd\u12f2\u12eb\u1293 \u12a0\u121b\u129e\u127d\u121d \u1201\u1209 \u1230\u120b\u121d\u1273 \u12eb\u1240\u122d\u1261\u120d\u1203\u120d\u1362"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u130c\u1273 \u12a8\u12a0\u1295\u1270 \u130b\u122d \u12ed\u1201\u1295\u1362\n\u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1338\u130b \u12a8\u12a5\u1293\u1295\u1270 \u130b\u122d \u12ed\u1201\u1295\u1362"
                }
            }
        }
