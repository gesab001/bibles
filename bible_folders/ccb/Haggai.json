                "7": {
                    "verse_nr": "7",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u4f60\u4eec\u8981\u8ba4\u771f\u53cd\u7701\u81ea\u5df1\u7684\u884c\u4e3a\u3002"
