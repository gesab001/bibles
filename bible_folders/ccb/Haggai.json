                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u4eec\u8981\u4e0a\u5c71\u4f10\u6728\uff0c\u91cd\u5efa\u8fd9\u6bbf\uff0c\u597d\u8ba9\u6211\u6b22\u559c\u5e76\u5f97\u5230\u5c0a\u5d07\u3002\u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4f60\u4eec\u76fc\u671b\u4e30\u6536\uff0c\u7ed3\u679c\u6536\u7684\u5f88\u5c11\uff1b\u4f60\u4eec\u628a\u6536\u6210\u5e26\u56de\u5bb6\uff0c\u6211\u5374\u628a\u5b83\u4eec\u5439\u8d70\u3002\u8fd9\u662f\u4ec0\u4e48\u7f18\u6545\uff1f\u4e07\u519b\u4e4b\u8036\u548c\u534e\u544a\u8bc9\u4f60\u4eec\uff0c\u2018\u56e0\u4e3a\u4f60\u4eec\u90fd\u5fd9\u4e8e\u5efa\u81ea\u5bb6\u7684\u623f\u5b50\uff0c\u4efb\u7531\u6211\u7684\u6bbf\u4e00\u7247\u5e9f\u589f\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6240\u4ee5\uff0c\u5929\u4e0d\u964d\u96e8\u9732\uff0c\u5730\u65e0\u51fa\u4ea7\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u8ba9\u65f1\u707e\u4e34\u5230\u5927\u5730\u3001\u7fa4\u5c71\u3001\u4e94\u8c37\u3001\u8461\u8404\u3001\u6a44\u6984\u7b49\u5730\u91cc\u7684\u51fa\u4ea7\u4ee5\u53ca\u4eba\u755c\uff0c\u4f7f\u4f60\u4eec\u4e00\u5207\u7684\u52b3\u788c\u4ed8\u8bf8\u4e1c\u6d41\u3002\u2019\u201d"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6492\u62c9\u94c1\u7684\u513f\u5b50\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\u3001\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u548c\u6240\u6709\u7684\u4f59\u6c11\u542c\u5230\u4ed6\u4eec\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u501f\u5148\u77e5\u54c8\u8be5\u6240\u8bb2\u7684\u8bdd\uff0c\u90fd\u987a\u670d\u8036\u548c\u534e\u7684\u8bdd\u3002\u767e\u59d3\u90fd\u656c\u754f\u8036\u548c\u534e\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8036\u548c\u534e\u7684\u4f7f\u8005\u54c8\u8be5\u5411\u767e\u59d3\u4f20\u8fbe\u8036\u548c\u534e\u7684\u8bdd\uff0c\u8bf4\uff1a\u201c\u8036\u548c\u534e\u8bf4\uff0c\u2018\u6211\u4e0e\u4f60\u4eec\u540c\u5728\u3002\u2019\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8036\u548c\u534e\u611f\u52a8\u6492\u62c9\u94c1\u7684\u513f\u5b50\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\u3001\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u548c\u6240\u6709\u7684\u4f59\u6c11\uff0c\u4ed6\u4eec\u5c31\u52a8\u5de5\u5efa\u9020\u4ed6\u4eec\u7684\u4e0a\u5e1d\u4e07\u519b\u4e4b\u8036\u548c\u534e\u7684\u6bbf\uff0c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u65f6\u503c\u5927\u6d41\u58eb\u738b\u6267\u653f\u7b2c\u4e8c\u5e74\u516d\u6708\u4e8c\u5341\u56db\u65e5\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5bf9\u65b0\u6bbf\u7684\u5e94\u8bb8 - \u4e03\u6708\u4e8c\u5341\u4e00\u65e5\uff0c\u8036\u548c\u534e\u5bf9\u5148\u77e5\u54c8\u8be5\u8bf4\uff1a"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201c\u4f60\u53bb\u544a\u8bc9\u6492\u62c9\u94c1\u7684\u513f\u5b50\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\u3001\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u548c\u6240\u6709\u7684\u4f59\u6c11\uff0c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u2018\u4f60\u4eec\u5f53\u4e2d\u6709\u8c01\u89c1\u8fc7\u8fd9\u6bbf\u6614\u65e5\u7684\u8363\u8000\u5462\uff1f\u73b0\u5728\u4f60\u4eec\u770b\u8fd9\u6bbf\u5982\u4f55\uff1f\u770b\u4e0a\u53bb\u5c82\u4e0d\u662f\u4e00\u6587\u4e0d\u503c\u5417\uff1f"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6240\u7f57\u5df4\u4f2f\u554a\uff0c\u4f60\u8981\u521a\u5f3a\uff0c\u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u554a\uff0c\u4f60\u8981\u521a\u5f3a\uff1b\u8fd9\u5730\u65b9\u7684\u6240\u6709\u767e\u59d3\u554a\uff0c\u4f60\u4eec\u8981\u521a\u5f3a\u3002\u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002\u4f60\u4eec\u8981\u52a8\u5de5\uff0c\u56e0\u4e3a\u6211\u4e0e\u4f60\u4eec\u540c\u5728\u3002\u8fd9\u662f\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u8fd9\u662f\u4f60\u4eec\u79bb\u5f00\u57c3\u53ca\u65f6\u6211\u5bf9\u4f60\u4eec\u7684\u5e94\u8bb8\u3002\u4e0d\u8981\u60e7\u6015\uff0c\u6211\u7684\u7075\u4ecd\u4f4f\u5728\u4f60\u4eec\u5f53\u4e2d\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u201c\u2018\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u4e0d\u4e45\uff0c\u6211\u8981\u518d\u6b21\u9707\u52a8\u5929\u3001\u5730\u3001\u6d77\u6d0b\u548c\u65f1\u5730\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u6211\u8981\u9707\u52a8\u4e07\u56fd\uff0c\u4e07\u56fd\u7684\u73cd\u5b9d\u90fd\u8981\u9001\u5230\u8fd9\u6bbf\uff0c\u6211\u8981\u4f7f\u8fd9\u6bbf\u5145\u6ee1\u8363\u8000\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u91d1\u5b50\u662f\u6211\u7684\uff0c\u94f6\u5b50\u4e5f\u662f\u6211\u7684\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u6211\u8981\u4f7f\u8fd9\u6bbf\u6bd4\u4ece\u524d\u66f4\u52a0\u8363\u8000\u3002\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u6211\u8981\u5728\u8fd9\u5730\u65b9\u8d50\u4e0b\u5e73\u5b89\u3002\u2019\u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6307\u51fa\u767e\u59d3\u7684\u7f6a - \u5927\u6d41\u58eb\u738b\u6267\u653f\u7b2c\u4e8c\u5e74\u4e5d\u6708\u4e8c\u5341\u56db\u65e5\uff0c\u8036\u548c\u534e\u5bf9\u5148\u77e5\u54c8\u8be5\u8bf4\uff1a"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u201c\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u2018\u4f60\u53bb\u95ee\u796d\u53f8\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u5982\u679c\u6709\u4eba\u7528\u8863\u670d\u515c\u7740\u796d\u8089\uff0c\u800c\u8863\u670d\u78b0\u5230\u997c\u3001\u6c64\u3001\u9152\u3001\u6cb9\u6216\u5176\u4ed6\u98df\u7269\uff0c\u8fd9\u4e9b\u88ab\u8863\u670d\u78b0\u5230\u7684\u98df\u7269\u4f1a\u6210\u4e3a\u5723\u6d01\u4e4b\u7269\u5417\uff1f\u2019\u201d\u796d\u53f8\u4eec\u7b54\u9053\uff1a\u201c\u4e0d\u4f1a\u3002\u201d"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u54c8\u8be5\u53c8\u8bf4\uff1a\u201c\u5982\u679c\u56e0\u78b0\u8fc7\u5c38\u4f53\u800c\u53d8\u5f97\u4e0d\u6d01\u51c0\u7684\u4eba\u63a5\u89e6\u8fd9\u4e9b\u98df\u7269\uff0c\u8fd9\u4e9b\u98df\u7269\u4f1a\u53d8\u5f97\u4e0d\u6d01\u51c0\u5417\uff1f\u201d\u796d\u53f8\u7b54\u9053\uff1a\u201c\u4f1a\u53d8\u5f97\u4e0d\u6d01\u51c0\u3002\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e8e\u662f\u54c8\u8be5\u8bf4\uff1a\u201c\u8036\u548c\u534e\u8bf4\uff0c\u2018\u8fd9\u767e\u59d3\u548c\u8fd9\u56fd\u5bb6\u5728\u6211\u9762\u524d\u4e5f\u662f\u5982\u6b64\u3002\u4ed6\u4eec\u624b\u4e2d\u7684\u5404\u6837\u5de5\u4f5c\u548c\u4ed6\u4eec\u5728\u90a3\u91cc\u6240\u732e\u7684\u90fd\u4e0d\u6d01\u51c0\u3002\u2019"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8036\u548c\u534e\u5e94\u8bb8\u8d50\u798f - \u201c\u2018\u4ece\u4eca\u4ee5\u540e\uff0c\u4f60\u4eec\u8981\u56de\u60f3\u91cd\u5efa\u8036\u548c\u534e\u6bbf\u4e4b\u524d\u7684\u5149\u666f\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u90a3\u65f6\uff0c\u6709\u4eba\u76fc\u671b\u5f97\u5230\u4e8c\u5341\u6597\u8c37\u7269\uff0c\u5374\u53ea\u6709\u5341\u6597\u3002\u6709\u4eba\u76fc\u671b\u5f97\u5230\u4e94\u5341\u6876\u9152\uff0c\u69a8\u9152\u6c60\u4e2d\u5374\u53ea\u6709\u4e8c\u5341\u6876\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u6211\u66fe\u7528\u866b\u707e\u3001\u9709\u707e\u548c\u51b0\u96f9\u6bc1\u706d\u4f60\u4eec\u7684\u4f5c\u7269\uff0c\u4f60\u4eec\u4ecd\u6ca1\u6709\u5f52\u5411\u6211\u3002\u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u4e5d\u6708\u4e8c\u5341\u56db\u65e5\u662f\u6211\u7684\u6bbf\u5960\u5b9a\u6839\u57fa\u7684\u65e5\u5b50\u3002\u4f60\u4eec\u8981\u601d\u60f3\u8fd9\u4e2a\u65e5\u5b50\uff0c\u8981\u8ba4\u771f\u601d\u60f3\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u867d\u7136\u7cae\u4ed3\u91cc\u6ca1\u6709\u79cd\u5b50\uff0c\u8461\u8404\u6811\u3001\u65e0\u82b1\u679c\u6811\u3001\u77f3\u69b4\u6811\u548c\u6a44\u6984\u6811\u6ca1\u7ed3\u679c\u5b50\uff0c\u4f46\u4ece\u4eca\u4ee5\u540e\uff0c\u6211\u8981\u8d50\u798f\u7ed9\u4f60\u4eec\u3002\u2019\u201d"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u62e3\u9009\u6240\u7f57\u5df4\u4f2f - \u540c\u4e00\u5929\uff0c\u8036\u548c\u534e\u7b2c\u4e8c\u6b21\u5bf9\u54c8\u8be5\u8bf4\uff1a"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u201c\u4f60\u544a\u8bc9\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\uff0c\u2018\u6211\u5fc5\u9707\u52a8\u5929\u5730\uff0c"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u63a8\u7ffb\u541b\u738b\u7684\u5b9d\u5ea7\uff0c\u6d88\u706d\u5217\u56fd\u7684\u52bf\u529b\uff0c\u6380\u7ffb\u4ed6\u4eec\u7684\u6218\u8f66\u548c\u9a7e\u8f66\u7684\u4eba\uff0c\u4f7f\u6218\u9a6c\u8dcc\u5012\u3001\u9a91\u58eb\u81ea\u76f8\u6b8b\u6740\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff0c\u6211\u7684\u4ec6\u4eba\u6492\u62c9\u94c1\u7684\u513f\u5b50\u6240\u7f57\u5df4\u4f2f\u554a\uff0c\u5230\u90a3\u65e5\uff0c\u6211\u5fc5\u4f7f\u4f60\u50cf\u6211\u76d6\u5370\u7684\u6212\u6307\uff0c\u56e0\u4e3a\u6211\u62e3\u9009\u4e86\u4f60\u3002\u8fd9\u662f\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\u7684\u3002\u2019\u201d"
                }
            }
        }
    }
