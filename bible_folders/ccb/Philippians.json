        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u4fdd\u7f57\u548c\u63d0\u6469\u592a\u662f\u57fa\u7763\u8036\u7a23\u7684\u5974\u4ec6\uff0c\u5199\u4fe1\u7ed9\u8153\u7acb\u6bd4\u5c5e\u4e8e\u57fa\u7763\u8036\u7a23\u7684\u4f17\u5723\u5f92\u3001\u8bf8\u4f4d\u76d1\u7763\u548c\u6267\u4e8b\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u613f\u6211\u4eec\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u8d50\u7ed9\u4f60\u4eec\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u611f\u6069\u548c\u7977\u544a - \u6211\u6bcf\u9022\u60f3\u8d77\u4f60\u4eec\uff0c\u5c31\u611f\u8c22\u6211\u7684\u4e0a\u5e1d\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6bcf\u6b21\u4e3a\u4f60\u4eec\u4f17\u4eba\u7977\u544a\u7684\u65f6\u5019\uff0c\u6211\u5fc3\u91cc\u90fd\u5f88\u559c\u4e50\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u56e0\u4e3a\u4f60\u4eec\u4ece\u5f00\u59cb\u76f4\u5230\u73b0\u5728\uff0c\u4e00\u76f4\u548c\u6211\u540c\u5fc3\u5408\u610f\u5730\u4f20\u626c\u798f\u97f3\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u6df1\u4fe1\uff0c\u4e0a\u5e1d\u65e2\u7136\u5728\u4f60\u4eec\u5fc3\u91cc\u5f00\u59cb\u4e86\u8fd9\u7f8e\u597d\u7684\u5de5\u4f5c\uff0c\u7942\u5fc5\u5728\u8036\u7a23\u57fa\u7763\u518d\u6765\u7684\u65f6\u5019\u5b8c\u6210\u8fd9\u5de5\u4f5c\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6211\u5bf9\u4f60\u4eec\u4f17\u4eba\u6709\u8fd9\u6837\u7684\u611f\u53d7\u662f\u5f88\u81ea\u7136\u7684\uff0c\u56e0\u4e3a\u6211\u5e38\u628a\u4f60\u4eec\u653e\u5728\u5fc3\u4e0a\u3002\u4e0d\u8bba\u6211\u662f\u8eab\u5728\u72f1\u4e2d\uff0c\u8fd8\u662f\u4e3a\u798f\u97f3\u8fa9\u62a4\u548c\u4f5c\u8bc1\u7684\u65f6\u5019\uff0c\u4f60\u4eec\u90fd\u4e0e\u6211\u540c\u8499\u6069\u5178\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e0a\u5e1d\u53ef\u4ee5\u4f5c\u8bc1\uff0c\u6211\u662f\u600e\u6837\u4ee5\u57fa\u7763\u8036\u7a23\u7684\u6148\u7231\u4e4b\u5fc3\u60f3\u5ff5\u4f60\u4eec\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6211\u6240\u7948\u6c42\u7684\u5c31\u662f\uff1a\u4f60\u4eec\u7684\u7231\u5fc3\u968f\u7740\u5c5e\u7075\u77e5\u8bc6\u7684\u589e\u52a0\u548c\u8fa8\u522b\u529b\u7684\u63d0\u9ad8\u800c\u65e5\u76ca\u589e\u957f\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4f7f\u4f60\u4eec\u77e5\u9053\u600e\u6837\u62e9\u5584\u800c\u4ece\uff0c\u505a\u8bda\u5b9e\u65e0\u8fc7\u7684\u4eba\uff0c\u4e00\u76f4\u5230\u57fa\u7763\u518d\u6765\u7684\u65e5\u5b50\uff0c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5e76\u9760\u7740\u8036\u7a23\u57fa\u7763\u7ed3\u6ee1\u4ec1\u4e49\u7684\u679c\u5b50\uff0c\u4f7f\u4e0a\u5e1d\u5f97\u5230\u8363\u8000\u4e0e\u9882\u8d5e\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4e3a\u57fa\u7763\u800c\u6d3b - \u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u5e0c\u671b\u4f60\u4eec\u77e5\u9053\uff0c\u6211\u7684\u906d\u9047\u53cd\u800c\u4f1a\u4f7f\u798f\u97f3\u4f20\u5f97\u66f4\u5e7f\uff0c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8fde\u6240\u6709\u7684\u7687\u5bb6\u536b\u5175\u548c\u5176\u4ed6\u4f17\u4eba\u90fd\u77e5\u9053\u6211\u662f\u4e3a\u4e86\u57fa\u7763\u7684\u7f18\u6545\u800c\u53d7\u56da\u7981\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5927\u591a\u6570\u4e3b\u5185\u7684\u5f1f\u5144\u59ca\u59b9\u770b\u89c1\u6211\u4e3a\u4e3b\u88ab\u56da\uff0c\u4fe1\u5fc3\u53d7\u5230\u9f13\u821e\uff0c\u66f4\u52a0\u653e\u80c6\u65e0\u60e7\u5730\u4f20\u626c\u4e0a\u5e1d\u7684\u9053\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u5f53\u7136\uff0c\u6709\u4e9b\u4eba\u5ba3\u8bb2\u57fa\u7763\u662f\u51fa\u4e8e\u5ac9\u5992\u548c\u4e89\u5f3a\u597d\u80dc\uff0c\u4e5f\u6709\u4e9b\u4eba\u662f\u51fa\u4e8e\u5584\u610f\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u540e\u8005\u662f\u51fa\u4e8e\u7231\u5fc3\uff0c\u77e5\u9053\u6211\u662f\u4e0a\u5e1d\u6d3e\u6765\u7ef4\u62a4\u798f\u97f3\u7684\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u524d\u8005\u4f20\u8bb2\u57fa\u7763\u662f\u51fa\u4e8e\u4e2a\u4eba\u91ce\u5fc3\uff0c\u52a8\u673a\u4e0d\u7eaf\uff0c\u4ee5\u4e3a\u53ef\u4ee5\u589e\u52a0\u6211\u5728\u72f1\u4e2d\u7684\u75db\u82e6\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u90a3\u53c8\u600e\u6837\u5462\uff1f\u65e0\u8bba\u4ed6\u4eec\u4f20\u798f\u97f3\u662f\u771f\u5fc3\u8fd8\u662f\u5047\u610f\uff0c\u57fa\u7763\u7684\u798f\u97f3\u6bd5\u7adf\u4f20\u5f00\u4e86\uff0c\u56e0\u6b64\u6211\u5f88\u6b22\u559c\uff0c\u800c\u4e14\u6b22\u559c\u4e0d\u5df2\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u56e0\u4e3a\u6211\u77e5\u9053\uff0c\u501f\u7740\u4f60\u4eec\u7684\u7977\u544a\u548c\u8036\u7a23\u57fa\u7763\u4e4b\u7075\u7684\u5e2e\u52a9\uff0c\u6211\u7ec8\u4f1a\u5f97\u5230\u91ca\u653e\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u6211\u6bb7\u5207\u5730\u671f\u5f85\u548c\u76fc\u671b\uff1a\u6211\u4e0d\u4f1a\u611f\u5230\u4efb\u4f55\u7f9e\u6127\uff0c\u800c\u662f\u653e\u80c6\u65e0\u60e7\uff0c\u4e0d\u7ba1\u662f\u751f\u662f\u6b7b\uff0c\u90fd\u8981\u4e00\u5982\u65e2\u5f80\u5730\u4f7f\u57fa\u7763\u5728\u6211\u8eab\u4e0a\u5f97\u5230\u5c0a\u5d07\u3002"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u56e0\u4e3a\u5bf9\u6211\u6765\u8bf4\uff0c\u6d3b\u7740\u662f\u4e3a\u4e86\u57fa\u7763\uff0c\u6b7b\u4e86\u4e5f\u6709\u76ca\u5904\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u5982\u679c\u6d3b\u5728\u4e16\u4e0a\uff0c\u6211\u53ef\u4ee5\u505a\u5de5\uff0c\u591a\u7ed3\u679c\u5b50\u3002\u6211\u771f\u4e0d\u77e5\u9053\u8be5\u600e\u4e48\u9009\u62e9\uff01"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u6211\u5904\u5728\u4e24\u96be\u4e4b\u95f4\u3002\u6211\u60c5\u613f\u79bb\u5f00\u4e16\u754c\uff0c\u4e0e\u57fa\u7763\u5728\u4e00\u8d77\uff0c\u90a3\u5b9e\u5728\u662f\u518d\u597d\u4e0d\u8fc7\u4e86\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u53ef\u662f\u4e3a\u4e86\u4f60\u4eec\uff0c\u6211\u5e94\u8be5\u6d3b\u5728\u4e16\u4e0a\u3002"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u6211\u65e2\u7136\u5bf9\u6b64\u6df1\u4fe1\u4e0d\u7591\uff0c\u5c31\u77e5\u9053\u81ea\u5df1\u4ecd\u8981\u6d3b\u4e0b\u53bb\uff0c\u7ee7\u7eed\u8ddf\u4f60\u4eec\u5728\u4e00\u8d77\uff0c\u597d\u4f7f\u4f60\u4eec\u5728\u4fe1\u4ef0\u4e0a\u53c8\u957f\u8fdb\u53c8\u5145\u6ee1\u559c\u4e50\u3002"
                },
                "26": {
                    "verse_nr": "26",
                    "verse": "\u8fd9\u6837\uff0c\u6211\u518d\u5230\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u4f60\u4eec\u53ef\u4ee5\u56e0\u4e3a\u6211\u7684\u7f18\u6545\u66f4\u52a0\u4ee5\u57fa\u7763\u8036\u7a23\u4e3a\u8363\u3002"
                },
                "27": {
                    "verse_nr": "27",
                    "verse": "\u6700\u8981\u7d27\u7684\u662f\uff1a\u4f60\u4eec\u884c\u4e8b\u4e3a\u4eba\u8981\u4e0e\u57fa\u7763\u7684\u798f\u97f3\u76f8\u79f0\u3002\u8fd9\u6837\uff0c\u4e0d\u7ba1\u662f\u53bb\u770b\u4f60\u4eec\uff0c\u8fd8\u662f\u53ea\u542c\u5230\u4f60\u4eec\u7684\u6d88\u606f\uff0c\u6211\u90fd\u80fd\u77e5\u9053\u4f60\u4eec\u540c\u5fc3\u5408\u610f\uff0c\u575a\u5b9a\u4e0d\u79fb\u5730\u4e00\u8d77\u4e3a\u798f\u97f3\u594b\u6597\uff0c"
                },
                "28": {
                    "verse_nr": "28",
                    "verse": "\u6beb\u4e0d\u60e7\u6015\u654c\u4eba\u7684\u6050\u5413\u3002\u8fd9\u6e05\u695a\u8868\u660e\u4ed6\u4eec\u5fc5\u706d\u4ea1\uff0c\u4f60\u4eec\u5fc5\u5f97\u6551\uff0c\u8fd9\u662f\u51fa\u4e8e\u4e0a\u5e1d\u3002"
                },
                "29": {
                    "verse_nr": "29",
                    "verse": "\u56e0\u4e3a\u4e0a\u5e1d\u4e3a\u4e86\u57fa\u7763\u7684\u7f18\u6545\uff0c\u8d50\u6069\u4f7f\u4f60\u4eec\u4e0d\u4f46\u4fe1\u57fa\u7763\uff0c\u800c\u4e14\u4e3a\u7942\u53d7\u82e6\uff0c"
                },
                "30": {
                    "verse_nr": "30",
                    "verse": "\u7ecf\u5386\u4f60\u4eec\u4ece\u524d\u5728\u6211\u8eab\u4e0a\u89c1\u8fc7\uff0c\u73b0\u5728\u53c8\u4ece\u6211\u8fd9\u91cc\u542c\u5230\u7684\u540c\u6837\u4e89\u6218\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6548\u6cd5\u57fa\u7763 - \u6240\u4ee5\uff0c\u5982\u679c\u4f60\u4eec\u5728\u57fa\u7763\u91cc\u53d7\u5230\u9f13\u52b1\uff0c\u5f97\u5230\u7231\u7684\u5b89\u6170\uff0c\u4e0e\u5723\u7075\u76f8\u901a\uff0c\u6709\u6148\u60b2\u548c\u601c\u60af\u4e4b\u5fc3\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u5c31\u8981\u540c\u5fc3\u5408\u610f\uff0c\u5f7c\u6b64\u76f8\u7231\uff0c\u7075\u91cc\u5408\u4e00\uff0c\u601d\u60f3\u4e00\u81f4\uff0c\u597d\u8ba9\u6211\u7684\u559c\u4e50\u66f4\u5145\u8db3\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u51e1\u4e8b\u4e0d\u53ef\u81ea\u79c1\u81ea\u5229\u3001\u7231\u6155\u865a\u8363\uff0c\u8981\u5fc3\u5b58\u8c26\u5351\uff0c\u770b\u522b\u4eba\u6bd4\u81ea\u5df1\u5f3a\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5404\u4eba\u4e0d\u8981\u53ea\u987e\u81ea\u5df1\u7684\u4e8b\uff0c\u4e5f\u8981\u4e3a\u522b\u4eba\u7684\u9700\u8981\u7740\u60f3\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u4eec\u5e94\u5f53\u6709\u57fa\u7763\u8036\u7a23\u90a3\u6837\u7684\u5fc3\u80a0\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u7942\u867d\u7136\u672c\u8d28\u4e0a\u662f\u4e0a\u5e1d\uff0c \u5374\u6ca1\u6709\u7d27\u7d27\u6293\u4f4f\u81ea\u5df1\u4e0e\u4e0a\u5e1d\u5e73\u7b49\u7684\u5730\u4f4d\u4e0d\u653e\uff0c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u53cd\u800c\u7518\u613f\u653e\u4e0b\u4e00\u5207\uff0c \u53d6\u4e86\u5974\u4ec6\u7684\u5f62\u8c61\uff0c \u964d\u751f\u4e3a\u4eba\u7684\u6837\u5b50\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u7942\u4ee5\u4eba\u7684\u6837\u5b50\u51fa\u73b0\u540e\uff0c \u5c31\u81ea\u613f\u5351\u5fae\uff0c\u987a\u670d\u81f3\u6b7b\uff0c \u800c\u4e14\u6b7b\u5728\u5341\u5b57\u67b6\u4e0a\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u56e0\u6b64\uff0c\u4e0a\u5e1d\u5c06\u7942\u5347\u4e3a\u81f3\u5c0a\uff0c \u8d50\u7ed9\u7942\u8d85\u4e4e\u4e07\u540d\u4e4b\u4e0a\u7684\u540d\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4f7f\u4e00\u5207\u5929\u4e0a\u7684\u3001 \u5730\u4e0a\u7684\u548c\u5730\u5e95\u4e0b\u7684\uff0c \u65e0\u4e0d\u5c48\u819d\u8dea\u62dc\u5728\u8036\u7a23\u7684\u540d\u4e0b\uff0c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u65e0\u4e0d\u53e3\u79f0\u8036\u7a23\u57fa\u7763\u662f\u4e3b\uff0c \u5c06\u8363\u8000\u5f52\u4e8e\u7236\u4e0a\u5e1d\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6697\u4e16\u660e\u706f - \u6240\u4ee5\uff0c\u6211\u4eb2\u7231\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u65e2\u7136\u6211\u5728\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u4f60\u4eec\u4e00\u5411\u90fd\u5f88\u987a\u670d\uff1b\u5982\u4eca\u6211\u4e0d\u5728\u4f60\u4eec\u90a3\u91cc\uff0c\u4f60\u4eec\u66f4\u8981\u987a\u670d\uff0c\u8981\u6218\u6218\u5162\u5162\u5730\u6d3b\u51fa\u5f97\u6551\u540e\u5e94\u6709\u7684\u751f\u547d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u56e0\u4e3a\u4f60\u4eec\u7acb\u5fd7\u548c\u884c\u4e8b\u90fd\u662f\u4e0a\u5e1d\u5728\u4f60\u4eec\u5fc3\u4e2d\u5de5\u4f5c\uff0c\u4e3a\u8981\u6210\u5c31\u7942\u7f8e\u597d\u7684\u65e8\u610f\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u65e0\u8bba\u505a\u4ec0\u4e48\u4e8b\uff0c\u90fd\u4e0d\u8981\u62b1\u6028\uff0c\u4e5f\u4e0d\u8981\u4e0e\u4eba\u4e89\u8bba\uff0c"
                },
                "15": {
                    "verse_nr": "15",
