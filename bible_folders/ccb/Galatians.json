        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u4fdd\u7f57\uff0c\u6211\u505a\u4f7f\u5f92\u5e76\u975e\u53d7\u4eba\u59d4\u6d3e\uff0c\u4e5f\u4e0d\u662f\u501a\u9760\u4eba\uff0c\u800c\u662f\u7531\u8036\u7a23\u57fa\u7763\u548c\u53eb\u7942\u4ece\u6b7b\u91cc\u590d\u6d3b\u7684\u7236\u4e0a\u5e1d\u59d4\u6d3e\u7684\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u548c\u6240\u6709\u8ddf\u6211\u5728\u4e00\u8d77\u7684\u5f1f\u5144\u5199\u4fe1\u7ed9\u52a0\u62c9\u592a\u7684\u5404\u6559\u4f1a\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u613f\u6211\u4eec\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u8d50\u7ed9\u4f60\u4eec\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u57fa\u7763\u9075\u7167\u6211\u4eec\u7236\u4e0a\u5e1d\u7684\u65e8\u610f\u4e3a\u6211\u4eec\u7684\u7f6a\u732e\u4e0a\u4e86\u81ea\u5df1\uff0c\u597d\u62ef\u6551\u6211\u4eec\u8131\u79bb\u8fd9\u7f6a\u6076\u7684\u4e16\u4ee3\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u613f\u8363\u8000\u5f52\u7ed9\u4e0a\u5e1d\uff0c\u76f4\u5230\u6c38\u6c38\u8fdc\u8fdc\u3002\u963f\u4eec\uff01"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u52ff\u968f\u522b\u7684\u798f\u97f3 - \u6211\u5f88\u60ca\u8bb6\uff0c\u4f60\u4eec\u7adf\u7136\u90a3\u4e48\u5feb\u5c31\u80cc\u5f03\u4e86\u501f\u7740\u57fa\u7763\u7684\u6069\u5178\u547c\u53ec\u4f60\u4eec\u7684\u4e0a\u5e1d\uff0c\u53bb\u8ffd\u968f\u522b\u7684\u798f\u97f3\uff01"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5176\u5b9e\u90a3\u5e76\u4e0d\u662f\u798f\u97f3\uff0c\u53ea\u662f\u67d0\u4e9b\u4eba\u6270\u4e71\u4f60\u4eec\u7684\u4fe1\u4ef0\uff0c\u60f3\u7be1\u6539\u57fa\u7763\u7684\u798f\u97f3\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5373\u4fbf\u662f\u6211\u4eec\u6216\u5929\u4f7f\uff0c\u82e5\u53e6\u4f20\u798f\u97f3\u7ed9\u4f60\u4eec\uff0c\u4e0e\u6211\u4eec\u4ee5\u524d\u4f20\u7ed9\u4f60\u4eec\u7684\u76f8\u6096\uff0c\u4e5f\u8be5\u53d7\u5492\u8bc5\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6211\u4eec\u5df2\u7ecf\u8bf4\u8fc7\uff0c\u73b0\u5728\u6211\u518d\u8bf4\u4e00\u6b21\uff1a\u5982\u679c\u6709\u4eba\u5411\u4f60\u4eec\u4f20\u522b\u7684\u798f\u97f3\uff0c\u8ddf\u4f60\u4eec\u4ee5\u524d\u63a5\u53d7\u7684\u4e0d\u540c\uff0c\u90a3\u4eba\u8be5\u53d7\u5492\u8bc5\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u73b0\u5728\u662f\u8981\u8d62\u5f97\u4eba\u7684\u8d5e\u540c\u5417\uff1f\u8fd8\u662f\u8981\u8d62\u5f97\u4e0a\u5e1d\u7684\u8d5e\u540c\uff1f\u96be\u9053\u6211\u662f\u8981\u8ba8\u597d\u4eba\u5417\uff1f\u5982\u679c\u6211\u4ecd\u65e7\u8981\u8ba8\u597d\u4eba\uff0c\u6211\u5c31\u4e0d\u662f\u57fa\u7763\u7684\u5974\u4ec6\u4e86\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4fdd\u7f57\u53d7\u4e0a\u5e1d\u59d4\u6d3e\u4f20\u798f\u97f3 - \u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u544a\u8bc9\u4f60\u4eec\uff0c\u6211\u4f20\u7684\u798f\u97f3\u4e0d\u662f\u51fa\u4e8e\u4eba\u7684\u610f\u601d\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u56e0\u4e3a\u8fd9\u798f\u97f3\u65e2\u4e0d\u662f\u6211\u4ece\u4eba\u90a3\u91cc\u9886\u53d7\u7684\uff0c\u4e5f\u4e0d\u662f\u6211\u8ddf\u4eba\u5b66\u6765\u7684\uff0c\u800c\u662f\u8036\u7a23\u57fa\u7763\u4eb2\u81ea\u542f\u793a\u6211\u7684\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u4eec\u90fd\u542c\u8bf4\u8fc7\u6211\u4fe1\u5949\u72b9\u592a\u6559\u65f6\u7684\u6240\u4f5c\u6240\u4e3a\uff0c\u6211\u600e\u6837\u6b8b\u9177\u5730\u8feb\u5bb3\u4e0a\u5e1d\u7684\u6559\u4f1a\uff0c\u8bd5\u56fe\u6467\u6bc1\u5b83\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6211\u5728\u72b9\u592a\u6559\u91cc\u6bd4\u8bb8\u591a\u540c\u8f88\u7684\u72b9\u592a\u4eba\u66f4\u8fdb\u53d6\uff0c\u72c2\u70ed\u5730\u7ef4\u62a4\u7956\u5148\u7684\u4f20\u7edf\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u7136\u800c\uff0c\u5f53\u6211\u8fd8\u5728\u6bcd\u8179\u4e2d\u7684\u65f6\u5019\uff0c\u4e0a\u5e1d\u5c31\u65bd\u6069\u5c06\u6211\u5206\u522b\u51fa\u6765\u5e76\u547c\u53ec\u4e86\u6211\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4e0a\u5e1d\u65e2\u7136\u4e50\u610f\u628a\u7942\u7684\u513f\u5b50\u542f\u793a\u7ed9\u6211\uff0c\u53eb\u6211\u5411\u5916\u65cf\u4eba\u4f20\u626c\u7942\u7684\u798f\u97f3\uff0c\u6211\u5c31\u6ca1\u6709\u8ddf\u4efb\u4f55\u4eba\u5546\u91cf\uff0c"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4e5f\u6ca1\u6709\u4e0a\u8036\u8def\u6492\u51b7\u53bb\u89c1\u90a3\u4e9b\u6bd4\u6211\u5148\u505a\u4f7f\u5f92\u7684\u4eba\uff0c\u800c\u662f\u7acb\u523b\u53bb\u4e86\u963f\u62c9\u4f2f\uff0c\u7136\u540e\u56de\u5230\u5927\u9a6c\u58eb\u9769\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u8fc7\u4e86\u4e09\u5e74\uff0c\u6211\u624d\u5230\u8036\u8def\u6492\u51b7\u53bb\u62dc\u4f1a\u5f7c\u5f97\uff0c\u548c\u4ed6\u4e00\u8d77\u4f4f\u4e86\u5341\u4e94\u5929\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u6211\u4e5f\u89c1\u4e86\u4e3b\u8036\u7a23\u7684\u5f1f\u5f1f\u96c5\u5404\uff0c\u6b64\u5916\u6ca1\u6709\u89c1\u8fc7\u5176\u4ed6\u4f7f\u5f92\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u6211\u5728\u4e0a\u5e1d\u9762\u524d\u4fdd\u8bc1\uff0c\u6211\u5199\u7ed9\u4f60\u4eec\u7684\u7edd\u65e0\u8c0e\u8a00\u3002"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u540e\u6765\u6211\u53c8\u5230\u4e86\u53d9\u5229\u4e9a\u548c\u57fa\u5229\u8fe6\u5730\u533a\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u90a3\u65f6\uff0c\u72b9\u592a\u5883\u5185\u57fa\u7763\u7684\u4f17\u6559\u4f1a\u90fd\u8fd8\u6ca1\u6709\u89c1\u8fc7\u6211\u7684\u9762\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u4ed6\u4eec\u53ea\u662f\u542c\u4eba\u8bf4\uff1a\u201c\u90a3\u4e2a\u4ece\u524d\u8feb\u5bb3\u6211\u4eec\u7684\u4eba\u5982\u4eca\u5728\u4f20\u626c\u4ed6\u66fe\u8bd5\u56fe\u6467\u6bc1\u7684\u4fe1\u4ef0\u3002\u201d"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u4e8e\u662f\u4ed6\u4eec\u56e0\u4e3a\u6211\u7684\u7f18\u6545\u5c06\u8363\u8000\u5f52\u7ed9\u4e0a\u5e1d\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4f17\u4f7f\u5f92\u63a5\u7eb3\u4fdd\u7f57 - \u5341\u56db\u5e74\u540e\uff0c\u6211\u548c\u5df4\u62ff\u5df4\u53c8\u53bb\u8036\u8def\u6492\u51b7\uff0c\u5e76\u5e26\u4e86\u63d0\u591a\u540c\u53bb\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u662f\u9075\u7167\u4e0a\u5e1d\u7684\u542f\u793a\u53bb\u7684\u3002\u6211\u79c1\u4e0b\u62dc\u4f1a\u4e86\u90a3\u4e9b\u6709\u540d\u671b\u7684\u6559\u4f1a\u9886\u8896\uff0c\u9648\u660e\u6211\u5728\u5916\u65cf\u4eba\u4e2d\u95f4\u6240\u4f20\u7684\u798f\u97f3\uff0c\u514d\u5f97\u6211\u8fc7\u53bb\u6216\u662f\u73b0\u5728\u7684\u52aa\u529b\u90fd\u767d\u8d39\u4e86\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8ddf\u6211\u540c\u53bb\u7684\u63d0\u591a\u867d\u7136\u662f\u5e0c\u814a\u4eba\uff0c\u4f46\u6ca1\u6709\u4eba\u52c9\u5f3a\u4ed6\u63a5\u53d7\u5272\u793c\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4e8b\u60c5\u7684\u7f18\u7531\u662f\u56e0\u4e3a\u6709\u4e00\u4e9b\u5047\u4fe1\u5f92\u5077\u5077\u6df7\u4e86\u8fdb\u6765\uff0c\u8981\u7aa5\u63a2\u6211\u4eec\u5728\u57fa\u7763\u8036\u7a23\u91cc\u4eab\u6709\u7684\u81ea\u7531\uff0c\u60f3\u53eb\u6211\u4eec\u505a\u5f8b\u6cd5\u7684\u5974\u96b6\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f46\u4e3a\u4e86\u53eb\u798f\u97f3\u7684\u771f\u7406\u5e38\u5728\u4f60\u4eec\u5f53\u4e2d\uff0c\u6211\u4eec\u4e1d\u6beb\u6ca1\u6709\u5411\u4ed6\u4eec\u59a5\u534f\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u81f3\u4e8e\u90a3\u4e9b\u5fb7\u9ad8\u671b\u91cd\u7684\u6559\u4f1a\u9886\u8896\uff0c\u4ed6\u4eec\u7684\u5730\u4f4d\u5bf9\u6211\u6765\u8bf4\u65e0\u5173\u7d27\u8981\uff0c\u56e0\u4e3a\u4e0a\u5e1d\u4e0d\u4ee5\u8c8c\u53d6\u4eba\u3002\u4ed6\u4eec\u5bf9\u6211\u6240\u4f20\u7684\u5e76\u6ca1\u6709\u589e\u52a0\u4ec0\u4e48\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u76f8\u53cd\uff0c\u4ed6\u4eec\u90fd\u5df2\u770b\u5230\uff0c\u4e0a\u5e1d\u5dee\u9063\u4e86\u6211\u5411\u5916\u65cf\u4eba\u4f20\u798f\u97f3\uff0c\u5c31\u50cf\u7942\u5dee\u9063\u4e86\u5f7c\u5f97\u5411\u72b9\u592a\u4eba\u4f20\u798f\u97f3\u4e00\u6837\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e0a\u5e1d\u611f\u52a8\u4e86\u5f7c\u5f97\uff0c\u547c\u53ec\u4ed6\u505a\u72b9\u592a\u4eba\u7684\u4f7f\u5f92\uff0c\u7942\u4e5f\u540c\u6837\u611f\u52a8\u4e86\u6211\uff0c\u547c\u53ec\u6211\u505a\u5916\u65cf\u4eba\u7684\u4f7f\u5f92\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5f53\u65f6\u88ab\u8a89\u4e3a\u6559\u4f1a\u67f1\u77f3\u7684\u96c5\u5404\u3001\u5f7c\u5f97\u548c\u7ea6\u7ff0\u660e\u767d\u4e86\u4e0a\u5e1d\u8d50\u7ed9\u6211\u7684\u6069\u5178\u4e4b\u540e\uff0c\u5c31\u4e0e\u6211\u548c\u5df4\u62ff\u5df4\u7528\u53f3\u624b\u884c\u76f8\u4ea4\u4e4b\u793c\uff0c\u8ba9\u6211\u4eec\u5411\u5916\u65cf\u4eba\u4f20\u798f\u97f3\uff0c\u4ed6\u4eec\u5411\u53d7\u5272\u793c\u7684\u4eba\u4f20\u798f\u97f3\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4ed6\u4eec\u53ea\u8981\u6c42\u6211\u4eec\u7167\u987e\u90a3\u4e9b\u8d2b\u56f0\u7684\u4eba\uff0c\u8fd9\u6b63\u662f\u6211\u4e00\u5411\u70ed\u8877\u7684\u4e8b\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4fdd\u7f57\u9762\u8d23\u5f7c\u5f97 - \u540e\u6765\uff0c\u5f7c\u5f97\u5230\u4e86\u5b89\u63d0\u963f\uff0c\u56e0\u4ed6\u505a\u9519\u4e86\u4e8b\uff0c\u6211\u5c31\u5f53\u9762\u8d23\u5907\u4ed6\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4ece\u96c5\u5404\u90a3\u91cc\u6765\u7684\u4eba\u8fd8\u6ca1\u6709\u62b5\u8fbe\u4e4b\u524d\uff0c\u5f7c\u5f97\u548c\u5916\u65cf\u7684\u4fe1\u5f92\u4e00\u8d77\u5403\u996d\u3002\u4f46\u90a3\u4e9b\u4eba\u62b5\u8fbe\u4ee5\u540e\uff0c\u5f7c\u5f97\u56e0\u4e3a\u6015\u90a3\u4e9b\u575a\u6301\u884c\u5272\u793c\u7684\u72b9\u592a\u4eba\u6279\u8bc4\uff0c\u5c31\u4e0e\u5916\u65cf\u7684\u4fe1\u5f92\u5206\u5f00\u4e86\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u5176\u4ed6\u7684\u72b9\u592a\u57fa\u7763\u5f92\u4e5f\u8ddf\u7740\u5f7c\u5f97\u88c5\u5047\uff0c\u751a\u81f3\u8fde\u5df4\u62ff\u5df4\u4e5f\u968f\u4ece\u4e86\u4ed6\u4eec\u7684\u865a\u4f2a\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6211\u770b\u89c1\u4ed6\u4eec\u4e0d\u7167\u798f\u97f3\u7684\u771f\u7406\u884c\uff0c\u5c31\u5f53\u4f17\u5bf9\u5f7c\u5f97\u8bf4\uff1a\u201c\u4f60\u8eab\u4e3a\u72b9\u592a\u4eba\uff0c\u5982\u679c\u884c\u4e8b\u4e3a\u4eba\u50cf\u5916\u65cf\u4eba\uff0c\u4e0d\u50cf\u72b9\u592a\u4eba\uff0c\u53c8\u600e\u80fd\u5f3a\u8feb\u5916\u65cf\u4eba\u6309\u72b9\u592a\u4eba\u7684\u89c4\u77e9\u751f\u6d3b\u5462\uff1f\u201d"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6211\u4eec\u751f\u6765\u662f\u72b9\u592a\u4eba\uff0c\u4e0d\u662f\u5916\u65cf\u7f6a\u4eba\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4f46\u6211\u4eec\u77e5\u9053\u4eba\u88ab\u79f0\u4e3a\u4e49\u4eba\u4e0d\u662f\u9760\u9075\u884c\u5f8b\u6cd5\uff0c\u800c\u662f\u9760\u4fe1\u8036\u7a23\u57fa\u7763\u3002\u6240\u4ee5\u6211\u4eec\u4fe1\u4e86\u57fa\u7763\u8036\u7a23\uff0c\u4ee5\u4fbf\u56e0\u4fe1\u57fa\u7763\u800c\u88ab\u79f0\u4e3a\u4e49\u4eba\uff0c\u800c\u4e0d\u662f\u9760\u9075\u884c\u5f8b\u6cd5\uff0c\u56e0\u4e3a\u65e0\u4eba\u80fd\u591f\u9760\u9075\u884c\u5f8b\u6cd5\u800c\u88ab\u79f0\u4e3a\u4e49\u4eba\u3002"
                },
                "17": {
