                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u751a\u81f3\u8981\u4ece\u9ea6\u6346\u4e2d\u62bd\u4e00\u4e9b\u51fa\u6765\u8ba9\u5979\u6361\uff0c\u4e0d\u8981\u8d23\u9a82\u5979\u3002\u201d"
