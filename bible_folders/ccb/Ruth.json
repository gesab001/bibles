                "12": {
                    "verse_nr": "12",
                    "verse": "\u4e0d\u9519\uff0c\u6211\u662f\u4f60\u7684\u8fd1\u4eb2\uff0c\u53ef\u662f\u6709\u4e00\u4e2a\u4eba\u6bd4\u6211\u66f4\u8fd1\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u4eca\u665a\u5c31\u7559\u5728\u8fd9\u91cc\uff0c\u660e\u5929\u65e9\u4e0a\uff0c\u5982\u679c\u4ed6\u7b54\u5e94\u5c3d\u4eb2\u5c5e\u7684\u4e49\u52a1\uff0c\u5c31\u7531\u4ed6\u7167\u987e\u4f60\uff1b\u5982\u679c\u4ed6\u4e0d\u80af\uff0c\u6211\u51ed\u6c38\u6d3b\u7684\u8036\u548c\u534e\u8d77\u8a93\uff0c\u6211\u4f1a\u5c3d\u6211\u7684\u672c\u5206\u3002\u73b0\u5728\u4f60\u53ea\u7ba1\u5b89\u5fc3\u7761\u89c9\u5427\u3002\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8def\u5f97\u5c31\u8eba\u5728\u6ce2\u963f\u65af\u7684\u811a\u65c1\u3002\u5929\u672a\u4eae\uff0c\u8fd8\u770b\u4e0d\u6e05\u4eba\u7684\u65f6\u5019\uff0c\u8def\u5f97\u5c31\u8d77\u6765\u4e86\u3002\u56e0\u4e3a\u6ce2\u963f\u65af\u66fe\u5bf9\u5979\u8bf4\uff1a\u201c\u4e0d\u53ef\u8ba9\u522b\u4eba\u77e5\u9053\u6709\u5973\u4eba\u6765\u8fc7\u8fd9\u91cc\u3002\u201d"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u79bb\u5f00\u7684\u65f6\u5019\uff0c\u6ce2\u963f\u65af\u5bf9\u5979\u8bf4\uff1a\u201c\u628a\u4f60\u7684\u5916\u8863\u62ff\u6765\u94fa\u5f00\u3002\u201d\u8def\u5f97\u5c31\u7167\u505a\u4e86\u3002\u4ed6\u7528\u8def\u5f97\u7684\u5916\u8863\u5305\u4e86\u516d\u7c38\u7b95\u5927\u9ea6\uff0c\u5e2e\u5979\u625b\u5728\u80a9\u4e0a\uff0c\u5979\u4fbf\u56de\u57ce\u4e86\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u56de\u5230\u5bb6\u4e2d\uff0c\u5a46\u5a46\u95ee\u5979\uff1a\u201c\u5973\u513f\u554a\uff0c\u600e\u4e48\u6837\u4e86\uff1f\u201d\u8def\u5f97\u4fbf\u5c06\u4e8b\u60c5\u7684\u7ecf\u8fc7\u90fd\u544a\u8bc9\u4e86\u5a46\u5a46\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u5979\u53c8\u8bf4\uff1a\u201c\u90a3\u4e2a\u4eba\u7ed9\u4e86\u6211\u516d\u7c38\u7b95\u5927\u9ea6\uff0c\u5bf9\u6211\u8bf4\uff0c\u2018\u4e0d\u8981\u7a7a\u624b\u56de\u53bb\u89c1\u4f60\u5a46\u5a46\u3002\u2019\u201d"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u62ff\u4fc4\u7c73\u8bf4\uff1a\u201c\u5973\u513f\u554a\uff01\u4f60\u53ea\u7ba1\u5b89\u9759\u7b49\u5019\uff0c\u770b\u8fd9\u4e8b\u600e\u6837\u53d1\u5c55\uff0c\u56e0\u4e3a\u90a3\u4eba\u4eca\u5929\u4e0d\u628a\u4e8b\u60c5\u529e\u59a5\uff0c\u662f\u7edd\u4e0d\u4f1a\u4f11\u606f\u7684\u3002\u201d"
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6ce2\u963f\u65af\u5a36\u8def\u5f97 - \u6ce2\u963f\u65af\u6765\u5230\u57ce\u95e8\u53e3\uff0c\u5750\u5728\u90a3\u91cc\uff0c\u6070\u5de7\u770b\u89c1\u4ed6\u6240\u8bf4\u7684\u90a3\u4f4d\u81f3\u4eb2\u7ecf\u8fc7\uff0c\u4fbf\u62db\u547c\u4ed6\u8bf4\uff1a\u201c\u67d0\u4eba\u554a\uff0c\u8bf7\u8fc7\u6765\u5750\u4e00\u5750\u3002\u201d\u90a3\u4eba\u4fbf\u8d70\u8fc7\u6765\u5750\u4e0b\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6ce2\u963f\u65af\u53c8\u9080\u8bf7\u4e86\u57ce\u91cc\u7684\u5341\u4f4d\u957f\u8001\uff0c\u5bf9\u4ed6\u4eec\u8bf4\uff1a\u201c\u8bf7\u8fc7\u6765\u5750\u4e00\u5750\u3002\u201d\u4ed6\u4eec\u90fd\u5750\u4e0b\u4ee5\u540e\uff0c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u6ce2\u963f\u65af\u4fbf\u5bf9\u90a3\u4f4d\u81f3\u4eb2\u8bf4\uff1a\u201c\u62ff\u4fc4\u7c73\u4ece\u6469\u62bc\u56de\u6765\u4e86\uff0c\u73b0\u5728\u8981\u5356\u6211\u4eec\u65cf\u5144\u4ee5\u5229\u7c73\u52d2\u7684\u90a3\u5757\u5730\uff0c"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6211\u60f3\u5e94\u8be5\u8ba9\u4f60\u77e5\u9053\u8fd9\u4ef6\u4e8b\u3002\u4f60\u6700\u6709\u6743\u8d4e\u90a3\u5757\u5730\uff0c\u5176\u6b21\u624d\u662f\u6211\uff0c\u6b64\u5916\u518d\u6ca1\u6709\u522b\u4eba\u4e86\u3002\u5982\u679c\u4f60\u613f\u610f\u8d4e\u56de\u90a3\u5757\u5730\uff0c\u5e0c\u671b\u4f60\u5f53\u7740\u5728\u5ea7\u5404\u4f4d\u957f\u8001\u7684\u9762\u8868\u660e\u3002\u5982\u679c\u4e0d\u613f\u610f\uff0c\u8bf7\u4f60\u8ba9\u6211\u77e5\u9053\u3002\u201d\u90a3\u4eba\u8bf4\uff1a\u201c\u6211\u613f\u610f\u8d4e\u3002\u201d"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u6ce2\u963f\u65af\u8bf4\uff1a\u201c\u90a3\u4e48\uff0c\u4f60\u4ece\u62ff\u4fc4\u7c73\u624b\u4e2d\u4e70\u5730\u7684\u90a3\u5929\uff0c\u4e5f\u8981\u5a36\u5df2\u6b7b\u4e4b\u4eba\u7684\u59bb\u5b50\u6469\u62bc\u5973\u5b50\u8def\u5f97\uff0c\u597d\u8ba9\u6b7b\u8005\u7ee7\u7eed\u5728\u4ea7\u4e1a\u4e0a\u7559\u540d\u3002\u201d"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u90a3\u4f4d\u81f3\u4eb2\u8bf4\uff1a\u201c\u90a3\u6211\u5c31\u4e0d\u80fd\u8d4e\u4e86\uff0c\u514d\u5f97\u635f\u5bb3\u5230\u6211\u7684\u4ea7\u4e1a\u3002\u4f60\u6765\u8d4e\u5427\uff0c\u6211\u4e0d\u80fd\u8d4e\u3002\u201d"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4ece\u524d\u5728\u4ee5\u8272\u5217\uff0c\u4e70\u8d4e\u6216\u4ea4\u6613\u6709\u8fd9\u6837\u7684\u89c4\u77e9\uff1a\u4e00\u65e6\u6210\u4ea4\uff0c\u4e00\u65b9\u8981\u628a\u978b\u8131\u4e0b\u6765\u4ea4\u7ed9\u53e6\u4e00\u65b9\uff0c\u4ee5\u8272\u5217\u4eba\u4ee5\u6b64\u4e3a\u6210\u4ea4\u7684\u51ed\u636e\u3002"
                },
                "8": {
