{
    "version": "ccb",
    "book_name": "1 Peter",
    "book_nr": "60",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u8036\u7a23\u57fa\u7763\u7684\u4f7f\u5f92\u5f7c\u5f97\uff0c\u5199\u4fe1\u7ed9\u8499\u4e0a\u5e1d\u62e3\u9009\uff0c\u5bc4\u5c45\u5728\u672c\u90fd\u3001\u52a0\u62c9\u592a\u3001\u52a0\u5e15\u591a\u8fe6\u3001\u4e9a\u7ec6\u4e9a\u3001\u5e87\u63a8\u5c3c\u5404\u5730\u7684\u4eba\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4f60\u4eec\u8499\u62e3\u9009\u662f\u6309\u7167\u7236\u4e0a\u5e1d\u9884\u5148\u5b9a\u597d\u7684\u65e8\u610f\uff0c\u501f\u7740\u5723\u7075\u5f97\u4ee5\u5723\u6d01\uff0c\u597d\u987a\u670d\u8036\u7a23\u57fa\u7763\uff0c\u5e76\u9760\u7942\u7684\u8840\u5f97\u5230\u6d01\u51c0\u3002 \u613f\u4e0a\u5e1d\u8d50\u7ed9\u4f60\u4eec\u4e30\u4e30\u5bcc\u5bcc\u7684\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u6d3b\u6cfc\u7684\u76fc\u671b - \u613f\u9882\u8d5e\u5f52\u4e8e\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u7236\u4e0a\u5e1d\uff01\u7942\u6709\u65e0\u7a77\u7684\u601c\u60af\uff0c\u501f\u7740\u8036\u7a23\u57fa\u7763\u4ece\u6b7b\u91cc\u590d\u6d3b\u4f7f\u6211\u4eec\u83b7\u5f97\u91cd\u751f\uff0c\u6709\u6d3b\u6cfc\u7684\u76fc\u671b\uff0c"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u53ef\u4ee5\u627f\u53d7\u90a3\u4e0d\u4f1a\u673d\u574f\u3001\u6ca1\u6709\u6c61\u70b9\u3001\u4e0d\u4f1a\u8870\u6b8b\u3001\u4e3a\u4f60\u4eec\u5b58\u7559\u5728\u5929\u4e0a\u7684\u4ea7\u4e1a\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u4eec\u8fd9\u4e9b\u56e0\u4fe1\u800c\u8499\u4e0a\u5e1d\u7528\u5927\u80fd\u4fdd\u5b88\u7684\u4eba\uff0c\u5fc5\u80fd\u5f97\u5230\u90a3\u5df2\u7ecf\u9884\u5907\u597d\u3001\u5728\u672b\u4e16\u8981\u663e\u660e\u7684\u6551\u6069\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e3a\u6b64\uff0c\u5373\u4f7f\u73b0\u4eca\u4f60\u4eec\u5fc5\u987b\u6682\u65f6\u5728\u767e\u822c\u8bd5\u70bc\u4e2d\u5fcd\u53d7\u75db\u82e6\uff0c\u4e5f\u8981\u6ee1\u6000\u559c\u4e50\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8fd9\u6837\uff0c\u4f60\u4eec\u7684\u4fe1\u5fc3\u7ecf\u8fc7\u8bd5\u9a8c\u4fbf\u663e\u660e\u662f\u771f\u5b9e\u7684\uff0c\u6bd4\u90a3\u7ecf\u8fc7\u706b\u70bc\u4ecd\u4f1a\u574f\u6389\u7684\u91d1\u5b50\u66f4\u52a0\u5b9d\u8d35\uff0c\u4f7f\u4f60\u4eec\u53ef\u4ee5\u5728\u8036\u7a23\u57fa\u7763\u663e\u73b0\u65f6\u5f97\u5230\u79f0\u8d5e\u3001\u8363\u8000\u548c\u5c0a\u8d35\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u4eec\u867d\u7136\u6ca1\u6709\u89c1\u8fc7\u57fa\u7763\uff0c\u5374\u7231\u7942\uff1b\u867d\u7136\u73b0\u5728\u770b\u4e0d\u89c1\u7942\uff0c\u5374\u4fe1\u7942\uff0c\u5e76\u4e14\u6709\u65e0\u6cd5\u5f62\u5bb9\u3001\u5145\u6ee1\u8363\u8000\u7684\u559c\u4e50\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u56e0\u4e3a\u4f60\u4eec\u5f97\u5230\u4e86\u4fe1\u5fc3\u5e26\u6765\u7684\u679c\u6548\uff0c\u5c31\u662f\u4f60\u4eec\u7684\u7075\u9b42\u5f97\u6551\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6709\u5173\u8fd9\u6551\u6069\uff0c\u90a3\u4e9b\u9884\u8a00\u6069\u5178\u8981\u4e34\u5230\u4f60\u4eec\u7684\u4f17\u5148\u77e5\u65e9\u5df2\u8be6\u7ec6\u5bfb\u6c42\u67e5\u8003\u4e86\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u57fa\u7763\u7684\u7075\u66fe\u5728\u4ed6\u4eec\u5fc3\u4e2d\u6307\u793a\u4ed6\u4eec\uff0c\u9884\u8a00\u57fa\u7763\u8981\u53d7\u82e6\uff0c\u7136\u540e\u5f97\u8363\u8000\u3002\u4ed6\u4eec\u4fbf\u8be6\u7ec6\u67e5\u8003\u8fd9\u4e9b\u4e8b\u4f1a\u5728\u4ec0\u4e48\u65f6\u5019\u3001\u4ec0\u4e48\u60c5\u51b5\u4e0b\u53d1\u751f\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4ed6\u4eec\u5f97\u5230\u542f\u793a\uff0c\u77e5\u9053\u4ed6\u4eec\u4f20\u8bb2\u8fd9\u4e9b\u4e8b\u4e0d\u662f\u4e3a\u4e86\u81ea\u5df1\uff0c\u800c\u662f\u4e3a\u4e86\u4f60\u4eec\u3002\u73b0\u5728\uff0c\u90a3\u4e9b\u9760\u7740\u4ece\u5929\u4e0a\u5dee\u6765\u7684\u5723\u7075\u5411\u4f60\u4eec\u4f20\u798f\u97f3\u7684\u4eba\uff0c\u5df2\u7ecf\u5c06\u8fd9\u4e9b\u4e8b\u4f20\u7ed9\u4f60\u4eec\u4e86\u3002\u8fde\u5929\u4f7f\u4e5f\u60f3\u628a\u8fd9\u4e9b\u4e8b\u770b\u4e2a\u660e\u767d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8981\u5723\u6d01\u50cf\u4e3b - \u6240\u4ee5\uff0c\u8981\u9884\u5907\u597d\u4f60\u4eec\u7684\u5fc3\uff0c\u8c28\u614e\u81ea\u5f8b\uff0c\u4e13\u5fc3\u76fc\u671b\u8036\u7a23\u57fa\u7763\u663e\u73b0\u65f6\u8981\u5e26\u7ed9\u4f60\u4eec\u7684\u6069\u5178\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4f60\u4eec\u65e2\u662f\u987a\u670d\u7684\u513f\u5973\uff0c\u5c31\u4e0d\u8981\u518d\u50cf\u4ece\u524d\u65e0\u77e5\u7684\u65f6\u5019\u90a3\u6837\u653e\u7eb5\u79c1\u6b32\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u90a3\u547c\u53ec\u4f60\u4eec\u7684\u662f\u5723\u6d01\u7684\uff0c\u4f60\u4eec\u5728\u4e00\u5207\u4e8b\u4e0a\u4e5f\u8981\u5723\u6d01\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u56e0\u4e3a\u5723\u7ecf\u4e0a\u8bf4\uff1a\u201c\u4f60\u4eec\u8981\u5723\u6d01\uff0c\u56e0\u4e3a\u6211\u662f\u5723\u6d01\u7684\u3002\u201d"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u65e2\u7136\u4f60\u4eec\u79f0\u547c\u90a3\u4f4d\u6309\u5404\u4eba\u7684\u884c\u4e3a\u516c\u6b63\u65e0\u79c1\u5730\u5ba1\u5224\u4eba\u7684\u4e0a\u5e1d\u4e3a\u7236\uff0c\u5c31\u5e94\u8be5\u5b58\u7740\u656c\u754f\u7684\u5fc3\u8fc7\u4f60\u4eec\u5728\u4e16\u4e0a\u5bc4\u5c45\u7684\u65e5\u5b50\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u8981\u77e5\u9053\uff0c\u4f60\u4eec\u4ece\u4f20\u7edf\u7684\u3001\u6ca1\u6709\u610f\u4e49\u7684\u751f\u6d3b\u4e2d\u88ab\u6551\u8d4e\u51fa\u6765\uff0c\u4e0d\u662f\u9760\u91d1\u94f6\u7b49\u4f1a\u673d\u574f\u7684\u4e1c\u897f\uff0c"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u800c\u662f\u9760\u65e0\u7455\u65e0\u75b5\u7684\u4ee3\u7f6a\u7f94\u7f8a\u2014\u2014\u57fa\u7763\u7684\u5b9d\u8840\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u57fa\u7763\u662f\u4e0a\u5e1d\u5728\u521b\u4e16\u4ee5\u524d\u9884\u5148\u62e3\u9009\u7684\uff0c\u5374\u5728\u8fd9\u672b\u4e16\u4e3a\u4f60\u4eec\u663e\u73b0\u3002"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u4f60\u4eec\u501f\u7740\u57fa\u7763\u4fe1\u4e86\u4f7f\u7942\u4ece\u6b7b\u91cc\u590d\u6d3b\u3001\u8d50\u7942\u8363\u8000\u7684\u4e0a\u5e1d\uff0c\u6240\u4ee5\uff0c\u4f60\u4eec\u7684\u4fe1\u5fc3\u548c\u76fc\u671b\u90fd\u5728\u4e8e\u4e0a\u5e1d\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u4f60\u4eec\u65e2\u56e0\u987a\u670d\u771f\u7406\uff0c\u6d01\u51c0\u4e86\u81ea\u5df1\u7684\u5fc3\uff0c\u80fd\u591f\u771f\u8bda\u5730\u7231\u5f1f\u5144\u59ca\u59b9\uff0c\u5c31\u5f53\u4ee5\u6e05\u6d01\u7684\u5fc3\u5f7c\u6b64\u5207\u5b9e\u76f8\u7231\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u4f60\u4eec\u83b7\u5f97\u91cd\u751f\uff0c\u4e0d\u662f\u501f\u7740\u4f1a\u8150\u70c2\u7684\u79cd\u5b50\uff0c\u800c\u662f\u501f\u7740\u4e0d\u4f1a\u8150\u70c2\u7684\u79cd\u5b50\u2014\u2014\u4e0a\u5e1d\u6d3b\u6cfc\u6c38\u5b58\u7684\u9053\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u56e0\u4e3a \u201c\u82b8\u82b8\u4f17\u751f\u5c3d\u5982\u8349\uff0c \u8363\u534e\u4e0d\u8fc7\u8349\u4e0a\u82b1\uff1b \u8349\u5fc5\u67af\u5e72\u82b1\u5fc5\u6b8b\uff0c"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u552f\u6709\u4e3b\u9053\u6c38\u957f\u5b58\u3002\u201d \u8fd9\u9053\u5c31\u662f\u4f20\u7ed9\u4f60\u4eec\u7684\u798f\u97f3\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6e34\u6155\u7075\u5976 - \u56e0\u6b64\uff0c\u4f60\u4eec\u65e2\u7136\u5df2\u7ecf\u9664\u53bb\u4e00\u5207\u6076\u6bd2\u3001\u6b3a\u8bc8\u3001\u865a\u4f2a\u3001\u5ac9\u5992\u548c\u6bc1\u8c24\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u5c31\u8981\u50cf\u521d\u751f\u7684\u5a74\u513f\u4e00\u6837\u6e34\u6155\u7eaf\u51c0\u7684\u7075\u5976\uff0c\u597d\u4e0d\u65ad\u6210\u957f\uff0c\u76f4\u5230\u5b8c\u5168\u5f97\u6551\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u56e0\u4e3a\u4f60\u4eec\u5c1d\u8fc7\u4e3b\u6069\u7684\u6ecb\u5473\u662f\u4f55\u7b49\u7518\u7f8e\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7075\u5bab\u6d3b\u77f3 - \u4e3b\u662f\u6d3b\u77f3\uff0c\u867d\u7136\u88ab\u4eba\u5f03\u7edd\uff0c\u5374\u88ab\u4e0a\u5e1d\u62e3\u9009\u3001\u89c6\u4e3a\u5b9d\u8d35\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u4eec\u4e5f\u5982\u540c\u6d3b\u77f3\uff0c\u88ab\u7528\u6765\u5efa\u9020\u5c5e\u7075\u7684\u5bab\u6bbf\uff0c\u597d\u505a\u5723\u6d01\u7684\u796d\u53f8\uff0c\u501f\u7740\u8036\u7a23\u57fa\u7763\u732e\u4e0a\u8499\u4e0a\u5e1d\u60a6\u7eb3\u7684\u7075\u796d\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u56e0\u4e3a\u5723\u7ecf\u4e0a\u8bf4\uff1a \u201c\u770b\u554a\uff0c\u6211\u5728\u9521\u5b89\u653e\u4e86\u4e00\u5757\u8499\u62e3\u9009\u7684\u5b9d\u8d35\u623f\u89d2\u77f3\uff0c \u4fe1\u9760\u7942\u7684\u4eba\u5fc5\u4e0d\u4f1a\u8499\u7f9e\u3002\u201d"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6240\u4ee5\uff0c\u5bf9\u4f60\u4eec\u4fe1\u7684\u4eba\u800c\u8a00\uff0c\u8fd9\u77f3\u5934\u662f\u5b9d\u8d35\u7684\uff1b\u4f46\u5bf9\u90a3\u4e9b\u4e0d\u4fe1\u7684\u4eba\u6765\u8bf4\uff0c\u5374\u662f\uff1a \u201c\u5de5\u5320\u4e22\u5f03\u7684\u77f3\u5934\uff0c \u5df2\u6210\u4e86\u623f\u89d2\u77f3\u3002\u201d"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e5f\u662f\uff1a \u201c\u7eca\u811a\u77f3\u548c\u4f7f\u4eba\u8dcc\u5012\u7684\u78d0\u77f3\u3002\u201d \u4ed6\u4eec\u8dcc\u5012\u662f\u56e0\u4e3a\u4ed6\u4eec\u4e0d\u987a\u670d\u771f\u9053\uff0c\u8fd9\u79cd\u4e0b\u573a\u65e9\u5df2\u6ce8\u5b9a\u4e86\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4f46\u4f60\u4eec\u662f\u8499\u62e3\u9009\u7684\u65cf\u7fa4\uff0c\u662f\u541b\u5c0a\u7684\u796d\u53f8\uff0c\u662f\u5723\u6d01\u7684\u56fd\u5ea6\uff0c\u662f\u4e0a\u5e1d\u7684\u5b50\u6c11\uff0c\u56e0\u6b64\u4f60\u4eec\u53ef\u4ee5\u5ba3\u626c\u4e0a\u5e1d\u7684\u7f8e\u5fb7\u3002\u7942\u66fe\u547c\u53ec\u4f60\u4eec\u79bb\u5f00\u9ed1\u6697\uff0c\u8fdb\u5165\u7942\u5947\u5999\u7684\u5149\u660e\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4ece\u524d\u4f60\u4eec\u4e0d\u662f\u4e0a\u5e1d\u7684\u5b50\u6c11\uff0c\u73b0\u5728\u5374\u505a\u4e86\u7942\u7684\u5b50\u6c11\uff1b\u4ece\u524d\u4f60\u4eec\u672a\u8499\u601c\u60af\uff0c\u73b0\u5728\u5374\u8499\u4e86\u601c\u60af\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4e0a\u5e1d\u7684\u4ec6\u4eba - \u4eb2\u7231\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u4eec\u662f\u5ba2\u65c5\uff0c\u662f\u5bc4\u5c45\u7684\uff0c\u6211\u529d\u4f60\u4eec\u8981\u7981\u6212\u4e0e\u7075\u9b42\u4e3a\u654c\u7684\u90aa\u60c5\u79c1\u6b32\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f60\u4eec\u5728\u5f02\u6559\u5f92\u5f53\u4e2d\u8981\u54c1\u884c\u7aef\u6b63\u3002\u8fd9\u6837\uff0c\u5c3d\u7ba1\u4ed6\u4eec\u6bc1\u8c24\u4f60\u4eec\u662f\u4f5c\u6076\u7684\u4eba\uff0c\u4f46\u770b\u89c1\u4f60\u4eec\u7684\u597d\u884c\u4e3a\uff0c\u4e5f\u4f1a\u5728\u4e3b\u6765\u7684\u65e5\u5b50\u628a\u8363\u8000\u5f52\u7ed9\u4e0a\u5e1d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4e3a\u4e3b\u7684\u7f18\u6545\uff0c\u4f60\u4eec\u8981\u670d\u4ece\u4eba\u95f4\u7684\u4e00\u5207\u6743\u67c4\uff0c\u4e0d\u7ba1\u662f\u5c45\u9996\u4f4d\u7684\u541b\u738b\uff0c"
                },
                "14": {
                    "verse_nr": "14",
