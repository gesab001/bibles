    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u72b9\u5927\u738b\u4e9a\u4eec\u7684\u513f\u5b50\u7ea6\u897f\u4e9a\u6267\u653f\u671f\u95f4\uff0c\u8036\u548c\u534e\u5bf9\u5e0c\u897f\u8fe6\u7684\u7384\u5b59\u3001\u4e9a\u739b\u5229\u4e9a\u7684\u66fe\u5b59\u3001\u57fa\u5927\u5229\u7684\u5b59\u5b50\u3001\u53e4\u793a\u7684\u513f\u5b50\u897f\u756a\u96c5\u8bf4\uff1a"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u8036\u548c\u534e\u5ba1\u5224\u7684\u65e5\u5b50 - \u201c\u6211\u5fc5\u6bc1\u706d\u5730\u4e0a\u7684\u4e00\u5207\u3002 \u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u6211\u5fc5\u6bc1\u706d\u4eba\u7c7b\u3001\u517d\u7c7b\u3001 \u5929\u4e0a\u7684\u9e1f\u548c\u6d77\u91cc\u7684\u9c7c\u3002 \u6211\u5fc5\u4f7f\u6076\u4eba\u5012\u6bd9\uff0c \u6211\u5fc5\u94f2\u9664\u5730\u4e0a\u7684\u4eba\u7c7b\u3002 \u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u201c\u6211\u5fc5\u4f38\u624b\u653b\u51fb\u72b9\u5927\uff0c \u4ee5\u53ca\u6240\u6709\u4f4f\u5728\u8036\u8def\u6492\u51b7\u7684\u4eba\uff0c \u94f2\u9664\u5df4\u529b\u7684\u4f59\u8ff9\u53ca\u62dc\u5076\u50cf\u4e4b\u796d\u53f8\u7684\u540d\u53f7\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u6211\u5fc5\u94f2\u9664\u90a3\u4e9b\u5728\u5c4b\u9876\u796d\u62dc\u5929\u4e0a\u4e07\u8c61\u7684\u4eba\uff0c \u94f2\u9664\u656c\u62dc\u6211\u3001\u51ed\u6211\u8d77\u8a93\u53c8\u51ed\u7c73\u52d2\u516c\u8d77\u8a93\u7684\u4eba\uff0c"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u94f2\u9664\u79bb\u5f03\u6211\u3001\u4e0d\u5bfb\u6c42\u6211\u3001\u4e0d\u6c42\u95ee\u6211\u7684\u4eba\u3002\u201d"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8981\u5728\u4e3b\u8036\u548c\u534e\u9762\u524d\u8083\u9759\uff0c \u56e0\u4e3a\u8036\u548c\u534e\u7684\u65e5\u5b50\u8fd1\u4e86\u3002 \u8036\u548c\u534e\u5df2\u51c6\u5907\u597d\u796d\u7269\uff0c \u6d01\u51c0\u4e86\u7942\u9080\u8bf7\u7684\u4eba\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a\u201c\u5728\u6211\u732e\u796d\u7684\u65e5\u5b50\uff0c \u6211\u5fc5\u60e9\u7f5a\u9996\u9886\u548c\u738b\u5b50\uff0c \u4ee5\u53ca\u6240\u6709\u7a7f\u5916\u65cf\u670d\u88c5\u7684\u4eba\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5230\u90a3\u65e5\uff0c\u6211\u5fc5\u60e9\u7f5a\u6240\u6709\u8df3\u8fc7\u95e8\u69db\u3001 \u4f7f\u4e3b\u4eba\u7684\u5bb6\u5145\u6ee1\u66b4\u529b\u548c\u6b3a\u8bc8\u7684\u4eba\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5230\u90a3\u65e5\uff0c\u9c7c\u95e8\u5fc5\u4f20\u51fa\u54ed\u558a\u58f0\uff0c \u65b0\u533a\u5fc5\u54cd\u8d77\u54c0\u53f7\u58f0\uff0c \u5c71\u9675\u5fc5\u53d1\u51fa\u5d29\u88c2\u7684\u5de8\u54cd\u3002 \u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5e02\u573a\u533a\u7684\u5c45\u6c11\u554a\uff0c\u54c0\u54ed\u5427\uff01 \u56e0\u4e3a\u6240\u6709\u7684\u5546\u4eba\u5fc5\u706d\u4ea1\uff0c \u6240\u6709\u505a\u4e70\u5356\u7684\u5fc5\u88ab\u94f2\u9664\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u90a3\u65f6\uff0c\u6211\u5fc5\u63d0\u7740\u706f\u5de1\u67e5\u8036\u8def\u6492\u51b7\uff0c \u60e9\u7f5a\u90a3\u4e9b\u5b89\u4e8e\u7f6a\u4e2d\u7684\u4eba\u3002 \u4ed6\u4eec\u5fc3\u60f3\uff0c\u2018\u8036\u548c\u534e\u4e0d\u8d50\u798f\u4e5f\u4e0d\u964d\u7978\u3002\u2019"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4ed6\u4eec\u7684\u8d22\u7269\u5fc5\u906d\u62a2\u63a0\uff0c \u5bb6\u56ed\u5fc5\u6ca6\u4e3a\u5e9f\u589f\u3002 \u4ed6\u4eec\u5efa\u9020\u623f\u5c4b\uff0c\u5374\u4e0d\u80fd\u4f4f\u5728\u91cc\u9762\uff1b \u683d\u79cd\u8461\u8404\u56ed\uff0c\u5374\u559d\u4e0d\u5230\u8461\u8404\u9152\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u201c\u8036\u548c\u534e\u7684\u5927\u65e5\u5b50\u8fd1\u4e86\uff0c \u8fd1\u4e86\uff0c\u5f88\u5feb\u5c31\u5230\u4e86\u3002 \u90a3\u5c06\u662f\u75db\u82e6\u7684\u65e5\u5b50\uff0c \u52c7\u58eb\u4e5f\u5fc5\u51c4\u58f0\u54c0\u53f7\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u90a3\u662f\u964d\u70c8\u6012\u7684\u65e5\u5b50\uff0c \u662f\u56f0\u82e6\u8270\u96be\u7684\u65e5\u5b50\uff0c \u662f\u6467\u6b8b\u6bc1\u574f\u7684\u65e5\u5b50\uff0c \u662f\u9ed1\u6697\u5e7d\u51a5\u7684\u65e5\u5b50\uff0c \u662f\u9634\u973e\u5bc6\u5e03\u7684\u65e5\u5b50\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u662f\u5439\u53f7\u5450\u558a\u3001 \u653b\u6253\u575a\u57ce\u9ad8\u5792\u7684\u65e5\u5b50\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u201c\u6211\u8981\u4f7f\u4eba\u4eec\u707e\u96be\u4e34\u5934\uff0c \u4ee5\u81f4\u4ed6\u4eec\u884c\u8def\u50cf\u778e\u5b50\uff0c \u56e0\u4e3a\u4ed6\u4eec\u5f97\u7f6a\u4e86\u6211\u3002 \u4ed6\u4eec\u7684\u8840\u5fc5\u88ab\u5012\u51fa\uff0c\u5982\u540c\u7070\u5c18\uff1b \u4ed6\u4eec\u7684\u5c38\u4f53\u5fc5\u88ab\u4e22\u5f03\uff0c\u5982\u540c\u7caa\u4fbf\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u5728\u8036\u548c\u534e\u53d1\u6012\u7684\u65e5\u5b50\uff0c \u4ed6\u4eec\u7684\u91d1\u94f6\u6551\u4e0d\u4e86\u4ed6\u4eec\u3002 \u7942\u7684\u6012\u706b\u5fc5\u541e\u566c\u5927\u5730\uff0c \u7942\u5fc5\u9aa4\u7136\u6bc1\u706d\u4e00\u5207\u4e16\u4eba\u3002\u201d"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u547c\u5401\u767e\u59d3\u6094\u6539 - \u4e0d\u77e5\u7f9e\u803b\u7684\u56fd\u6c11\u554a\uff0c \u8d81\u547d\u4ee4\u5c1a\u672a\u53d1\u51fa\u3001 \u65e5\u5b50\u5c1a\u672a\u5982\u98ce\u4e2d\u7684\u7ce0\u75be\u98de\u800c\u53bb\u3001 \u8036\u548c\u534e\u7684\u70c8\u6012\u5c1a\u672a\u4e34\u5230\u4f60\u4eec\u3001 \u7942\u5411\u4f60\u4eec\u53d1\u6012\u7684\u65e5\u5b50\u5c1a\u672a\u6765\u4e34\uff0c \u8981\u805a\u96c6\uff0c\u805a\u96c6\u8d77\u6765\uff01"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u547c\u5401\u767e\u59d3\u6094\u6539 - \u4e0d\u77e5\u7f9e\u803b\u7684\u56fd\u6c11\u554a\uff0c \u8d81\u547d\u4ee4\u5c1a\u672a\u53d1\u51fa\u3001 \u65e5\u5b50\u5c1a\u672a\u5982\u98ce\u4e2d\u7684\u7ce0\u75be\u98de\u800c\u53bb\u3001 \u8036\u548c\u534e\u7684\u70c8\u6012\u5c1a\u672a\u4e34\u5230\u4f60\u4eec\u3001 \u7942\u5411\u4f60\u4eec\u53d1\u6012\u7684\u65e5\u5b50\u5c1a\u672a\u6765\u4e34\uff0c \u8981\u805a\u96c6\uff0c\u805a\u96c6\u8d77\u6765\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4e16\u4e0a\u6240\u6709\u9075\u5b88\u8036\u548c\u534e\u5178\u7ae0\u7684\u8c26\u5351\u4eba\u554a\uff0c \u8981\u5bfb\u6c42\u8036\u548c\u534e\uff0c\u8ffd\u6c42\u516c\u4e49\uff0c\u4e3a\u4eba\u8c26\u5351\u3002 \u4e5f\u8bb8\u5728\u8036\u548c\u534e\u53d1\u6012\u7684\u65e5\u5b50\uff0c \u4f60\u4eec\u53ef\u4ee5\u5f97\u5230\u5e87\u62a4\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5ba1\u5224\u4ee5\u8272\u5217\u7684\u90bb\u56fd - \u8fe6\u8428\u5fc5\u88ab\u629b\u5f03\uff0c \u4e9a\u5b9e\u57fa\u4f26\u5fc5\u6ca6\u4e3a\u5e9f\u589f\uff0c \u4e9a\u5b9e\u7a81\u7684\u5c45\u6c11\u5fc5\u5728\u4e2d\u5348\u88ab\u8d76\u8d70\uff0c \u4ee5\u9769\u4f26\u5fc5\u88ab\u8fde\u6839\u62d4\u9664\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f4f\u5728\u6d77\u8fb9\u7684\u57fa\u5229\u63d0\u4eba\u554a\uff0c \u4f60\u4eec\u6709\u7978\u4e86\uff01 \u975e\u5229\u58eb\u4eba\u7684\u571f\u5730\u2014\u2014\u8fe6\u5357\u554a\uff0c \u8036\u548c\u534e\u5ba3\u544a\u8981\u6bc1\u706d\u4f60\uff0c \u4f7f\u4f60\u6773\u65e0\u4eba\u70df\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6cbf\u6d77\u5730\u533a\u5fc5\u6210\u4e3a\u8349\u573a\uff0c \u6709\u7267\u4eba\u7684\u5c45\u6240\u548c\u7f8a\u5708\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u90a3\u91cc\u5fc5\u5f52\u7ed9\u72b9\u5927\u5bb6\u7684\u4f59\u6c11\uff0c \u4ed6\u4eec\u5fc5\u5728\u90a3\u91cc\u653e\u7267\uff0c \u665a\u4e0a\u7761\u5728\u4e9a\u5b9e\u57fa\u4f26\u7684\u623f\u5c4b\u91cc\uff0c \u56e0\u4e3a\u4ed6\u4eec\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u5fc5\u7737\u987e\u4ed6\u4eec\uff0c \u4f7f\u4ed6\u4eec\u56de\u5230\u6545\u571f\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ee5\u8272\u5217\u7684\u4e0a\u5e1d\u2014\u2014\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a \u201c\u6211\u542c\u89c1\u4e86\u6469\u62bc\u4eba\u7684\u6bc1\u8c24\u548c\u4e9a\u626a\u4eba\u7684\u8fb1\u9a82\u3002 \u4ed6\u4eec\u6bc1\u8c24\u6211\u7684\u5b50\u6c11\uff0c\u626c\u8a00\u4fb5\u5360\u4ed6\u4eec\u7684\u571f\u5730\u3002 \u6211\u51ed\u81ea\u5df1\u7684\u6c38\u6052\u8d77\u8a93\uff0c \u6469\u62bc\u548c\u4e9a\u626a\u5fc5\u523a\u8349\u4e1b\u751f\u3001\u76d0\u5751\u904d\u5730\u3001\u6c38\u8fdc\u8352\u5e9f\uff0c \u5982\u6240\u591a\u739b\u548c\u86fe\u6469\u62c9\u4e00\u6837\u3002 \u6211\u4f59\u4e0b\u7684\u5b50\u6c11\u5fc5\u63b3\u63a0\u4ed6\u4eec\uff0c \u6211\u5e78\u5b58\u7684\u767e\u59d3\u5fc5\u627f\u53d7\u4ed6\u4eec\u7684\u571f\u5730\u3002\u201d"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4ee5\u8272\u5217\u7684\u4e0a\u5e1d\u2014\u2014\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a \u201c\u6211\u542c\u89c1\u4e86\u6469\u62bc\u4eba\u7684\u6bc1\u8c24\u548c\u4e9a\u626a\u4eba\u7684\u8fb1\u9a82\u3002 \u4ed6\u4eec\u6bc1\u8c24\u6211\u7684\u5b50\u6c11\uff0c\u626c\u8a00\u4fb5\u5360\u4ed6\u4eec\u7684\u571f\u5730\u3002 \u6211\u51ed\u81ea\u5df1\u7684\u6c38\u6052\u8d77\u8a93\uff0c \u6469\u62bc\u548c\u4e9a\u626a\u5fc5\u523a\u8349\u4e1b\u751f\u3001\u76d0\u5751\u904d\u5730\u3001\u6c38\u8fdc\u8352\u5e9f\uff0c \u5982\u6240\u591a\u739b\u548c\u86fe\u6469\u62c9\u4e00\u6837\u3002 \u6211\u4f59\u4e0b\u7684\u5b50\u6c11\u5fc5\u63b3\u63a0\u4ed6\u4eec\uff0c \u6211\u5e78\u5b58\u7684\u767e\u59d3\u5fc5\u627f\u53d7\u4ed6\u4eec\u7684\u571f\u5730\u3002\u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u8fd9\u662f\u4ed6\u4eec\u7684\u62a5\u5e94\uff0c \u56e0\u4e3a\u4ed6\u4eec\u72c2\u5984\u81ea\u5927\uff0c \u6bc1\u8c24\u4e07\u519b\u4e4b\u8036\u548c\u534e\u7684\u5b50\u6c11\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u8036\u548c\u534e\u5fc5\u4ee4\u4ed6\u4eec\u6050\u60e7\uff0c \u56e0\u4e3a\u7942\u8981\u6d88\u706d\u4e16\u4e0a\u7684\u795e\u660e\u3002 \u5929\u4e0b\u4e07\u6c11\u5fc5\u5404\u5728\u672c\u5730\u656c\u62dc\u7942\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u53e4\u5b9e\u4eba\u554a\uff0c \u4f60\u4eec\u5fc5\u6b7b\u5728\u8036\u548c\u534e\u7684\u5200\u4e0b\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u7942\u5fc5\u4f38\u624b\u653b\u51fb\u5317\u65b9\uff0c\u6bc1\u706d\u4e9a\u8ff0\uff0c \u4f7f\u5c3c\u5c3c\u5fae\u4e00\u7247\u8352\u51c9\uff0c \u5982\u5e72\u65f1\u7684\u65f7\u91ce\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u7272\u755c\u548c\u91ce\u517d\u5fc5\u8eba\u5367\u5728\u90a3\u91cc\uff0c \u732b\u5934\u9e70\u548c\u523a\u732c\u6816\u8eab\u5728\u623f\u67f1\u4e0a\uff0c \u7a97\u53e3\u4f20\u6765\u9e1f\u9e23\uff0c \u95e8\u5ead\u90fd\u5df2\u7834\u8d25\uff0c \u9732\u51fa\u9999\u67cf\u6728\u6881\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8fd9\u5c31\u662f\u90a3\u6b22\u4e50\u5b89\u9038\u3001 \u81ea\u4ee5\u4e3a\u65e0\u4e0e\u4f26\u6bd4\u7684\u57ce\uff0c \u73b0\u5728\u7adf\u7136\u5982\u6b64\u8352\u51c9\uff0c \u6210\u4e3a\u91ce\u517d\u6816\u606f\u4e4b\u5730\uff01 \u8def\u8fc7\u7684\u4eba\u90fd\u6447\u624b\u5632\u7b11\u5b83\u3002"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u8d25\u574f - \u8fd9\u53db\u9006\u3001\u6c61\u79fd\u3001\u5145\u6ee1\u538b\u8feb\u7684\u57ce\u6709\u7978\u4e86\uff01"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u5979\u4e0d\u542c\u547d\u4ee4\uff0c\u4e0d\u53d7\u7ba1\u6559\uff1b \u4e0d\u501a\u9760\u8036\u548c\u534e\uff0c \u4e0d\u4eb2\u8fd1\u5979\u7684\u4e0a\u5e1d\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u5979\u7684\u9996\u9886\u662f\u5486\u54ee\u7684\u72ee\u5b50\uff0c \u5979\u7684\u5ba1\u5224\u5b98\u662f\u591c\u95f4\u7684\u8c7a\u72fc\uff0c \u4e0d\u7559\u4e00\u70b9\u730e\u7269\u5230\u5929\u660e\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5979\u7684\u5148\u77e5\u662f\u72c2\u5984\u8be1\u8bc8\u4e4b\u5f92\uff0c \u5979\u7684\u796d\u53f8\u4eb5\u6e0e\u5723\u6bbf\uff0c\u8fdd\u80cc\u5f8b\u6cd5\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f46\u57ce\u4e2d\u7684\u8036\u548c\u534e\u662f\u516c\u4e49\u7684\uff0c \u7942\u7edd\u4e0d\u505a\u4e0d\u4e49\u4e4b\u4e8b\u3002 \u7942\u6bcf\u5929\u65e9\u6668\u4f38\u5f20\u6b63\u4e49\uff0c\u4ece\u4e0d\u95f4\u65ad\u3002 \u7136\u800c\uff0c\u4e0d\u4e49\u4e4b\u4eba\u5374\u4e0d\u77e5\u7f9e\u803b\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u201c\u6211\u5df2\u6d88\u706d\u5217\u56fd\uff0c \u6467\u6bc1\u4ed6\u4eec\u7684\u57ce\u697c\uff0c \u4f7f\u4ed6\u4eec\u7684\u8857\u9053\u8352\u5e9f\uff0c\u6ca1\u6709\u8def\u4eba\uff1b \u4ed6\u4eec\u7684\u57ce\u9091\u6ca6\u4e3a\u5e9f\u589f\uff0c\u6773\u65e0\u4eba\u8ff9\uff0c\u6ca1\u6709\u5c45\u6c11\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6211\u4ee5\u4e3a\u8fd9\u57ce\u4e00\u5b9a\u4f1a\u656c\u754f\u6211\uff0c\u63a5\u53d7\u7ba1\u6559\uff0c \u8fd9\u6837\u6211\u5c31\u4e0d\u5fc5\u7167\u6240\u5b9a\u7684\u6bc1\u706d\u5979\u3002 \u7136\u800c\uff0c\u4ed6\u4eec\u5374\u66f4\u52a0\u70ed\u8877\u4e8e\u72af\u7f6a\u4f5c\u6076\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u201c\u56e0\u6b64\uff0c\u7b49\u7740\u6211\u5427\uff0c \u7b49\u7740\u6211\u6307\u63a7\u7684\u90a3\u4e00\u5929\u5427\u3002 \u6211\u5df2\u51b3\u5b9a\u53ec\u96c6\u5217\u56fd\u5217\u90a6\uff0c \u5411\u4ed6\u4eec\u503e\u5012\u6211\u7684\u70c8\u6012\uff0c \u6211\u7684\u6012\u706b\u5fc5\u541e\u566c\u5929\u4e0b\u3002 \u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8036\u548c\u534e\u7684\u62ef\u6551 - \u201c\u90a3\u65f6\uff0c\u6211\u5fc5\u6d01\u51c0\u4e07\u6c11\u7684\u53e3\uff0c \u4ee5\u4fbf\u4ed6\u4eec\u53ef\u4ee5\u6c42\u544a\u6211\u7684\u540d\uff0c \u540c\u5fc3\u5408\u610f\u5730\u4e8b\u5949\u6211\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u90a3\u4e9b\u656c\u62dc\u6211\u7684\uff0c \u5c31\u662f\u6211\u90a3\u4e9b\u5206\u6563\u7684\u5b50\u6c11\u5fc5\u4ece\u53e4\u5b9e\u6cb3\u5bf9\u5cb8\u5e26\u7740\u4f9b\u7269\u6765\u732e\u7ed9\u6211\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5230\u90a3\u5929\uff0c\u4f60\u5fc5\u4e0d\u518d\u56e0\u80cc\u53db\u6211\u800c\u611f\u5230\u7f9e\u6127\uff0c \u56e0\u4e3a\u90a3\u65f6\u6211\u5fc5\u9664\u6389\u4f60\u90a3\u91cc\u72c2\u5984\u81ea\u5927\u7684\u4eba\uff0c \u4f60\u5728\u6211\u7684\u5723\u5c71\u4e0a\u5fc5\u4e0d\u518d\u9ad8\u50b2\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u5fc5\u628a\u8c26\u5351\u8d2b\u82e6\u7684\u4eba\u7559\u5728\u4f60\u90a3\u91cc\uff0c \u4ed6\u4eec\u5fc5\u6295\u9760\u5728\u6211\u8036\u548c\u534e\u7684\u540d\u4e0b\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4ee5\u8272\u5217\u7684\u4f59\u6c11\u5fc5\u4e0d\u518d\u4f5c\u6076\uff0c \u6492\u8c0e\u6216\u51fa\u8be1\u8bc8\u4e4b\u8a00\u3002 \u4ed6\u4eec\u5403\u559d\u8eba\u5367\uff0c\u65e0\u4eba\u60ca\u6270\u3002\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6b22\u4e50\u4e4b\u6b4c - \u9521\u5b89\u57ce\u554a\uff0c\u6b4c\u5531\u5427\uff01 \u4ee5\u8272\u5217\u554a\uff0c\u6b22\u547c\u5427\uff01 \u8036\u8def\u6492\u51b7\u57ce\u554a\uff0c\u5c3d\u60c5\u5730\u6b22\u559c\u5feb\u4e50\u5427\uff01"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8036\u548c\u534e\u5df2\u7ecf\u64a4\u9500\u5bf9\u4f60\u7684\u5211\u7f5a\uff0c \u8d76\u8d70\u4f60\u7684\u4ec7\u654c\uff0c \u4ee5\u8272\u5217\u7684\u738b\u8036\u548c\u534e\u4e0e\u4f60\u540c\u5728\uff0c \u4f60\u5fc5\u4e0d\u518d\u60e7\u6015\u707e\u7978\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u5230\u90a3\u5929\uff0c\u8036\u8def\u6492\u51b7\u5fc5\u542c\u5230\u8fd9\u6837\u7684\u8bdd\uff1a \u201c\u9521\u5b89\u554a\uff0c\u4e0d\u8981\u5bb3\u6015\uff01\u4e0d\u8981\u53cc\u624b\u53d1\u8f6f\uff01"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4f60\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u4e0e\u4f60\u540c\u5728\uff0c \u7942\u662f\u5927\u80fd\u7684\u62ef\u6551\u8005\uff0c \u7942\u5fc5\u56e0\u4f60\u800c\u6b22\u559c\u5feb\u4e50\uff0c\u6b22\u7136\u6b4c\u5531\uff0c \u7528\u7942\u7684\u7231\u5b89\u6170\u4f60\u3002\u201d"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u201c\u6211\u5fc5\u62db\u805a\u4f60\u90a3\u91cc\u56e0\u4e0d\u80fd\u5b88\u8282\u671f\u800c\u5fe7\u6101\u7684\u4eba\uff0c \u4f7f\u4f60\u4e0d\u518d\u8499\u53d7\u7f9e\u8fb1\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u770b\u554a\uff0c\u90a3\u65f6\u6211\u5fc5\u60e9\u7f5a\u6240\u6709\u6b3a\u538b\u4f60\u7684\u4eba\uff0c \u62ef\u6551\u7638\u817f\u7684\uff0c\u53ec\u56de\u5206\u6563\u7684\uff0c \u4f7f\u4ed6\u4eec\u7684\u7f9e\u8fb1\u5316\u4e3a\u5c0a\u8363\uff0c\u58f0\u540d\u8fdc\u64ad\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u90a3\u65f6\uff0c\u6211\u5fc5\u62db\u805a\u4f60\u4eec\uff0c\u5e26\u4f60\u4eec\u56de\u5230\u6545\u571f\u3002 \u4f60\u4eec\u56de\u5230\u6545\u571f\u540e\uff0c \u6211\u5fc5\u4f7f\u4f60\u4eec\u5728\u5929\u4e0b\u4e07\u6c11\u4e2d\u58f0\u540d\u8fdc\u64ad\uff0c\u5f97\u5230\u5c0a\u8363\u3002\u201d \u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002"
                }
            }
        }
    }
