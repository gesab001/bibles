                "1": {
                    "verse_nr": "1",
                    "verse": "\u4ee5\u4e0b\u662f\u54c8\u5df4\u8c37\u5148\u77e5\u5f97\u5230\u7684\u542f\u793a\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u54c8\u5df4\u8c37\u7684\u62b1\u6028 - \u8036\u548c\u534e\u554a\uff0c \u6211\u8981\u5411\u4f60\u547c\u6551\u5230\u4f55\u65f6\uff0c \u4f60\u624d\u5782\u542c\u5462\uff1f \u66b4\u8650\u6a2a\u884c\uff0c\u6211\u5411\u4f60\u547c\u6c42\uff0c \u4f60\u5374\u4e0d\u62ef\u6551\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4f60\u4e3a\u4f55\u8ba9\u6211\u76ee\u7779\u7f6a\u6076\uff1f \u4f60\u4e3a\u4f55\u5bb9\u5fcd\u90aa\u6076\uff1f \u6211\u773c\u524d\u5c3d\u662f\u6bc1\u706d\u548c\u66b4\u529b\uff0c \u5230\u5904\u662f\u7eb7\u4e89\u548c\u51b2\u7a81\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u56e0\u6b64\u5f8b\u6cd5\u5931\u6548\uff0c \u6b63\u4e49\u4e0d\u5f70\uff0c \u6076\u4eba\u5305\u56f4\u4e49\u4eba\uff0c \u6b63\u4e49\u88ab\u626d\u66f2\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u8036\u548c\u534e\u7684\u7b54\u590d - \u8036\u548c\u534e\u8bf4\uff1a\u201c\u4f60\u4eec\u73af\u987e\u5217\u56fd\uff0c \u4ed4\u7ec6\u5bdf\u770b\uff0c \u5fc5\u60ca\u5947\u4e0d\u5df2\uff0c \u56e0\u4e3a\u6211\u8981\u5728\u4f60\u4eec\u7684\u65f6\u4ee3\u884c\u4e00\u4ef6\u4e8b\uff0c \u5373\u4f7f\u4f60\u4eec\u542c\u8bf4\u4e86\u4e5f\u4e0d\u4f1a\u76f8\u4fe1\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u8981\u4f7f\u51f6\u6b8b\u3001\u66b4\u8650\u7684\u8fe6\u52d2\u5e95\u4eba\u5174\u8d77\u3002 \u4ed6\u4eec\u8981\u5e2d\u5377\u5929\u4e0b\uff0c \u5f3a\u5360\u522b\u4eba\u7684\u5bb6\u56ed\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4ed6\u4eec\u4efb\u610f\u5984\u4e3a\uff0c \u4ee4\u4eba\u80c6\u6218\u5fc3\u60ca\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u201c\u4ed6\u4eec\u7684\u9a6c\u6bd4\u8c79\u5b50\u8fd8\u5feb\uff0c \u6bd4\u591c\u72fc\u8fd8\u731b\u3002 \u4ed6\u4eec\u7684\u9a91\u5175\u4ece\u8fdc\u65b9\u98de\u9a70\u800c\u6765\uff0c \u5982\u9e70\u6251\u98df\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4ed6\u4eec\u6b8b\u66b4\u6210\u6027\uff0c \u50cf\u72c2\u98ce\u4e00\u6837\u6251\u6765\uff0c \u6293\u83b7\u7684\u4fd8\u864f\u591a\u5982\u5c18\u6c99\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4ed6\u4eec\u5632\u7b11\u541b\u738b\uff0c\u8ba5\u8bbd\u9996\u9886\uff0c \u5bf9\u4e00\u5207\u575a\u57ce\u55e4\u4e4b\u4ee5\u9f3b\uff0c \u4fee\u7b51\u9ad8\u53f0\u5c06\u5176\u653b\u53d6\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4ed6\u4eec\u5982\u72c2\u98ce\u6a2a\u626b\u800c\u8fc7\uff0c \u8fd9\u4e9b\u6709\u7f6a\u7684\u4eba\u628a\u81ea\u5df1\u7684\u529b\u91cf\u5949\u4e3a\u795e\u660e\u3002\u201d"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u54c8\u5df4\u8c37\u518d\u6b21\u62b1\u6028 - \u8036\u548c\u534e\uff0c \u6211\u7684\u4e0a\u5e1d\uff0c\u6211\u7684\u5723\u8005\u554a\uff0c \u4f60\u4ece\u4e98\u53e4\u5c31\u5b58\u5728\uff0c \u4f60\u6c38\u4e0d\u6d88\u901d\u3002 \u8036\u548c\u534e\u554a\uff0c\u4f60\u6d3e\u8fe6\u52d2\u5e95\u4eba\u6765\u5ba1\u5224\uff1b \u78d0\u77f3\u554a\uff0c\u4f60\u7acb\u4ed6\u4eec\u4f5c\u65bd\u7f5a\u8005\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u7684\u773c\u76ee\u6781\u4e3a\u6e05\u6d01\uff0c \u89c1\u4e0d\u5f97\u90aa\u6076\uff0c \u4e5f\u4e0d\u80fd\u5bb9\u5fcd\u7f6a\u6076\u3002 \u4f46\u4f60\u4e3a\u4f55\u5bb9\u5fcd\u5978\u6076\u4e4b\u4eba\u5462\uff1f \u6076\u4eba\u541e\u706d\u6bd4\u4ed6\u4eec\u516c\u4e49\u7684\u4eba\uff0c \u4f60\u4e3a\u4f55\u9ed8\u7136\u4e0d\u8bed\u5462\uff1f"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4f60\u4e3a\u4f55\u4f7f\u4eba\u50cf\u6d77\u91cc\u7684\u9c7c\uff0c \u50cf\u6ca1\u6709\u9996\u9886\u7684\u722c\u866b\u5462\uff1f"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8fe6\u52d2\u5e95\u4eba\u7528\u94a9\u5b50\u94a9\u4f4f\u4ed6\u4eec\uff0c \u7528\u6e14\u7f51\u7f51\u4f4f\u4ed6\u4eec\uff0c \u7528\u62d6\u7f51\u628a\u4ed6\u4eec\u805a\u5728\u4e00\u8d77\uff0c \u5e76\u56e0\u6b64\u800c\u6b22\u559c\u5feb\u4e50\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u8fe6\u52d2\u5e95\u4eba\u5411\u6e14\u7f51\u732e\u796d\uff0c \u5411\u6e14\u7f51\u70e7\u9999\uff0c \u56e0\u4e3a\u4ed6\u4eec\u9760\u6e14\u7f51\u800c\u5bcc\u88d5\uff0c \u98df\u7269\u5145\u8db3\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u96be\u9053\u4ed6\u4eec\u8981\u65e0\u4f11\u6b62\u5730\u6492\u7f51\uff0c \u65e0\u60c5\u5730\u6bc1\u706d\u5217\u56fd\u5417\uff1f"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u8981\u767b\u4e0a\u77ad\u671b\u5854\uff0c \u7ad9\u5728\u671b\u697c\u4e0a\u7b49\u5019\uff0c \u770b\u8036\u548c\u534e\u8981\u5bf9\u6211\u8bf4\u4ec0\u4e48\uff0c \u8981\u600e\u6837\u7b54\u590d\u6211\u7684\u62b1\u6028\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u8036\u548c\u534e\u7684\u7b54\u590d - \u8036\u548c\u534e\u5bf9\u6211\u8bf4\uff1a \u201c\u5c06\u6211\u7684\u542f\u793a\u8bb0\u4e0b\u6765\uff0c \u6e05\u695a\u5730\u523b\u5728\u7248\u4e0a\uff0c \u4fbf\u4e8e\u4eba\u4eec\u9605\u8bfb\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8fd9\u542f\u793a\u5b9e\u73b0\u7684\u65e5\u671f\u8fd8\u6ca1\u5230\uff0c \u5b83\u662f\u5173\u4e8e\u672b\u540e\u7684\u4e8b\uff0c\u6ca1\u6709\u865a\u5047\u3002 \u5373\u4f7f\u7f13\u6162\uff0c\u4e5f\u8981\u7b49\u5019\uff0c \u56e0\u4e3a\u8fd9\u4e8b\u7ec8\u5fc5\u53d1\u751f\uff0c\u4e0d\u518d\u8fdf\u5ef6\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u770b\u90a3\u9a84\u50b2\u4e4b\u4eba\uff0c\u4ed6\u4eec\u5fc3\u672f\u4e0d\u6b63\uff0c \u4f46\u4e49\u4eba\u5fc5\u9760\u4fe1\u5fc3\u800c\u6d3b\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u8d22\u5bcc\u4f1a\u6b3a\u9a97\u4eba\uff0c \u4f7f\u9a84\u50b2\u4e4b\u4eba\u6c38\u4e0d\u5b89\u5206\uff0c \u50cf\u9634\u95f4\u4e00\u6837\u8d2a\u5f97\u65e0\u538c\uff0c \u5982\u6b7b\u4ea1\u4e00\u822c\u6c38\u4e0d\u6ee1\u8db3\u3002 \u56e0\u6b64\uff0c\u4ed6\u4eec\u5f81\u670d\u5217\u56fd\uff0c \u63b3\u63a0\u4e07\u6c11\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u201c\u4f46\u90a3\u4e9b\u88ab\u63b3\u7684\u4eba\u5fc5\u7528\u8bd7\u6b4c\u548c\u4fd7\u8bed\u5632\u8bbd\u4ed6\u4eec\uff0c\u8bf4\uff0c \u2018\u4f60\u4eec\u6709\u7978\u4e86\uff01 \u4f60\u4eec\u541e\u6ca1\u522b\u4eba\u7684\u8d22\u4ea7\uff0c \u9760\u5265\u524a\u522b\u4eba\u56e4\u79ef\u8d22\u5bcc\uff0c \u8981\u5230\u4f55\u65f6\u5462\uff1f\u2019"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5411\u4f60\u4eec\u501f\u503a\u7684\u4eba\u5fc5\u7a81\u7136\u653b\u51fb\u4f60\u4eec\uff0c \u4f7f\u4f60\u4eec\u6218\u6218\u5162\u5162\uff0c \u6210\u4e3a\u4fd8\u864f\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u4eec\u4ece\u524d\u63b3\u63a0\u8bb8\u591a\u56fd\u5bb6\uff0c \u6740\u4eba\u6d41\u8840\uff0c \u6467\u6bc1\u4ed6\u4eec\u7684\u571f\u5730\u3001\u57ce\u5e02\u548c\u4eba\u6c11\uff0c \u6240\u4ee5\u5404\u56fd\u7684\u4f59\u6c11\u5fc5\u63a0\u593a\u4f60\u4eec\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u201c\u4e3a\u81ea\u5df1\u7684\u5bb6\u79ef\u84c4\u4e0d\u4e49\u4e4b\u8d22\u3001 \u5728\u9ad8\u5904\u642d\u7a9d\u8eb2\u907f\u707e\u7978\u7684\u4eba\u554a\uff0c \u4f60\u4eec\u6709\u7978\u4e86\uff01"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4f60\u4eec\u9634\u8c0b\u6bc1\u706d\u5404\u65cf\uff0c \u4ee5\u81f4\u81ea\u8fb1\u5df1\u5bb6\uff0c\u81ea\u5bb3\u5df1\u547d\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5899\u4e0a\u7684\u77f3\u5934\u5fc5\u9ad8\u58f0\u6307\u63a7\u4f60\u4eec\uff0c \u623f\u5b50\u7684\u6881\u6728\u5fc5\u540c\u58f0\u54cd\u5e94\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u201c\u4ee5\u4eba\u8840\u5efa\u57ce\u9091\uff0c \u4ee5\u7f6a\u6076\u9020\u57ce\u6c60\u7684\u4eba\u554a\uff0c \u4f60\u4eec\u6709\u7978\u4e86\uff01"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u5217\u90a6\u7684\u8d22\u5bcc\u90fd\u5316\u4e3a\u7070\u70ec\uff0c \u5217\u56fd\u52b3\u788c\u5374\u662f\u4e00\u573a\u7a7a\u3002 \u8fd9\u4e0d\u90fd\u662f\u51fa\u4e8e\u4e07\u519b\u4e4b\u8036\u548c\u534e\u5417\uff1f"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5927\u5730\u5fc5\u5145\u6ee1\u5bf9\u8036\u548c\u534e\u4e4b\u8363\u8000\u7684\u8ba4\u8bc6\uff0c \u5c31\u50cf\u6d77\u6d0b\u5145\u6ee1\u4e86\u6c34\u4e00\u6837\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u201c\u7528\u9152\u704c\u9189\u90bb\u5c45\u4ee5\u89c2\u770b\u4ed6\u4eec\u797c\u4f53\u7684\u4eba\u554a\uff0c \u4f60\u4eec\u6709\u7978\u4e86\uff01"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4f60\u4eec\u5fc5\u9971\u53d7\u7f9e\u8fb1\uff0c\u6beb\u65e0\u8363\u8000\u3002 \u4f60\u4eec\u559d\u5427\uff0c\u9732\u51fa\u4e0b\u4f53\u5427\uff01 \u8036\u548c\u534e\u53f3\u624b\u4e2d\u7684\u676f\u5c06\u4f20\u7ed9\u4f60\u4eec\uff0c \u4f60\u4eec\u7684\u8363\u8000\u5c06\u53d8\u6210\u7f9e\u8fb1\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4f60\u4eec\u4ee5\u66b4\u8650\u5bf9\u5f85\u9ece\u5df4\u5ae9\uff0c \u81ea\u5df1\u5fc5\u88ab\u66b4\u8650\u541e\u566c\uff1b \u4f60\u4eec\u6bc1\u706d\u91ce\u517d\uff0c \u81ea\u5df1\u5fc5\u88ab\u6050\u6016\u7b3c\u7f69\u3002 \u56e0\u4e3a\u4f60\u4eec\u6740\u4eba\u6d41\u8840\uff0c \u6467\u6bc1\u571f\u5730\u3001\u57ce\u5e02\u548c\u4eba\u6c11\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u201c\u96d5\u523b\u7684\u5076\u50cf\u6709\u4ec0\u4e48\u76ca\u5904\uff1f \u9a97\u4eba\u7684\u94f8\u50cf\u6709\u4ec0\u4e48\u7528\u5904\uff1f \u5236\u9020\u8005\u7adf\u4fe1\u9760\u81ea\u5df1\u9020\u7684\u54d1\u5df4\u5076\u50cf\uff01"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u5bf9\u6728\u5934\u8bf4\u2018\u9192\u9192\u5427\u2019\uff0c \u5bf9\u54d1\u5df4\u77f3\u5934\u8bf4\u2018\u8d77\u6765\u5427\u2019\u7684\u4eba\u554a\uff0c\u4f60\u4eec\u6709\u7978\u4e86\uff01 \u5b83\u4eec\u80fd\u6559\u5bfc\u4f60\u4eec\u5417\uff1f \u770b\u554a\uff0c\u5b83\u4eec\u5916\u9762\u5305\u91d1\u88f9\u94f6\uff0c \u91cc\u9762\u5374\u6beb\u65e0\u751f\u547d\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4f46\u8036\u548c\u534e\u4f4f\u5728\u7942\u7684\u5723\u6bbf\u4e2d\uff0c \u4e16\u4eba\u90fd\u8981\u5728\u7942\u9762\u524d\u8083\u9759\u3002\u201d"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u54c8\u5df4\u8c37\u7684\u7977\u544a - \u4ee5\u4e0b\u662f\u54c8\u5df4\u8c37\u5148\u77e5\u7684\u7977\u544a\uff0c\u91c7\u7528\u6d41\u79bb\u8c03\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u8036\u548c\u534e\u554a\uff0c\u6211\u542c\u8fc7\u4f60\u7684\u5a01\u540d\uff1b \u8036\u548c\u534e\u554a\uff0c\u6211\u5bf9\u4f60\u7684\u4f5c\u4e3a\u5145\u6ee1\u656c\u754f\u3002 \u6c42\u4f60\u5982\u4eca\u518d\u6b21\u5f70\u663e\u4f60\u7684\u4f5c\u4e3a\uff0c \u6c42\u4f60\u53d1\u6012\u7684\u65f6\u5019\u4ecd\u4ee5\u601c\u60af\u4e3a\u5ff5\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4e0a\u5e1d\u4ece\u63d0\u5e54\u6765\uff0c \u5723\u8005\u4ece\u5df4\u5170\u5c71\u6765\u3002\uff08\u7ec6\u62c9\uff09 \u7942\u7684\u8363\u8000\u906e\u853d\u8bf8\u5929\uff0c \u9882\u8d5e\u7942\u7684\u58f0\u97f3\u54cd\u5f7b\u5927\u5730\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7942\u7684\u8363\u5149\u72b9\u5982\u592a\u9633\u7684\u5149\u8f89\uff0c \u7942\u7684\u624b\u5c04\u51fa\u5149\u8292\uff0c \u5176\u4e2d\u8574\u85cf\u7740\u5927\u80fd\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u761f\u75ab\u884c\u5728\u7942\u524d\u9762\uff0c \u707e\u75c5\u8ddf\u5728\u7942\u811a\u540e\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u7942\u505c\u4e0b\uff0c\u5927\u5730\u5c31\u9707\u52a8\uff1b \u7942\u89c2\u770b\uff0c\u4e07\u6c11\u5c31\u6218\u6817\u3002 \u53e4\u8001\u7684\u5c71\u5cb3\u5d29\u88c2\uff0c \u8fdc\u53e4\u7684\u4e18\u9675\u584c\u9677\uff0c \u4f46\u7942\u7684\u4f5c\u4e3a\u6c38\u8fdc\u4e0d\u53d8\u3002"
