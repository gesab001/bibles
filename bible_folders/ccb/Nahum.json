    "book_name": "Nahum",
    "book_nr": "34",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4ee5\u4e0b\u662f\u5173\u4e8e\u5c3c\u5c3c\u5fae\u7684\u9884\u8a00\uff0c\u662f\u4f0a\u52d2\u6b4c\u65af\u4eba\u90a3\u9e3f\u7684\u542f\u793a\u4e66\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u8036\u548c\u534e\u5411\u5c3c\u5c3c\u5fae\u53d1\u6012 - \u8036\u548c\u534e\u662f\u75db\u6068\u4e0d\u8d1e\u3001\u65bd\u884c\u62a5\u5e94\u7684\u4e0a\u5e1d\u3002 \u8036\u548c\u534e\u65bd\u884c\u62a5\u5e94\uff0c\u5145\u6ee1\u70c8\u6012\u3002 \u8036\u548c\u534e\u62a5\u5e94\u7942\u7684\u4ec7\u654c\uff0c\u5411\u4ed6\u4eec\u53d1\u70c8\u6012\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8036\u548c\u534e\u4e0d\u8f7b\u6613\u53d1\u6012\uff0c\u6709\u4f1f\u5927\u7684\u80fd\u529b\u3002 \u8036\u548c\u534e\u65ad\u4e0d\u4ee5\u6709\u7f6a\u7684\u4e3a\u65e0\u7f6a\u3002 \u7942\u884c\u8d70\u5728\u65cb\u98ce\u548c\u66b4\u98ce\u4e2d\uff0c \u4e91\u5f69\u662f\u7942\u811a\u4e0b\u7684\u5c18\u571f\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7942\u65a5\u8d23\u5927\u6d77\uff0c\u4f7f\u5927\u6d77\u5e72\u6db8\uff1b \u7942\u4f7f\u4e00\u5207\u6cb3\u6d41\u67af\u7aed\u3002 \u5df4\u73ca\u548c\u8fe6\u5bc6\u7684\u8349\u6728\u51cb\u96f6\uff0c \u9ece\u5df4\u5ae9\u7684\u9c9c\u82b1\u8870\u6b8b\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5728\u7942\u9762\u524d\uff0c\u7fa4\u5c71\u9707\u52a8\uff0c\u4e18\u9675\u6d88\u878d\u3002 \u5728\u7942\u9762\u524d\uff0c\u5927\u5730\u9686\u8d77\uff0c \u4e16\u754c\u548c\u4e16\u4e0a\u7684\u4e00\u5207\u90fd\u6218\u6817\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u7942\u53d1\u6012\uff0c\u8c01\u80fd\u7ad9\u7acb\uff1f \u7942\u53d1\u70c8\u6012\uff0c\u8c01\u80fd\u627f\u53d7\uff1f \u7942\u7684\u6124\u6012\u5982\u706b\u7130\u55b7\u6d8c\u800c\u51fa\uff0c \u78d0\u77f3\u5728\u7942\u9762\u524d\u5d29\u88c2\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8036\u548c\u534e\u662f\u826f\u5584\u7684\uff0c \u662f\u4eba\u60a3\u96be\u65f6\u7684\u907f\u96be\u6240\uff1b \u7942\u770b\u987e\u90a3\u4e9b\u4fe1\u9760\u7942\u7684\u4eba\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u7942\u5fc5\u7528\u6ed4\u6ed4\u6d2a\u6c34\u706d\u7edd\u7942\u7684\u654c\u4eba\uff0c \u628a\u4ed6\u4eec\u9a71\u9010\u5230\u9ed1\u6697\u4e2d\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5c3c\u5c3c\u5fae\u4eba\u554a\uff0c \u4f60\u4eec\u4e3a\u4f55\u56fe\u8c0b\u62b5\u6321\u8036\u548c\u534e\uff1f \u7942\u8981\u5f7b\u5e95\u6bc1\u706d\u4f60\u4eec\uff0c \u65e0\u9700\u51fb\u6253\u4e24\u6b21\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4f60\u4eec\u50cf\u7ea0\u7ed3\u7684\u8346\u68d8\uff0c \u53c8\u50cf\u9169\u914a\u5927\u9189\u7684\u4eba\uff0c \u8981\u5982\u5e72\u79f8\u88ab\u706b\u70e7\u5c3d\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4f60\u4eec\u5f53\u4e2d\u6709\u4e00\u4e2a\u4eba\uff0c \u4e00\u4e2a\u90aa\u6076\u7684\u9634\u8c0b\u8005\uff0c \u56fe\u8c0b\u62b5\u6321\u8036\u548c\u534e\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a \u201c\u5c3d\u7ba1\u5c3c\u5c3c\u5fae\u52bf\u529b\u5f3a\u5927\u3001\u4eba\u53e3\u4f17\u591a\uff0c \u4f46\u5fc5\u88ab\u94f2\u9664\uff0c\u5316\u4e3a\u4e4c\u6709\u3002 \u6211\u7684\u5b50\u6c11\u554a\uff0c\u6211\u4f7f\u4f60\u4eec\u53d7\u4e86\u82e6\uff0c \u4f46\u5fc5\u4e0d\u518d\u4f7f\u4f60\u4eec\u53d7\u82e6\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u73b0\u5728\u6211\u8981\u6253\u788e\u4ed6\u4eec\u5957\u5728\u4f60\u4eec\u9888\u9879\u4e0a\u7684\u8f6d\uff0c \u677e\u5f00\u4f60\u4eec\u8eab\u4e0a\u7684\u9501\u94fe\u3002\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5c3c\u5c3c\u5fae\u554a\uff0c \u8036\u548c\u534e\u5df2\u53d1\u51fa\u6709\u5173\u4f60\u7684\u547d\u4ee4\uff1a \u201c\u4f60\u7684\u540d\u4e0d\u4f1a\u4f20\u4e8e\u540e\u4e16\u3002 \u6211\u8981\u6467\u6bc1\u4f60\u795e\u5e99\u4e2d\u96d5\u523b\u548c\u94f8\u9020\u7684\u5076\u50cf\u3002 \u6211\u8981\u4e3a\u4f60\u6398\u597d\u575f\u5893\uff0c \u56e0\u4e3a\u4f60\u6beb\u65e0\u7528\u5904\u3002\u201d"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u770b\u554a\uff0c \u4f20\u559c\u8baf\u3001\u62a5\u5e73\u5b89\u7684\u4eba\u7a7f\u5c71\u8d8a\u5cad\u800c\u6765\u3002 \u72b9\u5927\u554a\uff0c \u5e86\u795d\u4f60\u7684\u8282\u671f\uff0c \u8fd8\u4f60\u8bb8\u7684\u613f\u5427\uff01 \u90aa\u6076\u4e4b\u4eba\u5c06\u4e0d\u518d\u4fb5\u6270\u4f60\uff0c \u4ed6\u4eec\u90fd\u5c06\u88ab\u5f7b\u5e95\u6bc1\u706d\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5c3c\u5c3c\u5fae\u7684\u8986\u706d - \u5c3c\u5c3c\u5fae\u554a\uff0c \u653b\u51fb\u8005\u5df2\u7ecf\u5411\u4f60\u6251\u6765\u3002 \u4f60\u8981\u8fdb\u9a7b\u5821\u5792\uff0c \u628a\u5b88\u9053\u8def\uff0c \u96c6\u4e2d\u5168\u529b\uff0c \u51c6\u5907\u4e0a\u9635\uff01"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u867d\u7136\u63b3\u63a0\u8005\u66fe\u63b3\u63a0\u96c5\u5404\uff0c \u63b3\u63a0\u4ee5\u8272\u5217\uff0c \u6bc1\u574f\u4ed6\u4eec\u7684\u8461\u8404\u6811\u679d\uff0c \u4f46\u8036\u548c\u534e\u5fc5\u6062\u590d\u96c5\u5404\u7684\u8363\u8000\uff0c \u6062\u590d\u4ee5\u8272\u5217\u7684\u8363\u8000\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4ed6\u7684\u52c7\u58eb\u624b\u6301\u7ea2\u8272\u76fe\u724c\uff0c \u4ed6\u7684\u58eb\u5175\u8eab\u7a7f\u6731\u7ea2\u6218\u8863\u3002 \u6574\u88c5\u5f85\u53d1\u4e4b\u65e5\uff0c \u6218\u8f66\u94c1\u5149\u95ea\u95ea\uff0c \u677e\u6728\u77db\u67aa\u5728\u7a7a\u4e2d\u6325\u821e\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6218\u8f66\u5728\u8857\u9053\u4e0a\u72c2\u5954\uff0c \u5728\u5e7f\u573a\u4e0a\u6a2a\u51b2\u76f4\u649e\uff0c \u4eae\u5982\u706b\u70ac\uff0c\u75be\u4f3c\u95ea\u7535\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4e9a\u8ff0\u738b\u53ec\u96c6\u5c06\u9886\uff0c \u4ed6\u4eec\u4e00\u8def\u8dcc\u8dcc\u649e\u649e\uff0c \u5954\u4e0a\u57ce\u5899\uff0c\u67b6\u8d77\u9632\u5fa1\u76fe\u724c\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6cb3\u95f8\u5927\u5f00\uff0c\u738b\u5bab\u574d\u584c\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u738b\u540e\u8d64\u8eab\u88ab\u63b3\uff1b \u5bab\u5973\u6376\u80f8\u60b2\u6ce3\uff0c \u50cf\u9e3d\u5b50\u4e00\u6837\u54c0\u9e23\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5c3c\u5c3c\u5fae\u57ce\u81ea\u53e4\u4ee5\u6765\u5c31\u50cf\u805a\u6c34\u7684\u6c60\u5b50\uff0c \u5982\u4eca\u5c45\u6c11\u5374\u50cf\u6cc4\u6f0f\u7684\u6c60\u6c34\u56db\u6563\u5954\u9003\u3002 \u867d\u6709\u4eba\u558a\uff1a\u201c\u7ad9\u4f4f\uff01\u7ad9\u4f4f\uff01\u201d \u5374\u65e0\u4eba\u56de\u5934\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u62a2\u94f6\u5b50\u5427\uff01\u62a2\u91d1\u5b50\u5427\uff01 \u57ce\u91cc\u6709\u65e0\u5c3d\u7684\u8d22\u5bcc\u548c\u6570\u4e0d\u6e05\u7684\u5b9d\u7269\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5c3c\u5c3c\u5fae\u88ab\u6d17\u52ab\u4e00\u7a7a\uff01 \u88ab\u6d17\u52ab\u4e00\u7a7a\uff01 \u4e00\u7247\u8352\u51c9\uff01 \u4eba\u4eec\u80c6\u6218\u5fc3\u60ca\uff0c\u53cc\u819d\u54c6\u55e6\uff0c \u6d51\u8eab\u53d1\u6296\uff0c\u9762\u8272\u82cd\u767d\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5982\u4eca\u90a3\u72ee\u5b50\u7684\u6d1e\u7a74\uff0c \u90a3\u5582\u517b\u58ee\u72ee\u7684\u5730\u65b9\u5728\u54ea\u91cc\u5462\uff1f \u4ece\u524d\uff0c\u96c4\u72ee\u3001\u6bcd\u72ee\u548c\u5e7c\u72ee\u66fe\u5728\u90a3\u91cc\u51fa\u5165\uff0c \u65e0\u4eba\u60ca\u6270\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u96c4\u72ee\u4e3a\u5e7c\u72ee\u6495\u788e\u5145\u8db3\u7684\u98df\u7269\uff0c \u4e3a\u6bcd\u72ee\u6390\u6b7b\u730e\u7269\uff0c \u6d1e\u7a74\u91cc\u5806\u6ee1\u730e\u7269\uff0c \u5806\u6ee1\u6495\u788e\u7684\u8089\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a \u201c\u6211\u8981\u4e0e\u4f60\u4e3a\u654c\uff0c \u70e7\u6bc1\u4f60\u7684\u6218\u8f66\uff0c \u4f7f\u5200\u5251\u541e\u566c\u4f60\u7684\u58ee\u72ee\uff1b \u6211\u8981\u4f7f\u4f60\u65e0\u6cd5\u518d\u63b3\u63a0\u4ed6\u56fd\uff0c \u4f60\u4f7f\u8005\u7684\u58f0\u97f3\u5c06\u4ece\u6b64\u6d88\u901d\u3002\u201d"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5c3c\u5c3c\u5fae\u7684\u707e\u7978 - \u5145\u6ee1\u8840\u8165\u7684\u5c3c\u5c3c\u5fae\u6709\u7978\u4e86\uff01 \u57ce\u4e2d\u5145\u6ee1\u8be1\u8bc8\u548c\u66b4\u529b\uff0c \u5bb3\u4eba\u7684\u4e8b\u4ece\u672a\u6b62\u606f\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u97ad\u58f0\u556a\u556a\uff0c\u8f66\u8f6e\u9686\u9686\uff0c \u6218\u9a6c\u5954\u817e\uff0c\u6218\u8f66\u75be\u9a70\uff0c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u9a91\u58eb\u51b2\u950b\uff0c\u5200\u5149\u95ea\u95ea\uff0c\u77db\u67aa\u751f\u8f89\uff1b \u88ab\u6740\u8005\u4f17\u591a\uff0c\u5c38\u9aa8\u6210\u5806\uff0c \u4eba\u4eec\u88ab\u904d\u5730\u7684\u5c38\u4f53\u7eca\u5012\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u8fd9\u5168\u662f\u56e0\u4e3a\u5c3c\u5c3c\u5fae\u50cf\u5996\u5a9a\u7684\u5993\u5973\u5927\u8086\u6deb\u4e71\uff0c \u60ef\u884c\u90aa\u672f\uff0c \u4ee5\u6deb\u884c\u8bf1\u60d1\u5217\u56fd\uff0c \u9760\u90aa\u672f\u6b3a\u9a97\u5404\u65cf\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a \u201c\u6211\u8981\u4e0e\u4f60\u4e3a\u654c\u3002 \u6211\u8981\u628a\u4f60\u7684\u8863\u88d9\u6380\u5230\u4f60\u8138\u4e0a\uff0c \u4f7f\u5217\u56fd\u770b\u89c1\u4f60\u7684\u88f8\u4f53\uff0c \u4f7f\u5217\u90a6\u770b\u89c1\u4f60\u7684\u7f9e\u803b\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u8981\u628a\u53ef\u618e\u4e4b\u7269\u629b\u5728\u4f60\u8eab\u4e0a\uff0c \u4fae\u8fb1\u4f60\uff0c \u8ba9\u4f17\u4eba\u89c2\u770b\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u51e1\u770b\u89c1\u4f60\u7684\u90fd\u8981\u9003\u907f\uff0c\u8bf4\uff0c \u2018\u5c3c\u5c3c\u5fae\u6bc1\u706d\u4e86\uff01 \u8c01\u4f1a\u4e3a\u5979\u60b2\u4f24\u5462\uff1f\u2019 \u6211\u5230\u54ea\u91cc\u53bb\u5bfb\u627e\u5b89\u6170\u4f60\u7684\u4eba\u5462\uff1f\u201d"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u80fd\u5f3a\u8fc7\u5e95\u6bd4\u65af\u5417\uff1f \u5e95\u6bd4\u65af\u4f4d\u4e8e\u5c3c\u7f57\u6cb3\u4e0a\uff0c \u56db\u9762\u73af\u6c34\uff0c \u6c34\u662f\u5979\u7684\u5c4f\u969c\uff0c \u53c8\u662f\u5979\u7684\u57ce\u5899\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u53e4\u5b9e\u548c\u57c3\u53ca\u662f\u5979\u65e0\u7a77\u7684\u529b\u91cf\uff0c \u5f17\u4eba\u548c\u5229\u6bd4\u4e9a\u4eba\u662f\u5979\u7684\u5e2e\u624b\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u53ef\u662f\uff0c\u5979\u5374\u88ab\u63b3\u5230\u8fdc\u65b9\uff0c \u5979\u7684\u5a74\u5b69\u88ab\u6454\u6b7b\u5728\u5404\u4e2a\u8857\u5934\u3002 \u5979\u7684\u5b98\u957f\u88ab\u62bd\u7b7e\u74dc\u5206\uff0c \u5979\u6240\u6709\u7684\u9996\u9886\u90fd\u88ab\u9501\u94fe\u9501\u7740\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5c3c\u5c3c\u5fae\u554a\uff0c \u4f60\u4e5f\u5fc5\u50cf\u9189\u6c49\u4e00\u6837\uff0c \u4f60\u5fc5\u5bfb\u627e\u85cf\u8eab\u4e4b\u5904\u8eb2\u907f\u654c\u4eba\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f60\u6240\u6709\u7684\u5821\u5792\u90fd\u50cf\u65e0\u82b1\u679c\u6811\u4e0a\u7684\u521d\u719f\u679c\u5b50\uff0c \u4e00\u6447\u6811\uff0c\u679c\u5b50\u5c31\u6389\u8fdb\u5403\u7684\u4eba\u53e3\u4e2d\u3002"
                },
                "13": {
                    "verse_nr": "13",
