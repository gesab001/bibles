{
    "version": "ccb",
    "book_name": "2 John",
    "book_nr": "63",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u8fd9\u505a\u957f\u8001\u7684\u5199\u4fe1\u7ed9\u8499\u62e3\u9009\u7684\u5973\u58eb\u548c\u4f60\u7684\u513f\u5973\uff0c\u5c31\u662f\u6211\u672c\u7740\u771f\u7406\u6240\u7231\u7684\u3002\u4f60\u4eec\u4e0d\u5355\u662f\u6211\u6240\u7231\u7684\uff0c\u4e5f\u662f\u4e00\u5207\u8ba4\u8bc6\u771f\u7406\u7684\u4eba\u6240\u7231\u7684\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u4eec\u7231\u4f60\u4eec\uff0c\u56e0\u4e3a\u771f\u7406\u5728\u6211\u4eec\u91cc\u9762\uff0c\u5e76\u4e14\u6c38\u8fdc\u4e0e\u6211\u4eec\u540c\u5728\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4ece\u7236\u4e0a\u5e1d\u548c\u7942\u513f\u5b50\u8036\u7a23\u57fa\u7763\u800c\u6765\u7684\u6069\u5178\u3001\u601c\u60af\u548c\u5e73\u5b89\u4e5f\u5fc5\u5728\u771f\u7406\u548c\u7231\u4e2d\u4e0e\u6211\u4eec\u540c\u5728\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5f7c\u6b64\u76f8\u7231 - \u4f60\u7684\u513f\u5973\u5f53\u4e2d\u6709\u4eba\u6309\u7167\u6211\u4eec\u4ece\u7236\u9886\u53d7\u7684\u547d\u4ee4\u9075\u884c\u771f\u7406\uff0c\u6211\u77e5\u9053\u540e\u975e\u5e38\u6b23\u6170\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5973\u58eb\u554a\uff0c\u6211\u73b0\u5728\u529d\u4f60\uff0c\u6211\u4eec\u5927\u5bb6\u8981\u5f7c\u6b64\u76f8\u7231\u3002\u8fd9\u4e0d\u662f\u65b0\u547d\u4ee4\uff0c\u800c\u662f\u6211\u4eec\u4ece\u8d77\u521d\u5c31\u5df2\u7ecf\u9886\u53d7\u7684\u547d\u4ee4\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u7231\u5c31\u662f\u9075\u884c\u4e0a\u5e1d\u7684\u547d\u4ee4\uff0c\u8fd9\u662f\u4f60\u4eec\u4ece\u8d77\u521d\u5c31\u542c\u8fc7\u53c8\u5f53\u9075\u884c\u7684\u547d\u4ee4\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u63d0\u9632\u9a97\u5b50 - \u56e0\u4e3a\u73b0\u5728\u4e16\u4e0a\u6709\u8bb8\u591a\u9a97\u5b50\u51fa\u6765\u8ff7\u60d1\u4eba\uff0c\u4ed6\u4eec\u5426\u8ba4\u8036\u7a23\u57fa\u7763\u66fe\u964d\u4e16\u4e3a\u4eba\u3002\u8fd9\u6837\u7684\u4eba\u662f\u9a97\u5b50\uff0c\u662f\u654c\u57fa\u7763\u8005\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u4eec\u8981\u8c28\u614e\uff0c\u597d\u5f97\u5230\u5b8c\u6ee1\u7684\u8d4f\u8d50\uff0c\u514d\u5f97\u6211\u4eec\u524d\u529f\u5c3d\u5f03\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8c01\u504f\u79bb\u57fa\u7763\u7684\u6559\u5bfc\uff0c\u8c01\u5fc3\u4e2d\u5c31\u6ca1\u6709\u4e0a\u5e1d\uff1b\u8c01\u9075\u5b88\u57fa\u7763\u7684\u6559\u5bfc\uff0c\u8c01\u5c31\u6709\u7236\u548c\u5b50\u4e0e\u4ed6\u540c\u5728\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u82e5\u6709\u4eba\u5230\u4f60\u4eec\u90a3\u91cc\u4e0d\u4f20\u57fa\u7763\u7684\u6559\u5bfc\uff0c\u4e0d\u8981\u8bf7\u4ed6\u5230\u5bb6\u91cc\uff0c\u4e5f\u4e0d\u8981\u95ee\u5019\u4ed6\uff0c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u4e3a\u95ee\u5019\u4ed6\u5c31\u662f\u4e0e\u4ed6\u540c\u6d41\u5408\u6c61\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u7ed3\u8bed - \u6211\u8fd8\u6709\u8bb8\u591a\u4e8b\u8981\u544a\u8bc9\u4f60\u4eec\uff0c\u4f46\u4e0d\u60f3\u501f\u52a9\u7eb8\u58a8\uff0c\u800c\u662f\u5e0c\u671b\u6709\u673a\u4f1a\u5230\u4f60\u4eec\u90a3\u91cc\u4e0e\u4f60\u4eec\u5f53\u9762\u7545\u8c08\uff0c\u4f7f\u6211\u4eec\u5145\u6ee1\u559c\u4e50\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u90a3\u8499\u62e3\u9009\u4e4b\u59ca\u59b9\u7684\u513f\u5973\u95ee\u5019\u4f60\u3002"
                }
            }
        }
    }
}