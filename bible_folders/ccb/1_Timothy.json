                    "verse": "\u505a\u5974\u4ec6\u7684\u8981\u5168\u5fc3\u5c0a\u91cd\u81ea\u5df1\u7684\u4e3b\u4eba\uff0c\u514d\u5f97\u4e0a\u5e1d\u7684\u540d\u548c\u6211\u4eec\u7684\u6559\u5bfc\u88ab\u4eba\u4eb5\u6e0e\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u82e5\u4e3b\u4eba\u662f\u57fa\u7763\u5f92\uff0c\u505a\u5974\u4ec6\u7684\u4e0d\u53ef\u56e0\u4e3a\u540c\u662f\u4fe1\u5f92\u5c31\u4e0d\u5c0a\u656c\u81ea\u5df1\u7684\u4e3b\u4eba\uff0c\u53cd\u800c\u8981\u66f4\u597d\u5730\u670d\u4f8d\u4ed6\uff0c\u56e0\u4e3a\u8fd9\u4e9b\u63a5\u53d7\u670d\u4f8d\u7684\u4eba\u662f\u4fe1\u5f92\uff0c\u662f\u4e3b\u6240\u7231\u7684\u3002\u4f60\u8981\u6559\u5bfc\u8fd9\u4e9b\u4e8b\u5e76\u52c9\u52b1\u4f17\u4eba\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u5f02\u7aef\u4e0e\u8d2a\u8d22 - \u5982\u679c\u6709\u4eba\u4f20\u7684\u6559\u4e49\u4e0d\u7b26\u5408\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7eaf\u6b63\u7684\u9053\u548c\u656c\u8654\u7684\u6559\u5bfc\uff0c"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4ed6\u5c31\u662f\u72c2\u5984\u81ea\u5927\u3001\u4e00\u65e0\u6240\u77e5\u3002\u8fd9\u79cd\u4eba\u4e13\u597d\u95ee\u96be\uff0c\u4e89\u8fa9\u5b57\u53e5\uff0c\u7ed3\u679c\u5f15\u8d77\u5ac9\u5992\u3001\u7eb7\u4e89\u3001\u6bc1\u8c24\u548c\u731c\u5fcc\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f7f\u90a3\u4e9b\u601d\u60f3\u8d25\u574f\u3001\u5931\u53bb\u771f\u7406\u7684\u4eba\u4e89\u8bba\u4e0d\u4f11\uff0c\u751a\u81f3\u628a\u656c\u8654\u5f53\u4f5c\u751f\u8d22\u4e4b\u9053\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u5176\u5b9e\u656c\u8654\u548c\u77e5\u8db3\u624d\u662f\u771f\u6b63\u7684\u8d22\u5bcc\uff0c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u56e0\u4e3a\u6211\u4eec\u7a7a\u624b\u6765\u5230\u4e16\u4e0a\uff0c\u4e5f\u8981\u7a7a\u624b\u79bb\u5f00\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u53ea\u8981\u6709\u5403\u6709\u7a7f\uff0c\u5c31\u8be5\u77e5\u8db3\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u90a3\u4e9b\u6e34\u671b\u53d1\u8d22\u7684\u4eba\u5374\u9677\u5165\u8bf1\u60d1\u3001\u7f51\u7f57\u548c\u8bb8\u591a\u611a\u8822\u6709\u5bb3\u7684\u6b32\u671b\u4e2d\uff0c\u4ee5\u81f4\u843d\u5165\u8d25\u574f\u548c\u706d\u4ea1\u4e2d\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u8d2a\u8d22\u662f\u4e07\u6076\u4e4b\u6839\uff0c\u6709\u4eba\u56e0\u4e3a\u8d2a\u8d22\u5c31\u79bb\u5f03\u4e86\u4fe1\u4ef0\uff0c\u7ed3\u679c\u9971\u5c1d\u79cd\u79cd\u9525\u5fc3\u822c\u7684\u75db\u82e6\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4fdd\u7f57\u5bf9\u63d0\u6469\u592a\u7684\u5631\u5490 - \u4f46\u4f60\u8fd9\u5c5e\u4e0a\u5e1d\u7684\u4eba\u8981\u8fdc\u907f\u8fd9\u4e9b\u4e8b\uff0c\u8981\u8ffd\u6c42\u516c\u4e49\u3001\u656c\u8654\u3001\u4fe1\u5fc3\u3001\u7231\u5fc3\u3001\u5fcd\u8010\u548c\u6e29\u67d4\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8981\u4e3a\u771f\u9053\u6253\u7f8e\u597d\u7684\u4ed7\uff0c\u8981\u6301\u5b9a\u6c38\u751f\u3002\u4f60\u662f\u4e3a\u6c38\u751f\u8499\u53ec\u7684\uff0c\u5e76\u5728\u8bb8\u591a\u89c1\u8bc1\u4eba\u9762\u524d\u505a\u8fc7\u7f8e\u597d\u7684\u89c1\u8bc1\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u5728\u8d50\u4e07\u7269\u751f\u547d\u7684\u4e0a\u5e1d\u9762\u524d\uff0c\u4e5f\u5728\u5411\u672c\u4e22\u00b7\u5f7c\u62c9\u591a\u505a\u8fc7\u7f8e\u597d\u89c1\u8bc1\u7684\u57fa\u7763\u8036\u7a23\u9762\u524d\uff0c\u6211\u5631\u5490\u4f60\uff1a"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8981\u9075\u5b88\u8fd9\u547d\u4ee4\uff0c\u505a\u7eaf\u6d01\u65e0\u7455\u3001\u65e0\u53ef\u6307\u8d23\u7684\u4eba\uff0c\u4e00\u76f4\u5230\u6211\u4eec\u7684\u4e3b\u8036\u7a23\u57fa\u7763\u518d\u6765\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u5230\u4e86\u65f6\u5019\uff0c\u914d\u5f97\u79f0\u9882\u3001\u72ec\u6709\u6743\u80fd\u7684\u4e07\u738b\u4e4b\u738b\u3001\u4e07\u4e3b\u4e4b\u4e3b\u8981\u4f7f\u57fa\u7763\u663e\u73b0\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4e0a\u5e1d\u662f\u72ec\u4e00\u4e0d\u673d\u7684\uff0c\u4f4f\u5728\u4eba\u4e0d\u80fd\u63a5\u8fd1\u7684\u5149\u4e2d\uff0c\u4ece\u6765\u6ca1\u6709\u4eba\u89c1\u8fc7\u7942\uff0c\u4e5f\u6ca1\u6709\u4eba\u80fd\u591f\u770b\u89c1\u7942\u3002\u613f\u5c0a\u8d35\u548c\u6c38\u8fdc\u7684\u6743\u67c4\u90fd\u5f52\u7ed9\u7942\u3002\u963f\u4eec\uff01"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4f60\u8981\u529d\u8beb\u90a3\u4e9b\u4eca\u4e16\u5bcc\u8db3\u7684\u4eba\u4e0d\u8981\u9ad8\u50b2\uff0c\u4e5f\u4e0d\u8981\u5180\u671b\u4e8e\u9760\u4e0d\u4f4f\u7684\u94b1\u8d22\uff0c\u8981\u4fe1\u9760\u5c06\u4e07\u7269\u539a\u8d50\u7ed9\u6211\u4eec\u4eab\u7528\u7684\u4e0a\u5e1d\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u4f60\u8981\u529d\u4ed6\u4eec\u591a\u591a\u884c\u5584\u3001\u6177\u6168\u65bd\u820d\u3001\u4e50\u610f\u52a9\u4eba\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u8fd9\u6837\uff0c\u4ed6\u4eec\u53ef\u4ee5\u4e3a\u81ea\u5df1\u7684\u5c06\u6765\u5960\u5b9a\u7f8e\u597d\u7684\u57fa\u7840\uff0c\u4ee5\u4fbf\u6301\u5b88\u771f\u6b63\u7684\u751f\u547d\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u63d0\u6469\u592a\u554a\uff0c\u4f60\u8981\u6301\u5b88\u6240\u6258\u4ed8\u7ed9\u4f60\u7684\uff0c\u907f\u5f00\u4e16\u4fd7\u7684\u7a7a\u8c08\u548c\u90a3\u4e9b\u8fdd\u80cc\u771f\u7406\u3001\u4f3c\u662f\u800c\u975e\u7684\u5b66\u95ee\u3002"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u6709\u4e9b\u81ea\u79f0\u6709\u8fd9\u79cd\u5b66\u95ee\u7684\u4eba\u5df2\u7ecf\u504f\u79bb\u4e86\u4fe1\u4ef0\u3002 \u613f\u6069\u5178\u4e0e\u4f60\u4eec\u540c\u5728\uff01"
                }
            }
        }
    }
}