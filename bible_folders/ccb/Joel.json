    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4ee5\u4e0b\u662f\u8036\u548c\u534e\u5bf9\u6bd7\u571f\u73e5\u7684\u513f\u5b50\u7ea6\u73e5\u6240\u8bf4\u7684\u8bdd\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4e25\u91cd\u7684\u8757\u707e - \u957f\u8001\u4eec\u554a\uff0c\u8bf7\u542c\u6211\u8bf4\uff01 \u5730\u4e0a\u7684\u6240\u6709\u5c45\u6c11\u554a\uff0c\u8981\u7559\u5fc3\u542c\uff01 \u5728\u4f60\u4eec\u6216\u4f60\u4eec\u7956\u5148\u7684\u65f6\u4ee3\u53ef\u66fe\u53d1\u751f\u8fc7\u8fd9\u6837\u7684\u4e8b\uff1f"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8981\u628a\u8fd9\u4e8b\u544a\u8bc9\u4f60\u4eec\u7684\u5b50\u5b50\u5b59\u5b59\uff0c \u4f20\u5230\u4e16\u4e16\u4ee3\u4ee3\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u8757\u866b\u5403\u5269\u7684\uff0c\u877b\u5b50\u6765\u5403\uff1b \u877b\u5b50\u5403\u5269\u7684\uff0c\u86b1\u8722\u6765\u5403\uff1b \u86b1\u8722\u5403\u5269\u7684\uff0c\u8682\u86b1\u6765\u5403\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u9189\u6c49\u554a\uff0c\u9192\u8fc7\u6765\u54ed\u6ce3\u5427\uff01 \u8d2a\u676f\u7684\u4eba\u554a\uff0c\u54c0\u53f7\u5427\uff01 \u4f60\u4eec\u518d\u65e0\u7f8e\u9152\u53ef\u996e\u4e86\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6709\u4e00\u7fa4\u51f6\u731b\u7684\u8757\u866b\u4fb5\u72af\u6211\u7684\u56fd\u571f\uff0c \u5b83\u4eec\u4e0d\u8ba1\u5176\u6570\uff0c \u6709\u96c4\u72ee\u7684\u7360\u7259\u548c\u6bcd\u72ee\u7684\u5229\u9f7f\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5b83\u4eec\u6bc1\u574f\u6211\u7684\u8461\u8404\u6811\u548c\u65e0\u82b1\u679c\u6811\uff0c \u5265\u5149\u6811\u76ae\uff0c \u53ea\u7559\u4e0b\u5149\u79c3\u7684\u6811\u679d\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u7684\u5b50\u6c11\u554a\uff0c\u4f60\u4eec\u8981\u50cf\u6b7b\u4e86\u672a\u5a5a\u592b\u7684\u5c11\u5973\uff0c \u7a7f\u4e0a\u4e27\u670d\u75db\u54ed\u54c0\u53f7\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8036\u548c\u534e\u6bbf\u4e2d\u7684\u7d20\u796d\u548c\u5960\u796d\u90fd\u505c\u6b62\u4e86\uff0c \u4e8b\u5949\u8036\u548c\u534e\u7684\u796d\u53f8\u90fd\u54c0\u4f24\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u7530\u56ed\u8352\u5e9f\uff0c\u904d\u5730\u51c4\u51c9\uff0c \u4e94\u8c37\u5c3d\u6bc1\uff0c \u518d\u4e5f\u6ca1\u6709\u65b0\u9152\u548c\u6cb9\u4e86\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u519c\u592b\u554a\uff0c\u4f60\u4eec\u8981\u5fe7\u4f24\uff01 \u7167\u6599\u8461\u8404\u56ed\u7684\u4eba\u554a\uff0c \u4f60\u4eec\u8981\u54c0\u53f7\uff01 \u56e0\u4e3a\u7530\u95f4\u7684\u5927\u9ea6\u548c\u5c0f\u9ea6\u5168\u6bc1\u4e86\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8461\u8404\u6811\u67af\u6b7b\uff0c \u65e0\u82b1\u679c\u6811\u51cb\u6b8b\uff0c \u77f3\u69b4\u6811\u3001\u68d5\u6811\u3001\u82f9\u679c\u6811\u7b49\u7530\u91ce\u7684\u4e00\u5207\u6811\u6728\u67af\u5e72\u3002 \u4eba\u95f4\u7684\u6b22\u4e50\u6d88\u901d\u4e86\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u796d\u53f8\u554a\uff0c\u7a7f\u7740\u4e27\u670d\u75db\u54ed\u5427\uff1b \u5728\u796d\u575b\u524d\u4e8b\u5949\u7684\u4eba\u554a\uff0c\u8981\u54c0\u53f7\uff1b \u6211\u4e0a\u5e1d\u7684\u4ec6\u4eba\u554a\uff0c\u8981\u62ab\u4e0a\u9ebb\u8863\u8fc7\u591c\uff1b \u56e0\u4e3a\u518d\u65e0\u7d20\u796d\u548c\u5960\u796d\u732e\u5230\u4f60\u4eec\u4e0a\u5e1d\u7684\u6bbf\u4e2d\u4e86\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4f60\u4eec\u8981\u5b9a\u4e00\u4e2a\u7981\u98df\u7684\u65e5\u5b50\uff0c \u4e3e\u884c\u5e84\u4e25\u7684\u805a\u4f1a\uff0c \u53ec\u96c6\u5168\u56fd\u7684\u957f\u8001\u548c\u4eba\u6c11\uff0c \u5230\u4f60\u4eec\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u7684\u6bbf\u4e2d\uff0c \u5411\u8036\u548c\u534e\u547c\u6c42\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u592a\u53ef\u6015\u4e86\uff01\u8036\u548c\u534e\u7684\u65e5\u5b50\u8fd1\u4e86\uff0c \u90a3\u662f\u5168\u80fd\u8005\u65bd\u884c\u6bc1\u706d\u7684\u65e5\u5b50\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u6211\u4eec\u7684\u7cae\u98df\u65ad\u7edd\u4e86\uff0c \u6211\u4eec\u4e0a\u5e1d\u6bbf\u4e2d\u7684\u6b22\u4e50\u6d88\u5931\u4e86\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u79cd\u5b50\u5728\u571f\u91cc\u8150\u70c2\uff0c \u4e94\u8c37\u5e72\u6b7b\uff0c\u4ed3\u5e93\u8352\u5e9f\uff0c \u8c37\u4ed3\u88ab\u62c6\u6bc1\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u7272\u755c\u54c0\u9e23\uff0c \u725b\u7fa4\u56e0\u627e\u4e0d\u5230\u8349\u573a\u800c\u56db\u5904\u4e71\u7a9c\uff0c \u7f8a\u7fa4\u4e5f\u53d7\u82e6\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u8036\u548c\u534e\u554a\uff0c\u6211\u5411\u4f60\u547c\u6c42\uff0c \u56e0\u4e3a\u706b\u7130\u541e\u706d\u4e86\u65f7\u91ce\u7684\u8349\u573a\uff0c \u70c8\u706b\u70e7\u5149\u4e86\u7530\u91ce\u7684\u6811\u6728\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u7530\u91ce\u7684\u8d70\u517d\u5411\u4f60\u547c\u6c42\uff0c \u56e0\u4e3a\u6eaa\u6c34\u5df2\u7ecf\u67af\u5e72\uff0c \u706b\u4e5f\u70e7\u5c3d\u4e86\u65f7\u91ce\u7684\u8349\u573a\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8036\u548c\u534e\u5ba1\u5224\u7684\u65e5\u5b50 - \u4f60\u4eec\u8981\u5728\u9521\u5b89\u5439\u54cd\u53f7\u89d2\uff0c \u5728\u6211\u7684\u5723\u5c71\u4e0a\u53d1\u51fa\u8b66\u8baf\uff0c \u5730\u4e0a\u7684\u5c45\u6c11\u90fd\u8981\u6218\u6296\uff0c \u56e0\u4e3a\u8036\u548c\u534e\u7684\u65e5\u5b50\u5c06\u8981\u6765\u4e34\uff0c \u5df2\u7ecf\u8fd1\u4e86\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u90a3\u5c06\u662f\u4e4c\u4e91\u5bc6\u5e03\u3001 \u5929\u660f\u5730\u6697\u7684\u65e5\u5b50\u3002 \u6765\u4e86\u4e00\u652f\u7a7a\u524d\u7edd\u540e\u3001\u5f3a\u76db\u65e0\u6bd4\u7684\u8757\u866b\u5927\u519b\uff0c \u5b83\u4eec\u50cf\u66d9\u5149\u4e00\u6837\u5e03\u6ee1\u5c71\u5934\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u5b83\u4eec\u7684\u524d\u961f\u5982\u706b\u71ce\u539f\uff0c \u540e\u961f\u5982\u718a\u718a\u70c8\u7130\u3002 \u5b83\u4eec\u672a\u5230\u4ee5\u524d\uff0c \u5927\u5730\u7f8e\u5f97\u50cf\u4f0a\u7538\u56ed\uff1b \u5b83\u4eec\u8fc7\u53bb\u4e4b\u540e\uff0c \u5927\u5730\u53d8\u6210\u4e00\u7247\u8352\u573a\u3002 \u5730\u4e0a\u7684\u4e00\u5207\u90fd\u9003\u4e0d\u8fc7\u8fd9\u4e00\u573a\u6d69\u52ab\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5b83\u4eec\u7684\u5f62\u72b6\u50cf\u9a6c\uff0c \u5feb\u901f\u5954\u9a70\u5982\u6218\u9a6c\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5728\u5c71\u5cad\u4e0a\u8df3\u8dc3\u5954\u817e\uff0c \u5982\u9686\u9686\u6218\u8f66\uff0c \u597d\u50cf\u70c8\u706b\u541e\u566c\u5e72\u8349\u7684\u58f0\u97f3\uff0c \u53c8\u5982\u5927\u519b\u6446\u5f00\u9635\u5f0f\u51c6\u5907\u6253\u4ed7\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u5b83\u4eec\u4e00\u6765\uff0c\u767e\u59d3\u5c31\u60ca\u614c\u5931\u63aa\uff0c \u9762\u5982\u6b7b\u7070\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5b83\u4eec\u5feb\u901f\u5954\u8dd1\u50cf\u52c7\u58eb\uff0c \u6500\u767b\u57ce\u5899\u5982\u6218\u58eb\uff0c \u6709\u6761\u4e0d\u7d0a\u5730\u524d\u8fdb\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u961f\u4f0d\u6574\u9f50\uff0c\u5404\u884c\u5176\u9053\uff0c \u52bf\u5982\u7834\u7af9\uff0c\u65e0\u575a\u4e0d\u6467\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5b83\u4eec\u51b2\u5411\u57ce\u9091\uff0c\u8e7f\u4e0a\u57ce\u5899\uff0c \u722c\u8fdb\u623f\u5c4b\uff0c \u5982\u76d7\u8d3c\u822c\u7834\u7a97\u800c\u5165\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6240\u5230\u4e4b\u5904\uff0c \u5929\u6447\u5730\u52a8\uff0c \u65e5\u6708\u660f\u6697\uff0c \u661f\u8fb0\u65e0\u5149\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u8036\u548c\u534e\u5411\u7942\u7684\u519b\u961f\u53d1\u53f7\u65bd\u4ee4\uff0c \u7942\u7684\u961f\u4f0d\u4e0d\u8ba1\u5176\u6570\uff0c \u6267\u884c\u7942\u547d\u4ee4\u7684\u662f\u4e00\u652f\u52b2\u65c5\u3002 \u8036\u548c\u534e\u7684\u65e5\u5b50\u4f1f\u5927\u800c\u53ef\u754f\uff0c \u8c01\u80fd\u627f\u53d7\u5f97\u4f4f\u5462\uff1f"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u547c\u5401\u4eba\u6c11\u6094\u6539 - \u8036\u548c\u534e\u8bf4\uff1a \u201c\u73b0\u5728\uff0c\u4f60\u4eec\u8981\u7981\u98df\u3001\u54ed\u6ce3\u3001\u54c0\u53f7\uff0c \u5168\u5fc3\u56de\u8f6c\u5f52\u5411\u6211\u3002\u201d"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u4eec\u8981\u6495\u5fc3\u822c\u5730\u6094\u6539\uff0c \u800c\u4e0d\u662f\u6495\u88c2\u8863\u670d\u3002 \u5f52\u5411\u4f60\u4eec\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u5427\uff0c \u56e0\u4e3a\u7942\u6709\u6069\u5178\u548c\u601c\u60af\uff0c \u4e0d\u8f7b\u6613\u53d1\u6012\uff0c \u6709\u65e0\u9650\u7684\u6148\u7231\uff0c \u4e0d\u5fcd\u5fc3\u964d\u707e\u7978\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8c01\u77e5\u9053\u5462\uff1f \u6216\u8bb8\u7942\u8f6c\u5ff5\u5fc3\u751f\u601c\u60af\uff0c \u7ed9\u4f60\u4eec\u7559\u4e0b\u795d\u798f\uff0c \u4f7f\u4f60\u4eec\u53ef\u4ee5\u518d\u6b21\u5411\u4f60\u4eec\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u732e\u4e0a\u7d20\u796d\u548c\u5960\u796d\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8981\u5728\u9521\u5b89\u5c71\u5439\u54cd\u53f7\u89d2\uff0c \u5ba3\u5e03\u7981\u98df\u7684\u65e5\u5b50\uff0c \u4e3e\u884c\u5e84\u4e25\u7684\u805a\u4f1a\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u8981\u62db\u805a\u8001\u4eba\uff0c\u805a\u96c6\u5b69\u7ae5\uff0c \u5305\u62ec\u5403\u5976\u7684\u5a74\u513f\uff1b \u8981\u5429\u5490\u65b0\u90ce\u51fa\u6d1e\u623f\u3001 \u65b0\u5a18\u51fa\u5185\u5ba4\uff1b \u8981\u53ec\u96c6\u5168\u4f53\u4eba\u6c11\uff0c \u8ba9\u4ed6\u4eec\u6d01\u51c0\u81ea\u5df1\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u8ba9\u4e8b\u5949\u8036\u548c\u534e\u7684\u796d\u53f8\u7ad9\u5728\u5723\u6bbf\u95e8\u5eca\u548c\u796d\u575b\u4e2d\u95f4\uff0c \u54ed\u6ce3\u6073\u6c42\u8bf4\uff1a \u201c\u8036\u548c\u534e\u554a\uff0c \u6c42\u4f60\u987e\u60dc\u4f60\u7684\u5b50\u6c11\uff0c \u4e0d\u8981\u8ba9\u5916\u65cf\u4eba\u4fae\u8fb1\u3001 \u8ba5\u7b11\u4f60\u7684\u4ea7\u4e1a\u8bf4\uff0c \u2018\u4f60\u4eec\u7684\u4e0a\u5e1d\u5728\u54ea\u91cc\u5462\uff1f\u2019\u201d"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u518d\u8d50\u795d\u798f - \u8036\u548c\u534e\u4e3a\u81ea\u5df1\u7684\u571f\u5730\u53d1\u70ed\u5fc3\uff0c \u7942\u601c\u60af\u81ea\u5df1\u7684\u5b50\u6c11\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u7942\u56de\u5e94\u4ed6\u4eec\u8bf4\uff1a \u201c\u6211\u5fc5\u8d50\u4e94\u8c37\u3001\u65b0\u9152\u548c\u6cb9\u7ed9\u4f60\u4eec\uff0c \u4f7f\u4f60\u4eec\u9971\u8db3\uff0c \u4e0d\u518d\u8ba9\u4f60\u4eec\u53d7\u5217\u56fd\u7684\u7f9e\u8fb1\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u6211\u8981\u8d76\u51fa\u4ece\u5317\u65b9\u6765\u7684\u519b\u961f\uff0c \u628a\u4ed6\u4eec\u9a71\u9010\u5230\u4e00\u4e2a\u5e72\u65f1\u8352\u829c\u7684\u5730\u65b9\uff0c \u628a\u4ed6\u4eec\u7684\u9886\u5934\u90e8\u961f\u8d76\u8fdb\u6b7b\u6d77\uff0c \u518d\u628a\u4ed6\u4eec\u7684\u6bbf\u540e\u90e8\u961f\u8d76\u8fdb\u5730\u4e2d\u6d77\uff0c \u90a3\u65f6\u4ed6\u4eec\u5fc5\u81ed\u6c14\u51b2\u5929\u3001 \u8165\u5473\u817e\u7a7a\uff0c \u56e0\u4e3a\u8036\u548c\u534e\u4e3a\u4f60\u4eec\u884c\u4e86\u5927\u4e8b\u3002\u201d"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u5927\u5730\u554a\uff0c\u4e0d\u7528\u60e7\u6015\uff0c \u5012\u8981\u6b22\u559c\u5feb\u4e50\uff1b \u56e0\u4e3a\u8036\u548c\u534e\u884c\u4e86\u5927\u4e8b\u3002"
                },
                "22": {
                    "verse_nr": "22",
