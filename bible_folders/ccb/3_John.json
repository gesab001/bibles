    "version": "ccb",
    "book_name": "3 John",
    "book_nr": "64",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u8fd9\u505a\u957f\u8001\u7684\u5199\u4fe1\u7ed9\u4eb2\u7231\u7684\u8be5\u72b9\uff0c\u5c31\u662f\u6211\u672c\u7740\u771f\u7406\u6240\u7231\u7684\u4eba\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4eb2\u7231\u7684\u5f1f\u5144\uff0c\u8877\u5fc3\u5730\u795d\u4f60\u51e1\u4e8b\u987a\u5229\uff0c\u8eab\u4f53\u548c\u7075\u9b42\u4e00\u6837\u5065\u5eb7\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8be5\u72b9\u7684\u597d\u54c1\u884c - \u6709\u5f1f\u5144\u4eec\u6765\u4e3a\u4f60\u7684\u884c\u4e3a\u505a\u89c1\u8bc1\uff0c\u8bf4\u4f60\u5fe0\u5fc3\u5730\u9075\u884c\u771f\u7406\uff0c\u8fd9\u4f7f\u6211\u975e\u5e38\u6b23\u6170\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6ca1\u6709\u6bd4\u542c\u89c1\u81ea\u5df1\u7684\u5b69\u5b50\u9075\u884c\u771f\u7406\u66f4\u4f7f\u6211\u6b22\u559c\u4e86\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4eb2\u7231\u7684\u5f1f\u5144\uff0c\u4f60\u5e38\u5e38\u5fe0\u5fc3\u5730\u7167\u987e\u7d20\u4e0d\u76f8\u8bc6\u7684\u5f1f\u5144\uff0c"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4ed6\u4eec\u628a\u4f60\u7684\u7231\u5fc3\u544a\u8bc9\u4e86\u6559\u4f1a\u3002\u613f\u4f60\u7167\u7740\u4e0a\u5e1d\u6240\u559c\u60a6\u7684\u65b9\u5f0f\u7ee7\u7eed\u5e2e\u52a9\u4ed6\u4eec\uff0c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u56e0\u4e3a\u4ed6\u4eec\u4e3a\u4e86\u4e3b\u8036\u7a23\u7684\u540d\u56db\u5904\u4f20\u9053\uff0c\u4e0d\u6536\u53d6\u975e\u4fe1\u5f92\u7684\u4efb\u4f55\u4e1c\u897f\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u4eec\u5e94\u8be5\u63a5\u5f85\u8fd9\u6837\u7684\u4eba\uff0c\u597d\u4e0e\u4ed6\u4eec\u4e00\u540c\u4f20\u626c\u771f\u7406\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4e22\u7279\u8153\u7684\u6076\u884c - \u6211\u66fe\u5199\u4fe1\u7ed9\u4f60\u90a3\u91cc\u7684\u6559\u4f1a\uff0c\u4f46\u559c\u6b22\u505a\u9886\u8896\u7684\u4e22\u7279\u8153\u4e0d\u7406\u4f1a\u6211\u4eec\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u56e0\u6b64\uff0c\u6211\u82e5\u53bb\u4f60\u4eec\u90a3\u91cc\uff0c\u4f1a\u63d0\u53ca\u4ed6\u7684\u6240\u4f5c\u6240\u4e3a\uff0c\u5c31\u662f\u4ed6\u600e\u6837\u6076\u8a00\u4e2d\u4f24\u6211\u4eec\u3002\u4e0d\u4ec5\u5982\u6b64\uff0c\u4ed6\u8fd8\u62d2\u7edd\u63a5\u5f85\u5f1f\u5144\uff0c\u5e76\u4e14\u7981\u6b62\u522b\u4eba\u63a5\u5f85\uff0c\u751a\u81f3\u5c06\u63a5\u5f85\u7684\u4eba\u8d76\u51fa\u6559\u4f1a\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6291\u6076\u626c\u5584 - \u4eb2\u7231\u7684\u5f1f\u5144\uff0c\u4e0d\u8981\u6548\u6cd5\u7f6a\u6076\uff0c\u8981\u6548\u6cd5\u7f8e\u5584\u3002\u884c\u5584\u7684\u4eba\u5c5e\u4e8e\u4e0a\u5e1d\uff0c\u884c\u6076\u7684\u4eba\u4e0d\u8ba4\u8bc6\u4e0a\u5e1d\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f4e\u7c73\u4e22\u5f97\u5230\u4e86\u4f17\u4eba\u7684\u597d\u8bc4\uff0c\u771f\u7406\u672c\u8eab\u4e5f\u4e3a\u4ed6\u505a\u89c1\u8bc1\uff0c\u6211\u4eec\u4e5f\u4e3a\u4ed6\u505a\u89c1\u8bc1\u3002\u4f60\u4eec\u77e5\u9053\u6211\u4eec\u7684\u89c1\u8bc1\u662f\u771f\u5b9e\u7684\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u7ed3\u8bed - \u6211\u672c\u6765\u6709\u5f88\u591a\u4e8b\u60f3\u8ddf\u4f60\u8c08\uff0c\u4f46\u4e0d\u60f3\u7528\u7b14\u58a8\u5199\u7ed9\u4f60\uff0c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u53ea\u5e0c\u671b\u53ef\u4ee5\u5feb\u5feb\u5730\u89c1\u5230\u4f60\uff0c\u8ddf\u4f60\u5f53\u9762\u7545\u8c08\u3002"
                }
            }
        }
    }
}