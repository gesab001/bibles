{
    "version": "ccb",
    "book_name": "Jude",
    "book_nr": "65",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u8036\u7a23\u57fa\u7763\u7684\u5974\u4ec6\u3001\u96c5\u5404\u7684\u5144\u5f1f\u72b9\u5927\uff0c\u73b0\u5728\u5199\u4fe1\u7ed9\u8499\u7236\u4e0a\u5e1d\u547c\u53ec\u3001\u7737\u7231\u3001\u88ab\u8036\u7a23\u57fa\u7763\u770b\u987e\u7684\u4eba\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u613f\u4e0a\u5e1d\u4e30\u4e30\u5bcc\u5bcc\u5730\u8d50\u7ed9\u4f60\u4eec\u601c\u60af\u3001\u5e73\u5b89\u548c\u6148\u7231\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u634d\u536b\u771f\u9053 - \u4eb2\u7231\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u4e00\u76f4\u8feb\u5207\u5730\u60f3\u5199\u4fe1\u8ddf\u4f60\u4eec\u8c08\u8c08\u6211\u4eec\u6240\u5171\u4eab\u7684\u6551\u6069\uff0c\u4f46\u73b0\u5728\u6211\u89c9\u5f97\u6709\u5fc5\u8981\u5199\u4fe1\u529d\u52c9\u4f60\u4eec\u7aed\u529b\u62a4\u536b\u4e00\u6b21\u5c31\u5b8c\u6574\u5730\u4ea4\u7ed9\u5723\u5f92\u7684\u771f\u9053\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u56e0\u4e3a\u6709\u4e9b\u4e0d\u656c\u8654\u7684\u4eba\u5077\u5077\u5730\u6df7\u8fdb\u4f60\u4eec\u4e2d\u95f4\uff0c\u4ee5\u4e0a\u5e1d\u7684\u6069\u5178\u4f5c\u501f\u53e3\uff0c\u653e\u7eb5\u60c5\u6b32\uff0c\u5426\u8ba4\u72ec\u4e00\u7684\u4e3b\u5bb0\u2014\u2014\u6211\u4eec\u7684\u4e3b\u8036\u7a23\u57fa\u7763\u3002\u5723\u7ecf\u4e0a\u65e9\u5df2\u8bb0\u8f7d\uff0c\u8fd9\u6837\u7684\u4eba\u5fc5\u53d7\u5230\u5ba1\u5224\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u524d\u8f66\u4e4b\u9274 - \u4ee5\u4e0b\u7684\u4e8b\u60c5\uff0c\u4f60\u4eec\u867d\u7136\u90fd\u77e5\u9053\uff0c\u4f46\u6211\u8fd8\u8981\u518d\u63d0\u9192\u4f60\u4eec\uff1a\u4ece\u524d\u4e0a\u5e1d\u628a\u7942\u7684\u5b50\u6c11\u4ece\u57c3\u53ca\u6551\u51fa\u6765\uff0c\u540e\u6765\u628a\u5176\u4e2d\u4e0d\u4fe1\u7684\u4eba\u706d\u7edd\u4e86\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u81f3\u4e8e\u4e0d\u5b88\u672c\u5206\u3001\u64c5\u79bb\u5c97\u4f4d\u7684\u5929\u4f7f\uff0c\u4e0a\u5e1d\u4e5f\u7528\u9501\u94fe\u5c06\u4ed6\u4eec\u6c38\u8fdc\u56da\u7981\u5728\u5e7d\u6697\u91cc\uff0c\u7b49\u5019\u6700\u540e\u5ba1\u5224\u7684\u5927\u65e5\u5b50\u5230\u6765\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6b64\u5916\uff0c\u6240\u591a\u739b\u3001\u86fe\u6469\u62c9\u53ca\u5176\u9644\u8fd1\u57ce\u9547\u7684\u4eba\u540c\u6837\u56e0\u4e3a\u8352\u6deb\u65e0\u5ea6\u3001\u6c89\u6eba\u4e8e\u53d8\u6001\u7684\u60c5\u6b32\u800c\u906d\u5230\u6c38\u706b\u7684\u5211\u7f5a\u3002\u8fd9\u4e9b\u4e8b\u90fd\u6210\u4e3a\u6211\u4eec\u7684\u8b66\u6212\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5047\u6559\u5e08\u7684\u6076\u884c - \u540c\u6837\uff0c\u8fd9\u4e9b\u505a\u68a6\u7684\u4eba\u73b7\u6c61\u81ea\u5df1\u7684\u8eab\u4f53\uff0c\u4e0d\u670d\u6743\u67c4\uff0c\u4eb5\u6e0e\u6709\u5c0a\u8363\u7684\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5f53\u5929\u4f7f\u957f\u7c73\u8fe6\u52d2\u4e3a\u6469\u897f\u7684\u5c38\u4f53\u8ddf\u9b54\u9b3c\u4e89\u8fa9\u7684\u65f6\u5019\uff0c\u5c1a\u4e14\u4e0d\u6562\u7528\u6bc1\u8c24\u7684\u8bdd\u8c34\u8d23\u5b83\uff0c\u53ea\u8bf4\uff1a\u201c\u613f\u4e3b\u8d23\u7f5a\u4f60\uff01\u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u8fd9\u4e9b\u4eba\u5374\u6bc1\u8c24\u81ea\u5df1\u4e0d\u660e\u767d\u7684\u4e8b\uff0c\u50cf\u6ca1\u6709\u7406\u6027\u7684\u91ce\u517d\u4e00\u6837\u51ed\u672c\u80fd\u884c\u4e8b\uff0c\u7ed3\u679c\u81ea\u53d6\u706d\u4ea1\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4ed6\u4eec\u5927\u7978\u4e34\u5934\u4e86\uff01\u4ed6\u4eec\u6b65\u4e86\u8be5\u9690\u7684\u540e\u5c18\uff0c\u4e3a\u8c0b\u5229\u800c\u91cd\u8e48\u5df4\u5170\u7684\u8c2c\u8bef\uff0c\u53c8\u50cf\u53ef\u62c9\u4e00\u6837\u56e0\u53db\u9006\u800c\u706d\u4ea1\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8fd9\u4e9b\u4eba\u5728\u4f60\u4eec\u7684\u7231\u5bb4\u4e2d\u662f\u8d25\u7c7b\u3002\u4ed6\u4eec\u8086\u65e0\u5fcc\u60ee\u5730\u5403\u559d\uff0c\u662f\u53ea\u987e\u5582\u517b\u81ea\u5df1\u7684\u7267\u4eba\uff1b\u662f\u6ca1\u6709\u96e8\u7684\u4e91\uff0c\u968f\u98ce\u98d8\u8361\uff1b\u662f\u6df1\u79cb\u4e0d\u7ed3\u679c\u5b50\u7684\u6811\uff0c\u88ab\u8fde\u6839\u62d4\u8d77\uff0c\u5f7b\u5e95\u67af\u6b7b\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4ed6\u4eec\u662f\u6d77\u4e2d\u7684\u72c2\u6d9b\uff0c\u7ffb\u52a8\u7740\u81ea\u5df1\u53ef\u803b\u7684\u6ce1\u6cab\uff0c\u662f\u6d41\u8361\u7684\u661f\u661f\uff0c\u6709\u5e7d\u5e7d\u9ed1\u6697\u6c38\u8fdc\u7559\u7ed9\u4ed6\u4eec\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e9a\u5f53\u7684\u7b2c\u4e03\u4ee3\u5b50\u5b59\u4ee5\u8bfa\u66fe\u7ecf\u9488\u5bf9\u8fd9\u4e9b\u4eba\u8bf4\u9884\u8a00\uff1a\u201c\u770b\u554a\uff01\u4e3b\u7387\u9886\u7942\u5343\u4e07\u7684\u5723\u8005\u4e00\u540c\u964d\u4e34\uff0c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8981\u5ba1\u5224\u6240\u6709\u7684\u4eba\uff0c\u6309\u4e0d\u8654\u4e0d\u656c\u7684\u7f6a\u4eba\u6240\u884c\u7684\u6076\u4e8b\u548c\u4ed6\u4eec\u4eb5\u6e0e\u4e0a\u5e1d\u7684\u8bdd\u5b9a\u4ed6\u4eec\u7684\u7f6a\u3002\u201d"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u8fd9\u4e9b\u4eba\u6ee1\u8179\u7262\u9a9a\uff0c\u6028\u5929\u5c24\u4eba\uff0c\u653e\u7eb5\u81ea\u5df1\u7684\u90aa\u60c5\u79c1\u6b32\u3002\u4ed6\u4eec\u53e3\u51fa\u72c2\u8a00\uff0c\u4e3a\u4e86\u8c0b\u5229\u800c\u963f\u8c00\u5949\u627f\u4ed6\u4eba\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u7262\u8bb0\u8b66\u544a - \u4eb2\u7231\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u8981\u8c28\u8bb0\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u4f7f\u5f92\u4ece\u524d\u7ed9\u4f60\u4eec\u7684\u8b66\u544a\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u4ed6\u4eec\u66fe\u5bf9\u4f60\u4eec\u8bf4\uff1a\u201c\u672b\u4e16\u7684\u65f6\u5019\uff0c\u5fc5\u5b9a\u6709\u4e0d\u656c\u8654\u3001\u597d\u8ba5\u7b11\u7684\u4eba\u653e\u7eb5\u81ea\u5df1\u7684\u90aa\u60c5\u79c1\u6b32\u3002\u201d"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u8fd9\u4e9b\u4eba\u5236\u9020\u5206\u88c2\uff0c\u8840\u6c14\u7528\u4e8b\uff0c\u5fc3\u4e2d\u6ca1\u6709\u5723\u7075\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4eb2\u7231\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u4eec\u8981\u5728\u81f3\u5723\u7684\u771f\u9053\u4e0a\u9020\u5c31\u81ea\u5df1\uff0c\u5728\u5723\u7075\u7684\u5f15\u5bfc\u4e0b\u7977\u544a\uff0c"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u5e38\u5728\u4e0a\u5e1d\u7684\u7231\u4e2d\uff0c\u7b49\u5019\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u65bd\u601c\u60af\u8d50\u7ed9\u4f60\u4eec\u6c38\u751f\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u90a3\u4e9b\u5fc3\u5b58\u7591\u60d1\u7684\u4eba\uff0c\u4f60\u4eec\u8981\u601c\u60af\u4ed6\u4eec\uff1b"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u6709\u4e9b\u4eba\uff0c\u4f60\u4eec\u8981\u5c06\u4ed6\u4eec\u4ece\u706b\u4e2d\u62a2\u6551\u51fa\u6765\uff1b\u8fd8\u6709\u4e9b\u4eba\uff0c\u4f60\u4eec\u8981\u6000\u7740\u754f\u60e7\u7684\u5fc3\u601c\u60af\u4ed6\u4eec\uff0c\u751a\u81f3\u8981\u538c\u6076\u88ab\u4ed6\u4eec\u7684\u90aa\u60c5\u79c1\u6b32\u73b7\u6c61\u7684\u8863\u670d\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u795d\u9882 - \u613f\u8363\u8000\u5f52\u7ed9\u6211\u4eec\u7684\u6551\u4e3b\u2014\u2014\u72ec\u4e00\u7684\u4e0a\u5e1d\uff01\u7942\u80fd\u4fdd\u5b88\u4f60\u4eec\u4e0d\u5931\u8db3\u72af\u7f6a\uff0c\u4f7f\u4f60\u4eec\u65e0\u7455\u65e0\u75b5\u3001\u6b22\u6b22\u559c\u559c\u5730\u7ad9\u5728\u7942\u7684\u8363\u8000\u9762\u524d\u3002"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u613f\u8363\u8000\u3001\u5a01\u4e25\u3001\u80fd\u529b\u548c\u6743\u67c4\u501f\u7740\u6211\u4eec\u7684\u4e3b\u8036\u7a23\u57fa\u7763\u90fd\u5f52\u7ed9\u7942\uff0c\u4ece\u4e07\u4e16\u4ee5\u524d\u76f4\u5230\u73b0\u4eca\uff0c\u4e00\u76f4\u5230\u6c38\u6c38\u8fdc\u8fdc\u3002\u963f\u4eec\uff01"
                }
            }
        }
    }
}