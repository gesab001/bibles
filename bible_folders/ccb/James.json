                    "verse": "\u6240\u4ee5\u4f60\u4eec\u8981\u5f7c\u6b64\u8ba4\u7f6a\uff0c\u4e92\u76f8\u4ee3\u7977\uff0c\u597d\u5f97\u5230\u533b\u6cbb\u3002\u4e49\u4eba\u7684\u7977\u544a\u6709\u6781\u5927\u7684\u529b\u91cf\u548c\u529f\u6548\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4ee5\u5229\u4e9a\u5148\u77e5\u548c\u6211\u4eec\u4e00\u6837\u540c\u662f\u8840\u8089\u4e4b\u8eaf\uff0c\u4ed6\u6073\u5207\u5730\u7948\u6c42\u4e0d\u8981\u4e0b\u96e8\uff0c\u7ed3\u679c\u4e09\u5e74\u534a\u6ca1\u6709\u4e0b\u96e8\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u540e\u6765\u4ed6\u518d\u7977\u544a\u6c42\u96e8\uff0c\u5929\u5c31\u964d\u96e8\uff0c\u5730\u4e5f\u957f\u51fa\u4e86\u5e84\u7a3c\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u6211\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u5982\u679c\u6709\u4eba\u9886\u4e00\u4e2a\u504f\u79bb\u771f\u9053\u7684\u4eba\u5f52\u56de\u6b63\u8def\uff0c"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4ed6\u8be5\u77e5\u9053\uff1a\u4f7f\u4e00\u4e2a\u8ff7\u9014\u4e2d\u7684\u7f6a\u4eba\u5f52\u56de\u6b63\u8def\u5c31\u662f\u6551\u4e00\u4e2a\u7075\u9b42\u8131\u79bb\u6b7b\u4ea1\uff0c\u5e76\u4e14\u4f1a\u906e\u76d6\u8bb8\u591a\u7f6a\u3002"
                }
            }
        }
    }
