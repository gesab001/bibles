                    "verse": "\u4e0d\u77e5\u4e0d\u89c9\uff0c \u6211\u7684\u5fc3\u628a\u6211\u5e26\u5230\u6211\u5c0a\u957f\u7684\u8f66\u4e0a\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c11\u5973\uff1a \u56de\u6765\u5427\uff0c\u56de\u6765\u5427\uff0c \u4e66\u62c9\u5bc6\u7684\u5c11\u5973\uff01 \u56de\u6765\u5427\uff0c\u56de\u6765\u5427\uff0c \u597d\u8ba9\u6211\u4eec\u518d\u770b\u770b\u4f60\uff01 \u7537\u5b50\uff1a\u4f60\u4eec\u4e3a\u4f55\u76ee\u4e0d\u8f6c\u775b\u5730\u770b\u7740\u4e66\u62c9\u5bc6\u7684\u5c11\u5973\uff0c \u597d\u50cf\u89c2\u770b\u739b\u54c8\u5ff5\u7684\u821e\u8e48\u5462\uff1f"
                }
            }
        },
        "7": {
            "chapter_nr": "7",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u7684\u516c\u4e3b\u554a\uff0c\u4f60\u7684\u53cc\u811a\u914d\u4e0a\u978b\u5b50\u591a\u4e48\u7f8e\u554a\uff01 \u4f60\u7f8e\u7389\u822c\u5706\u6da6\u7684\u817f\u662f\u5de7\u5320\u7684\u6770\u4f5c\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4f60\u7684\u809a\u8110\u597d\u50cf\u5706\u676f\uff0c \u4e0d\u7f3a\u8c03\u548c\u7684\u9152\u3002 \u4f60\u7684\u8170\u597d\u6bd4\u4e00\u5806\u9ea6\u5b50\uff0c \u56db\u5468\u56f4\u7ed5\u7740\u767e\u5408\u82b1\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4f60\u7684\u80f8\u812f\u597d\u50cf\u4e00\u5bf9\u5b6a\u751f\u7684\u5c0f\u9e7f\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4f60\u7684\u8116\u5b50\u5982\u8c61\u7259\u5854\uff0c \u773c\u775b\u597d\u50cf\u5e0c\u5b9e\u672c\u5883\u5185\u5df4\u7279\u00b7\u62c9\u5e76\u95e8\u65c1\u7684\u6c34\u6c60\u3002 \u4f60\u7684\u9f3b\u5b50\u597d\u50cf\u773a\u671b\u5927\u9a6c\u58eb\u9769\u7684\u9ece\u5df4\u5ae9\u5854\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u7684\u5934\u5982\u540c\u8fe6\u5bc6\u5c71\uff0c \u4f60\u7684\u79c0\u53d1\u597d\u50cf\u7d2b\u9ed1\u8272\u7684\u7f0e\u5b50\uff0c \u541b\u738b\u7684\u5fc3\u88ab\u4f60\u7684\u7f15\u7f15\u9752\u4e1d\u7cfb\u4f4f\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u7684\u7231\u4eba\uff0c\u4f60\u662f\u591a\u4e48\u7f8e\u4e3d\uff0c \u591a\u4e48\u8ba8\u4eba\u559c\u7231\uff0c\u4ee4\u4eba\u9676\u9189\u554a\uff01"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4f60\u4ead\u4ead\u7389\u7acb\u5982\u540c\u68d5\u6988\u6811\uff0c \u80f8\u812f\u5c31\u50cf\u6811\u4e0a\u7684\u679c\u4e32\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u8981\u6500\u4e0a\u8fd9\u68f5\u6811\uff0c \u63e1\u4f4f\u6811\u4e0a\u7684\u679c\u5b50\u3002 \u613f\u4f60\u7684\u80f8\u812f\u50cf\u8461\u8404\u4e32\uff0c \u4f60\u7684\u6c14\u606f\u5982\u82f9\u679c\u822c\u6e05\u9999\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4f60\u7684\u4eb2\u543b\u50cf\u4e0a\u597d\u7684\u7f8e\u9152\u3002 \u5973\u5b50\uff1a\u613f\u7f8e\u9152\u6d41\u5165\u6211\u826f\u4eba\u7684\u53e3\u4e2d\uff0c \u7f13\u7f13\u6ce8\u5165\u4ed6\u9676\u9189\u7684\u5507\u9f7f\u95f4\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u5c5e\u4e8e\u6211\u7684\u826f\u4eba\uff0c \u662f\u4ed6\u6240\u604b\u6155\u7684\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u7684\u826f\u4eba\uff0c\u6765\u5427\uff01 \u8ba9\u6211\u4eec\u5230\u7530\u95f4\u53bb\uff0c \u5728\u6751\u5e84\u91cc\u4f4f\u5bbf\u5427\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u4eec\u6e05\u6668\u53bb\u8461\u8404\u56ed\uff0c \u770b\u770b\u8461\u8404\u6811\u662f\u5426\u5df2\u7ecf\u53d1\u82bd\u5f00\u82b1\uff0c \u77f3\u69b4\u6811\u662f\u5426\u6b63\u5728\u5f00\u82b1\u3002 \u6211\u8981\u5728\u90a3\u91cc\u628a\u6211\u7684\u7231\u60c5\u732e\u7ed9\u4f60\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u90a3\u91cc\u98ce\u8304\u98d8\u9999\uff0c \u95e8\u53e3\u6709\u5404\u6837\u4e0a\u597d\u7684\u65b0\u9648\u7f8e\u679c\u3002 \u6211\u7684\u826f\u4eba\uff0c\u8fd9\u90fd\u662f\u6211\u4e3a\u4f60\u5b58\u7559\u7684\u3002"
                }
            }
        },
        "8": {
            "chapter_nr": "8",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5982\u679c\u4f60\u662f\u6211\u7684\u540c\u80de\u5144\u5f1f\u5c31\u597d\u4e86\u3002 \u8fd9\u6837\uff0c\u5728\u5916\u5934\u9047\u89c1\u4f60\uff0c \u6211\u4e5f\u53ef\u4ee5\u4eb2\u543b\u4f60\uff0c \u6ca1\u6709\u4eba\u4f1a\u53d6\u7b11\u6211\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u53ef\u4ee5\u628a\u4f60\u5e26\u8fdb\u6211\u6bcd\u4eb2\u7684\u5bb6\u2014\u2014\u5979\u6559\u517b\u4e86\u6211\u3002 \u6211\u8981\u7ed9\u4f60\u559d\u77f3\u69b4\u6c41\u917f\u7684\u9187\u9152\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4f60\u7684\u5de6\u624b\u6276\u7740\u6211\u7684\u5934\uff0c \u53f3\u624b\u7d27\u62b1\u7740\u6211\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c11\u5973\u554a\uff0c \u6211\u6073\u6c42\u4f60\u4eec\uff0c \u4e0d\u8981\u53eb\u9192\u6216\u60ca\u52a8\u7231\u60c5\uff0c \u7b49\u5b83\u81ea\u53d1\u5427\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c11\u5973\uff1a \u90a3\u4ece\u65f7\u91ce\u4e0a\u6765\u3001\u9760\u5728\u826f\u4eba\u8eab\u65c1\u7684\u662f\u8c01\u5462\uff1f \u5973\u5b50\uff1a\u6211\u5728\u82f9\u679c\u6811\u4e0b\u5524\u9192\u4f60\uff0c \u4f60\u6bcd\u4eb2\u5728\u90a3\u91cc\u6000\u4f60\u517b\u4f60\uff0c \u4e3a\u4f60\u53d7\u751f\u4ea7\u4e4b\u82e6\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u8bf7\u628a\u6211\u5370\u5728\u4f60\u7684\u5fc3\u4e0a\uff0c \u523b\u5728\u4f60\u7684\u624b\u81c2\u4e0a\uff0c \u56e0\u4e3a\u7231\u60c5\u5982\u6b7b\u4ea1\u4e00\u822c\u575a\u56fa\uff0c \u5ac9\u5992\u50cf\u9634\u95f4\u4e00\u6837\u6b8b\u9177\uff0c \u5b83\u71c3\u8d77\u7684\u706b\u7130\u662f\u70e7\u706d\u4e00\u5207\u7684\u70c8\u706b\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u7231\u60c5\uff0c\u5927\u6c34\u4e0d\u80fd\u7184\u706d\uff0c \u6c5f\u6cb3\u4e0d\u80fd\u6df9\u6ca1\u3002 \u82e5\u6709\u4eba\u60f3\u503e\u5176\u5bb6\u8d22\u4e70\u7231\u60c5\uff0c \u5fc5\u906d\u553e\u5f03\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5973\u5b50\u7684\u5144\u957f\uff1a \u6211\u4eec\u6709\u4e00\u4e2a\u5c0f\u59b9\uff0c \u5979\u7684\u80f8\u812f\u8fd8\u6ca1\u6709\u53d1\u80b2\u3002 \u82e5\u6709\u4eba\u6765\u63d0\u4eb2\uff0c \u6211\u4eec\u8be5\u600e\u4e48\u529e\uff1f"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5979\u82e5\u662f\u5899\uff0c \u6211\u4eec\u8981\u5728\u4e0a\u9762\u5efa\u9020\u94f6\u5854\uff1b \u5979\u82e5\u662f\u95e8\uff0c \u6211\u4eec\u8981\u7528\u9999\u67cf\u6728\u677f\u56f4\u62a4\u5979\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5973\u5b50\uff1a \u6211\u662f\u4e00\u9762\u5899\uff0c \u53cc\u4e73\u50cf\u57ce\u697c\uff0c \u6211\u5728\u4ed6\u773c\u4e2d\u50cf\u627e\u5230\u5e73\u5b89\u7684\u4eba\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6240\u7f57\u95e8\u5728\u5df4\u529b\u00b7\u54c8\u4eec\u6709\u4e00\u4e2a\u8461\u8404\u56ed\uff0c \u4ed6\u628a\u8461\u8404\u56ed\u79df\u7ed9\u519c\u592b\uff0c \u79df\u91d1\u662f\u6bcf\u4eba\u4e00\u5343\u5757\u94f6\u5b50\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u7684\u8461\u8404\u56ed\uff0c\u6211\u81ea\u5df1\u4f5c\u4e3b\u3002 \u6240\u7f57\u95e8\u554a\uff0c\u4f60\u7684\u4e00\u5343\u5757\u94f6\u5b50\u81ea\u5df1\u7559\u7740\uff0c \u4e8c\u767e\u5757\u94f6\u5b50\u7ed9\u7ba1\u679c\u56ed\u7684\u4eba\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u7537\u5b50\uff1a \u4f60\u8fd9\u4f4f\u5728\u56ed\u4e2d\u7684\u4eba\u554a\uff0c \u540c\u4f34\u4eec\u90fd\u5728\u8046\u542c\u4f60\u7684\u58f0\u97f3\uff0c \u6c42\u4f60\u8ba9\u6211\u4e5f\u542c\u89c1\u5427\uff01"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5973\u5b50\uff1a \u6211\u7684\u826f\u4eba\u554a\uff0c \u613f\u4f60\u50cf\u9999\u8349\u5c71\u4e0a\u7684\u7f9a\u7f8a\u548c\u5c0f\u9e7f\uff0c \u5feb\u5feb\u6765\u5230\u6211\u8eab\u8fb9\uff01"
                }
            }
        }
    }
