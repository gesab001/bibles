        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u6211\u8981\u5dee\u9063\u6211\u7684\u4f7f\u8005\u4e3a\u6211\u5f00\u8def\u3002\u4f60\u4eec\u6240\u5bfb\u6c42\u7684\u4e3b\u5fc5\u7a81\u7136\u6765\u5230\u7942\u7684\u6bbf\u4e2d\u3002\u4f60\u4eec\u6240\u671f\u76fc\u7684\u90a3\u4f4d\u7acb\u7ea6\u7684\u4f7f\u8005\u5feb\u8981\u6765\u4e86\uff01\u201d"
