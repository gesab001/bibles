        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u7ea6\u62ff\u7684\u7977\u544a - \u7ea6\u62ff\u5728\u9c7c\u8179\u4e2d\u5411\u4ed6\u7684\u4e0a\u5e1d\u8036\u548c\u534e\u7977\u544a\u8bf4\uff1a"
