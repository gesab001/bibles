                "2": {
                    "verse_nr": "2",
                    "verse": "\u201c\u4f60\u8d77\u8eab\u524d\u5f80\u5c3c\u5c3c\u5fae\u5927\u57ce\uff0c\u5411\u90a3\u91cc\u7684\u5c45\u6c11\u5ba3\u544a\u6211\u5148\u524d\u5429\u5490\u4f60\u7684\u8bdd\u3002\u201d"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u7ea6\u62ff\u4fbf\u7167\u8036\u548c\u534e\u7684\u5429\u5490\u524d\u5f80\u5c3c\u5c3c\u5fae\u57ce\u3002\u5c3c\u5c3c\u5fae\u57ce\u5f88\u5927\uff0c\u8981\u4e09\u5929\u624d\u80fd\u8d70\u904d\u3002"
