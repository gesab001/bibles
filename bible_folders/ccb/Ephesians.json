    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u5949\u4e0a\u5e1d\u65e8\u610f\u505a\u57fa\u7763\u8036\u7a23\u4f7f\u5f92\u7684\u4fdd\u7f57\uff0c\u5199\u4fe1\u7ed9\u4ee5\u5f17\u6240\u5fe0\u4e8e\u57fa\u7763\u8036\u7a23\u7684\u4f17\u5723\u5f92\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u613f\u6211\u4eec\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u8d50\u7ed9\u4f60\u4eec\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u57fa\u7763\u5f92\u7684\u5c5e\u7075\u6069\u798f - \u8d5e\u7f8e\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u7236\u4e0a\u5e1d\uff01\u7942\u5728\u57fa\u7763\u91cc\u8d50\u7ed9\u4e86\u6211\u4eec\u5929\u4e0a\u5404\u6837\u5c5e\u7075\u7684\u6069\u798f\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u65e9\u5728\u521b\u9020\u4e16\u754c\u4ee5\u524d\uff0c\u7942\u5df2\u7ecf\u5728\u57fa\u7763\u91cc\u62e3\u9009\u4e86\u6211\u4eec\uff0c\u4f7f\u6211\u4eec\u5728\u7942\u773c\u4e2d\u6210\u4e3a\u5723\u6d01\u65e0\u7455\u7684\u4eba\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4e0a\u5e1d\u56e0\u4e3a\u7231\u6211\u4eec\uff0c\u5c31\u6309\u7167\u7942\u81ea\u5df1\u7f8e\u597d\u7684\u65e8\u610f\uff0c\u9884\u5b9a\u6211\u4eec\u501f\u7740\u8036\u7a23\u57fa\u7763\u5f97\u5230\u505a\u7942\u513f\u5973\u7684\u540d\u5206\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u4eec\u8d5e\u7f8e\u7942\u5947\u5999\u65e0\u6bd4\u7684\u6069\u5178\uff0c\u8fd9\u6069\u5178\u662f\u4e0a\u5e1d\u501f\u7740\u7942\u7684\u7231\u5b50\u8d50\u7ed9\u6211\u4eec\u7684\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6211\u4eec\u501f\u7740\u7942\u7231\u5b50\u7684\u8840\u8499\u6551\u8d4e\uff0c\u8fc7\u72af\u5f97\u5230\u8d66\u514d\uff0c\u8fd9\u90fd\u662f\u51fa\u4e8e\u7942\u7684\u6d2a\u6069\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e0a\u5e1d\u7528\u5404\u79cd\u667a\u6167\u548c\u806a\u660e\u628a\u8fd9\u6069\u5178\u4e30\u4e30\u5bcc\u5bcc\u5730\u8d50\u7ed9\u6211\u4eec\uff0c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7167\u7740\u7942\u5728\u57fa\u7763\u91cc\u6240\u5b9a\u7684\u7f8e\u597d\u8ba1\u5212\u53eb\u6211\u4eec\u77e5\u9053\u7942\u65e8\u610f\u7684\u5965\u79d8\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u7b49\u6240\u5b9a\u7684\u65f6\u5019\u4e00\u5230\uff0c\u53eb\u5929\u5730\u4e07\u7269\u4e00\u540c\u5f52\u5728\u57fa\u7763\u7684\u540d\u4e0b\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u4eec\u4e5f\u5728\u57fa\u7763\u91cc\u88ab\u62e3\u9009\u6210\u4e3a\u7942\u7684\u5b50\u6c11\u3002\u8fd9\u90fd\u662f\u6309\u81ea\u5df1\u65e8\u610f\u884c\u4e07\u4e8b\u7684\u4e0a\u5e1d\u9884\u5148\u5b9a\u4e0b\u7684\u8ba1\u5212\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u597d\u8ba9\u6211\u4eec\u8fd9\u4e9b\u9996\u5148\u5728\u57fa\u7763\u91cc\u5f97\u5230\u76fc\u671b\u7684\u4eba\u90fd\u6765\u9882\u8d5e\u7942\u7684\u8363\u8000\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u4eec\u542c\u8fc7\u771f\u7406\u4e4b\u9053\uff0c\u5c31\u662f\u90a3\u4f7f\u4f60\u4eec\u5f97\u6551\u7684\u798f\u97f3\uff0c\u800c\u4e14\u4e5f\u4fe1\u4e86\u57fa\u7763\u3002\u4f60\u4eec\u65e2\u7136\u4fe1\u7942\uff0c\u5c31\u9886\u53d7\u4e86\u4e0a\u5e1d\u5e94\u8bb8\u8d50\u4e0b\u7684\u5723\u7075\u4e3a\u5370\u8bb0\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5723\u7075\u662f\u6211\u4eec\u9886\u53d7\u4ea7\u4e1a\u7684\u62c5\u4fdd\uff0c\u76f4\u5230\u4e0a\u5e1d\u7684\u5b50\u6c11\u5f97\u5230\u6551\u8d4e\uff0c\u4f7f\u7942\u7684\u8363\u8000\u53d7\u5230\u9882\u8d5e\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u4fdd\u7f57\u7684\u7977\u544a - \u6211\u542c\u5230\u4f60\u4eec\u5bf9\u4e3b\u8036\u7a23\u7684\u4fe1\u5fc3\u548c\u5bf9\u4f17\u5723\u5f92\u7684\u7231\u5fc3\u540e\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4e0d\u65ad\u5730\u4e3a\u4f60\u4eec\u611f\u8c22\u4e0a\u5e1d\uff0c\u7977\u544a\u7684\u65f6\u5019\u5e38\u5e38\u63d0\u5230\u4f60\u4eec\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u6211\u6c42\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u4e0a\u5e1d\u2014\u2014\u8363\u8000\u7684\u7236\u628a\u8d50\u667a\u6167\u548c\u542f\u793a\u7684\u7075\u7ed9\u4f60\u4eec\uff0c\u597d\u4f7f\u4f60\u4eec\u66f4\u6df1\u5730\u8ba4\u8bc6\u7942\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u6211\u4e5f\u6c42\u4e0a\u5e1d\u7167\u4eae\u4f60\u4eec\u5fc3\u4e2d\u7684\u773c\u775b\uff0c\u4f7f\u4f60\u4eec\u77e5\u9053\u7942\u7684\u547c\u53ec\u7ed9\u4f60\u4eec\u5e26\u6765\u4e86\u4f55\u7b49\u7684\u76fc\u671b\uff0c\u7942\u5e94\u8bb8\u8d50\u7ed9\u4f17\u5723\u5f92\u7684\u4ea7\u4e1a\u6709\u4f55\u7b49\u4e30\u5bcc\u7684\u8363\u8000\uff0c"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u5e76\u4e14\u7942\u5728\u6211\u4eec\u8fd9\u4e9b\u4fe1\u7684\u4eba\u8eab\u4e0a\u6240\u8fd0\u884c\u7684\u80fd\u529b\u662f\u4f55\u7b49\u6d69\u5927\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4e0a\u5e1d\u66fe\u7528\u8fd9\u5927\u80fd\u4f7f\u57fa\u7763\u4ece\u6b7b\u91cc\u590d\u6d3b\uff0c\u4f7f\u57fa\u7763\u5728\u5929\u4e0a\u5750\u5728\u81ea\u5df1\u53f3\u8fb9\uff0c"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u8fdc\u8fdc\u8d85\u8d8a\u4eca\u751f\u6c38\u4e16\u6240\u6709\u6267\u653f\u7684\u3001\u638c\u6743\u7684\u3001\u6709\u80fd\u529b\u7684\u3001\u505a\u4e3b\u5bb0\u7684\u548c\u4e00\u5207\u7684\u6743\u52bf\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u7942\u53c8\u4f7f\u4e07\u7269\u964d\u670d\u5728\u57fa\u7763\u811a\u4e0b\uff0c\u4f7f\u57fa\u7763\u4e3a\u6559\u4f1a\u505a\u4e07\u7269\u7684\u5143\u9996\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u6559\u4f1a\u662f\u57fa\u7763\u7684\u8eab\u4f53\uff0c\u8574\u6db5\u7740\u90a3\u65e0\u6240\u4e0d\u5728\u3001\u5145\u6ee1\u4e07\u7269\u8005\u7684\u4e30\u76db\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u9760\u6069\u5178\u3001\u51ed\u4fe1\u5fc3 - \u4f60\u4eec\u4ece\u524d\u6b7b\u5728\u8fc7\u72af\u548c\u7f6a\u6076\u4e4b\u4e2d\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u90a3\u65f6\uff0c\u4f60\u4eec\u968f\u4ece\u4eca\u4e16\u7684\u98ce\u4fd7\uff0c\u987a\u670d\u5728\u7a7a\u4e2d\u638c\u6743\u7684\u9b54\u9b3c\uff0c\u5c31\u662f\u5982\u4eca\u5728\u4e00\u5207\u53db\u9006\u4e0a\u5e1d\u4e4b\u4eba\u5fc3\u4e2d\u8fd0\u884c\u7684\u90aa\u7075\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u6211\u4eec\u4ee5\u524d\u4e5f\u5728\u4ed6\u4eec\u5f53\u4e2d\u968f\u4ece\u7f6a\u6076\u7684\u672c\u6027\uff0c\u653e\u7eb5\u8089\u4f53\u548c\u5fc3\u4e2d\u7684\u79c1\u6b32\uff0c\u4e0e\u5176\u4ed6\u4eba\u4e00\u6837\u672c\u662f\u60f9\u4e0a\u5e1d\u53d1\u6012\u7684\u4eba\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7136\u800c\u4e0a\u5e1d\u6709\u4e30\u5bcc\u7684\u601c\u60af\uff0c\u7942\u6df1\u7231\u6211\u4eec\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5c3d\u7ba1\u6211\u4eec\u6b7b\u5728\u8fc7\u72af\u4e4b\u4e2d\uff0c\u7942\u4ecd\u7136\u4f7f\u6211\u4eec\u548c\u57fa\u7763\u4e00\u540c\u6d3b\u4e86\u8fc7\u6765\u3002\u4f60\u4eec\u5f97\u6551\u662f\u56e0\u4e3a\u4e0a\u5e1d\u7684\u6069\u5178\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u5728\u57fa\u7763\u8036\u7a23\u91cc\uff0c\u4e0a\u5e1d\u4f7f\u6211\u4eec\u4e0e\u57fa\u7763\u4e00\u540c\u590d\u6d3b\uff0c\u4e00\u540c\u5750\u5728\u5929\u4e0a\uff0c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u597d\u501f\u7740\u7942\u5728\u57fa\u7763\u8036\u7a23\u91cc\u5411\u6211\u4eec\u6240\u65bd\u7684\u6069\u6148\uff0c\u5411\u540e\u4e16\u663e\u660e\u7942\u65e0\u53ef\u6bd4\u62df\u7684\u4e30\u5bcc\u6069\u5178\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u4eec\u5f97\u6551\u662f\u9760\u6069\u5178\uff0c\u51ed\u4fe1\u5fc3\u3002\u8fd9\u4e0d\u662f\u51fa\u4e8e\u4f60\u4eec\u81ea\u5df1\uff0c\u800c\u662f\u4e0a\u5e1d\u7684\u793c\u7269\uff1b"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4e5f\u4e0d\u662f\u7531\u4e8e\u4f60\u4eec\u7684\u884c\u4e3a\uff0c\u4ee5\u514d\u6709\u4eba\u81ea\u5938\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u4eec\u662f\u4e0a\u5e1d\u7684\u6770\u4f5c\uff0c\u662f\u5728\u57fa\u7763\u8036\u7a23\u91cc\u521b\u9020\u7684\uff0c\u4e3a\u8981\u53eb\u6211\u4eec\u505a\u7942\u9884\u5148\u5b89\u6392\u7ed9\u6211\u4eec\u7684\u7f8e\u5584\u4e4b\u4e8b\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5728\u57fa\u7763\u91cc\u5408\u800c\u4e3a\u4e00 - \u56e0\u6b64\uff0c\u4f60\u4eec\u8981\u8bb0\u4f4f\uff0c\u4ece\u524d\u6309\u7167\u8089\u4f53\u6765\u8bf4\u4f60\u4eec\u662f\u5916\u65cf\u4eba\uff0c\u90a3\u4e9b\u8089\u4f53\u4e0a\u53d7\u8fc7\u4eba\u624b\u6240\u884c\u5272\u793c\u7684\u4ee5\u8272\u5217\u4eba\u79f0\u4f60\u4eec\u4e3a\u201c\u6ca1\u6709\u53d7\u8fc7\u5272\u793c\u7684\u201d\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u90a3\u65f6\uff0c\u4f60\u4eec\u4e0e\u57fa\u7763\u65e0\u5173\uff0c\u53c8\u4e0d\u662f\u4ee5\u8272\u5217\u4eba\uff0c\u5728\u4e0a\u5e1d\u5e94\u8bb8\u7684\u8bf8\u7ea6\u4e0a\u662f\u5c40\u5916\u4eba\uff0c\u4f60\u4eec\u6d3b\u5728\u4e16\u4e0a\u6ca1\u6709\u76fc\u671b\uff0c\u6ca1\u6709\u4e0a\u5e1d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f46\u4f60\u4eec\u8fd9\u4e9b\u4ece\u524d\u8fdc\u79bb\u4e0a\u5e1d\u7684\u4eba\uff0c\u5982\u4eca\u5728\u57fa\u7763\u8036\u7a23\u91cc\uff0c\u9760\u7740\u7942\u6240\u6d41\u7684\u8840\u5df2\u7ecf\u88ab\u5e26\u5230\u4e0a\u5e1d\u9762\u524d\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u56e0\u4e3a\u57fa\u7763\u8036\u7a23\u4eb2\u81ea\u7ed9\u6211\u4eec\u5e26\u6765\u4e86\u548c\u5e73\uff0c\u4ee5\u81ea\u5df1\u7684\u8eab\u4f53\u62c6\u6bc1\u4e86\u4ee5\u8272\u5217\u4eba\u548c\u5916\u65cf\u4eba\u4e4b\u95f4\u5145\u6ee1\u654c\u610f\u7684\u9694\u5899\uff0c\u4f7f\u53cc\u65b9\u5408\u800c\u4e3a\u4e00\uff0c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u5e76\u5e9f\u9664\u4e86\u4ee5\u8beb\u547d\u548c\u89c4\u6761\u4e3a\u5185\u5bb9\u7684\u5f8b\u6cd5\u5236\u5ea6\u3002\u7942\u8fd9\u6837\u505a\u662f\u4e3a\u4e86\u5728\u81ea\u5df1\u91cc\u9762\u628a\u53cc\u65b9\u9020\u6210\u4e00\u4e2a\u65b0\u4eba\uff0c\u6210\u5c31\u548c\u5e73\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u5e76\u501f\u7740\u5341\u5b57\u67b6\u6d88\u706d\u5f7c\u6b64\u95f4\u7684\u654c\u610f\uff0c\u4f7f\u53cc\u65b9\u501f\u7740\u4e00\u4e2a\u8eab\u4f53\u4e0e\u4e0a\u5e1d\u548c\u597d\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u7942\u6765\u4f20\u626c\u8fd9\u5e73\u5b89\u7684\u798f\u97f3\u7ed9\u4f60\u4eec\u8fd9\u4e9b\u79bb\u4e0a\u5e1d\u5f88\u8fdc\u7684\u5916\u65cf\u4eba\uff0c\u4e5f\u7ed9\u6211\u4eec\u8fd9\u4e9b\u79bb\u4e0a\u5e1d\u5f88\u8fd1\u7684\u4ee5\u8272\u5217\u4eba\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u6211\u4eec\u53cc\u65b9\u90fd\u662f\u9760\u7740\u7942\u5728\u540c\u4e00\u4f4d\u5723\u7075\u7684\u5f15\u5bfc\u4e0b\u6765\u5230\u7236\u4e0a\u5e1d\u9762\u524d\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u4ece\u6b64\uff0c\u4f60\u4eec\u4e0d\u518d\u662f\u5916\u4eba\u6216\u8fc7\u5ba2\uff0c\u4f60\u4eec\u4e0e\u4f17\u5723\u5f92\u4e00\u6837\u662f\u5929\u56fd\u7684\u5b50\u6c11\uff0c\u662f\u4e0a\u5e1d\u5bb6\u91cc\u7684\u4eba\u4e86\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4f60\u4eec\u5df2\u88ab\u5efa\u7acb\u5728\u4f17\u4f7f\u5f92\u548c\u5148\u77e5\u4eec\u7684\u57fa\u7840\u4e0a\uff0c\u57fa\u7763\u8036\u7a23\u81ea\u5df1\u662f\u623f\u89d2\u77f3\u3002"
                },
