        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u4e3a\u4e86\u4f20\u626c\u57fa\u7763\u8036\u7a23\u800c\u88ab\u56da\u7981\u7684\u4fdd\u7f57\uff0c\u4e0e\u63d0\u6469\u592a\u5f1f\u5144\u5199\u4fe1\u7ed9\u6211\u4eec\u4eb2\u7231\u7684\u540c\u5de5\u8153\u5229\u95e8\u5f1f\u5144\u3001"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4e9a\u8153\u4e9a\u59ca\u59b9\u548c\u6211\u4eec\u7684\u6218\u53cb\u4e9a\u57fa\u5e03\uff0c\u4ee5\u53ca\u5728\u8153\u5229\u95e8\u5bb6\u91cc\u805a\u4f1a\u7684\u5f1f\u5144\u59ca\u59b9\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u613f\u6211\u4eec\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u8d50\u7ed9\u4f60\u4eec\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u79f0\u8d5e\u8153\u5229\u95e8 - \u8153\u5229\u95e8\u5f1f\u5144\uff0c\u6211\u5e38\u5e38\u5728\u7977\u544a\u4e2d\u4e3a\u4f60\u611f\u8c22\u4e0a\u5e1d\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u56e0\u4e3a\u6211\u542c\u8bf4\u4e86\u4f60\u5bf9\u4e3b\u8036\u7a23\u7684\u4fe1\u5fc3\u548c\u5bf9\u4f17\u5723\u5f92\u7684\u7231\u5fc3\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u6c42\u4e0a\u5e1d\u4f7f\u4f60\u80fd\u6709\u6548\u5730\u4e0e\u4eba\u5206\u4eab\u4fe1\u4ef0\uff0c\u660e\u767d\u6211\u4eec\u6240\u505a\u7684\u4e00\u5207\u7f8e\u4e8b\u90fd\u662f\u4e3a\u4e86\u57fa\u7763\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5f1f\u5144\u554a\uff0c\u4f60\u7684\u7231\u5fc3\u7ed9\u6211\u5e26\u6765\u6781\u5927\u7684\u559c\u4e50\u548c\u5b89\u6170\uff0c\u56e0\u4e3a\u4f60\u4f7f\u4f17\u5723\u5f92\u611f\u5230\u6b23\u6170\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u51ed\u7231\u5fc3\u6c42\u60c5 - \u56e0\u6b64\uff0c\u6211\u5c3d\u7ba1\u53ef\u4ee5\u5949\u57fa\u7763\u7684\u540d\u5429\u5490\u4f60\u53bb\u505a\u5f53\u505a\u7684\u4e8b\uff0c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7136\u800c\u6211\u8fd9\u4e0a\u4e86\u5e74\u7eaa\u53c8\u4e3a\u57fa\u7763\u8036\u7a23\u7684\u7f18\u6545\u88ab\u56da\u7981\u7684\u4fdd\u7f57\u5b81\u613f\u51ed\u7231\u5fc3\u6c42\u4f60\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5c31\u662f\u66ff\u963f\u5c3c\u897f\u8c0b\u6c42\u4f60\u3002\u4ed6\u662f\u6211\u5728\u72f1\u4e2d\u5e26\u9886\u5f52\u4e3b\u7684\u5c5e\u7075\u513f\u5b50\uff0c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4ed6\u8fc7\u53bb\u5bf9\u4f60\u6ca1\u6709\u4ec0\u4e48\u76ca\u5904\uff0c\u4f46\u73b0\u5728\u5bf9\u4f60\u5bf9\u6211\u90fd\u6709\u76ca\u5904\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u73b0\u5728\u6d3e\u6211\u6df1\u7231\u7684\u963f\u5c3c\u897f\u8c0b\u56de\u4f60\u90a3\u91cc\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u6211\u672c\u60f3\u628a\u4ed6\u7559\u5728\u8eab\u8fb9\uff0c\u8ba9\u4ed6\u5728\u6211\u4e3a\u4f20\u798f\u97f3\u800c\u5750\u7262\u671f\u95f4\u4ee3\u66ff\u4f60\u670d\u4f8d\u6211\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e0d\u8fc7\uff0c\u672a\u7ecf\u4f60\u540c\u610f\uff0c\u6211\u4e0d\u613f\u610f\u8fd9\u6837\u505a\uff0c\u56e0\u4e3a\u6211\u76fc\u671b\u4f60\u7684\u5584\u884c\u662f\u51fa\u4e8e\u7518\u5fc3\uff0c\u800c\u975e\u52c9\u5f3a\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6216\u8bb8\u4ed6\u6682\u65f6\u79bb\u5f00\u4f60\u662f\u4e3a\u4e86\u8ba9\u4f60\u4ee5\u540e\u6c38\u8fdc\u5f97\u5230\u4ed6\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4f60\u5f97\u5230\u7684\u4e0d\u518d\u662f\u4e00\u4e2a\u5974\u96b6\uff0c\u800c\u662f\u4e00\u4f4d\u8fdc\u8d85\u8fc7\u5974\u96b6\u7684\u4eb2\u7231\u5f1f\u5144\u3002\u5bf9\u6211\u800c\u8a00\uff0c\u7684\u786e\u5982\u6b64\uff0c\u66f4\u4f55\u51b5\u5bf9\u4f60\u5462\uff01\u5c31\u8089\u8eab\u8bf4\u4f60\u4eec\u662f\u4e3b\u4ec6\uff0c\u4f46\u5728\u4e3b\u91cc\u4f60\u4eec\u662f\u5f1f\u5144\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u5982\u679c\u4f60\u5f53\u6211\u662f\u540c\u4f34\uff0c\u5c31\u8bf7\u4f60\u50cf\u63a5\u7eb3\u6211\u4e00\u6837\u63a5\u7eb3\u4ed6\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u5982\u679c\u4ed6\u5f97\u7f6a\u4f60\uff0c\u6216\u4e8f\u6b20\u4f60\u4ec0\u4e48\uff0c\u90fd\u8bb0\u5728\u6211\u7684\u8d26\u4e0a\u5427\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u6211\u4fdd\u7f57\u5728\u6b64\u4eb2\u7b14\u4fdd\u8bc1\uff0c\u6211\u5fc5\u507f\u8fd8\u3002\u5176\u5b9e\u6211\u4e0d\u8bf4\u4f60\u4e5f\u77e5\u9053\uff0c\u8fde\u4f60\u81ea\u5df1\u4e5f\u6b20\u6211\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u5f1f\u5144\u554a\uff0c\u770b\u5728\u4e3b\u7684\u4efd\u4e0a\uff0c\u8bf7\u4f60\u7b54\u5e94\u6211\u7684\u8bf7\u6c42\uff0c\u5728\u57fa\u7763\u91cc\u8ba9\u6211\u7684\u5fc3\u5f97\u5230\u5b89\u6170\u5427\uff01"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u6211\u5199\u4fe1\u7ed9\u4f60\uff0c\u6df1\u4fe1\u4f60\u4e00\u5b9a\u4f1a\u7167\u529e\uff0c\u751a\u81f3\u8d85\u8fc7\u6211\u7684\u8981\u6c42\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u540c\u65f6\uff0c\u4e5f\u8bf7\u4f60\u4e3a\u6211\u9884\u5907\u4f4f\u5904\uff0c\u56e0\u4e3a\u6211\u76fc\u671b\u501f\u7740\u4f60\u4eec\u7684\u7977\u544a\uff0c\u6211\u53ef\u4ee5\u8499\u6069\u5230\u4f60\u4eec\u90a3\u91cc\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u95ee\u5019 - \u4e3a\u4e86\u57fa\u7763\u8036\u7a23\u7684\u7f18\u6545\u548c\u6211\u4e00\u540c\u5750\u7262\u7684\u4ee5\u5df4\u5f17\u95ee\u5019\u4f60\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u6b64\u5916\uff0c\u6211\u7684\u540c\u5de5\u9a6c\u53ef\u3001\u4e9a\u91cc\u8fbe\u53e4\u3001\u5e95\u9a6c\u548c\u8def\u52a0\u90fd\u95ee\u5019\u4f60\u3002"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u613f\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u6069\u5178\u4e0e\u4f60\u4eec\u540c\u5728\u3002\u963f\u4eec\uff01"
                }
            }
        }
    }
}