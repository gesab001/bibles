                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u5982\u679c\u6211\u4eec\u8bf4\u4e0e\u7942\u76f8\u4ea4\uff0c\u5374\u4ecd\u8fc7\u7740\u9ed1\u6697\u7684\u751f\u6d3b\uff0c\u5c31\u662f\u6492\u8c0e\uff0c\u6ca1\u6709\u9075\u884c\u771f\u7406\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5982\u679c\u6211\u4eec\u751f\u6d3b\u5728\u5149\u660e\u4e4b\u4e2d\uff0c\u50cf\u4e0a\u5e1d\u5728\u5149\u660e\u4e2d\u4e00\u6837\uff0c\u5c31\u80fd\u591f\u5f7c\u6b64\u76f8\u4ea4\uff0c\u4e0a\u5e1d\u513f\u5b50\u8036\u7a23\u7684\u8840\u80fd\u6d17\u51c0\u6211\u4eec\u4e00\u5207\u7684\u7f6a\u3002"
