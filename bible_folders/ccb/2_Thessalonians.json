    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u4fdd\u7f57\u540c\u897f\u62c9\u548c\u63d0\u6469\u592a\u5199\u4fe1\u7ed9\u5e16\u6492\u7f57\u5c3c\u8fe6\u5c5e\u4e8e\u6211\u4eec\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u6559\u4f1a\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u613f\u6211\u4eec\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u8d50\u7ed9\u4f60\u4eec\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u611f\u6069\u4e0e\u529d\u52c9 - \u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u4eec\u5e94\u5f53\u4e3a\u4f60\u4eec\u5e38\u5e38\u611f\u8c22\u4e0a\u5e1d\uff0c\u8fd9\u662f\u5408\u5b9c\u7684\uff0c\u56e0\u4e3a\u4f60\u4eec\u7684\u4fe1\u5fc3\u4e0d\u65ad\u589e\u957f\uff0c\u5f7c\u6b64\u76f8\u7231\u7684\u5fc3\u4e5f\u4e0d\u65ad\u589e\u52a0\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u56e0\u6b64\uff0c\u6211\u4eec\u5728\u4e0a\u5e1d\u7684\u4f17\u6559\u4f1a\u4e2d\u5938\u5956\u4f60\u4eec\u5728\u5404\u79cd\u8feb\u5bb3\u548c\u60a3\u96be\u4e2d\u7684\u575a\u5fcd\u548c\u4fe1\u5fc3\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u4eec\u8fd9\u79cd\u8868\u73b0\u6b63\u662f\u4e0a\u5e1d\u516c\u4e49\u5ba1\u5224\u7684\u660e\u8bc1\uff0c\u4f7f\u4f60\u4eec\u914d\u8fdb\u4e0a\u5e1d\u7684\u56fd\uff0c\u4f60\u4eec\u6b63\u5728\u4e3a\u8fd9\u56fd\u53d7\u82e6\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e0a\u5e1d\u662f\u516c\u4e49\u7684\uff0c\u7942\u5fc5\u4ee5\u60a3\u96be\u6765\u62a5\u5e94\u90a3\u4e9b\u8feb\u5bb3\u4f60\u4eec\u7684\u4eba\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5f53\u4e3b\u8036\u7a23\u548c\u7942\u5927\u80fd\u7684\u5929\u4f7f\u5728\u70c8\u7130\u4e2d\u4ece\u5929\u4e0a\u663e\u73b0\u65f6\uff0c\u7942\u5fc5\u4f7f\u4f60\u4eec\u8fd9\u4e9b\u53d7\u82e6\u7684\u4eba\u548c\u6211\u4eec\u540c\u5f97\u5b89\u6170\uff0c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u60e9\u7f5a\u90a3\u4e9b\u4e0d\u8ba4\u8bc6\u4e0a\u5e1d\u3001\u4e0d\u542c\u4ece\u6709\u5173\u6211\u4eec\u4e3b\u8036\u7a23\u4e4b\u798f\u97f3\u7684\u4eba\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u90a3\u4e9b\u4eba\u8981\u53d7\u7684\u5211\u7f5a\u5c31\u662f\u79bb\u5f00\u4e3b\u7684\u9762\u548c\u7942\u8363\u8000\u7684\u6743\u80fd\uff0c\u6c38\u8fdc\u706d\u4ea1\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4e3b\u964d\u4e34\u7684\u90a3\u65e5\uff0c\u7942\u8981\u5728\u7942\u7684\u4f17\u5723\u5f92\u4e2d\u5f97\u5230\u8363\u8000\uff0c\u4f7f\u6240\u6709\u7684\u4fe1\u5f92\u60ca\u53f9\u4e0d\u5df2\u3002\u4f60\u4eec\u4e5f\u4f1a\u5728\u5f53\u4e2d\uff0c\u56e0\u4e3a\u4f60\u4eec\u76f8\u4fe1\u4e86\u6211\u4eec\u505a\u7684\u89c1\u8bc1\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u6b64\uff0c\u6211\u4eec\u5e38\u5e38\u4e3a\u4f60\u4eec\u7977\u544a\uff0c\u613f\u6211\u4eec\u7684\u4e0a\u5e1d\u770b\u4f60\u4eec\u914d\u5f97\u7942\u7684\u547c\u53ec\uff0c\u7528\u5927\u80fd\u6210\u5168\u4f60\u4eec\u4e00\u5207\u7f8e\u597d\u7684\u5fc3\u613f\u548c\u51ed\u4fe1\u5fc3\u6240\u505a\u7684\u5de5\u4f5c\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8fd9\u6837\uff0c\u6309\u7167\u6211\u4eec\u7684\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u6240\u8d50\u7684\u6069\u5178\uff0c\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u540d\u4fbf\u5728\u4f60\u4eec\u8eab\u4e0a\u5f97\u5230\u8363\u8000\uff0c\u4f60\u4eec\u4e5f\u5728\u7942\u8eab\u4e0a\u5f97\u5230\u8363\u8000\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4e3b\u518d\u6765\u7684\u5148\u5146 - \u5f1f\u5144\u59ca\u59b9\uff0c\u5173\u4e8e\u4e3b\u8036\u7a23\u57fa\u7763\u518d\u6765\u548c\u6211\u4eec\u5230\u7942\u90a3\u91cc\u76f8\u805a\u7684\u4e8b\u60c5\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u4eec\u73b0\u5728\u5949\u529d\u4f60\u4eec\uff1a\u65e0\u8bba\u662f\u4ec0\u4e48\u7075\uff0c\u6216\u662f\u4f20\u95fb\uff0c\u6216\u662f\u5192\u5145\u6211\u4eec\u5199\u7684\u4fe1\uff0c\u8bf4\u4e3b\u7684\u65e5\u5b50\u5df2\u7ecf\u5230\u4e86\uff0c\u4f60\u4eec\u90fd\u4e0d\u8981\u8f7b\u6613\u52a8\u6447\uff0c\u4e5f\u4e0d\u8981\u60ca\u614c\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4e0d\u7ba1\u522b\u4eba\u7528\u4ec0\u4e48\u8be1\u8ba1\uff0c\u4f60\u4eec\u90fd\u4e0d\u8981\u4e0a\u5f53\u3002\u56e0\u4e3a\u90a3\u65e5\u5b50\u6765\u4e34\u4e4b\u524d\uff0c\u5fc5\u6709\u79bb\u7ecf\u53db\u9053\u7684\u4e8b\u53d1\u751f\uff0c\u800c\u90a3\u4e0d\u6cd5\u4e4b\u5f92\uff0c\u5c31\u662f\u90a3\u6ce8\u5b9a\u706d\u4ea1\u7684\u4eba\u4e5f\u8981\u51fa\u73b0\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4ed6\u4f1a\u62b5\u6321\u4e3b\uff0c\u9ad8\u62ac\u81ea\u5df1\u8d85\u8fc7\u4e00\u5207\u6240\u8c13\u7684\u795e\u660e\u548c\u4eba\u4eec\u5d07\u62dc\u7684\u5bf9\u8c61\uff0c\u751a\u81f3\u5750\u5728\u4e0a\u5e1d\u7684\u6bbf\u4e2d\u4ee5\u4e0a\u5e1d\u81ea\u5c45\uff01"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u6211\u8fd8\u5728\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u66fe\u544a\u8bc9\u8fc7\u4f60\u4eec\u8fd9\u4e9b\u4e8b\uff0c\u4f60\u4eec\u5fd8\u8bb0\u4e86\u5417\uff1f"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u73b0\u5728\uff0c\u4f60\u4eec\u77e5\u9053\u662f\u4ec0\u4e48\u62e6\u963b\u4ed6\uff0c\u4f7f\u4ed6\u7b49\u5230\u7279\u5b9a\u7684\u65f6\u95f4\u624d\u51fa\u73b0\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5176\u5b9e\u90a3\u4e0d\u6cd5\u4e4b\u5f92\u7684\u9634\u8c0b\u5df2\u7ecf\u5728\u915d\u917f\u4e2d\uff0c\u7136\u800c\u73b0\u5728\u6709\u4e00\u4f4d\u5728\u62e6\u963b\u4ed6\u3002\u7b49\u5230\u62e6\u963b\u4ed6\u7684\u90a3\u4f4d\u4e00\u79bb\u5f00\uff0c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ed6\u5c31\u4f1a\u51fa\u73b0\uff0c\u4f46\u4e3b\u8036\u7a23\u4f1a\u7528\u81ea\u5df1\u53e3\u4e2d\u7684\u6c14\u6bc1\u706d\u4ed6\uff0c\u7528\u4ece\u5929\u964d\u4e34\u7684\u8363\u5149\u5e9f\u6389\u4ed6\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4ed6\u6765\u8981\u6309\u7167\u6492\u65e6\u7684\u4f0e\u4fe9\u884c\u5404\u6837\u865a\u5047\u7684\u5f02\u80fd\u3001\u795e\u8ff9\u548c\u5947\u4e8b\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u7528\u5c3d\u5404\u6837\u8be1\u8ba1\u6b3a\u9a97\u90a3\u4e9b\u5c06\u8981\u706d\u4ea1\u7684\u4eba\uff0c\u56e0\u4e3a\u4ed6\u4eec\u4e0d\u559c\u6b22\u63a5\u53d7\u90a3\u80fd\u62ef\u6551\u4ed6\u4eec\u7684\u771f\u7406\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4e0a\u5e1d\u5c31\u8ba9\u4ed6\u4eec\u662f\u975e\u4e0d\u8fa8\uff0c\u53bb\u76f8\u4fe1\u90a3\u4e9b\u865a\u5047\u7684\u8c0e\u8a00\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f7f\u4e00\u5207\u4e0d\u76f8\u4fe1\u771f\u7406\u3001\u53cd\u559c\u7231\u4e0d\u4e49\u7684\u4eba\u90fd\u88ab\u5b9a\u7f6a\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8981\u575a\u5b9a\u4e0d\u79fb - \u4e3b\u6240\u7231\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u4eec\u5e94\u8be5\u65f6\u5e38\u4e3a\u4f60\u4eec\u611f\u8c22\u4e0a\u5e1d\uff0c\u56e0\u4e3a\u4e0a\u5e1d\u4e00\u5f00\u59cb\u5c31\u62e3\u9009\u4e86\u4f60\u4eec\uff0c\u4e3a\u4e86\u4f7f\u4f60\u4eec\u501f\u7740\u5723\u7075\u5f97\u4ee5\u5723\u6d01\uff0c\u5e76\u4e14\u76f8\u4fe1\u771f\u7406\uff0c\u4ece\u800c\u5f97\u6551\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e0a\u5e1d\u501f\u7740\u6211\u4eec\u6240\u4f20\u7684\u798f\u97f3\u547c\u53ec\u4e86\u4f60\u4eec\uff0c\u4f7f\u4f60\u4eec\u53ef\u4ee5\u5f97\u5230\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u8363\u8000\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6240\u4ee5\uff0c\u5404\u4f4d\u5f1f\u5144\u59ca\u59b9\uff0c\u52a1\u8981\u575a\u5b9a\u4e0d\u79fb\uff0c\u65e0\u8bba\u662f\u6211\u4eec\u4fe1\u4e0a\u7684\u6559\u5bfc\u8fd8\u662f\u53e3\u5934\u7684\u6559\u5bfc\uff0c\u4f60\u4eec\u90fd\u8981\u575a\u5b88\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u613f\u4e3b\u8036\u7a23\u57fa\u7763\u548c\u7231\u6211\u4eec\u3001\u5f00\u6069\u5c06\u6c38\u8fdc\u7684\u5b89\u6170\u548c\u7f8e\u597d\u7684\u76fc\u671b\u8d50\u7ed9\u6211\u4eec\u7684\u7236\u4e0a\u5e1d\uff0c"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u5b89\u6170\u4f60\u4eec\u7684\u5fc3\uff0c\u4f7f\u4f60\u4eec\u5728\u4e00\u5207\u5584\u884c\u5584\u8a00\u4e0a\u521a\u5f3a\u3002"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8bf7\u6c42\u4ee3\u7977 - \u5f1f\u5144\u59ca\u59b9\uff0c\u6700\u540e\u6211\u8fd8\u8981\u8bf7\u4f60\u4eec\u4e3a\u6211\u4eec\u7977\u544a\uff0c\u597d\u4f7f\u4e3b\u7684\u9053\u50cf\u5728\u4f60\u4eec\u90a3\u91cc\u4e00\u6837\u8fc5\u901f\u4f20\u5f00\uff0c\u53d7\u5230\u5c0a\u5d07\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u5e76\u4f7f\u6211\u4eec\u80fd\u907f\u5f00\u90a3\u4e9b\u90aa\u6076\u7684\u4eba\uff0c\u56e0\u4e3a\u4e0d\u662f\u4eba\u4eba\u90fd\u6709\u4fe1\u5fc3\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4e3b\u662f\u4fe1\u5b9e\u7684\uff0c\u7942\u5fc5\u4f7f\u4f60\u4eec\u521a\u5f3a\uff0c\u4fdd\u62a4\u4f60\u4eec\u8131\u79bb\u90a3\u6076\u8005\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6211\u4eec\u9760\u7740\u4e3b\u6df1\u4fe1\u4f60\u4eec\u73b0\u5728\u9075\u884c\u4e86\u6211\u4eec\u7684\u5429\u5490\uff0c\u800c\u4e14\u4ee5\u540e\u8fd8\u4f1a\u7ee7\u7eed\u9075\u884c\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u613f\u4e3b\u5f15\u5bfc\u4f60\u4eec\u7684\u5fc3\u9886\u609f\u4e0a\u5e1d\u7684\u7231\u548c\u57fa\u7763\u7684\u575a\u5fcd\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e0d\u53ef\u6e38\u624b\u597d\u95f2 - \u5f1f\u5144\u59ca\u59b9\uff0c\u73b0\u5728\u6211\u4eec\u5949\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u540d\u5429\u5490\u4f60\u4eec\uff0c\u8981\u8fdc\u79bb\u90a3\u4e9b\u6e38\u624b\u597d\u95f2\u3001\u4e0d\u9075\u5b88\u6211\u4eec\u6559\u5bfc\u7684\u4fe1\u5f92\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4f60\u4eec\u81ea\u5df1\u77e5\u9053\u5e94\u8be5\u600e\u6837\u6548\u6cd5\u6211\u4eec\uff0c\u56e0\u4e3a\u6211\u4eec\u5728\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u5e76\u6ca1\u6709\u61d2\u61d2\u6563\u6563\uff0c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e5f\u6ca1\u6709\u767d\u5403\u8c01\u7684\u996d\uff0c\u800c\u662f\u663c\u591c\u8f9b\u52e4\u5de5\u4f5c\uff0c\u514d\u5f97\u6210\u4e3a\u4f60\u4eec\u4efb\u4f55\u4eba\u7684\u8d1f\u62c5\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6211\u4eec\u5e76\u4e0d\u662f\u6ca1\u6709\u6743\u5229\u63a5\u53d7\u4f9b\u5e94\uff0c\u800c\u662f\u8981\u7ed9\u4f60\u4eec\u6811\u7acb\u5b66\u4e60\u7684\u699c\u6837\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u4eec\u5728\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u66fe\u5429\u5490\u8fc7\u4f60\u4eec\uff1a\u4e0d\u80af\u5de5\u4f5c\u7684\u4eba\u6ca1\u6709\u8d44\u683c\u5403\u996d\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u4eec\u542c\u8bf4\u5728\u4f60\u4eec\u5f53\u4e2d\u6709\u4e9b\u4eba\u6e38\u624b\u597d\u95f2\uff0c\u65e0\u6240\u4e8b\u4e8b\uff0c\u5374\u4e13\u7ba1\u95f2\u4e8b\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u4eec\u5949\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u540d\u5429\u5490\u5e76\u529d\u8beb\u8fd9\u4e9b\u4eba\u8981\u5b89\u5206\u5b88\u5df1\uff0c\u81ea\u98df\u5176\u529b\u3002"
                },
