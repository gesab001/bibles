            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u95ee\u5019 - \u6211\u4fdd\u7f57\u505a\u4e0a\u5e1d\u7684\u5974\u4ec6\u548c\u8036\u7a23\u57fa\u7763\u7684\u4f7f\u5f92\uff0c\u662f\u4e3a\u4e86\u4f7f\u4e0a\u5e1d\u62e3\u9009\u7684\u4eba\u6709\u4fe1\u5fc3\u3001\u660e\u767d\u771f\u7406\uff0c\u4ece\u800c\u8fc7\u656c\u8654\u7684\u751f\u6d3b\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6709\u6c38\u751f\u7684\u76fc\u671b\u3002\u8fd9\u6c38\u751f\u662f\u4ece\u4e0d\u8bf4\u8c0e\u7684\u4e0a\u5e1d\u5728\u4e98\u53e4\u4ee5\u524d\u5e94\u8bb8\u7684\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u7942\u5728\u6240\u5b9a\u7684\u65f6\u5019\uff0c\u501f\u7740\u4eba\u7684\u4f20\u626c\u5c06\u81ea\u5df1\u7684\u9053\u663e\u660e\u51fa\u6765\u3002\u6211\u5949\u6211\u4eec\u6551\u4e3b\u4e0a\u5e1d\u7684\u547d\u4ee4\u4f20\u626c\u8fd9\u9053\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u63d0\u591a\u554a\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\uff0c\u5c31\u6211\u4eec\u5171\u540c\u7684\u4fe1\u4ef0\u6765\u8bf4\uff0c\u4f60\u662f\u6211\u771f\u6b63\u7684\u513f\u5b50\u3002 \u613f\u7236\u4e0a\u5e1d\u548c\u6211\u4eec\u7684\u6551\u4e3b\u57fa\u7763\u8036\u7a23\u8d50\u7ed9\u4f60\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u957f\u8001\u7684\u8d44\u683c - \u5f53\u521d\uff0c\u6211\u628a\u4f60\u7559\u5728\u514b\u91cc\u7279\u5c9b\uff0c\u662f\u8981\u4f60\u5b8c\u6210\u672a\u529e\u5b8c\u7684\u4e8b\u5de5\uff0c\u5e76\u7167\u6211\u7684\u6307\u793a\u5728\u5404\u57ce\u9009\u7acb\u957f\u8001\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u505a\u957f\u8001\u7684\u5fc5\u987b\u65e0\u53ef\u6307\u8d23\uff0c\u53ea\u6709\u4e00\u4f4d\u59bb\u5b50\uff0c\u513f\u5973\u90fd\u4fe1\u4e3b\u3001\u6ca1\u6709\u653e\u8361\u4e0d\u7f81\u7684\u884c\u4e3a\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8eab\u4e3a\u4e0a\u5e1d\u7684\u7ba1\u5bb6\uff0c\u505a\u76d1\u7763\u7684\u5fc5\u987b\u65e0\u53ef\u6307\u8d23\uff0c\u4e0d\u50b2\u6162\uff0c\u4e0d\u66b4\u8e81\uff0c\u4e0d\u9157\u9152\uff0c\u4e0d\u597d\u6597\uff0c\u4e0d\u8d2a\u8d22\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ed6\u5fc5\u987b\u597d\u5ba2\u3001\u4e50\u5584\u3001\u81ea\u5236\u3001\u6b63\u76f4\u3001\u5723\u6d01\u3001\u81ea\u5f8b\uff0c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6301\u5b88\u6240\u9886\u53d7\u7684\u771f\u9053\uff0c\u4ee5\u4fbf\u80fd\u591f\u4ee5\u7eaf\u6b63\u7684\u6559\u5bfc\u529d\u52c9\u4eba\uff0c\u9a73\u5012\u90a3\u4e9b\u53cd\u5bf9\u7684\u4eba\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u56e0\u4e3a\u6709\u8bb8\u591a\u6096\u9006\u4e4b\u4eba\u559c\u6b22\u7a7a\u8c08\uff0c\u5584\u4e8e\u6b3a\u9a97\uff0c\u5c24\u5176\u662f\u90a3\u4e9b\u5949\u884c\u5272\u793c\u7684\u4eba\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4f60\u4e00\u5b9a\u8981\u5835\u4f4f\u4ed6\u4eec\u7684\u53e3\uff0c\u56e0\u4e3a\u4ed6\u4eec\u4e3a\u4e86\u4e0d\u4e49\u4e4b\u8d22\u6559\u4e0d\u8be5\u6559\u7684\uff0c\u8bef\u5bfc\u4e86\u522b\u4eba\u5168\u5bb6\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u514b\u91cc\u7279\u4eba\u81ea\u5df1\u7684\u4e00\u4f4d\u5148\u77e5\u8bf4\uff1a\u201c\u514b\u91cc\u7279\u4eba\u8bf4\u8c0e\u6210\u6027\uff0c\u90aa\u6076\u5982\u517d\uff0c\u597d\u5403\u61d2\u505a\u3002\u201d"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8fd9\u8bdd\u662f\u771f\u7684\u3002\u56e0\u6b64\u4f60\u8981\u4e25\u6b63\u5730\u65a5\u8d23\u4ed6\u4eec\uff0c\u4f7f\u4ed6\u4eec\u6709\u7eaf\u6b63\u7684\u4fe1\u4ef0\uff0c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e0d\u7406\u4f1a\u72b9\u592a\u4eba\u8352\u8bde\u7684\u4f20\u8bf4\u548c\u90a3\u4e9b\u8fdd\u80cc\u771f\u7406\u4e4b\u4eba\u7684\u8beb\u5f8b\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u5bf9\u6d01\u51c0\u7684\u4eba\u800c\u8a00\uff0c\u4e00\u5207\u90fd\u662f\u6d01\u51c0\u7684\uff1b\u5bf9\u6c61\u79fd\u4e0d\u4fe1\u7684\u4eba\u800c\u8a00\uff0c\u4ec0\u4e48\u90fd\u4e0d\u6d01\u51c0\uff0c\u8fde\u4ed6\u4eec\u7684\u601d\u60f3\u548c\u826f\u5fc3\u90fd\u662f\u6c61\u79fd\u7684\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4ed6\u4eec\u81ea\u79f0\u8ba4\u8bc6\u4e0a\u5e1d\uff0c\u5728\u884c\u4e3a\u4e0a\u5374\u5426\u8ba4\u7942\u3002\u4ed6\u4eec\u4ee4\u4eba\u53ef\u618e\uff0c\u6096\u9006\u6210\u6027\uff0c\u4e00\u65e0\u662f\u5904\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6559\u5bfc\u7eaf\u6b63\u7684\u6559\u4e49 - \u4f60\u7684\u6559\u5bfc\u8981\u7b26\u5408\u7eaf\u6b63\u7684\u6559\u4e49\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u529d\u5e74\u957f\u7684\u7537\u5b50\u8981\u8282\u5236\u3001\u5e84\u91cd\u3001\u81ea\u5f8b\uff0c\u6709\u5168\u5907\u7684\u4fe1\u5fc3\u3001\u7231\u5fc3\u548c\u8010\u5fc3\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u529d\u5e74\u957f\u7684\u5987\u5973\u8981\u8fc7\u656c\u8654\u7684\u751f\u6d3b\uff0c\u4e0d\u8bf4\u957f\u9053\u77ed\uff0c\u4e0d\u505a\u9152\u7684\u5974\u96b6\uff0c\u8981\u4ee5\u8eab\u4f5c\u5219\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u8fd9\u6837\uff0c\u5979\u4eec\u5c31\u80fd\u6559\u5bfc\u5e74\u8f7b\u7684\u5987\u5973\u7231\u4e08\u592b\u3001\u7231\u513f\u5973\u3001"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u81ea\u5236\u3001\u8d1e\u6d01\uff0c\u6301\u5bb6\u3001\u548c\u5584\u3001\u987a\u670d\u4e08\u592b\uff0c\u514d\u5f97\u4e0a\u5e1d\u7684\u9053\u88ab\u4eba\u6bc1\u8c24\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u529d\u52c9\u5e74\u8f7b\u7684\u7537\u5b50\u8981\u81ea\u5236\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4f60\u81ea\u5df1\u8981\u5728\u5404\u6837\u7684\u5584\u884c\u4e0a\u505a\u4f17\u4eba\u7684\u699c\u6837\uff0c\u5728\u6559\u5bfc\u4e0a\u8981\u8bda\u6073\u3001\u8ba4\u771f\u3001"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u8a00\u8bed\u7eaf\u5168\u3001\u65e0\u53ef\u6307\u8d23\uff0c\u8ba9\u90a3\u4e9b\u53cd\u5bf9\u7684\u4eba\u65e0\u8bdd\u53ef\u8bf4\u3001\u81ea\u89c9\u7f9e\u6127\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u529d\u52c9\u505a\u5974\u4ec6\u7684\u8981\u51e1\u4e8b\u987a\u670d\u4e3b\u4eba\uff0c\u8ba9\u4e3b\u4eba\u6ee1\u610f\uff0c\u4e0d\u9876\u649e\u4e3b\u4eba\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4e0d\u5077\u62ff\u4e3b\u4eba\u7684\u4e1c\u897f\uff0c\u4e3a\u4eba\u8981\u5fe0\u4fe1\u53ef\u9760\uff0c\u597d\u51e1\u4e8b\u5c0a\u5d07\u6211\u4eec\u6551\u4e3b\u4e0a\u5e1d\u7684\u6559\u5bfc\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u4e3a\u4e0a\u5e1d\u62ef\u6551\u4e16\u4eba\u7684\u6069\u5178\u5df2\u7ecf\u663e\u660e\u4e86\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8fd9\u6069\u5178\u6559\u5bfc\u6211\u4eec\u9664\u6389\u4e0d\u656c\u8654\u7684\u5fc3\u548c\u4e16\u4fd7\u7684\u79c1\u6b32\uff0c\u5728\u4eca\u4e16\u8fc7\u81ea\u5f8b\u3001\u516c\u4e49\u3001\u656c\u754f\u4e0a\u5e1d\u7684\u751f\u6d3b\uff0c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u6000\u7740\u7f8e\u597d\u7684\u76fc\u671b\u7b49\u5019\u8036\u7a23\u57fa\u7763\u2014\u2014\u6211\u4eec\u4f1f\u5927\u7684\u4e0a\u5e1d\u548c\u6551\u4e3b\u7684\u8363\u8000\u663e\u73b0\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e3b\u8036\u7a23\u4e3a\u6211\u4eec\u727a\u7272\u81ea\u5df1\uff0c\u8981\u6551\u8d4e\u6211\u4eec\u8131\u79bb\u4e00\u5207\u7f6a\u6076\uff0c\u5e76\u6d01\u51c0\u6211\u4eec\uff0c\u4f7f\u6211\u4eec\u505a\u7942\u7684\u5b50\u6c11\uff0c\u6210\u4e3a\u70ed\u5fc3\u884c\u5584\u7684\u4eba\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u4f60\u8981\u8bb2\u660e\u8fd9\u4e9b\u4e8b\uff0c\u8fd0\u7528\u5404\u6837\u6743\u67c4\u529d\u52c9\u4eba\u3001\u8d23\u5907\u4eba\uff0c\u522b\u8ba9\u4eba\u8f7b\u770b\u4f60\u3002"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4e13\u5fc3\u884c\u5584 - \u4f60\u8981\u63d0\u9192\u4f17\u4eba\u987a\u670d\u6267\u653f\u638c\u6743\u8005\uff0c\u9075\u5b88\u6cd5\u4ee4\uff0c\u968f\u65f6\u51c6\u5907\u505a\u5584\u4e8b\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4e0d\u8981\u6bc1\u8c24\uff0c\u4e0d\u8981\u4e89\u5435\uff0c\u8981\u8c26\u548c\u3001\u606d\u656c\u5730\u5bf9\u5f85\u6bcf\u4e00\u4e2a\u4eba\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4ece\u524d\u6211\u4eec\u4e5f\u611a\u8822\u3001\u6096\u9006\u3001\u6613\u53d7\u8ff7\u60d1\uff0c\u53d7\u5404\u79cd\u79c1\u6b32\u548c\u4eab\u4e50\u7684\u9a71\u4f7f\uff0c\u5fc3\u91cc\u5145\u6ee1\u4e86\u5ac9\u5992\u548c\u6076\u5ff5\uff0c\u4ee4\u4eba\u618e\u6076\uff0c\u4e5f\u5f7c\u6b64\u618e\u6068\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4f46\u6211\u4eec\u7684\u6551\u4e3b\u4e0a\u5e1d\u5411\u4eba\u5f70\u663e\u4e86\u7942\u7684\u6069\u6148\u548c\u4ec1\u7231\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u501f\u7740\u91cd\u751f\u4e4b\u6d17\u548c\u5723\u7075\u7684\u66f4\u65b0\u62ef\u6551\u4e86\u6211\u4eec\uff0c\u8fd9\u5e76\u975e\u56e0\u4e3a\u6211\u4eec\u6709\u4e49\u884c\uff0c\u800c\u662f\u56e0\u4e3a\u7942\u7684\u601c\u60af\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e0a\u5e1d\u501f\u7740\u6211\u4eec\u7684\u6551\u4e3b\u8036\u7a23\u57fa\u7763\u628a\u5723\u7075\u4e30\u4e30\u5bcc\u5bcc\u5730\u6d47\u704c\u5728\u6211\u4eec\u8eab\u4e0a\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8fd9\u6837\uff0c\u6211\u4eec\u65e2\u7136\u9760\u7740\u7942\u7684\u6069\u5178\u800c\u88ab\u79f0\u4e3a\u4e49\u4eba\uff0c\u5c31\u53ef\u4ee5\u6210\u4e3a\u540e\u55e3\uff0c\u6709\u6c38\u751f\u7684\u76fc\u671b\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ee5\u4e0a\u8fd9\u4e9b\u8bdd\u662f\u53ef\u4fe1\u7684\uff0c\u5e0c\u671b\u4f60\u8ba4\u771f\u6559\u5bfc\uff0c\u4f7f\u4fe1\u4e0a\u5e1d\u7684\u4eba\u4e13\u5fc3\u884c\u5584\u3002\u8fd9\u90fd\u662f\u9020\u798f\u4f17\u4eba\u7684\u7f8e\u4e8b\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8981\u907f\u514d\u611a\u6627\u7684\u4e89\u8bba\u3001\u6709\u5173\u5bb6\u8c31\u7684\u8fa9\u9a73\u548c\u5f8b\u6cd5\u4e0a\u7684\u4e89\u6267\uff0c\u56e0\u4e3a\u8fd9\u4e9b\u6beb\u65e0\u76ca\u5904\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5bf9\u4e8e\u5236\u9020\u5206\u88c2\u7684\u4eba\uff0c\u8b66\u544a\u8fc7\u4e00\u4e24\u6b21\u540e\uff0c\u8981\u548c\u4ed6\u65ad\u7edd\u6765\u5f80\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u4e3a\u4f60\u77e5\u9053\u8fd9\u79cd\u4eba\u5df2\u7ecf\u80cc\u9053\u72af\u7f6a\uff0c\u81ea\u5b9a\u5df1\u7f6a\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4fe1\u672b\u7684\u5631\u5490\u4e0e\u95ee\u5019 - \u6211\u4f1a\u6d3e\u4e9a\u63d0\u9a6c\u6216\u8005\u63a8\u57fa\u53e4\u53bb\u89c1\u4f60\u3002\u5230\u65f6\u5019\uff0c\u4f60\u8981\u7acb\u523b\u5230\u5c3c\u54e5\u6ce2\u7acb\u6765\u89c1\u6211\uff0c\u56e0\u4e3a\u6211\u51b3\u5b9a\u5728\u90a3\u91cc\u8fc7\u51ac\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u8981\u4e3a\u897f\u7eb3\u5f8b\u5e08\u548c\u4e9a\u6ce2\u7f57\u9001\u884c\uff0c\u5c3d\u529b\u5e2e\u52a9\u4ed6\u4eec\uff0c\u8ba9\u4ed6\u4eec\u8def\u4e0a\u4e00\u65e0\u6240\u7f3a\u3002"
                },
                "14": {
