                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211 \u5011 \u611f \u8b1d \u795e \u3001 \u6211 \u5011 \u4e3b \u8036 \u7a4c \u57fa \u7763 \u7684 \u7236 \uff0c \u5e38 \u5e38 \u70ba \u4f60 \u5011 \u79b1 \u544a \uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u56e0 \u807d \u898b \u4f60 \u5011 \u5728 \u57fa \u7763 \u8036 \u7a4c \u88e1 \u7684 \u4fe1 \u5fc3 \uff0c \u4e26 \u5411 \u773e \u8056 \u5f92 \u7684 \u611b \u5fc3 \uff0c\n"
