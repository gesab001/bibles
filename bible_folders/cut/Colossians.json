                "6": {
                    "verse_nr": 6,
                    "verse": "\u9019 \u798f \u97f3 \u50b3 \u5230 \u4f60 \u5011 \u90a3 \u88e1 \uff0c \u4e5f \u50b3 \u5230 \u666e \u5929 \u4e4b \u4e0b \uff0c \u4e26 \u4e14 \u7d50 \u679c \uff0c \u589e \u9577 \uff0c \u5982 \u540c \u5728 \u4f60 \u5011 \u4e2d \u9593 \uff0c \u81ea \u5f9e \u4f60 \u5011 \u807d \u898b \u798f \u97f3 \uff0c \u771f \u77e5 \u9053 \u795e \u6069 \u60e0 \u7684 \u65e5 \u5b50 \u4e00 \u6a23 \u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6b63 \u5982 \u4f60 \u5011 \u5f9e \u6211 \u5011 \u6240 \u89aa \u611b \u3001 \u4e00 \u540c \u4f5c \u50d5 \u4eba \u7684 \u4ee5 \u5df4 \u5f17 \u6240 \u5b78 \u7684 \u3002 \u4ed6 \u70ba \u6211 \u5011 \uff08 \u6709 \u53e4 \u5377 \uff1a \u4f60 \u5011 \uff09 \u4f5c \u4e86 \u57fa \u7763 \u5fe0 \u5fc3 \u7684 \u57f7 \u4e8b \uff0c\n"
