                "8": {
                    "verse_nr": 8,
                    "verse": "\u4e5f \u628a \u4f60 \u5011 \u56e0 \u8056 \u9748 \u6240 \u5b58 \u7684 \u611b \u5fc3 \u544a \u8a34 \u4e86 \u6211 \u5011 \u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u56e0 \u6b64 \uff0c \u6211 \u5011 \u81ea \u5f9e \u807d \u898b \u7684 \u65e5 \u5b50 \uff0c \u4e5f \u5c31 \u70ba \u4f60 \u5011 \u4e0d \u4f4f \u7684 \u79b1 \u544a \u7948 \u6c42 \uff0c \u9858 \u4f60 \u5011 \u5728 \u4e00 \u5207 \u5c6c \u9748 \u7684 \u667a \u6167 \u609f \u6027 \u4e0a \uff0c \u6eff \u5fc3 \u77e5 \u9053 \u795e \u7684 \u65e8 \u610f \uff1b\n"
