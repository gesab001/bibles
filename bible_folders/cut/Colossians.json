                "1": {
                    "verse_nr": 1,
                    "verse": "\u5949 \u795e \u65e8 \u610f \uff0c \u4f5c \u57fa \u7763 \u8036 \u7a4c \u4f7f \u5f92 \u7684 \u4fdd \u7f85 \u548c \u5144 \u5f1f \u63d0 \u6469 \u592a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5beb \u4fe1 \u7d66 \u6b4c \u7f85 \u897f \u7684 \u8056 \u5f92 \uff0c \u5728 \u57fa \u7763 \u88e1 \u6709 \u5fe0 \u5fc3 \u7684 \u5f1f \u5144 \u3002 \u9858 \u6069 \u60e0 \u3001 \u5e73 \u5b89 \u5f9e \u795e \u6211 \u5011 \u7684 \u7236 \u6b78 \u8207 \u4f60 \u5011 \uff01\n"
