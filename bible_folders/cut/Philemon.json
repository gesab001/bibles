        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u70ba \u57fa \u7763 \u8036 \u7a4c \u88ab \u56da \u7684 \u4fdd \u7f85 \uff0c \u540c \u5144 \u5f1f \u63d0 \u6469 \u592a \u5beb \u4fe1 \u7d66 \u6211 \u5011 \u6240 \u89aa \u611b \u7684 \u540c \u5de5 \u8153 \u5229 \u9580 \uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u548c \u59b9 \u5b50 \u4e9e \u8153 \u4e9e \u4e26 \u8207 \u6211 \u5011 \u540c \u7576 \u5175 \u7684 \u4e9e \u57fa \u5e03 \uff0c \u4ee5 \u53ca \u5728 \u4f60 \u5bb6 \u7684 \u6559 \u6703 \u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u9858 \u6069 \u60e0 \u3001 \u5e73 \u5b89 \u5f9e \u795e \u6211 \u5011 \u7684 \u7236 \u548c \u4e3b \u8036 \u7a4c \u57fa \u7763 \u6b78 \u8207 \u4f60 \u5011 \uff01\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211 \u79b1 \u544a \u7684 \u6642 \u5019 \u63d0 \u5230 \u4f60 \uff0c \u5e38 \u70ba \u4f60 \u611f \u8b1d \u6211 \u7684 \u795e \uff1b\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0 \u807d \u8aaa \u4f60 \u7684 \u611b \u5fc3 \u4e26 \u4f60 \u5411 \u4e3b \u8036 \u7a4c \u548c \u773e \u8056 \u5f92 \u7684 \u4fe1 \u5fc3 \uff08 \u6216 \u4f5c \uff1a \u56e0 \u807d \u8aaa \u4f60 \u5411 \u4e3b \u8036 \u7a4c \u548c \u773e \u8056 \u5f92 \u6709 \u611b \u5fc3 \u6709 \u4fe1 \u5fc3 \uff09 \u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u9858 \u4f60 \u8207 \u4eba \u6240 \u540c \u6709 \u7684 \u4fe1 \u5fc3 \u986f \u51fa \u529f \u6548 \uff0c \u4f7f \u4eba \u77e5 \u9053 \u4f60 \u5011 \u5404 \u6a23 \u5584 \u4e8b \u90fd \u662f \u70ba \u57fa \u7763 \u505a \u7684 \u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5144 \u5f1f \u963f \uff0c \u6211 \u70ba \u4f60 \u7684 \u611b \u5fc3 \uff0c \u5927 \u6709 \u5feb \u6a02 \uff0c \u5927 \u5f97 \u5b89 \u6170 \uff0c \u56e0 \u773e \u8056 \u5f92 \u7684 \u5fc3 \u5f9e \u4f60 \u5f97 \u4e86 \u66a2 \u5feb \u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211 \u96d6 \u7136 \u9760 \u8457 \u57fa \u7763 \u80fd \u653e \u81bd \u5429 \u5490 \u4f60 \u5408 \u5b9c \u7684 \u4e8b \uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7136 \u800c \u50cf \u6211 \u9019 \u6709 \u5e74 \u7d00 \u7684 \u4fdd \u7f85 \uff0c \u73fe \u5728 \u53c8 \u662f \u70ba \u57fa \u7763 \u8036 \u7a4c \u88ab \u56da \u7684 \uff0c \u5be7 \u53ef \u6191 \u8457 \u611b \u5fc3 \u6c42 \u4f60 \uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5c31 \u662f \u70ba \u6211 \u5728 \u6346 \u9396 \u4e2d \u6240 \u751f \u7684 \u5152 \u5b50 \u963f \u5c3c \u897f \u6bcd \uff08 \u5c31 \u662f \u6709 \u76ca \u8655 \u7684 \u610f \u601d \uff09 \u6c42 \u4f60 \u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6 \u5f9e \u524d \u8207 \u4f60 \u6c92 \u6709 \u76ca \u8655 \uff0c \u4f46 \u5982 \u4eca \u8207 \u4f60 \u6211 \u90fd \u6709 \u76ca \u8655 \u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211 \u73fe \u5728 \u6253 \u767c \u4ed6 \u89aa \u81ea \u56de \u4f60 \u90a3 \u88e1 \u53bb \uff0c \u4ed6 \u662f \u6211 \u5fc3 \u4e0a \u7684 \u4eba \u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211 \u672c \u4f86 \u6709 \u610f \u5c07 \u4ed6 \u7559 \u4e0b \uff0c \u5728 \u6211 \u70ba \u798f \u97f3 \u6240 \u53d7 \u7684 \u6346 \u9396 \u4e2d \u66ff \u4f60 \u4f3a \u5019 \u6211 \u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f46 \u4e0d \u77e5 \u9053 \u4f60 \u7684 \u610f \u601d \uff0c \u6211 \u5c31 \u4e0d \u9858 \u610f \u9019 \u6a23 \u884c \uff0c \u53eb \u4f60 \u7684 \u5584 \u884c \u4e0d \u662f \u51fa \u65bc \u52c9 \u5f37 \uff0c \u4e43 \u662f \u51fa \u65bc \u7518 \u5fc3 \u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4ed6 \u66ab \u6642 \u96e2 \u958b \u4f60 \uff0c \u6216 \u8005 \u662f \u53eb \u4f60 \u6c38 \u9060 \u5f97 \u8457 \u4ed6 \uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4e0d \u518d \u662f \u5974 \u50d5 \uff0c \u4e43 \u662f \u9ad8 \u904e \u5974 \u50d5 \uff0c \u662f \u89aa \u611b \u7684 \u5144 \u5f1f \u3002 \u5728 \u6211 \u5be6 \u5728 \u662f \u5982 \u6b64 \uff0c \u4f55 \u6cc1 \u5728 \u4f60 \u5462 \uff01 \u9019 \u4e5f \u4e0d \u62d8 \u662f \u6309 \u8089 \u9ad4 \u8aaa \uff0c \u662f \u6309 \u4e3b \u8aaa \uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60 \u82e5 \u4ee5 \u6211 \u70ba \u540c \u4f34 \uff0c \u5c31 \u6536 \u7d0d \u4ed6 \uff0c \u5982 \u540c \u6536 \u7d0d \u6211 \u4e00 \u6a23 \u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6 \u82e5 \u8667 \u8ca0 \u4f60 \uff0c \u6216 \u6b20 \u4f60 \u751a \u9ebc \uff0c \u90fd \u6b78 \u5728 \u6211 \u7684 \u8cec \u4e0a \uff0c\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6211 \u5fc5 \u511f \u9084 \u3002 \u9019 \u662f \u6211 \u4fdd \u7f85 \u89aa \u7b46 \u5beb \u7684 \u3002 \u6211 \u4e26 \u4e0d \u7528 \u5c0d \u4f60 \u8aaa \uff0c \u9023 \u4f60 \u81ea \u5df1 \u4e5f \u662f \u8667 \u6b20 \u65bc \u6211 \u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5144 \u5f1f \u963f \uff0c \u671b \u4f60 \u4f7f \u6211 \u5728 \u4e3b \u88e1 \u56e0 \u4f60 \u5f97 \u5feb \u6a02 \uff08 \u6216 \u4f5c \uff1a \u76ca \u8655 \uff09 \u4e26 \u671b \u4f60 \u4f7f \u6211 \u7684 \u5fc3 \u5728 \u57fa \u7763 \u88e1 \u5f97 \u66a2 \u5feb \u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211 \u5beb \u4fe1 \u7d66 \u4f60 \uff0c \u6df1 \u4fe1 \u4f60 \u5fc5 \u9806 \u670d \uff0c \u77e5 \u9053 \u4f60 \u6240 \u8981 \u884c \u7684 \uff0c \u5fc5 \u904e \u65bc \u6211 \u6240 \u8aaa \u7684 \u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u6b64 \u5916 \u4f60 \u9084 \u8981 \u7d66 \u6211 \u9810 \u5099 \u4f4f \u8655 \uff1b \u56e0 \u70ba \u6211 \u76fc \u671b \u85c9 \u8457 \u4f60 \u5011 \u7684 \u79b1 \u544a \uff0c \u5fc5 \u8499 \u6069 \u5230 \u4f60 \u5011 \u90a3 \u88e1 \u53bb \u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u70ba \u57fa \u7763 \u8036 \u7a4c \u8207 \u6211 \u540c \u5750 \u76e3 \u7684 \u4ee5 \u5df4 \u5f17 \u554f \u4f60 \u5b89 \u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u8207 \u6211 \u540c \u5de5 \u7684 \u99ac \u53ef \u3001 \u4e9e \u91cc \u9054 \u53e4 \u3001 \u5e95 \u99ac \u3001 \u8def \u52a0 \u3001 \u4e5f \u90fd \u554f \u4f60 \u5b89 \u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u9858 \u6211 \u5011 \u4e3b \u8036 \u7a4c \u57fa \u7763 \u7684 \u6069 \u5e38 \u5728 \u4f60 \u7684 \u5fc3 \u88e1 \u3002 \u963f \u5011 \uff01\n"
                }
            }
        }
    }
