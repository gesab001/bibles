    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036 \u7a4c \u57fa \u7763 \u7684 \u50d5 \u4eba \uff0c \u96c5 \u5404 \u7684 \u5f1f \u5144 \u7336 \u5927 \uff0c \u5beb \u4fe1 \u7d66 \u90a3  \u88ab \u53ec \u3001 \u5728 \u7236 \u795e \u88e1  \u8499 \u611b \u3001 \u70ba \u8036 \u7a4c \u57fa \u7763 \u4fdd \u5b88 \u7684 \u4eba \u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u9858 \u6190 \u6064 \u3001 \u5e73 \u5b89 \u3001 \u6148 \u611b \u591a \u591a \u7684 \u52a0 \u7d66 \u4f60 \u5011 \u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u89aa \u611b \u7684 \u5f1f \u5144 \u963f \uff0c \u6211 \u60f3 \u76e1 \u5fc3 \u5beb \u4fe1 \u7d66 \u4f60 \u5011 \uff0c \u8ad6 \u6211 \u5011 \u540c \u5f97 \u6551 \u6069 \u7684 \u6642 \u5019 \uff0c \u5c31 \u4e0d \u5f97 \u4e0d \u5beb \u4fe1 \u52f8 \u4f60 \u5011 \uff0c \u8981 \u70ba \u5f9e \u524d \u4e00 \u6b21 \u4ea4 \u4ed8 \u8056 \u5f92 \u7684 \u771f \u9053 \u7aed \u529b \u7684 \u722d \u8faf \u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u56e0 \u70ba \u6709 \u4e9b \u4eba \u5077 \u8457 \u9032 \u4f86 \uff0c \u5c31 \u662f \u81ea \u53e4 \u88ab \u5b9a \u53d7 \u5211 \u7f70 \u7684 \uff0c \u662f \u4e0d \u8654 \u8aa0 \u7684 \uff0c \u5c07  \u6211 \u5011 \u795e \u7684 \u6069 \u8b8a  \u653e \u7e31 \u60c5 \u617e \u7684 \u6a5f \u6703 \uff0c \u4e26 \u4e14 \u4e0d \u8a8d \u7368 \u4e00 \u7684 \u4e3b \u5bb0 \u2500 \u6211 \u5011 \uff08 \u6216 \u4f5c \u548c \u6211 \u5011 \uff09 \u4e3b \u8036 \u7a4c \u57fa \u7763 \u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5f9e \u524d \u4e3b \u6551 \u4e86 \u4ed6 \u7684 \u767e \u59d3 \u51fa \u57c3 \u53ca \u5730 \uff0c \u5f8c \u4f86  \u5c31 \u628a \u90a3 \u4e9b  \u4e0d \u4fe1 \u7684 \u6ec5 \u7d55 \u4e86 \u3002 \u9019 \u4e00 \u5207 \u7684 \u4e8b \uff0c \u4f60 \u5011 \u96d6 \u7136 \u90fd \u77e5 \u9053 \uff0c \u6211 \u537b \u4ecd \u8981 \u63d0 \u9192 \u4f60 \u5011 \u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u53c8 \u6709  \u4e0d \u5b88 \u672c \u4f4d \u3001 \u96e2 \u958b \u81ea \u5df1 \u4f4f \u8655 \u7684 \u5929 \u4f7f \uff0c \u4e3b \u7528 \u9396 \u934a \u628a \u4ed6 \u5011 \u6c38 \u9060 \u62d8 \u7559 \u5728 \u9ed1 \u6697 \u88e1 \uff0c \u7b49 \u5019 \u5927 \u65e5 \u7684 \u5be9 \u5224 \u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u53c8 \u5982 \u6240 \u591a \u746a \u3001 \u86fe \u6469 \u62c9 \u548c \u5468 \u570d \u57ce \u9091 \u7684 \u4eba \u4e5f \u7167 \u4ed6 \u5011 \u4e00 \u5473 \u7684 \u884c \u6deb \uff0c \u96a8 \u5f9e \u9006 \u6027 \u7684 \u60c5 \u617e \uff0c \u5c31 \u53d7 \u6c38 \u706b \u7684 \u5211 \u7f70 \uff0c \u4f5c \u70ba \u9451 \u6212 \u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u9019 \u4e9b \u505a \u5922 \u7684 \u4eba \u4e5f \u50cf \u4ed6 \u5011 \u6c61 \u7a62 \u8eab \u9ad4 \uff0c \u8f15 \u6162 \u4e3b \u6cbb \u7684 \uff0c \u6bc0 \u8b17 \u5728 \u5c0a \u4f4d \u7684 \u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5929 \u4f7f \u9577 \u7c73 \u8fe6 \u52d2 { \u70ba \u6469 \u897f \u7684 \u5c4d \u9996 \u8207 \u9b54 \u9b3c \u722d \u8faf \u7684 \u6642 \u5019 \uff0c \u5c1a \u4e14 \u4e0d \u6562 \u7528 \u6bc0 \u8b17 \u7684 \u8a71 \u7f6a \u8cac \u4ed6 \uff0c \u53ea \u8aaa \uff1a \u4e3b \u8cac \u5099 \u4f60 \u7f77 \uff01\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f46 \u9019 \u4e9b \u4eba \u6bc0 \u8b17 \u4ed6 \u5011 \u6240 \u4e0d \u77e5 \u9053 \u7684 \u3002 \u4ed6 \u5011 \u672c \u6027 \u6240 \u77e5 \u9053 \u7684 \u4e8b \u8207 \u90a3 \u6c92 \u6709 \u9748 \u6027 \u7684 \u755c \u985e \u4e00 \u6a23 \uff0c \u5728 \u9019 \u4e8b \u4e0a \u7adf \u6557 \u58de \u4e86 \u81ea \u5df1 \u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6 \u5011 \u6709 \u798d \u4e86 \uff01 \u56e0 \u70ba \u8d70 \u4e86 \u8a72 \u96b1 \u7684 \u9053 \u8def \uff0c \u53c8 \u70ba \u5229 \u5f80 \u5df4 \u862d \u7684 \u932f \u8b2c \u88e1 \u76f4 \u5954 \uff0c \u4e26 \u5728 \u53ef \u62c9 \u7684 \u80cc \u53db \u4e2d \u6ec5 \u4ea1 \u4e86 \u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u9019 \u6a23 \u7684 \u4eba \u5728 \u4f60 \u5011 \u7684 \u611b \u5e2d \u4e0a \u8207 \u4f60 \u5011 \u540c \u5403 \u7684 \u6642 \u5019 \uff0c \u6b63 \u662f \u7901 \u77f3 \uff08 \u6216 \u4f5c \uff1a \u73b7 \u6c61 \uff09 \u3002 \u4ed6 \u5011 \u4f5c \u7267 \u4eba \uff0c \u53ea \u77e5 \u9927 \u990a \u81ea \u5df1 \uff0c \u7121 \u6240 \u61fc \u6015 \uff1b \u662f \u6c92 \u6709 \u96e8 \u7684 \u96f2 \u5f69 \uff0c \u88ab \u98a8 \u98c4 \u8569 \uff1b \u662f \u79cb \u5929 \u6c92 \u6709 \u679c \u5b50 \u7684 \u6a39 \uff0c \u6b7b \u800c \u53c8 \u6b7b \uff0c \u9023 \u6839 \u88ab \u62d4 \u51fa \u4f86 \uff1b\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u662f \u6d77 \u88e1 \u7684 \u72c2 \u6d6a \uff0c \u6e67 \u51fa \u81ea \u5df1 \u53ef \u6065 \u7684 \u6cab \u5b50 \u4f86 \uff1b \u662f \u6d41 \u8569 \u7684 \u661f \uff0c \u6709 \u58a8 \u9ed1 \u7684 \u5e7d \u6697 \u70ba \u4ed6 \u5011 \u6c38 \u9060 \u5b58 \u7559 \u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e9e \u7576 \u7684 \u4e03 \u4e16 \u5b6b \u4ee5 \u8afe \uff0c \u66fe \u9810 \u8a00 \u9019 \u4e9b \u4eba \u8aaa \uff1a \u770b \u54ea \uff0c \u4e3b \u5e36 \u8457 \u4ed6 \u7684 \u5343 \u842c \u8056 \u8005 \u964d \u81e8 \uff0c\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8981 \u5728 \u773e \u4eba \u8eab \u4e0a \u884c \u5be9 \u5224 \uff0c \u8b49 \u5be6 \u90a3 \u4e00 \u5207 \u4e0d \u656c \u8654 \u7684 \u4eba \uff0c \u6240 \u5984 \u884c \u4e00 \u5207 \u4e0d \u656c \u8654 \u7684 \u4e8b \uff0c \u53c8 \u8b49 \u5be6 \u4e0d \u656c \u8654 \u4e4b \u7f6a \u4eba \u6240 \u8aaa \u9802 \u649e \u4ed6 \u7684 \u525b \u610e \u8a71 \u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u9019 \u4e9b \u4eba \u662f \u79c1 \u4e0b \u8b70 \u8ad6 \uff0c \u5e38 \u767c \u6028 \u8a00 \u7684 \uff0c \u96a8 \u5f9e \u81ea \u5df1 \u7684 \u60c5 \u617e \u800c \u884c \uff0c \u53e3 \u4e2d \u8aaa \u8a87 \u5927 \u7684 \u8a71 \uff0c \u70ba \u5f97 \u4fbf \u5b9c \u8ac2 \u5a9a \u4eba \u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u89aa \u611b \u7684 \u5f1f \u5144 \u963f \uff0c \u4f60 \u5011 \u8981 \u8a18 \u5ff5 \u6211 \u5011 \u4e3b \u8036 \u7a4c \u57fa \u7763 \u4e4b \u4f7f \u5f92 \u5f9e \u524d \u6240 \u8aaa \u7684 \u8a71 \u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6 \u5011 \u66fe \u5c0d \u4f60 \u5011 \u8aaa \u904e \uff0c \u672b \u4e16 \u5fc5 \u6709 \u597d \u8b4f \u8a9a \u7684 \u4eba \u96a8 \u5f9e \u81ea \u5df1 \u4e0d \u656c \u8654 \u7684 \u79c1 \u617e \u800c \u884c \u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u9019 \u5c31 \u662f \u90a3 \u4e9b \u5f15 \u4eba \u7d50 \u9ee8 \u3001 \u5c6c \u4e4e \u8840 \u6c23 \u3001 \u6c92 \u6709 \u8056 \u9748 \u7684 \u4eba \u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u89aa \u611b \u7684 \u5f1f \u5144 \u963f \uff0c \u4f60 \u5011 \u537b \u8981 \u5728 \u81f3 \u8056 \u7684 \u771f \u9053 \u4e0a \u9020 \u5c31 \u81ea \u5df1 \uff0c \u5728 \u8056 \u9748 \u88e1 \u79b1 \u544a \uff0c\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u4fdd \u5b88 \u81ea \u5df1 \u5e38 \u5728 \u795e \u7684 \u611b \u4e2d \uff0c \u4ef0 \u671b \u6211 \u5011 \u4e3b \u8036 \u7a4c \u57fa \u7763 \u7684 \u6190 \u61ab \uff0c \u76f4 \u5230 \u6c38 \u751f \u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u6709 \u4e9b \u4eba \u5b58 \u7591 \u5fc3 \uff0c \u4f60 \u5011 \u8981 \u6190 \u61ab \u4ed6 \u5011 \uff1b\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u6709 \u4e9b \u4eba \u4f60 \u5011 \u8981 \u5f9e \u706b \u4e2d \u6436 \u51fa \u4f86 \uff0c \u642d \u6551 \u4ed6 \u5011 \uff1b \u6709 \u4e9b \u4eba \u4f60 \u5011 \u8981 \u5b58 \u61fc \u6015 \u7684 \u5fc3 \u6190 \u61ab  \u4ed6 \u5011 \uff0c \u9023 \u90a3  \u88ab \u60c5 \u617e \u6cbe \u67d3 \u7684 \u8863 \u670d \u4e5f \u7576 \u53ad \u60e1 \u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u90a3  \u80fd \u4fdd \u5b88 \u4f60 \u5011 \u4e0d \u5931 \u8173 \u3001 \u53eb \u4f60 \u5011 \u7121 \u7455 \u7121 \u75b5 \u3001 \u6b61 \u6b61 \u559c \u559c \u7ad9 \u5728 \u4ed6 \u69ae \u8000 \u4e4b \u524d \u7684 \u6211 \u5011 \u7684 \u6551 \u4e3b \u2500 \u7368 \u4e00 \u7684 \u795e \uff0c\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u9858 \u69ae \u8000 \u3001 \u5a01 \u56b4 \u3001 \u80fd \u529b \u3001 \u6b0a \u67c4 \uff0c \u56e0 \u6211 \u5011 \u7684 \u4e3b \u8036 \u7a4c \u57fa \u7763 \u6b78 \u8207 \u4ed6 \uff0c \u5f9e \u842c \u53e4 \u4ee5 \u524d \u4e26 \u73fe \u4eca \uff0c \u76f4 \u5230 \u6c38 \u6c38 \u9060 \u9060 \u3002 \u963f \u5011 \u3002\n"
                }
            }
        }
    }
}