    "book_nr": "35",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5148\u77e5\u9996\u6b21\u7591\u95ee\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u534e\u554a\uff01\u6211\u6073\u6c42\uff0c\u4f60\u4e0d\u5782\u542c\uff0c\u8981\u5230\u51e0\u65f6\u5462\uff1f\u6211\u5411\u4f60\u547c\u53eb\u201c\u6709\u72c2\u66b4\u7684\u4e8b\u201d\uff0c\u4f60\u5374\u4e0d\u62ef\u6551\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u4e3a\u4ec0\u4e48\u4f7f\u6211\u770b\u89c1\u6076\u884c\uff1f\u6709\u5978\u6076\u7684\u4e8b\uff0c\u4f60\u4e3a\u4ec0\u4e48\u89c1\u800c\u4e0d\u7406\uff1f\u6bc1\u706d\u548c\u5f3a\u66b4\u5728\u6211\u9762\u524d\uff0c\u7eb7\u4e89\u548c\u76f8\u6597\u5e38\u5e38\u53d1\u751f\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u56e0\u6b64\u5f8b\u6cd5\u4e0d\u80fd\u751f\u6548\uff08\u201c\u5f8b\u6cd5\u4e0d\u80fd\u751f\u6548\u201d\u539f\u6587\u4f5c\u201c\u5f8b\u6cd5\u677e\u61c8\u201d\uff09\uff0c\u516c\u7406\u65e0\u6cd5\u5f70\u663e\u3002\u56e0\u4e3a\u6076\u4eba\u628a\u4e49\u4eba\u5305\u56f4\uff0c\u6240\u4ee5\u516c\u7406\u98a0\u5012\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8036\u548c\u534e\u7684\u56de\u7b54\u4f60\u4eec\u5f53\u770b\u5217\u56fd\uff0c\u8981\u5b9a\u775b\u89c2\u770b\uff0c\u5c31\u4f1a\u5927\u5927\u60ca\u5947\uff0c\u56e0\u4e3a\u5728\u4f60\u4eec\u7684\u65e5\u5b50\uff0c\u6211\u8981\u4f5c\u4e00\u4ef6\u4e8b\uff0c\u5373\u4f7f\u6709\u4eba\u8bf4\u4e86\u51fa\u6765\uff0c\u4f60\u4eec\u4e5f\u4e0d\u4f1a\u76f8\u4fe1\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5c31\u662f\u8981\u5174\u8d77\u8fe6\u52d2\u5e95\u4eba\uff0c\u90a3\u6b8b\u5fcd\u51f6\u66b4\u7684\u6c11\uff1b\u4ed6\u4eec\u904d\u884c\u5168\u5730\uff0c\u5360\u9886\u522b\u4eba\u7684\u5bb6\u56ed\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u4eec\u6050\u6016\u53ef\u6015\uff0c\u81ea\u4ee5\u4e3a\u4e49\uff0c\u8dbe\u9ad8\u6c14\u626c\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ed6\u4eec\u7684\u9a6c\u6bd4\u8c79\u66f4\u5feb\uff0c\u6bd4\u665a\u4e0a\u7684\u8c7a\u72fc\u66f4\u731b\u3002\u4ed6\u4eec\u7684\u9a91\u5175\u5954\u9a70\uff0c\u81ea\u8fdc\u800c\u6765\uff1b\u4ed6\u4eec\u5982\u9e70\u98de\u7fd4\uff0c\u8fc5\u901f\u541e\u566c\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u4eec\u5b9a\u7740\u8138\u9762\u5411\u524d\uff0c\u9f50\u6765\u884c\u66b4\uff0c\u63b3\u83b7\u6218\u4fd8\u591a\u5982\u5c18\u6c99\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4ed6\u4eec\u620f\u5f04\u541b\u738b\uff0c\u4ee5\u638c\u6743\u7684\u4e3a\u7b11\u67c4\uff1b\u4ed6\u4eec\u55e4\u7b11\u4e00\u5207\u57ce\u5821\uff0c\u7b51\u5792\u653b\u53d6\uff1b\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u7136\u540e\u626b\u8361\u5982\u98ce\u5439\u8fc7\u3002\u4ed6\u4eec\u662f\u6709\u7f6a\u7684\uff0c\u56e0\u4ed6\u4eec\u4ee5\u81ea\u5df1\u7684\u52bf\u529b\u4e3a\u795e\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5148\u77e5\u7b2c\u4e8c\u6b21\u7684\u7591\u95ee\u8036\u548c\u534e\u6211\u7684\u3000\u795e\uff0c\u6211\u7684\u5723\u8005\u554a\uff01\u4f60\u4e0d\u662f\u81ea\u53e4\u5c31\u6709\u7684\u5417\uff1f\u6211\u4eec\u4e0d\u4f1a\u6b7b\u7684\u3002\u8036\u548c\u534e\u554a\uff01\u4f60\u6d3e\u4ed6\u4eec\u884c\u5ba1\u5224\u3002\u78d0\u77f3\u554a\uff01\u4f60\u7acb\u4ed6\u4eec\u65bd\u60e9\u7f5a\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u7684\u773c\u76ee\u7eaf\u6d01\uff0c\u4e0d\u770b\u90aa\u6076\uff0c\u4e0d\u80fd\u5750\u89c6\u5978\u6076\uff1b\u4e3a\u4ec0\u4e48\u89c1\u884c\u8be1\u8bc8\u7684\u4eba\u800c\u4e0d\u7406\uff1f\u6076\u4eba\u541e\u706d\u6bd4\u81ea\u5df1\u516c\u4e49\u7684\u4eba\uff0c\u4f60\u4e3a\u4ec0\u4e48\u7f04\u9ed8\u5462\uff1f\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u7adf\u4f7f\u4eba\u50cf\u6d77\u91cc\u7684\u9c7c\uff0c\u50cf\u65e0\u4eba\u7ba1\u8f96\u7684\u722c\u866b\u7c7b\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8fe6\u52d2\u5e95\u4eba\u65e2\u7528\u94a9\u628a\u4ed6\u4eec\u9493\u8d77\u6765\uff0c\u7528\u7f51\u62d6\u8d70\uff0c\u7528\u9c7c\u7f51\u6536\u805a\u5728\u4e00\u5904\uff0c\u5c31\u6b22\u559c\u5feb\u4e50\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5411\u81ea\u5df1\u7684\u7f51\u732e\u796d\uff0c\u5bf9\u9c7c\u7f51\u70e7\u9999\uff0c\u56e0\u4ed6\u4eec\u501f\u6b64\u6536\u83b7\u4e30\u5bcc\uff0c\u996e\u98df\u5145\u88d5\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8fd9\u6837\uff0c\u4ed6\u4eec\u5012\u7a7a\u81ea\u5df1\u7684\u7f51\uff0c\u6beb\u4e0d\u7559\u60c5\u5730\u7ee7\u7eed\u6740\u622e\u5217\u56fd\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u534e\u7684\u56de\u7b54\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u534e\u56de\u7b54\u6211\u8bf4\uff1a\u201c\u628a\u5f02\u8c61\u5199\u4e0b\uff0c\u8bb0\u5728\u6ce5\u7248\u4e0a\uff0c\u4f7f\u8bfb\u7684\u4eba\u5bb9\u6613\u660e\u767d\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56e0\u8fd9\u5f02\u8c61\u5173\u4e4e\u4e00\u5b9a\u7684\u65e5\u671f\uff0c\u5f88\u5feb\u5c31\u8981\u5b9e\u73b0\uff0c\u51b3\u4e0d\u5f92\u7136\uff1b\u7eb5\u6709\u8fdf\u5ef6\uff0c\u4ecd\u5f53\u7b49\u5019\uff0c\u56e0\u4e3a\u5b83\u4e00\u5b9a\u4f1a\u6765\u5230\uff0c\u7edd\u4e0d\u803d\u8bef\u3002\u201d\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8fe6\u52d2\u5e95\u4eba\u81ea\u9ad8\u81ea\u5927\uff0c\u5fc3\u4e0d\u6b63\u76f4\uff1b\u7136\u800c\u4e49\u4eba\u5fc5\u56e0\u4fe1\u5f97\u751f\uff08\u201c\u5fc5\u56e0\u4fe1\u5f97\u751f\u201d\u6216\u8bd1\uff1a\u201c\u5fc5\u56e0\u4ed6\u7684\u4fe1\u5b9e\u5f97\u751f\u201d\uff09\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8d22\u5bcc\uff08\u4f20\u7edf\u4f5c\u201c\u9152\u201d\uff0c\u4eca\u7167\u6b7b\u6d77\u53e4\u5377\u8bd1\u4f5c\u201c\u8d22\u5bcc\u201d\uff09\u4f7f\u4eba\u5978\u8bc8\u72c2\u50b2\uff0c\u4e0d\u5f97\u5b89\u5b81\uff1b\u6269\u5f20\u6b32\u671b\uff0c\u5982\u540c\u9634\u95f4\uff1b\u53c8\u50cf\u6b7b\u4ea1\uff0c\u6c38\u4e0d\u6ee1\u8db3\u3002\u4ed6\u4eec\u62db\u805a\u4e07\u56fd\uff0c\u96c6\u5408\u4e07\u6c11\uff0c\u90fd\u5f52\u81ea\u5df1\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u7b2c\u4e00\u7978\u8fd9\u4e9b\u56fd\u6c11\u5c82\u4e0d\u90fd\u7528\u8b6c\u55bb\u3001\u8bbd\u523a\u3001\u6697\u8bed\u6307\u7740\u4ed6\u4eec\u8bf4\uff1a\u201c\u90a3\u4e9b\u6ee5\u5f97\u4ed6\u4eba\u8d22\u7269\u7684\uff0c\u6ee1\u8f7d\u522b\u4eba\u62b5\u62bc\u54c1\u7684\uff0c\u6709\u7978\u4e86\uff01\u4ed6\u4eec\u8fd9\u6837\u8981\u5230\u51e0\u65f6\u5462\uff1f\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u7684\u503a\u4e3b\uff08\u201c\u4f60\u7684\u503a\u4e3b\u201d\u6216\u8bd1\uff1a\u201c\u54ac\u4f60\u7684\u201d\uff09\u5c82\u4e0d\u5ffd\u7136\u8d77\u6765\uff1f\u6270\u4e71\u4f60\u7684\u5c82\u4e0d\u9192\u8d77\uff1f\u4f60\u5c31\u6210\u4e86\u4ed6\u4eec\u7684\u63b3\u7269\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u56e0\u4f60\u62a2\u63a0\u591a\u56fd\uff0c\u6740\u4eba\u6d41\u8840\uff0c\u5411\u5168\u5730\u5404\u57ce\u65bd\u884c\u66b4\u529b\uff0c\u6240\u4ee5\u5269\u4e0b\u7684\u4eba\u4e5f\u5fc5\u62a2\u63a0\u4f60\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7b2c\u4e8c\u7978\u4f60\u8fd9\u4e3a\u81ea\u5df1\u7684\u5bb6\u79ef\u805a\u4e0d\u4e49\u4e4b\u8d22\uff0c\u5728\u9ad8\u5904\u642d\u7a9d\uff0c\u4ee5\u9003\u907f\u707e\u5bb3\u7684\uff0c\u6709\u7978\u4e86\uff01\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f60\u526a\u9664\u591a\u56fd\u7684\u6c11\uff0c\u53cd\u5bb3\u4e86\u81ea\u5df1\uff1b\u4f60\u6240\u8c0b\u7b97\u7684\uff0c\u4f7f\u4f60\u7684\u5bb6\u8499\u7f9e\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u77f3\u5934\u5fc5\u4ece\u5899\u91cc\u547c\u53eb\uff0c\u6881\u6728\u5fc5\u5728\u6811\u4e2d\u5e94\u58f0\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u7b2c\u4e09\u7978\u4f60\u8fd9\u7528\u4eba\u8840\u505a\u57ce\uff0c\u51ed\u90aa\u6076\u7acb\u9547\u7684\uff0c\u6709\u7978\u4e86\uff01\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f17\u6c11\u52b3\u788c\u5f97\u6765\u7684\uff0c\u88ab\u706b\u711a\u70e7\uff1b\u5217\u56fd\u8f9b\u52b3\u800c\u5f97\u7684\uff0c\u7ec8\u5f52\u65e0\u6709\u3002\u8fd9\u4e0d\u90fd\u51fa\u4e8e\u4e07\u519b\u4e4b\u8036\u548c\u534e\u5417\uff1f\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u8ba4\u8bc6\u8036\u548c\u534e\u4e4b\u8363\u8000\u7684\u77e5\u8bc6\uff0c\u5fc5\u5145\u6ee1\u5168\u5730\uff0c\u597d\u50cf\u4f17\u6c34\u906e\u76d6\u6d77\u6d0b\u4e00\u6837\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u7b2c\u56db\u7978\u4f60\u8fd9\u8bf7\u90bb\u820d\u559d\u9152\uff0c\u5374\u628a\u6bd2\u7269\u6df7\u5165\uff0c\u4f7f\u4ed6\u9189\u5012\uff0c\u4e3a\u8981\u89c1\u4ed6\u8d64\u88f8\u7684\uff0c\u6709\u7978\u4e86\uff01\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f60\u6ee1\u6709\u7f9e\u8fb1\uff0c\u6ca1\u6709\u5c0a\u8363\uff0c\u4f60\u4e5f\u559d\u5230\u9732\u51fa\u4e0b\u4f53\u6765\u5427\uff01\u8036\u548c\u534e\u53f3\u624b\u7684\u676f\u5fc5\u4f20\u56de\u6765\u7ed9\u4f60\uff0c\u4f7f\u6781\u5927\u7684\u7f9e\u8fb1\u53d6\u4ee3\u4f60\u7684\u5c0a\u8363\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u56e0\u4f60\u5411\u9ece\u5df4\u5ae9\u4f7f\u7528\u66b4\u529b\uff0c\u6740\u706d\u60ca\u5413\u91ce\u517d\uff1b\u53c8\u6740\u4eba\u6d41\u8840\uff0c\u5411\u5168\u5730\u5404\u57ce\u5c45\u6c11\u65bd\u884c\u66b4\u529b\uff0c\u5c31\u5fc5\u53d7\u5230\u62a5\u5e94\uff08\u201c\u62a5\u5e94\u201d\u539f\u6587\u4f5c\u201c\u906e\u76d6\u201d\uff09\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u7b2c\u4e94\u7978\u96d5\u523b\u7684\u50cf\u6709\u4ec0\u4e48\u7528\u5904\u5462\uff1f\u4e0d\u8fc7\u662f\u5320\u4eba\u96d5\u523b\u51fa\u6765\u7684\uff1b\u94f8\u9020\u7684\u50cf\u3001\u865a\u5047\u7684\u6559\u5e08\uff0c\u6709\u4ec0\u4e48\u7528\u5904\u5462\uff1f\u5320\u4eba\u7adf\u501a\u9760\u81ea\u5df1\u6240\u505a\uff0c\u90a3\u4e0d\u80fd\u8bf4\u8bdd\u7684\u5047\u795e\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u4f60\u8fd9\u5bf9\u6728\u5934\u8bf4\u201c\u8d77\u6765\u201d\uff0c\u5bf9\u54d1\u77f3\u8bf4\u201c\u9192\u5427\u201d\u7684\u4eba\uff0c\u6709\u7978\u4e86\uff01\u5b83\u600e\u80fd\u6559\u5bfc\u4f60\u5462\uff1f\u770b\u54ea\uff01\u5b83\u5305\u91d1\u9576\u94f6\uff0c\u91cc\u9762\u5374\u5168\u65e0\u6c14\u606f\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u7136\u800c\u8036\u548c\u534e\u5728\u4ed6\u7684\u5723\u6bbf\u91cc\uff0c\u5168\u5730\u5f53\u5728\u4ed6\u9762\u524d\u8083\u9759\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u534e\u7684\u5a01\u4e25\u548c\u6743\u80fd\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u534e\u554a\uff01\u6211\u542c\u89c1\u4f60\u7684\u58f0\u97f3\uff0c\u60e7\u6015\u4f60\u7684\u4f5c\u4e3a\uff1b\u6c42\u4f60\u5728\u8fd9\u4e9b\u5e74\u95f4\u590d\u5174\uff0c\u5728\u8fd9\u4e9b\u5e74\u95f4\u5f70\u663e\uff0c\u53d1\u6012\u7684\u65f6\u5019\u4ee5\u601c\u60af\u4e3a\u6000\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u3000\u795e\u7531\u63d0\u5e54\u800c\u6765\uff0c\u5723\u8005\u4ece\u5df4\u5170\u5c71\u4e34\u5230\u3002\uff08\u7ec6\u62c9\uff09\u4ed6\u7684\u8363\u5149\u5f25\u6f2b\u7a79\u82cd\uff0c\u8d5e\u7f8e\u4ed6\u7684\u58f0\u97f3\u54cd\u5f7b\u5bf0\u5b87\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u7684\u8f89\u714c\u5982\u5149\uff0c\u624b\u4e2d\u56db\u5c04\u5149\u8292\uff0c\u5c31\u5728\u5176\u4e2d\u9690\u85cf\u7740\u80fd\u529b\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
