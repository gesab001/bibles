                "8": {
                    "verse_nr": 8,
                    "verse": "\u5723\u7ecf\u65e2\u7136\u9884\u5148\u770b\u89c1\u3000\u795e\u8981\u4f7f\u5916\u65cf\u4eba\u56e0\u4fe1\u79f0\u4e49\uff0c\u5c31\u9884\u5148\u628a\u597d\u4fe1\u606f\u4f20\u7ed9\u4e9a\u4f2f\u62c9\u7f55\uff1a\u201c\u4e07\u56fd\u90fd\u5fc5\u56e0\u4f60\u5f97\u798f\u3002\u201d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u8fd9\u6837\u770b\u6765\uff0c\u6709\u4fe1\u5fc3\u7684\u4eba\uff0c\u5fc5\u5b9a\u548c\u6709\u4fe1\u5fc3\u7684\u4e9a\u4f2f\u62c9\u7f55\u4e00\u540c\u5f97\u798f\u3002\n"
