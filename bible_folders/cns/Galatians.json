                "26": {
                    "verse_nr": 26,
                    "verse": "\u6211\u4eec\u4e0d\u53ef\u8d2a\u56fe\u865a\u8363\uff0c\u5f7c\u6b64\u89e6\u6012\uff0c\u4e92\u76f8\u5ac9\u5992\u3002\n"
                }
            }
        },
        "6": {
            "chapter_nr": 6,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4fe1\u5f92\u7684\u8d23\u4efb\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4f60\u4eec\u5404\u4eba\u7684\u91cd\u62c5\u8981\u4e92\u76f8\u62c5\u5f53\uff0c\u8fd9\u6837\u5c31\u6210\u5168\u4e86\u57fa\u7763\u7684\u5f8b\u6cd5\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5982\u679c\u6709\u4eba\u672c\u6765\u6ca1\u6709\u4ec0\u4e48\u4e86\u4e0d\u8d77\uff0c\u5374\u81ea\u4ee5\u4e3a\u662f\u4e86\u4e0d\u8d77\u7684\uff0c\u5c31\u662f\u6b3a\u9a97\u81ea\u5df1\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5404\u4eba\u5e94\u8be5\u7701\u5bdf\u81ea\u5df1\u6240\u4f5c\u7684\uff0c\u8fd9\u6837\uff0c\u4ed6\u5f15\u4ee5\u4e3a\u8363\u7684\uff0c\u5c31\u53ea\u5728\u81ea\u5df1\u800c\u4e0d\u5728\u522b\u4eba\u4e86\uff1b\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0\u4e3a\u5404\u4eba\u7684\u62c5\u5b50\uff0c\u662f\u8981\u81ea\u5df1\u62c5\u5f53\u7684\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5728\u5723\u9053\u4e0a\u53d7\u6559\u7684\uff0c\u5e94\u8be5\u548c\u65bd\u6559\u7684\u4eba\u5206\u4eab\u81ea\u5df1\u7684\u4e00\u5207\u7f8e\u7269\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4e0d\u8981\u81ea\u6b3a\uff0c\u3000\u795e\u662f\u4e0d\u53ef\u8f7b\u6162\u7684\u3002\u4eba\u79cd\u7684\u662f\u4ec0\u4e48\uff0c\u6536\u7684\u4e5f\u662f\u4ec0\u4e48\uff1a\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u987a\u7740\u81ea\u5df1\u7684\u8089\u4f53\u6492\u79cd\u7684\uff0c\u5fc5\u5b9a\u4ece\u8089\u4f53\u6536\u53d6\u8d25\u574f\uff1b\u987a\u7740\u5723\u7075\u6492\u79cd\u7684\uff0c\u5fc5\u5b9a\u4ece\u5723\u7075\u6536\u53d6\u6c38\u751f\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u4eec\u884c\u5584\uff0c\u4e0d\u8981\u89c9\u5f97\u538c\u70e6\uff1b\u5982\u679c\u4e0d\u677e\u61c8\uff0c\u5230\u4e86\u9002\u5f53\u7684\u65f6\u5019\uff0c\u5c31\u6709\u6536\u6210\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6240\u4ee5\uff0c\u6211\u4eec\u4e00\u6709\u673a\u4f1a\uff0c\u5c31\u5e94\u8be5\u5bf9\u4f17\u4eba\u884c\u5584\uff0c\u5bf9\u4fe1\u5f92\u66f4\u8981\u8fd9\u6837\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u529d\u52c9\u548c\u795d\u798f\u8bf7\u770b\uff0c\u6211\u4eb2\u624b\u5199\u7ed9\u4f60\u4eec\u7684\u5b57\u662f\u591a\u4e48\u7684\u5927\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u90a3\u4e9b\u5728\u5916\u8868\u4e0a\u8981\u4f53\u9762\u7684\u4eba\uff0c\u4ed6\u4eec\u52c9\u5f3a\u4f60\u4eec\u53d7\u5272\u793c\uff0c\u4e0d\u8fc7\u662f\u6015\u4e3a\u4e86\u57fa\u7763\u7684\u5341\u5b57\u67b6\u53d7\u8feb\u5bb3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u90a3\u4e9b\u53d7\u5272\u793c\u7684\u4eba\uff0c\u81ea\u5df1\u4e5f\u4e0d\u9075\u5b88\u5f8b\u6cd5\uff0c\u53cd\u800c\u8981\u4f60\u4eec\u53d7\u5272\u793c\uff0c\u4e3a\u7684\u662f\u8981\u501f\u7740\u4f60\u4eec\u7684\u8089\u8eab\u5938\u53e3\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u81f3\u4e8e\u6211\uff0c\u6211\u7edd\u4e0d\u4ee5\u522b\u7684\u5938\u53e3\uff0c\u53ea\u5938\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u5341\u5b57\u67b6\u3002\u501f\u7740\u8fd9\u5341\u5b57\u67b6\uff0c\u5728\u6211\u6765\u8bf4\uff0c\u4e16\u754c\u5df2\u7ecf\u9489\u5728\u5341\u5b57\u67b6\u4e0a\u4e86\uff1b\u5c31\u4e16\u754c\u6765\u8bf4\uff0c\u6211\u4e5f\u5df2\u7ecf\u9489\u5728\u5341\u5b57\u67b6\u4e0a\u4e86\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u53d7\u5272\u793c\u6216\u4e0d\u53d7\u5272\u793c\uff0c\u90fd\u7b97\u4e0d\u5f97\u4ec0\u4e48\uff0c\u8981\u7d27\u7684\u662f\u4f5c\u65b0\u9020\u7684\u4eba\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6240\u6709\u7167\u8fd9\u51c6\u5219\u800c\u884c\u7684\u4eba\uff0c\u613f\u5e73\u5b89\u601c\u60af\u4e34\u5230\u4ed6\u4eec\uff0c\u5c31\u662f\u4e34\u5230\u3000\u795e\u7684\u4ee5\u8272\u5217\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ece\u4eca\u4ee5\u540e\uff0c\u8c01\u4e5f\u4e0d\u8981\u6405\u6270\u6211\uff0c\u56e0\u4e3a\u6211\u8eab\u4e0a\u5e26\u7740\u8036\u7a23\u7684\u70d9\u75d5\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5f1f\u5144\u4eec\uff0c\u613f\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u6069\u60e0\u4e0e\u4f60\u4eec\u540c\u5728\uff08\u201c\u4e0e\u4f60\u4eec\u540c\u5728\u201d\u539f\u6587\u4f5c\u201c\u4e0e\u4f60\u4eec\u7684\u5fc3\u7075\u540c\u5728\u201d\uff09\u3002\u963f\u4eec\u3002\n"
                }
            }
        }
    }
