                "23": {
                    "verse_nr": 23,
                    "verse": "\u4f46\u4fe1\u7684\u9053\u7406\u8fd8\u6ca1\u6709\u6765\u5230\u4ee5\u524d\uff0c\u6211\u4eec\u5728\u5f8b\u6cd5\u4e0b\u88ab\u56da\u7981\u3001\u88ab\u56f4\u56f0\uff0c\u76f4\u5230\u90a3\u8981\u6765\u7684\u4fe1\u7684\u9053\u7406\u663e\u660e\u51fa\u6765\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u8fd9\u6837\uff0c\u5f8b\u6cd5\u6210\u4e86\u6211\u4eec\u7684\u542f\u8499\u6559\u5e08\uff0c\u9886\u6211\u4eec\u5230\u57fa\u7763\u90a3\u91cc\uff0c\u4f7f\u6211\u4eec\u53ef\u4ee5\u56e0\u4fe1\u79f0\u4e49\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u4f46\u4fe1\u7684\u9053\u7406\u65e2\u7136\u6765\u5230\uff0c\u6211\u4eec\u5c31\u4e0d\u518d\u5728\u542f\u8499\u6559\u5e08\u4e4b\u4e0b\u4e86\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u56e0\u4fe1\u4f5c\u627f\u53d7\u4ea7\u4e1a\u7684\u513f\u5b50\u4f60\u4eec\u56e0\u7740\u4fe1\uff0c\u5728\u57fa\u7763\u8036\u7a23\u91cc\u90fd\u4f5c\u4e86\u3000\u795e\u7684\u513f\u5b50\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u4f60\u4eec\u6240\u6709\u53d7\u6d17\u5f52\u5165\u57fa\u7763\u7684\u4eba\uff0c\u90fd\u662f\u62ab\u6234\u57fa\u7763\u7684\uff0c\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\u5e76\u4e0d\u5206\u72b9\u592a\u4eba\u6216\u5e0c\u814a\u4eba\uff0c\u4f5c\u5974\u4ec6\u7684\u6216\u81ea\u7531\u4eba\uff0c\u7537\u7684\u6216\u5973\u7684\uff0c\u56e0\u4e3a\u4f60\u4eec\u5728\u57fa\u7763\u8036\u7a23\u91cc\u90fd\u6210\u4e3a\u4e00\u4f53\u4e86\u3002\n"
                },
                "29": {
                    "verse_nr": 29,
                    "verse": "\u5982\u679c\u4f60\u4eec\u5c5e\u4e8e\u57fa\u7763\uff0c\u5c31\u662f\u4e9a\u4f2f\u62c9\u7f55\u7684\u540e\u88d4\uff0c\u662f\u6309\u7167\u5e94\u8bb8\u627f\u53d7\u4ea7\u4e1a\u7684\u4e86\u3002\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6211\u8bf4\uff0c\u90a3\u627f\u53d7\u4ea7\u4e1a\u7684\uff0c\u867d\u7136\u662f\u5168\u90e8\u4ea7\u4e1a\u7684\u4e3b\u4eba\uff0c\u4f46\u5728\u5b69\u7ae5\u7684\u65f6\u671f\uff0c\u5374\u548c\u5974\u4ec6\u6ca1\u6709\u5206\u522b\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4ed6\u662f\u5728\u76d1\u62a4\u4eba\u548c\u7ba1\u5bb6\u4e4b\u4e0b\uff0c\u76f4\u5230\u7236\u4eb2\u9884\u5148\u6307\u5b9a\u7684\u65f6\u5019\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u4eec\u4e5f\u662f\u8fd9\u6837\uff0c\u4f5c\u5b69\u7ae5\u7684\u65f6\u5019\uff0c\u88ab\u4e16\u4fd7\u7684\u8a00\u8bba\u6240\u5974\u5f79\uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4f46\u5230\u4e86\u65f6\u673a\u6210\u719f\uff0c\u3000\u795e\u5c31\u5dee\u9063\u4ed6\u7684\u513f\u5b50\uff0c\u7531\u5973\u4eba\u6240\u751f\uff0c\u800c\u4e14\u751f\u5728\u5f8b\u6cd5\u4e4b\u4e0b\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8981\u628a\u5f8b\u6cd5\u4e4b\u4e0b\u7684\u4eba\u6551\u8d4e\u51fa\u6765\uff0c\u597d\u8ba9\u6211\u4eec\u5f97\u7740\u55e3\u5b50\u7684\u540d\u5206\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u4eec\u65e2\u7136\u662f\u513f\u5b50\uff0c\u3000\u795e\u5c31\u5dee\u9063\u4ed6\u513f\u5b50\u7684\u7075\u8fdb\u5165\u6211\u4eec\u5fc3\u91cc\uff0c\u547c\u53eb\u201c\u963f\u7238\u3001\u7236\uff01\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u8fd9\u6837\uff0c\u4f60\u4e0d\u518d\u662f\u5974\u4ec6\uff0c\u800c\u662f\u513f\u5b50\uff1b\u65e2\u7136\u662f\u513f\u5b50\uff0c\u5c31\u9760\u7740\u3000\u795e\u627f\u53d7\u4ea7\u4e1a\u4e86\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4fdd\u7f57\u4e3a\u52a0\u62c9\u592a\u4fe1\u5f92\u62c5\u5fe7\u4ece\u524d\u4f60\u4eec\u4e0d\u8ba4\u8bc6\u3000\u795e\u7684\u65f6\u5019\uff0c\u662f\u7ed9\u90a3\u4e9b\u672c\u6765\u4e0d\u662f\u3000\u795e\u7684\u4f5c\u5974\u4ec6\uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u73b0\u5728\u4f60\u4eec\u65e2\u7136\u8ba4\u8bc6\u3000\u795e\uff0c\u66f4\u53ef\u4ee5\u8bf4\u662f\u3000\u795e\u6240\u8ba4\u8bc6\u7684\uff0c\u600e\u4e48\u8fd8\u56de\u5230\u90a3\u4e9b\u8f6f\u5f31\u8d2b\u4e4f\u7684\u8a00\u8bba\uff0c\u60c5\u613f\u518d\u4f5c\u5b83\u4eec\u7684\u5974\u4ec6\u5462\uff1f\n"
