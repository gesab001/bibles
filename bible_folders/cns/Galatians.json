                    "verse": "\u6b63\u5982\u4e9a\u4f2f\u62c9\u7f55\u4fe1\u3000\u795e\uff0c\u8fd9\u5c31\u7b97\u4e3a\u4ed6\u7684\u4e49\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6240\u4ee5\u4f60\u4eec\u8981\u77e5\u9053\uff0c\u6709\u4fe1\u5fc3\u7684\u4eba\uff0c\u5c31\u662f\u4e9a\u4f2f\u62c9\u7f55\u7684\u5b50\u5b59\u3002\n"
