{
    "version": "cns",
    "book_name": "Galatians",
    "book_nr": "48",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\u4f5c\u4f7f\u5f92\u7684\u4fdd\u7f57\uff08\u4e0d\u662f\u7531\u4e8e\u4eba\uff0c\u4e5f\u4e0d\u662f\u501f\u7740\u4eba\uff0c\u800c\u662f\u501f\u7740\u8036\u7a23\u57fa\u7763\u548c\u90a3\u4f7f\u4ed6\u4ece\u6b7b\u4eba\u4e2d\u590d\u6d3b\u7684\u7236\u3000\u795e\uff09\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u548c\u6240\u6709\u4e0e\u6211\u5728\u4e00\u8d77\u7684\u5f1f\u5144\uff0c\u5199\u4fe1\u7ed9\u52a0\u62c9\u592a\u7684\u4f17\u6559\u4f1a\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u613f\u6069\u60e0\u5e73\u5b89\u4ece\u6211\u4eec\u7684\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u4e34\u5230\u4f60\u4eec\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u57fa\u7763\u7167\u7740\u6211\u4eec\u7236\u3000\u795e\u7684\u65e8\u610f\uff0c\u66fe\u7ecf\u4e3a\u6211\u4eec\u7684\u7f6a\u820d\u5df1\uff0c\u4e3a\u7684\u662f\u8981\u6551\u6211\u4eec\u8131\u79bb\u73b0\u5728\u8fd9\u90aa\u6076\u7684\u4e16\u4ee3\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u613f\u8363\u8000\u5f52\u7ed9\u4ed6\uff0c\u76f4\u5230\u6c38\u6c38\u8fdc\u8fdc\u3002\u963f\u4eec\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u798f\u97f3\u53ea\u6709\u4e00\u4e2a\u6211\u5f88\u60ca\u5947\uff0c\u4f60\u4eec\u8fd9\u4e48\u5feb\u5c31\u79bb\u5f00\u4e86\u501f\u7740\u57fa\u7763\u7684\u6069\u547c\u53ec\u4f60\u4eec\u7684\u90a3\u4e00\u4f4d\uff0c\u53bb\u5f52\u5411\u522b\u7684\u798f\u97f3\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5176\u5b9e\u90a3\u5e76\u4e0d\u662f\u53e6\u4e00\u4e2a\u798f\u97f3\uff0c\u53ea\u662f\u6709\u4e9b\u6405\u6270\u4f60\u4eec\u7684\u4eba\uff0c\u60f3\u628a\u57fa\u7763\u7684\u798f\u97f3\u6539\u53d8\u4e86\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f46\u65e0\u8bba\u662f\u6211\u4eec\uff0c\u6216\u662f\u4ece\u5929\u4e0a\u6765\u7684\u4f7f\u8005\uff0c\u5982\u679c\u4f20\u7ed9\u4f60\u4eec\u7684\u548c\u6211\u4eec\u4ee5\u524d\u4f20\u7ed9\u4f60\u4eec\u7684\u798f\u97f3\u4e0d\u540c\uff0c\u4ed6\u5c31\u8be5\u53d7\u5492\u8bc5\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u4eec\u5df2\u7ecf\u8bf4\u8fc7\uff0c\u73b0\u5728\u6211\u8981\u518d\u8bf4\uff0c\u5982\u679c\u6709\u4eba\u4f20\u7ed9\u4f60\u4eec\u7684\u548c\u4f60\u4eec\u4ee5\u524d\u6240\u9886\u53d7\u7684\u798f\u97f3\u4e0d\u540c\uff0c\u4ed6\u5c31\u8be5\u53d7\u5492\u8bc5\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u73b0\u5728\u662f\u8981\u5f97\u4eba\u7684\u6b22\u5fc3\uff0c\u8fd8\u662f\u8981\u5f97\u3000\u795e\u7684\u6b22\u5fc3\u5462\uff1f\u96be\u9053\u6211\u60f3\u8ba8\u4eba\u6b22\u559c\u5417\uff1f\u5982\u679c\u6211\u4ecd\u7136\u8981\u8ba8\u4eba\u6b22\u559c\uff0c\u5c31\u4e0d\u662f\u57fa\u7763\u7684\u4ec6\u4eba\u4e86\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4fdd\u7f57\u5f97\u542f\u793a\u4f5c\u4f7f\u5f92\u5f1f\u5144\u4eec\uff0c\u6211\u8981\u4f60\u4eec\u77e5\u9053\uff0c\u6211\u6240\u4f20\u7684\u798f\u97f3\uff0c\u5e76\u4e0d\u662f\u7167\u7740\u4eba\u7684\u610f\u601d\uff0c\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u56e0\u4e3a\u8fd9\u798f\u97f3\u6211\u4e0d\u662f\u4ece\u4eba\u9886\u53d7\u7684\uff0c\u4e5f\u4e0d\u662f\u4eba\u6559\u5bfc\u6211\u7684\uff0c\u800c\u662f\u501f\u7740\u8036\u7a23\u57fa\u7763\u7684\u542f\u793a\u6765\u7684\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u4eec\u542c\u8fc7\u6211\u4ece\u524d\u5728\u72b9\u592a\u6559\u4e2d\u6240\u884c\u7684\uff1a\u600e\u6837\u6781\u529b\u903c\u8feb\u6b8b\u5bb3\u3000\u795e\u7684\u6559\u4f1a\uff0c\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u600e\u6837\u5728\u72b9\u592a\u6559\u4e2d\u6bd4\u8bb8\u591a\u672c\u65cf\u540c\u8f88\u7684\u4eba\u66f4\u6fc0\u8fdb\uff0c\u4e3a\u6211\u7956\u5148\u7684\u4f20\u7edf\u5206\u5916\u70ed\u5fc3\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u7136\u800c\uff0c\u5f53\u6211\u5728\u6bcd\u8179\u91cc\u5c31\u628a\u6211\u5206\u522b\u51fa\u6765\uff0c\u53c8\u7528\u4ed6\u7684\u6069\u547c\u53ec\u6211\u7684\u90a3\u4e00\u4f4d\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u65e2\u7136\u4e50\u610f\u628a\u81ea\u5df1\u7684\u513f\u5b50\u542f\u793a\u7ed9\u6211\uff0c\u4f7f\u6211\u53ef\u4ee5\u5728\u5916\u65cf\u4eba\u4e2d\u4f20\u626c\u4ed6\uff0c\u6211\u5c31\u6ca1\u6709\u548c\u4efb\u4f55\u4eba\uff08\u201c\u4eba\u201d\u76f4\u8bd1\u201c\u8089\u548c\u8840\u201d\uff09\u5546\u91cf\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4e5f\u6ca1\u6709\u4e0a\u8036\u8def\u6492\u51b7\u89c1\u90a3\u4e9b\u6bd4\u6211\u5148\u4f5c\u4f7f\u5f92\u7684\uff0c\u5374\u53bb\u4e86\u963f\u62c9\u4f2f\uff0c\u7136\u540e\u518d\u56de\u5230\u5927\u9a6c\u58eb\u9769\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u8fc7\u4e86\u4e09\u5e74\uff0c\u6211\u624d\u4e0a\u8036\u8def\u6492\u51b7\u53bb\u89c1\u77f6\u6cd5\uff0c\u548c\u4ed6\u4f4f\u4e86\u5341\u4e94\u5929\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u81f3\u4e8e\u5176\u4f59\u7684\u4f7f\u5f92\uff0c\u9664\u4e86\u4e3b\u7684\u5f1f\u5f1f\u96c5\u5404\u4ee5\u5916\uff0c\u6211\u90fd\u6ca1\u6709\u89c1\u8fc7\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5728\u3000\u795e\u9762\u524d\u6211\u6562\u8bf4\uff0c\u6211\u5199\u7ed9\u4f60\u4eec\u7684\u90fd\u4e0d\u662f\u8c0e\u8bdd\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u540e\u6765\uff0c\u6211\u5230\u4e86\u53d9\u5229\u4e9a\u548c\u57fa\u5229\u5bb6\u4e00\u5e26\u7684\u5730\u65b9\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u90a3\u65f6\uff0c\u5728\u57fa\u7763\u91cc\u7684\u72b9\u592a\u4f17\u6559\u4f1a\u8fd8\u6ca1\u6709\u89c1\u8fc7\u6211\u7684\u9762\uff0c\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u4e0d\u8fc7\u542c\u8bf4\u201c\u90a3\u4e2a\u4ece\u524d\u8feb\u5bb3\u6211\u4eec\u7684\uff0c\u73b0\u5728\u7adf\u7136\u4f20\u626c\u4ed6\u4ee5\u524d\u6240\u6b8b\u5bb3\u7684\u4fe1\u4ef0\u201d\uff0c\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u4ed6\u4eec\u5c31\u56e0\u7740\u6211\u7684\u7f18\u6545\u9882\u8d5e\u3000\u795e\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5176\u4ed6\u4f7f\u5f92\u63a5\u7eb3\u4fdd\u7f57\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6211\u662f\u987a\u4ece\u542f\u793a\u53bb\u7684\uff1b\u5728\u90a3\u91cc\u6211\u5bf9\u4ed6\u4eec\u8bf4\u660e\u6211\u5728\u5916\u65cf\u4eba\u4e2d\u6240\u4f20\u7684\u798f\u97f3\uff0c\u79c1\u4e0b\u8bb2\u4e86\u7ed9\u90a3\u4e9b\u6709\u540d\u671b\u7684\u4eba\u542c\uff0c\u514d\u5f97\u6211\u4ece\u524d\u6216\u73b0\u5728\u90fd\u767d\u8dd1\u4e86\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e0e\u6211\u540c\u884c\u7684\u63d0\u591a\uff0c\u867d\u7136\u662f\u5e0c\u814a\u4eba\uff0c\u4e5f\u6ca1\u6709\u5f3a\u8feb\u4ed6\u53d7\u5272\u793c\uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8fd9\u662f\u56e0\u4e3a\u6709\u4e9b\u6df7\u8fdb\u6765\u7684\u5047\u5f1f\u5144\uff0c\u6697\u6697\u5730\u6765\u4fa6\u67e5\u6211\u4eec\u5728\u57fa\u7763\u8036\u7a23\u91cc\u4eab\u6709\u7684\u81ea\u7531\uff0c\u4e3a\u7684\u662f\u8981\u8f96\u5236\u6211\u4eec\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5bf9\u8fd9\u4e9b\u4eba\uff0c\u6211\u4eec\u4e00\u70b9\u4e5f\u6ca1\u6709\u8ba9\u6b65\u59a5\u534f\uff0c\u4e3a\u4e86\u8981\u4f7f\u798f\u97f3\u7684\u771f\u7406\u5b58\u7559\u5728\u4f60\u4eec\u4e2d\u95f4\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u81f3\u4e8e\u90a3\u4e9b\u88ab\u8ba4\u4e3a\u6709\u540d\u671b\u7684\u4eba\uff08\u65e0\u8bba\u4ed6\u4eec\u4ece\u524d\u600e\u4e48\u6837\uff0c\u90fd\u4e0e\u6211\u65e0\u5173\uff1b\u3000\u795e\u4e0d\u4ee5\u5916\u8c8c\u53d6\u4eba\uff09\uff0c\u4ed6\u4eec\u5e76\u6ca1\u6709\u7ed9\u6211\u589e\u52a0\u4ec0\u4e48\uff1b\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u53cd\u800c\u56e0\u4e3a\u770b\u89c1\u6211\u53d7\u6258\u4f20\u798f\u97f3\u7ed9\u6ca1\u6709\u53d7\u5272\u793c\u7684\u4eba\uff0c\u5c31\u50cf\u5f7c\u5f97\u53d7\u6258\u4f20\u798f\u97f3\u7ed9\u53d7\u5272\u793c\u7684\u4eba\u4e00\u6837\uff0c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\uff08\u56e0\u4e3a\u8fd0\u884c\u5728\u5f7c\u5f97\u91cc\u9762\uff0c\u4f7f\u4ed6\u4f5c\u53d7\u5272\u793c\u8005\u7684\u4f7f\u5f92\u7684\u90a3\u4e00\u4f4d\uff0c\u4e5f\u8fd0\u884c\u5728\u6211\u91cc\u9762\uff0c\u4f7f\u6211\u4f5c\u5916\u65cf\u4eba\u7684\u4f7f\u5f92\uff0c\uff09\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u53c8\u77e5\u9053\u6211\u8499\u4e86\u3000\u795e\u7684\u6069\uff0c\u90a3\u51e0\u4f4d\u88ab\u8a89\u4e3a\u67f1\u77f3\u7684\u96c5\u5404\u3001\u77f6\u6cd5\u548c\u7ea6\u7ff0\uff0c\u5c31\u5411\u6211\u548c\u5df4\u62ff\u5df4\u4f38\u51fa\u53f3\u624b\u4e92\u76f8\u8bb8\u8bfa\uff0c\u6211\u4eec\u8981\u5230\u5916\u65cf\u4eba\u90a3\u91cc\u53bb\u4f20\u798f\u97f3\uff0c\u4ed6\u4eec\u8981\u5230\u53d7\u5272\u793c\u7684\u4eba\u90a3\u91cc\u53bb\u4f20\u798f\u97f3\uff1b\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u53ea\u662f\u8981\u6211\u4eec\u8bb0\u5ff5\u7a77\u4eba\uff0c\u8fd9\u672c\u6765\u4e5f\u662f\u6211\u4e00\u5411\u70ed\u5fc3\u4f5c\u7684\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5f7c\u5f97\u53d7\u8d23\u5907\u53ef\u662f\uff0c\u77f6\u6cd5\u5230\u4e86\u5b89\u63d0\u963f\u7684\u65f6\u5019\uff0c\u56e0\u4e3a\u4ed6\u6709\u8be5\u8d23\u5907\u7684\u5730\u65b9\uff0c\u6211\u5c31\u5f53\u9762\u53cd\u5bf9\u4ed6\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4ece\u96c5\u5404\u90a3\u91cc\u6765\u7684\u4eba\u8fd8\u6ca1\u6709\u5230\u4ee5\u524d\uff0c\u4ed6\u548c\u5916\u65cf\u4eba\u4e00\u540c\u5403\u996d\uff1b\u4f46\u4ed6\u4eec\u6765\u5230\u4e86\uff0c\u4ed6\u56e0\u4e3a\u6015\u90a3\u4e9b\u5b88\u5272\u793c\u7684\u4eba\uff0c\u5c31\u4ece\u5916\u65cf\u4eba\u4e2d\u9000\u51fa\u6765\uff0c\u548c\u4ed6\u4eec\u5206\u5f00\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u5176\u4f59\u7684\u72b9\u592a\u4eba\u4e5f\u548c\u4ed6\u4e00\u540c\u88c5\u5047\uff0c\u751a\u81f3\u8fde\u5df4\u62ff\u5df4\u4e5f\u53d7\u4e86\u5f71\u54cd\uff0c\u8ddf\u7740\u4ed6\u4eec\u88c5\u5047\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f46\u6211\u4e00\u89c1\u4ed6\u4eec\u6240\u884c\u7684\u4e0d\u5408\u798f\u97f3\u7684\u771f\u7406\uff0c\u5c31\u5f53\u4f17\u5bf9\u77f6\u6cd5\u8bf4\uff1a\u201c\u4f60\u662f\u72b9\u592a\u4eba\uff0c\u751f\u6d3b\u65e2\u7136\u50cf\u5916\u65cf\u4eba\u800c\u4e0d\u50cf\u72b9\u592a\u4eba\uff0c\u600e\u4e48\u8fd8\u52c9\u5f3a\u5916\u65cf\u4eba\u8ddf\u968f\u72b9\u592a\u4eba\u7684\u89c4\u77e9\u5462\uff1f\u201d\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u56e0\u4fe1\u79f0\u4e49\u6211\u4eec\u751f\u6765\u662f\u72b9\u592a\u4eba\uff0c\u4e0d\u662f\u5916\u65cf\u4e2d\u7684\u7f6a\u4eba\uff1b\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u65e2\u7136\u77e5\u9053\u4eba\u79f0\u4e49\u4e0d\u662f\u9760\u884c\u5f8b\u6cd5\uff0c\u800c\u662f\u56e0\u4fe1\u8036\u7a23\u57fa\u7763\uff0c\u6211\u4eec\u4e5f\u5c31\u4fe1\u4e86\u57fa\u7763\u8036\u7a23\uff0c\u4f7f\u6211\u4eec\u56e0\u4fe1\u57fa\u7763\u79f0\u4e49\uff0c\u4e0d\u662f\u9760\u884c\u5f8b\u6cd5\uff1b\u56e0\u4e3a\u6ca1\u6709\u4eba\u80fd\u9760\u884c\u5f8b\u6cd5\u79f0\u4e49\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u5982\u679c\u6211\u4eec\u5bfb\u6c42\u5728\u57fa\u7763\u91cc\u79f0\u4e49\uff0c\u5374\u4ecd\u7136\u662f\u7f6a\u4eba\uff0c\u96be\u9053\u57fa\u7763\u662f\u4f7f\u4eba\u72af\u7f6a\u7684\u5417\uff1f\u7edd\u5bf9\u4e0d\u662f\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
