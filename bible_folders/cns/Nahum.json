{
    "version": "cns",
    "book_name": "Nahum",
    "book_nr": "34",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6709\u5173\u5c3c\u5c3c\u5fae\u7684\u9884\u8a00\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5ac9\u6076\u548c\u65bd\u884c\u62a5\u590d\u7684\u3000\u795e\u8036\u548c\u534e\u662f\u5ac9\u6076\u548c\u65bd\u884c\u62a5\u590d\u7684\u3000\u795e\uff1b\u8036\u548c\u534e\u65bd\u884c\u62a5\u590d\uff0c\u5e76\u4e14\u6ee1\u6000\u70c8\u6012\uff1b\u8036\u548c\u534e\u5411\u4ed6\u7684\u5bf9\u5934\u65bd\u884c\u62a5\u590d\uff0c\u5411\u4ed6\u7684\u4ec7\u654c\u6000\u6012\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u8036\u548c\u534e\u4e0d\u8f7b\u6613\u53d1\u6012\uff0c\u53ef\u662f\u5927\u6709\u80fd\u529b\uff1b\u4ed6\u7edd\u4e0d\u4ee5\u6709\u7f6a\u7684\u4e3a\u65e0\u7f6a\u3002\u8036\u548c\u534e\u4e58\u7740\u72c2\u98ce\u66b4\u96e8\u800c\u6765\uff0c\u4e91\u5f69\u662f\u4ed6\u811a\u4e0b\u7684\u5c18\u571f\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u4e00\u65a5\u8d23\u6d77\uff0c\u6d77\u5c31\u5e72\u4e86\uff1b\u4ed6\u4f7f\u4e00\u5207\u6c5f\u6cb3\u67af\u5e72\u3002\u5df4\u73ca\u548c\u8fe6\u5bc6\u7684\u6811\u6797\u67af\u69c1\uff0c\u9ece\u5df4\u5ae9\u7684\u82b1\u5349\u51cb\u6b8b\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5927\u5c71\u5728\u4ed6\u9762\u524d\u9707\u52a8\uff0c\u5c0f\u5c71\u4e5f\u90fd\u878d\u5316\uff1b\u5927\u5730\u5728\u4ed6\u9762\u524d\u5e9f\u53bb\uff0c\u4e16\u754c\u548c\u6240\u6709\u4f4f\u5728\u4e16\u4e0a\u7684\uff0c\u4e5f\u90fd\u8fd9\u6837\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5728\u4ed6\u76db\u6012\u4e4b\u4e0b\uff0c\u8c01\u80fd\u7ad9\u5f97\u4f4f\u5462\uff1f\u4ed6\u7684\u70c8\u6012\u8c01\u80fd\u53d7\u5f97\u4e86\u5462\uff1f\u4ed6\u7684\u5fff\u6012\u50cf\u706b\u4e00\u822c\u55b7\u51fa\u6765\uff0c\u78d0\u77f3\u5728\u4ed6\u9762\u524d\u90fd\u5d29\u88c2\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u8036\u548c\u534e\u662f\u826f\u5584\u7684\uff1b\u5728\u60a3\u96be\u7684\u65f6\u5019\uff0c\u4ed6\u4f5c\u4eba\u7684\u907f\u96be\u6240\uff1b\u4fe1\u9760\u4ed6\u7684\u4eba\uff0c\u4ed6\u90fd\u8ba4\u8bc6\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f46\u4ed6\u5fc5\u7528\u6cdb\u6ee5\u7684\u6d2a\u6c34\uff0c\u5c3d\u884c\u6bc1\u706d\u5c3c\u5c3c\u5fae\u4e4b\u5730\uff0c\u628a\u4ed6\u7684\u4ec7\u654c\u8d76\u5165\u9ed1\u6697\u4e4b\u4e2d\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u8b66\u544a\u5c3c\u5c3c\u5fae\u5c3c\u5c3c\u5fae\u4eba\u54ea\uff0c\u4f60\u4eec\u5bf9\u8036\u548c\u534e\u8fd8\u6709\u4ec0\u4e48\u4f01\u56fe\uff1f\u4ed6\u5fc5\u5c3d\u884c\u6bc1\u706d\uff1b\u60a3\u96be\u5fc5\u4e0d\u518d\u6b21\u6765\u4e34\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4ed6\u4eec\u50cf\u7f20\u7ed3\u7740\u7684\u8346\u68d8\uff0c\u50cf\u559d\u9189\u4e86\u7684\u9152\u5f92\uff0c\u53c8\u50cf\u67af\u5e72\u7684\u79be\u79f8\uff0c\u5168\u90fd\u88ab\u541e\u706d\u4e86\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6709\u4eba\u4ece\u4f60\u90a3\u91cc\u51fa\u6765\uff0c\u5c31\u662f\u7b79\u5212\u5978\u6076\u7684\u4eba\uff0c\u56fe\u8c0b\u6076\u4e8b\u653b\u51fb\u8036\u548c\u534e\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5b89\u6170\u72b9\u5927\u8036\u548c\u534e\u8fd9\u6837\u8bf4\uff1a\u5c3c\u5c3c\u5fae\u4eba\u867d\u7136\u88c5\u5907\u5b8c\u6574\uff0c\u4eba\u6570\u4f17\u591a\uff0c\u4ed6\u4eec\u4ecd\u5fc5\u88ab\u526a\u9664\uff0c\u8fd9\u6837\u5c31\u5b8c\u4e86\u3002\u6211\u65e2\u7136\u4f7f\u4f60\u53d7\u8fc7\u82e6\uff0c\u5c31\u4e00\u5b9a\u4e0d\u518d\u53eb\u4f60\u53d7\u82e6\u4e86\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u72b9\u5927\u554a\uff01\u73b0\u5728\u6211\u5fc5\u7c89\u788e\u4ed6\u5728\u4f60\u8eab\u4e0a\u7684\u8f6d\uff0c\u6253\u65ad\u4f60\u7684\u9501\u94fe\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5c3c\u5c3c\u5fae\u554a\uff01\u8036\u548c\u534e\u5df2\u7ecf\u5411\u4f60\u4e0b\u4e86\u547d\u4ee4\uff1a\u4f60\u7684\u540d\u5b57\u5fc5\u4e0d\u518d\u4f20\u4e8e\u540e\u4e16\uff1b\u6211\u5fc5\u4ece\u4f60\u7684\u795e\u5e99\u4e2d\uff0c\u9664\u6389\u96d5\u523b\u548c\u94f8\u9020\u7684\u5076\u50cf\uff1b\u6211\u5fc5\u4e3a\u4f60\u9884\u5907\u575f\u5893\uff0c\u56e0\u4e3a\u4f60\u6ca1\u6709\u5b58\u5728\u7684\u4ef7\u503c\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u770b\u54ea\uff01\u90a3\u4f20\u62a5\u4f73\u97f3\u3001\u5ba3\u544a\u548c\u5e73\u4e4b\u4eba\u7684\u811a\uff0c\u5df2\u7ecf\u7ad9\u5728\u5c71\u4e0a\uff0c\u8bf4\uff1a\u72b9\u5927\u554a\uff01\u5b88\u4f60\u7684\u8282\u671f\uff0c\u8fd8\u4f60\u7684\u613f\u5427\uff01\u56e0\u4e3a\u90a3\u5978\u6076\u7684\u4eba\u6c38\u4e0d\u4f1a\u518d\u4ece\u4f60\u4e2d\u95f4\u7ecf\u8fc7\uff1b\u4ed6\u5df2\u88ab\u5f7b\u5e95\u9664\u706d\u4e86\u3002\uff08\u672c\u8282\u5728\u300a\u9a6c\u7d22\u62c9\u6284\u672c\u300b\u4e3a2:1\uff09\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5ba1\u5224\u5c3c\u5c3c\u5fae\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u7136\u800c\uff0c\u8036\u548c\u534e\u5fc5\u6062\u590d\u96c5\u5404\u7684\u5149\u8363\uff0c\u597d\u50cf\u4ee5\u8272\u5217\u7684\u5149\u8363\u4e00\u6837\uff1b\u56e0\u4e3a\u52ab\u63a0\u7684\u4eba\u66fe\u628a\u4ed6\u4eec\u52ab\u63a0\u4e00\u7a7a\uff0c\u53c8\u628a\u4ed6\u4eec\u7684\u8461\u8404\u679d\u5b50\u6bc1\u574f\u4e86\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56f4\u653b\u5c3c\u5c3c\u5fae\u4ed6\u52c7\u58eb\u7684\u76fe\u724c\u662f\u7ea2\u7684\uff0c\u6218\u58eb\u7684\u8863\u670d\u662f\u6731\u7ea2\u7684\u3002\u5728\u4ed6\u9884\u5907\u51fa\u51fb\u7684\u65f6\u5019\uff0c\u6218\u8f66\u7684\u94a2\u94c1\u95ea\u70c1\u5982\u706b\uff0c\u9a91\u5175\u75be\u9a70\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6218\u8f66\u5728\u8857\u4e0a\u72c2\u5954\uff0c\u5728\u5e7f\u573a\u4e0a\u4e1c\u5954\u897f\u9a70\uff1b\u5b83\u4eec\u770b\u8d77\u6765\u50cf\u706b\u628a\uff0c\u8dd1\u8d77\u6765\u50cf\u95ea\u7535\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4e9a\u8ff0\u738b\u53ec\u96c6\u4ed6\u7684\u6743\u81e3\uff0c\u4ed6\u4eec\u78b0\u78b0\u8dcc\u8dcc\u800c\u6765\uff0c\u6025\u6025\u5fd9\u5fd9\u8d70\u4e0a\u57ce\u5899\uff0c\u7ad6\u8d77\u4e86\u9632\u5fa1\u7684\u6321\u724c\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6cb3\u95f8\u90fd\u88ab\u6253\u5f00\uff0c\u5bab\u6bbf\u4e5f\u7ed9\u6467\u6bc1\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u738b\u540e\u8d64\u8eab\u88ab\u63b3\u53bb\uff1b\u5979\u7684\u5bab\u5973\u90fd\u54c0\u54ed\uff0c\u58f0\u5982\u9e3d\u5b50\uff0c\u5404\u4eba\u6376\u80f8\u75db\u54ed\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5c3c\u5c3c\u5fae\u88ab\u52ab\u63a0\u81ea\u53e4\u4ee5\u6765\uff0c\u5c3c\u5c3c\u5fae\u4e00\u76f4\u90fd\u50cf\u4e2a\u6c34\u6c60\uff0c\u6c60\u6c34\u4e0d\u65ad\u6d41\u51fa\uff1b\u867d\u7136\u6709\u4eba\u547c\u53eb\uff1a\u201c\u6b62\u4f4f\uff01\u6b62\u4f4f\uff01\u201d\u5374\u6ca1\u6709\u56de\u8f6c\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u4eec\u52ab\u63a0\u94f6\u5b50\uff0c\u62a2\u593a\u91d1\u5b50\u5427\uff01\u79ef\u84c4\u7684\u8d22\u5b9d\u65e0\u7a77\u65e0\u5c3d\uff0c\u73cd\u8d35\u7684\u5668\u76bf\u4e0d\u53ef\u80dc\u6570\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u73b0\u5728\u5374\u7a7a\u865a\u3001\u8352\u51c9\uff0c\u4e00\u65e0\u6240\u6709\uff1b\u4eba\u5fc3\u60ca\u60f6\uff0c\u4e24\u819d\u53d1\u6296\uff0c\u5168\u8eab\u75bc\u75db\uff0c\u9762\u65e0\u8840\u8272\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5c3c\u5c3c\u5fae\u88ab\u6bc1\u706d\u72ee\u5b50\u7684\u6d1e\u7a74\u5728\u54ea\u91cc\uff1f\u5582\u517b\u5e7c\u72ee\u7684\u5730\u65b9\u5728\u54ea\u91cc\uff1f\u516c\u72ee\u3001\u6bcd\u72ee\u548c\u5e7c\u72ee\u4e0d\u53d7\u60ca\u5413\u4e4b\u5904\uff0c\u53ef\u4ee5\u95f2\u6e38\u7684\u5730\u65b9\u53c8\u5728\u54ea\u91cc\u5462\uff1f\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u516c\u72ee\u4e3a\u5e7c\u72ee\u6495\u788e\u8db3\u591f\u7684\u98df\u7269\uff0c\u4e3a\u6bcd\u72ee\u6390\u6b7b\u6d3b\u7269\uff1b\u628a\u730e\u7269\u585e\u6ee1\u4e86\u5b83\u7684\u6d1e\uff0c\u628a\u6495\u788e\u7684\u586b\u6ee1\u4e86\u5b83\u7684\u7a74\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u770b\u54ea\uff01\u6211\u5fc5\u653b\u51fb\u4f60\uff0c\u6211\u8981\u628a\u4f60\u7684\u6218\u8f66\u711a\u70e7\u6210\u70df\uff0c\u5200\u5251\u5fc5\u541e\u706d\u4f60\u7684\u5e7c\u72ee\uff1b\u6211\u5fc5\u4ece\u5730\u4e0a\u9664\u53bb\u4f60\u7684\u730e\u7269\uff0c\u4f60\u4f7f\u8005\u7684\u58f0\u97f3\u4e5f\u5fc5\u4e0d\u518d\u6709\u4eba\u542c\u89c1\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5c3c\u5c3c\u5fae\u7f6a\u6709\u5e94\u5f97\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u97ad\u58f0\u98d5\u98d5\uff0c\u8f6e\u58f0\u8f9a\u8f9a\uff0c\u9a8f\u9a6c\u5954\u9a70\uff0c\u6218\u8f66\u98a0\u7c38\u8df3\u52a8\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u9a91\u5175\u817e\u8dc3\u51b2\u950b\uff1b\u5200\u5251\u70c1\u70c1\u53d1\u4eae\uff0c\u67aa\u77db\u95ea\u95ea\u751f\u5149\uff1b\u88ab\u6740\u7684\u4eba\u4f17\u591a\uff0c\u6b7b\u5c38\u6210\u5806\uff1b\u5c38\u4f53\u65e0\u6570\uff0c\u4f17\u4eba\u90fd\u88ab\u5c38\u4f53\u7eca\u5012\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u90fd\u56e0\u5993\u5973\u591a\u6709\u6deb\u884c\uff0c\u65bd\u90aa\u672f\u7684\u7f8e\u4e3d\u5973\u5deb\uff0c\u501f\u7740\u5979\u7684\u6deb\u884c\u8bf1\u60d1\u5217\u56fd\uff0c\u501f\u7740\u5979\u7684\u90aa\u672f\u6b3a\u9a97\u4e07\u65cf\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u770b\u54ea\uff01\u6211\u5fc5\u653b\u51fb\u4f60\uff1b\u6211\u8981\u628a\u4f60\u7684\u88d9\u5b50\u6380\u5230\u4f60\u7684\u8138\u4e0a\uff0c\u4f7f\u4e07\u56fd\u770b\u89c1\u4f60\u8d64\u8eab\u9732\u4f53\uff0c\u4f7f\u5217\u90a6\u770b\u89c1\u4f60\u7684\u7f9e\u803b\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u8981\u628a\u6c61\u79fd\u53ef\u618e\u4e4b\u7269\u62cb\u5728\u4f60\u8eab\u4e0a\uff0c\u4fae\u8fb1\u4f60\uff0c\u4f7f\u4f60\u6210\u4e3a\u4f17\u4eba\u89c2\u770b\u7684\u5bf9\u8c61\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u770b\u89c1\u4f60\u7684\u90fd\u5fc5\u9003\u907f\u4f60\uff0c\u8bf4\uff1a\u201c\u5c3c\u5c3c\u5fae\u8352\u51c9\u4e86\uff1b\u6709\u8c01\u4e3a\u5979\u60b2\u54c0\u5462\uff1f\u201d\u6211\u5728\u54ea\u91cc\u53ef\u4ee5\u627e\u5230\u5b89\u6170\u4f60\u7684\u4eba\u5462\uff1f\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u632a\u4e9a\u4eec\u4f5c\u524d\u8f66\u4e4b\u9274\u4f60\u6bd4\u632a\u4e9a\u4eec\u8fd8\u5f3a\u5417\uff1f\u632a\u4e9a\u4eec\u4f4d\u4e8e\u5c3c\u7f57\u6cb3\u6d41\u57df\uff0c\u5468\u56f4\u6709\u6c34\u73af\u7ed5\uff1b\u6d77\u662f\u5979\u7684\u5c4f\u969c\uff0c\u6c34\u4f5c\u5979\u7684\u5899\u57a3\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u53e4\u5b9e\u548c\u57c3\u53ca\uff0c\u662f\u5979\u65e0\u7a77\u7684\u529b\u91cf\uff0c\u5f17\u4eba\u548c\u8def\u6bd4\u65cf\uff0c\u662f\u5979\u7684\u52a9\u624b\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u7136\u800c\u5979\u5374\u88ab\u8fc1\u5f99\uff0c\u88ab\u63b3\u53bb\u4e86\uff1b\u5979\u7684\u5a74\u5b69\u5728\u5404\u8857\u5934\u88ab\u6454\u6b7b\uff1b\u4eba\u4eec\u4e3a\u5979\u7684\u5c0a\u8d35\u4eba\u62bd\u7b7e\u3002\u5979\u6240\u6709\u7684\u8fbe\u5b98\u8d35\u4eba\u90fd\u88ab\u9501\u94fe\u6346\u7740\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
