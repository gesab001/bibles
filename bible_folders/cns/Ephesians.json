            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u613f\u6069\u60e0\u5e73\u5b89\u4ece\u6211\u4eec\u7684\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u4e34\u5230\u4f60\u4eec\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5728\u57fa\u7763\u91cc\u5c5e\u7075\u7684\u798f\u5206\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u7236\u3000\u795e\u662f\u5e94\u5f53\u79f0\u9882\u7684\u3002\u4ed6\u5728\u57fa\u7763\u91cc\uff0c\u66fe\u7ecf\u628a\u5929\u4e0a\u5404\u79cd\u5c5e\u7075\u7684\u798f\u5206\u8d50\u7ed9\u6211\u4eec\uff1a\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5c31\u5982\u521b\u7acb\u4e16\u754c\u4ee5\u524d\uff0c\u4ed6\u5728\u57fa\u7763\u91cc\u62e3\u9009\u4e86\u6211\u4eec\uff0c\u4f7f\u6211\u4eec\u56e0\u7740\u7231\uff0c\u5728\u4ed6\u9762\u524d\u6210\u4e3a\u5723\u6d01\uff0c\u6ca1\u6709\u7455\u75b5\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4ed6\u53c8\u6309\u7740\u81ea\u5df1\u65e8\u610f\u6240\u559c\u60a6\u7684\uff0c\u9884\u5b9a\u6211\u4eec\u501f\u7740\u8036\u7a23\u57fa\u7763\u5f97\u513f\u5b50\u7684\u540d\u5206\uff0c\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u597d\u4f7f\u4ed6\u6069\u5178\u7684\u8363\u8000\u5f97\u7740\u9882\u8d5e\u3002\u8fd9\u6069\u5178\u662f\u4ed6\u5728\u7231\u5b50\u91cc\u8d50\u7ed9\u6211\u4eec\u7684\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6211\u4eec\u5728\u4ed6\u7231\u5b50\u91cc\uff0c\u501f\u7740\u4ed6\u7684\u8840\u8499\u4e86\u6551\u8d4e\uff0c\u8fc7\u72af\u5f97\u5230\u8d66\u514d\uff0c\u90fd\u662f\u6309\u7740\u4ed6\u4e30\u76db\u7684\u6069\u5178\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u8fd9\u6069\u5178\u662f\u4ed6\u7528\u5404\u6837\u7684\u667a\u6167\u548c\u806a\u660e\uff0c\u5145\u5145\u8db3\u8db3\u5730\u8d50\u7ed9\u6211\u4eec\u7684\uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u7167\u7740\u81ea\u5df1\u5728\u57fa\u7763\u91cc\u9884\u5148\u5b89\u6392\u7684\u7f8e\u610f\uff0c\u4f7f\u6211\u4eec\u77e5\u9053\u4ed6\u65e8\u610f\u7684\u5965\u79d8\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5230\u4e86\u6240\u8ba1\u5212\u7684\u65f6\u673a\u6210\u719f\uff0c\u5c31\u4f7f\u5929\u4e0a\u5730\u4e0a\u7684\u4e07\u6709\uff0c\u90fd\u5728\u57fa\u7763\u91cc\u540c\u5f52\u4e8e\u4e00\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u90a3\u51ed\u7740\u81ea\u5df1\u65e8\u610f\u6240\u8ba1\u5212\u800c\u884c\u4e07\u4e8b\u7684\uff0c\u6309\u7740\u4ed6\u9884\u5148\u6240\u5b89\u6392\u7684\uff0c\u9884\u5b9a\u6211\u4eec\u5728\u57fa\u7763\u91cc\u5f97\u57fa\u4e1a\uff08\u201c\u5f97\u57fa\u4e1a\u201d\u6216\u8bd1\uff1a\u201c\u6210\u4e3a\u3000\u795e\u7684\u4ea7\u4e1a\u201d\uff09\uff0c\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u501f\u7740\u6211\u4eec\u8fd9\u5728\u57fa\u7763\u91cc\u9996\u5148\u6709\u76fc\u671b\u7684\u4eba\uff0c\u4f7f\u4ed6\u7684\u8363\u8000\u5f97\u7740\u9882\u8d5e\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u4eec\u65e2\u7136\u542c\u4e86\u771f\u7406\u7684\u9053\uff0c\u5c31\u662f\u4f7f\u4f60\u4eec\u5f97\u6551\u7684\u798f\u97f3\uff0c\u4e5f\u4fe1\u4e86\u57fa\u7763\uff0c\u5c31\u5728\u4ed6\u91cc\u9762\u53d7\u4e86\u6240\u5e94\u8bb8\u7684\u5723\u7075\u4f5c\u4e3a\u5370\u8bb0\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u8fd9\u5723\u7075\u662f\u6211\u4eec\u5f97\u57fa\u4e1a\u7684\u51ed\u636e\uff0c\u76f4\u5230\u3000\u795e\u7684\u4ea7\u4e1a\u5f97\u8d4e\uff0c\u4f7f\u4ed6\u7684\u8363\u8000\u5f97\u7740\u9882\u8d5e\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4fdd\u7f57\u7684\u7948\u7977\u56e0\u6b64\uff0c\u6211\u542c\u5230\u4f60\u4eec\u5728\u4e3b\u8036\u7a23\u91cc\u7684\u4fe1\u5fc3\uff0c\u548c\u5bf9\u4f17\u5723\u5f92\u7684\u7231\u5fc3\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5c31\u4e0d\u4f4f\u5730\u4e3a\u4f60\u4eec\u732e\u4e0a\u611f\u8c22\u3002\u6211\u7977\u544a\u7684\u65f6\u5019\uff0c\u5e38\u5e38\u63d0\u5230\u4f60\u4eec\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u6c42\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u3000\u795e\uff0c\u8363\u8000\u7684\u7236\uff0c\u8d50\u7ed9\u4f60\u4eec\u667a\u6167\u548c\u542f\u793a\u7684\u7075\uff0c\u4f7f\u4f60\u4eec\u5145\u5206\u5730\u8ba4\u8bc6\u4ed6\uff0c\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5e76\u4e14\u4f7f\u4f60\u4eec\u5fc3\u7075\u7684\u773c\u775b\u660e\u4eae\uff0c\u53ef\u4ee5\u77e5\u9053\u4ed6\u7684\u547c\u53ec\u6709\u600e\u6837\u7684\u76fc\u671b\uff1b\u4ed6\u57fa\u4e1a\u7684\u8363\u8000\uff0c\u5728\u5723\u5f92\u4e2d\u662f\u591a\u4e48\u7684\u4e30\u76db\uff1b\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u3000\u795e\u7167\u7740\u4ed6\u5f3a\u5927\u7684\u529b\u91cf\uff0c\u5411\u6211\u4eec\u4fe1\u4ed6\u7684\u4eba\u663e\u51fa\u7684\u80fd\u529b\uff0c\u662f\u4f55\u7b49\u7684\u6d69\u5927\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u8fd9\u529b\u91cf\u8fd0\u884c\u5728\u57fa\u7763\u8eab\u4e0a\uff0c\u4f7f\u4ed6\u4ece\u6b7b\u4eba\u4e2d\u590d\u6d3b\uff0c\u5e76\u4e14\u5728\u5929\u4e0a\u5750\u5728\u81ea\u5df1\u7684\u53f3\u8fb9\uff0c\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u8fdc\u8d85\u8fc7\u4e00\u5207\u6267\u653f\u7684\u3001\u638c\u6743\u7684\u3001\u6709\u80fd\u7684\u3001\u4f5c\u4e3b\u7684\uff0c\u548c\u4eca\u751f\u6765\u4e16\u6240\u80fd\u4e3e\u51fa\u7684\u4e00\u5207\u540d\u8854\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u3000\u795e\u53c8\u4f7f\u4e07\u6709\u90fd\u5f52\u670d\u5728\u4ed6\u7684\u811a\u4e0b\uff0c\u5e76\u4e14\u4f7f\u4ed6\u4f5c\u6559\u4f1a\u81f3\u9ad8\u7684\u5143\u9996\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u6559\u4f1a\u662f\u4ed6\u7684\u8eab\u4f53\uff0c\u662f\u90a3\u5145\u6ee1\u4e07\u6709\u8005\u6240\u5b8c\u5168\u5145\u6ee1\u7684\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u51fa\u6b7b\u5165\u751f\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u90a3\u65f6\u4f60\u4eec\u5728\u8fc7\u72af\u548c\u7f6a\u6076\u4e2d\u884c\u4e8b\u4e3a\u4eba\uff0c\u968f\u7740\u65f6\u4ee3\u7684\u6f6e\u6d41\uff0c\u4e5f\u670d\u4ece\u7a7a\u4e2d\u638c\u6743\u7684\u9996\u9886\uff0c\u5c31\u662f\u73b0\u4eca\u5728\u6096\u9006\u7684\u4eba\u8eab\u4e0a\u8fd0\u884c\u7684\u7075\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u4eec\u4ece\u524d\u4e5f\u90fd\u548c\u4ed6\u4eec\u5728\u4e00\u8d77\uff0c\u653e\u7eb5\u8089\u4f53\u7684\u79c1\u6b32\uff0c\u968f\u7740\u8089\u4f53\u548c\u5fc3\u610f\u6240\u559c\u597d\u7684\u53bb\u884c\uff1b\u6211\u4eec\u4e0e\u522b\u4eba\u4e00\u6837\uff0c\u751f\u6765\u90fd\u662f\u53ef\u6012\u7684\u513f\u5973\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u7136\u800c\u3000\u795e\u6ee1\u6709\u601c\u60af\uff0c\u56e0\u7740\u4ed6\u7231\u6211\u4eec\u7684\u5927\u7231\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5c31\u5728\u6211\u4eec\u56e0\u8fc7\u72af\u6b7b\u4e86\u7684\u65f6\u5019\uff0c\u4f7f\u6211\u4eec\u4e0e\u57fa\u7763\u4e00\u540c\u6d3b\u8fc7\u6765\uff0c\uff08\u4f60\u4eec\u5f97\u6551\u662f\u9760\u7740\u6069\u5178\uff0c\uff09\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u53c8\u4f7f\u6211\u4eec\u5728\u57fa\u7763\u8036\u7a23\u91cc\uff0c\u4e0e\u4ed6\u4e00\u540c\u590d\u6d3b\uff0c\u4e00\u540c\u5750\u5728\u5929\u4e0a\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4e3a\u7684\u662f\u8981\u5728\u5c06\u6765\u7684\u4e16\u4ee3\u4e2d\uff0c\u663e\u660e\u4ed6\u5728\u57fa\u7763\u8036\u7a23\u91cc\u8d50\u7ed9\u6211\u4eec\u7684\u6069\u5178\uff0c\u662f\u591a\u4e48\u7684\u4e30\u76db\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u4eec\u5f97\u6551\u662f\u9760\u7740\u6069\u5178\uff0c\u501f\u7740\u4fe1\u5fc3\u3002\u8fd9\u4e0d\u662f\u51fa\u4e8e\u81ea\u5df1\uff0c\u800c\u662f\u3000\u795e\u6240\u8d50\u7684\uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u8fd9\u4e5f\u4e0d\u662f\u51fa\u4e8e\u884c\u4e3a\uff0c\u514d\u5f97\u6709\u4eba\u81ea\u5938\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u4eec\u539f\u662f\u3000\u795e\u6240\u4f5c\u6210\u7684\uff0c\u662f\u5728\u57fa\u7763\u8036\u7a23\u91cc\u521b\u9020\u7684\uff0c\u4e3a\u7684\u662f\u8981\u6211\u4eec\u884c\u5404\u6837\u7684\u5584\u4e8b\uff0c\u5c31\u662f\u3000\u795e\u9884\u5148\u6240\u5b89\u6392\u7684\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5728\u57fa\u7763\u91cc\u5408\u800c\u4e3a\u4e00\u4f60\u4eec\u5e94\u5f53\u8bb0\u5f97\uff0c\u4f60\u4eec\u4ece\u524d\u6309\u8089\u4f53\u6765\u8bf4\u662f\u5916\u65cf\u4eba\uff1b\u90a3\u4e9b\u6240\u8c13\u5728\u8089\u4f53\u4e0a\u53d7\u8fc7\u4eba\u624b\u6240\u884c\u7684\u5272\u793c\u7684\u4eba\uff0c\u79f0\u4f60\u4eec\u4e3a\u672a\u53d7\u5272\u793c\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u90a3\u65f6\uff0c\u4f60\u4eec\u662f\u5728\u57fa\u7763\u4ee5\u5916\uff0c\u4e0e\u4ee5\u8272\u5217\u56fd\u65e0\u5206\uff0c\u5728\u5e26\u6709\u5e94\u8bb8\u7684\u7ea6\u4e0a\u662f\u5916\u4eba\uff0c\u5728\u4e16\u4e0a\u6ca1\u6709\u76fc\u671b\uff0c\u6ca1\u6709\u3000\u795e\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u4eec\u4ece\u524d\u8fdc\u79bb\u7684\u4eba\uff0c\u73b0\u4eca\u5728\u57fa\u7763\u8036\u7a23\u91cc\uff0c\u9760\u7740\u4ed6\u7684\u8840\u5df2\u7ecf\u53ef\u4ee5\u4eb2\u8fd1\u4e86\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u57fa\u7763\u5c31\u662f\u6211\u4eec\u7684\u548c\u5e73\uff1a\u4ed6\u4f7f\u53cc\u65b9\u5408\u800c\u4e3a\u4e00\uff0c\u62c6\u6bc1\u4e86\u9694\u5728\u4e2d\u95f4\u7684\u5899\uff0c\u5c31\u662f\u4ee5\u81ea\u5df1\u7684\u8eab\u4f53\u9664\u6389\u53cc\u65b9\u7684\u4ec7\u6068\uff0c\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5e76\u4e14\u5e9f\u6389\u4e86\u5f8b\u6cd5\u7684\u89c4\u6761\uff0c\u4f7f\u4e24\u8005\u5728\u4ed6\u91cc\u9762\u6210\u4e3a\u4e00\u4e2a\u65b0\u4eba\uff0c\u8fd9\u6837\u5c31\u7f14\u9020\u4e86\u548c\u5e73\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u57fa\u7763\u65e2\u7136\u501f\u7740\u5341\u5b57\u67b6\u6d88\u706d\u4e86\u4ec7\u6068\uff0c\u5c31\u501f\u7740\u5341\u5b57\u67b6\u4f7f\u53cc\u65b9\u4e0e\u3000\u795e\u548c\u597d\uff0c\u6210\u4e3a\u4e00\u4f53\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u5e76\u4e14\u4ed6\u6765\u628a\u548c\u5e73\u7684\u798f\u97f3\u4f20\u7ed9\u4f60\u4eec\u5728\u8fdc\u5904\u7684\u4eba\uff0c\u4e5f\u7ed9\u5728\u8fd1\u5904\u7684\u4eba\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u6211\u4eec\u53cc\u65b9\u90fd\u501f\u7740\u4ed6\uff0c\u5728\u540c\u4e00\u4f4d\u5723\u7075\u91cc\uff0c\u53ef\u4ee5\u8fdb\u5230\u7236\u9762\u524d\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8fd9\u6837\u770b\u6765\uff0c\u4f60\u4eec\u4e0d\u518d\u662f\u5916\u4eba\u548c\u5ba2\u65c5\uff0c\u800c\u662f\u4e0e\u5723\u5f92\u4e00\u540c\u4f5c\u56fd\u6c11\uff0c\u662f\u3000\u795e\u5bb6\u91cc\u7684\u4eba\u4e86\uff0c\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5e76\u4e14\u5efa\u9020\u5728\u4f7f\u5f92\u548c\u5148\u77e5\u7684\u6839\u57fa\u4e0a\uff0c\u57fa\u7763\u8036\u7a23\u81ea\u5df1\u5c31\u662f\u5960\u57fa\u77f3\uff0c\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6574\u5ea7\u5efa\u7b51\u90fd\u9760\u7740\u4ed6\u8fde\u63a5\u914d\u5408\uff0c\u6e10\u6e10\u589e\u957f\u6210\u4e3a\u5728\u4e3b\u91cc\u9762\u7684\u5723\u6240\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4f60\u4eec\u5728\u4ed6\u91cc\u9762\u4e5f\u4e00\u540c\u88ab\u5efa\u9020\uff0c\u6210\u4e3a\u3000\u795e\u501f\u7740\u5723\u7075\u5c45\u4f4f\u7684\u6240\u5728\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4fdd\u7f57\u628a\u798f\u97f3\u4f20\u7ed9\u5916\u65cf\u4eba\n"
                },
                "2": {
