{
    "version": "cns",
    "book_name": "1 Thessalonians",
    "book_nr": "52",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5e16\u6492\u7f57\u5c3c\u8fe6\u6559\u4f1a\u7684\u699c\u6837\u6211\u4eec\u5e38\u5e38\u4e3a\u4f60\u4eec\u4f17\u4eba\u611f\u8c22\u3000\u795e\uff0c\u7977\u544a\u7684\u65f6\u5019\u63d0\u5230\u4f60\u4eec\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5728\u6211\u4eec\u7684\u7236\u3000\u795e\u9762\u524d\uff0c\u4e0d\u4f4f\u5730\u8bb0\u5ff5\u4f60\u4eec\u4fe1\u5fc3\u7684\u5de5\u4f5c\uff0c\u7231\u5fc3\u7684\u52b3\u82e6\u548c\u56e0\u76fc\u671b\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u800c\u6709\u7684\u575a\u5fcd\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u3000\u795e\u6240\u7231\u7684\u5f1f\u5144\u4eec\uff0c\u6211\u4eec\u77e5\u9053\u4f60\u4eec\u662f\u8499\u62e3\u9009\u7684\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0\u4e3a\u6211\u4eec\u7684\u798f\u97f3\u4f20\u5230\u4f60\u4eec\u90a3\u91cc\uff0c\u4e0d\u5355\u662f\u501f\u7740\u8a00\u8bed\uff0c\u4e5f\u662f\u501f\u7740\u6743\u80fd\uff0c\u501f\u7740\u5723\u7075\u548c\u5145\u8db3\u7684\u4fe1\u5fc3\u3002\u4e3a\u4e86\u4f60\u4eec\u7684\u7f18\u6545\uff0c\u6211\u4eec\u5728\u4f60\u4eec\u4e2d\u95f4\u4e3a\u4eba\u600e\u6837\uff0c\u8fd9\u662f\u4f60\u4eec\u77e5\u9053\u7684\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u4eec\u6548\u6cd5\u4e86\u6211\u4eec\uff0c\u4e5f\u6548\u6cd5\u4e86\u4e3b\uff0c\u5728\u5927\u60a3\u96be\u4e2d\uff0c\u5e26\u7740\u5723\u7075\u7684\u559c\u4e50\u63a5\u53d7\u4e86\u771f\u9053\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u8fd9\u6837\uff0c\u4f60\u4eec\u5c31\u6210\u4e86\u9a6c\u5176\u987f\u548c\u4e9a\u8be5\u4e9a\u6240\u6709\u4fe1\u5f92\u7684\u699c\u6837\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u56e0\u4e3a\u4e0d\u5355\u4e3b\u7684\u9053\u4ece\u4f60\u4eec\u90a3\u91cc\u4f20\u904d\u4e86\u9a6c\u5176\u987f\u548c\u4e9a\u8be5\u4e9a\uff0c\u5c31\u662f\u4f60\u4eec\u5bf9\u3000\u795e\u7684\u4fe1\u5fc3\u4e5f\u4f20\u904d\u4e86\u5404\u5904\uff0c\u6240\u4ee5\u6211\u4eec\u4e0d\u7528\u518d\u8bf4\u4ec0\u4e48\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u4eec\u90fd\u8ff0\u8bf4\u4f60\u4eec\u662f\u600e\u6837\u63a5\u5f85\u6211\u4eec\uff0c\u800c\u4e14\u600e\u6837\u79bb\u5f03\u5076\u50cf\u5f52\u5411\u3000\u795e\uff0c\u8981\u670d\u4e8b\u8fd9\u4f4d\u53c8\u771f\u53c8\u6d3b\u7684\u3000\u795e\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5e76\u4e14\u7b49\u5019\u4ed6\u7684\u513f\u5b50\u4ece\u5929\u964d\u4e34\u3002\u8fd9\u5c31\u662f\u3000\u795e\u4f7f\u4ed6\u4ece\u6b7b\u4eba\u4e2d\u590d\u6d3b\uff0c\u6551\u6211\u4eec\u8131\u79bb\u5c06\u6765\u7684\u5fff\u6012\u7684\u90a3\u4f4d\u8036\u7a23\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4fdd\u7f57\u5728\u5e16\u6492\u7f57\u5c3c\u8fe6\u7684\u5de5\u4f5c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6211\u4eec\u4ece\u524d\u5728\u8153\u7acb\u6bd4\u867d\u7136\u53d7\u4e86\u82e6\uff0c\u53c8\u88ab\u51cc\u8fb1\uff0c\u53ef\u662f\u9760\u7740\u6211\u4eec\u7684\u3000\u795e\uff0c\u5728\u5f3a\u70c8\u7684\u53cd\u5bf9\u4e4b\u4e0b\uff0c\u4ecd\u7136\u653e\u80c6\u5411\u4f60\u4eec\u8ff0\u8bf4\u3000\u795e\u7684\u798f\u97f3\uff0c\u8fd9\u662f\u4f60\u4eec\u77e5\u9053\u7684\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u4eec\u7684\u547c\u5401\uff0c\u4e0d\u662f\u9519\u8c2c\u7684\uff0c\u4e0d\u662f\u6c61\u79fd\u7684\uff0c\u4e5f\u6ca1\u6709\u8be1\u8bc8\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u76f8\u53cd\u5730\uff0c\u3000\u795e\u65e2\u7136\u8003\u9a8c\u8fc7\u6211\u4eec\uff0c\u628a\u798f\u97f3\u59d4\u6258\u7ed9\u6211\u4eec\uff0c\u6211\u4eec\u5c31\u4f20\u8bb2\uff0c\u4e0d\u50cf\u662f\u8ba8\u4eba\u6b22\u5fc3\u7684\uff0c\u800c\u662f\u8ba8\u90a3\u5bdf\u9a8c\u6211\u4eec\u5fc3\u601d\u7684\u3000\u795e\u7684\u559c\u60a6\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6211\u4eec\u4ece\u6765\u4e0d\u8bf4\u5949\u627f\u7684\u8bdd\uff0c\u8fd9\u662f\u4f60\u4eec\u77e5\u9053\u7684\uff0c\u3000\u795e\u53ef\u4ee5\u4f5c\u8bc1\uff0c\u6211\u4eec\u5e76\u6ca1\u6709\u501f\u6545\u8d77\u8d2a\u5fc3\uff0c\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4e5f\u6ca1\u6709\u5411\u4f60\u4eec\u6216\u522b\u4eba\u6c42\u53d6\u4eba\u7684\u8363\u8a89\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6211\u4eec\u8eab\u4e3a\u57fa\u7763\u4f7f\u5f92\u7684\uff0c\u867d\u7136\u6709\u6743\u5229\u53d7\u4eba\u5c0a\u656c\uff0c\u4f46\u6211\u4eec\u5728\u4f60\u4eec\u4e2d\u95f4\u5374\u662f\u8c26\u548c\u7684\uff0c\u5c31\u50cf\u6bcd\u4eb2\u4e73\u517b\u81ea\u5df1\u7684\u5b69\u5b50\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u4eec\u8fd9\u6837\u75bc\u7231\u4f60\u4eec\uff0c\u4e0d\u4f46\u4e50\u610f\u628a\u3000\u795e\u7684\u798f\u97f3\u4f20\u7ed9\u4f60\u4eec\uff0c\u8fde\u81ea\u5df1\u7684\u6027\u547d\u4e5f\u4e50\u610f\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u662f\u6211\u4eec\u6240\u7231\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u5e94\u8be5\u8bb0\u5f97\u6211\u4eec\u7684\u8f9b\u82e6\u548c\u52b3\u788c\uff1b\u6211\u4eec\u628a\u3000\u795e\u7684\u798f\u97f3\u4f20\u7ed9\u4f60\u4eec\u7684\u65f6\u5019\uff0c\u600e\u6837\u663c\u591c\u4f5c\u5de5\uff0c\u514d\u5f97\u4f60\u4eec\u6709\u4eba\u53d7\u7d2f\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u4eec\u5bf9\u4f60\u4eec\u4fe1\u7684\u4eba\u662f\u591a\u4e48\u5723\u6d01\u3001\u516c\u4e49\u3001\u65e0\u53ef\u6307\u6458\uff0c\u8fd9\u662f\u4f60\u4eec\u548c\u3000\u795e\u90fd\u53ef\u4ee5\u4f5c\u8bc1\u7684\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6b63\u5982\u4f60\u4eec\u77e5\u9053\u7684\uff0c\u6211\u4eec\u662f\u600e\u6837\u597d\u50cf\u7236\u4eb2\u5bf9\u5f85\u513f\u5973\u4e00\u6837\u5bf9\u5f85\u4f60\u4eec\u5404\u4eba\uff1a\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u529d\u52c9\u4f60\u4eec\uff0c\u9f13\u52b1\u4f60\u4eec\uff0c\u53ee\u5631\u4f60\u4eec\uff0c\u8981\u4f60\u4eec\u884c\u4e8b\u4e3a\u4eba\uff0c\u914d\u5f97\u4e0a\u90a3\u53ec\u4f60\u4eec\u8fdb\u5165\u4ed6\u7684\u56fd\u548c\u8363\u8000\u7684\u3000\u795e\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u4eec\u4e5f\u4e3a\u8fd9\u7f18\u6545\u4e0d\u4f4f\u611f\u8c22\u3000\u795e\uff0c\u56e0\u4e3a\u4f60\u4eec\u63a5\u53d7\u4e86\u6211\u4eec\u6240\u4f20\u7684\u3000\u795e\u7684\u9053\uff0c\u4e0d\u8ba4\u4e3a\u8fd9\u662f\u4eba\u7684\u9053\uff0c\u800c\u8ba4\u4e3a\u8fd9\u786e\u5b9e\u662f\u3000\u795e\u7684\u9053\u3002\u8fd9\u9053\u4e5f\u8fd0\u884c\u5728\u4f60\u4eec\u4fe1\u7684\u4eba\u91cc\u9762\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u597d\u50cf\u72b9\u592a\u5730\u5728\u57fa\u7763\u8036\u7a23\u91cc\u3000\u795e\u7684\u4f17\u6559\u4f1a\u4e00\u6837\uff0c\u4ed6\u4eec\u600e\u6837\u906d\u53d7\u72b9\u592a\u4eba\u7684\u8feb\u5bb3\uff0c\u4f60\u4eec\u4e5f\u7167\u6837\u906d\u53d7\u540c\u65cf\u4eba\u7684\u8feb\u5bb3\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8fd9\u4e9b\u72b9\u592a\u4eba\u6740\u4e86\u4e3b\u8036\u7a23\u548c\u4f17\u5148\u77e5\uff0c\u53c8\u628a\u6211\u4eec\u8d76\u51fa\u6765\u3002\u4ed6\u4eec\u5f97\u4e0d\u5230\u3000\u795e\u7684\u559c\u60a6\uff0c\u5e76\u4e14\u548c\u6240\u6709\u7684\u4eba\u4f5c\u5bf9\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u963b\u6320\u6211\u4eec\u5411\u5916\u65cf\u4eba\u4f20\u9053\uff0c\u4e0d\u8ba9\u4ed6\u4eec\u5f97\u6551\uff0c\u4ee5\u81f4\u6076\u8d2f\u6ee1\u76c8\u3002\u3000\u795e\u7684\u5fff\u6012\u7ec8\u5fc5\u4e34\u5230\u4ed6\u4eec\u8eab\u4e0a\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4fdd\u7f57\u6e34\u671b\u518d\u8bbf\u5e16\u6492\u7f57\u5c3c\u8fe6\u5f1f\u5144\u4eec\uff0c\u6211\u4eec\u88ab\u8feb\u6682\u65f6\u79bb\u5f00\u4f60\u4eec\uff0c\u4e0d\u8fc7\u662f\u8eab\u4f53\u79bb\u5f00\uff0c\u5fc3\u5374\u6ca1\u6709\u79bb\u5f00\u3002\u6211\u4eec\u975e\u5e38\u6e34\u671b\u518d\u89c1\u5230\u4f60\u4eec\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u56e0\u6b64\uff0c\u6211\u4eec\u5f88\u60f3\u5230\u4f60\u4eec\u90a3\u91cc\u53bb\uff0c\u6211\u4fdd\u7f57\u4e5f\u4e00\u518d\u60f3\u53bb\uff0c\u53ea\u662f\u6492\u4f46\u963b\u6321\u4e86\u6211\u4eec\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6211\u4eec\u4e3b\u8036\u7a23\u518d\u6765\u7684\u65f6\u5019\uff0c\u6211\u4eec\u5728\u4ed6\u9762\u524d\u7684\u76fc\u671b\u3001\u559c\u4e50\u6216\u6240\u5938\u8000\u7684\u51a0\u5195\u662f\u4ec0\u4e48\u5462\uff1f\u4e0d\u5c31\u662f\u4f60\u4eec\u5417\uff1f\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u662f\u7684\uff0c\u4f60\u4eec\u5c31\u662f\u6211\u4eec\u7684\u8363\u8000\uff0c\u6211\u4eec\u7684\u559c\u4e50\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6d3e\u63d0\u6469\u592a\u524d\u5f80\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6d3e\u6211\u4eec\u7684\u5f1f\u5144\uff0c\u5c31\u662f\u5728\u57fa\u7763\u7684\u798f\u97f3\u4e0a\u548c\u3000\u795e\u540c\u5de5\u7684\u63d0\u6469\u592a\u53bb\uff0c\u4e3a\u4e86\u8981\u5728\u4f60\u4eec\u7684\u4fe1\u4ef0\u4e0a\u575a\u5b9a\u4f60\u4eec\uff0c\u529d\u6170\u4f60\u4eec\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u514d\u5f97\u6709\u4eba\u5728\u5404\u6837\u7684\u60a3\u96be\u4e2d\u52a8\u6447\u4e86\u3002\u4f60\u4eec\u81ea\u5df1\u77e5\u9053\uff0c\u6211\u4eec\u53d7\u60a3\u96be\u539f\u662f\u547d\u5b9a\u7684\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u4eec\u5728\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u65e9\u5df2\u5bf9\u4f60\u4eec\u8bf4\u8fc7\uff0c\u6211\u4eec\u5c06\u4f1a\u906d\u53d7\u60a3\u96be\u3002\u540e\u6765\u5e94\u9a8c\u4e86\uff0c\u8fd9\u662f\u4f60\u4eec\u77e5\u9053\u7684\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0\u6b64\uff0c\u6211\u65e2\u7136\u4e0d\u80fd\u518d\u5fcd\u4e0b\u53bb\uff0c\u5c31\u6d3e\u4eba\u53bb\u6253\u542c\u4f60\u4eec\u7684\u4fe1\u5fc3\u600e\u6837\uff1b\u6050\u6015\u90a3\u8bd5\u63a2\u4eba\u7684\u8bf1\u60d1\u4e86\u4f60\u4eec\uff0c\u4ee5\u81f4\u6211\u4eec\u7684\u52b3\u82e6\u767d\u8d39\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u63d0\u6469\u592a\u521a\u521a\u4ece\u4f60\u4eec\u90a3\u91cc\u56de\u6765\uff0c\u628a\u4f60\u4eec\u4fe1\u5fc3\u548c\u7231\u5fc3\u7684\u597d\u6d88\u606f\u5e26\u7ed9\u6211\u4eec\u3002\u4ed6\u8fd8\u8bf4\uff0c\u4f60\u4eec\u5e38\u5e38\u6000\u5ff5\u6211\u4eec\uff0c\u5207\u5207\u60f3\u89c1\u6211\u4eec\uff0c\u597d\u50cf\u6211\u4eec\u60f3\u89c1\u4f60\u4eec\u4e00\u6837\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6240\u4ee5\u5f1f\u5144\u4eec\uff0c\u6211\u4eec\u5728\u8fd9\u4e00\u5207\u56f0\u82e6\u60a3\u96be\u4e2d\uff0c\u56e0\u7740\u4f60\u4eec\u7684\u4fe1\u5fc3\u5c31\u5f97\u4e86\u5b89\u6170\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5982\u679c\u4f60\u4eec\u5728\u4e3b\u91cc\u7ad9\u7acb\u5f97\u7a33\uff0c\u6211\u4eec\u5c31\u53ef\u4ee5\u6d3b\u4e0b\u53bb\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u4eec\u56e0\u4f60\u4eec\u7684\u7f18\u6545\uff0c\u5728\u6211\u4eec\u7684\u3000\u795e\u9762\u524d\u6ee1\u6709\u559c\u4e50\uff1b\u4e3a\u8fd9\u4e00\u5207\u559c\u4e50\uff0c\u6211\u4eec\u53ef\u4ee5\u4e3a\u4f60\u4eec\u5411\u3000\u795e\u732e\u4e0a\u600e\u4e48\u6837\u7684\u611f\u8c22\u5462\uff01\n"
                },
                "10": {
                    "verse_nr": 10,
