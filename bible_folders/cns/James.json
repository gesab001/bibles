    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4ee5\u8bd5\u70bc\u4e3a\u559c\u4e50\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6211\u7684\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u906d\u9047\u5404\u79cd\u8bd5\u70bc\u7684\u65f6\u5019\uff0c\u90fd\u8981\u770b\u4e3a\u559c\u4e50\uff1b\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56e0\u4e3a\u77e5\u9053\u4f60\u4eec\u7684\u4fe1\u5fc3\u7ecf\u8fc7\u8003\u9a8c\uff0c\u5c31\u4ea7\u751f\u5fcd\u8010\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4f46\u5fcd\u8010\u8981\u575a\u6301\u5230\u5e95\uff08\u201c\u575a\u6301\u5230\u5e95\u201d\u539f\u6587\u4f5c\u201c\u6709\u5b8c\u5168\u7684\u529f\u6548\u201d\uff09\uff0c\u4f7f\u4f60\u4eec\u53ef\u4ee5\u5b8c\u5168\uff0c\u6beb\u65e0\u7f3a\u4e4f\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u4eec\u4e2d\u95f4\u82e5\u6709\u4eba\u7f3a\u5c11\u667a\u6167\uff0c\u5c31\u5f53\u5411\u90a3\u539a\u8d50\u4f17\u4eba\uff0c\u800c\u4e14\u4e0d\u65a5\u8d23\u4eba\u7684\u3000\u795e\u7948\u6c42\uff0c\u4ed6\u5c31\u5fc5\u5f97\u7740\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u53ef\u662f\uff0c\u4ed6\u5e94\u8be5\u51ed\u7740\u4fe1\u5fc3\u7948\u6c42\uff0c\u4e0d\u8981\u6709\u7591\u60d1\uff1b\u56e0\u4e3a\u7591\u60d1\u7684\u4eba\uff0c\u5c31\u50cf\u88ab\u98ce\u5439\u8361\u7ffb\u817e\u7684\u6d77\u6d6a\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u90a3\u6837\u7684\u4eba\uff0c\u4e0d\u8981\u60f3\u4ece\u4e3b\u5f97\u5230\u4ec0\u4e48\uff1b\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u56e0\u4e3a\u4e09\u5fc3\u4e24\u610f\u7684\u4eba\uff0c\u5728\u4ed6\u7684\u4e00\u5207\u9053\u8def\u4e0a\uff0c\u90fd\u6447\u6446\u4e0d\u5b9a\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5bcc\u8d35\u8363\u534e\u8f6c\u773c\u6d88\u901d\u5351\u5fae\u7684\u5f1f\u5144\u5e94\u5f53\u4ee5\u9ad8\u5347\u4e3a\u8363\uff1b\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5bcc\u8db3\u7684\u4e5f\u4e0d\u5e94\u8be5\u4ee5\u964d\u5351\u4e3a\u8fb1\uff1b\u56e0\u4e3a\u4ed6\u5982\u540c\u8349\u4e0a\u7684\u82b1\uff0c\u5fc5\u8981\u8fc7\u53bb\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u592a\u9633\u4e00\u51fa\uff0c\u70ed\u98ce\u4e00\u5439\uff0c\u8349\u5fc5\u67af\u5e72\uff0c\u82b1\u5fc5\u51cb\u8c22\uff0c\u5b83\u7684\u7f8e\u5bb9\u5c31\u6d88\u5931\u4e86\uff1b\u5bcc\u8db3\u7684\u4eba\u4e5f\u5fc5\u5728\u4ed6\u7684\u5954\u6ce2\u7ecf\u8425\u4e2d\u8fd9\u6837\u8870\u843d\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5fcd\u53d7\u8bd5\u70bc\u7684\u4eba\u6709\u798f\u4e86\u80fd\u5fcd\u53d7\u8bd5\u70bc\u7684\u4eba\uff0c\u662f\u6709\u798f\u7684\uff1b\u56e0\u4e3a\u4ed6\u7ecf\u8fc7\u8003\u9a8c\u4e4b\u540e\uff0c\u5fc5\u5f97\u7740\u751f\u547d\u7684\u51a0\u5195\uff0c\u8fd9\u51a0\u5195\u662f\u4e3b\u5e94\u8bb8\u7ed9\u7231\u4ed6\u7684\u4eba\u7684\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4eba\u88ab\u8bd5\u63a2\uff0c\u4e0d\u53ef\u8bf4\u201c\u6211\u88ab\u3000\u795e\u8bd5\u63a2\u201d\uff1b\u56e0\u4e3a\u3000\u795e\u4e0d\u80fd\u88ab\u6076\u8bd5\u63a2\uff0c\u4ed6\u4e5f\u4e0d\u8bd5\u63a2\u4efb\u4f55\u4eba\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6bcf\u4e00\u4e2a\u4eba\u53d7\u8bd5\u63a2\uff0c\u90fd\u662f\u88ab\u81ea\u5df1\u7684\u79c1\u6b32\u6240\u52fe\u5f15\u8bf1\u60d1\u7684\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u79c1\u6b32\u6000\u4e86\u80ce\uff0c\u5c31\u751f\u51fa\u7f6a\uff1b\u7f6a\u957f\u6210\u4e86\uff0c\u5c31\u4ea7\u751f\u6b7b\u4ea1\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6211\u4eb2\u7231\u7684\u5f1f\u5144\u4eec\uff0c\u4e0d\u8981\u770b\u9519\u4e86\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u5404\u6837\u7f8e\u597d\u7684\u8d4f\u8d50\uff0c\u5404\u6837\u5b8c\u5907\u7684\u6069\u8d50\uff0c\u90fd\u662f\u4ece\u4e0a\u9762\u3001\u4ece\u4f17\u5149\u4e4b\u7236\u964d\u4e0b\u6765\u7684\uff0c\u4ed6\u672c\u8eab\u5e76\u6ca1\u6709\u6539\u53d8\uff0c\u4e5f\u6ca1\u6709\u8f6c\u52a8\u7684\u5f71\u5b50\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6\u51ed\u7740\u81ea\u5df1\u7684\u65e8\u610f\uff0c\u501f\u7740\u771f\u7406\u7684\u9053\u751f\u4e86\u6211\u4eec\uff0c\u4f7f\u6211\u4eec\u4f5c\u4ed6\u6240\u9020\u7684\u4e07\u7269\u4e2d\u521d\u719f\u7684\u679c\u5b50\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8981\u4f5c\u884c\u9053\u7684\u4eba\u6211\u4eb2\u7231\u7684\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u8981\u77e5\u9053\uff0c\u4eba\u4eba\u90fd\u5e94\u8be5\u5feb\u5feb\u5730\u542c\uff0c\u6162\u6162\u5730\u8bf4\uff0c\u6162\u4e00\u70b9\u52a8\u6012\uff1b\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u56e0\u4e3a\u4eba\u7684\u5fff\u6012\u5e76\u4e0d\u80fd\u6210\u5168\u3000\u795e\u7684\u4e49\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6240\u4ee5\u4f60\u4eec\u5e94\u5f53\u6446\u8131\u4e00\u5207\u6c61\u79fd\u548c\u6240\u6709\u7684\u90aa\u6076\uff0c\u4ee5\u6e29\u67d4\u7684\u5fc3\u9886\u53d7\u3000\u795e\u683d\u79cd\u7684\u9053\uff1b\u8fd9\u9053\u80fd\u6551\u4f60\u4eec\u7684\u7075\u9b42\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4f60\u4eec\u5e94\u8be5\u4f5c\u884c\u9053\u7684\u4eba\uff0c\u4e0d\u8981\u5355\u4f5c\u542c\u9053\u7684\u4eba\uff0c\u81ea\u5df1\u6b3a\u9a97\u81ea\u5df1\uff1b\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u56e0\u4e3a\u4eba\u82e5\u53ea\u4f5c\u542c\u9053\u7684\u4eba\uff0c\u4e0d\u4f5c\u884c\u9053\u7684\u4eba\uff0c\u4ed6\u5c31\u50cf\u4e00\u4e2a\u4eba\u5bf9\u7740\u955c\u5b50\u770b\u81ea\u5df1\u672c\u6765\u7684\u9762\u8c8c\uff0c\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u770b\u8fc7\u8d70\u5f00\u4ee5\u540e\uff0c\u9a6c\u4e0a\u5c31\u5fd8\u8bb0\u81ea\u5df1\u7684\u6837\u5b50\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u552f\u6709\u8be6\u7ec6\u5bdf\u770b\u90a3\u4f7f\u4eba\u81ea\u7531\u7684\u5168\u5907\u7684\u5f8b\u6cd5\uff0c\u5e76\u4e14\u65f6\u5e38\u9075\u5b88\u7684\u4eba\uff0c\u4ed6\u4e0d\u662f\u542c\u4e86\u5c31\u5fd8\u8bb0\uff0c\u800c\u662f\u5b9e\u884c\u51fa\u6765\uff0c\u5c31\u5fc5\u56e0\u81ea\u5df1\u6240\u4f5c\u7684\u8499\u798f\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u5982\u679c\u6709\u4eba\u81ea\u4ee5\u4e3a\u8654\u8bda\uff0c\u5374\u4e0d\u7ea6\u675f\u4ed6\u7684\u820c\u5934\uff0c\u53cd\u800c\u81ea\u5df1\u6b3a\u9a97\u81ea\u5df1\uff0c\u8fd9\u4eba\u7684\u8654\u8bda\u662f\u6ca1\u6709\u7528\u7684\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u5728\u7236\u3000\u795e\u770b\u6765\uff0c\u7eaf\u6d01\u65e0\u73b7\u6c61\u7684\u8654\u8bda\uff0c\u5c31\u662f\u7167\u987e\u60a3\u96be\u4e2d\u7684\u5b64\u513f\u5be1\u5987\uff0c\u5e76\u4e14\u4fdd\u5b88\u81ea\u5df1\u4e0d\u88ab\u4e16\u4fd7\u6240\u6c61\u67d3\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4e0d\u53ef\u91cd\u5bcc\u8f7b\u8d2b\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5982\u679c\u6709\u4e00\u4e2a\u624b\u6234\u91d1\u6212\u6307\u3001\u8eab\u7a7f\u534e\u4e3d\u8863\u670d\u7684\u4eba\uff0c\u8fdb\u5165\u4f60\u4eec\u7684\u4f1a\u5802\uff1b\u53c8\u6709\u4e00\u4e2a\u8863\u886b\u8934\u891b\u7684\u7a77\u4eba\uff0c\u4e5f\u8fdb\u53bb\u4e86\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u4eec\u5c31\u770b\u91cd\u90a3\u7a7f\u534e\u4e3d\u8863\u670d\u7684\u4eba\uff0c\u8bf4\uff1a\u201c\u8bf7\u5750\u5728\u8fd9\u597d\u4f4d\u4e0a\u3002\u201d\u53c8\u5bf9\u90a3\u4e2a\u7a77\u4eba\u8bf4\uff1a\u201c\u4f60\u7ad9\u5728\u90a3\u91cc\u3002\u201d\u6216\u8bf4\uff1a\u201c\u5750\u5728\u6211\u7684\u811a\u51f3\u4e0b\u8fb9\u3002\u201d\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8fd9\u4e0d\u662f\u4f60\u4eec\u5bf9\u4eba\u6709\u6b67\u89c6\uff0c\u6210\u4e86\u5fc3\u6000\u6076\u610f\u7684\u5ba1\u5224\u5b98\u5417\uff1f\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6211\u4eb2\u7231\u7684\u5f1f\u5144\u4eec\uff0c\u8bf7\u542c\uff1a\u3000\u795e\u4e0d\u662f\u62e3\u9009\u4e86\u5728\u4e16\u4e0a\u88ab\u8ba4\u4e3a\u8d2b\u7a77\u7684\u4eba\u5417\uff1f\u8fd9\u4e9b\u4eba\u5374\u5728\u4fe1\u5fc3\u4e0a\u5bcc\u8db3\uff0c\u800c\u4e14\u662f\u627f\u53d7\u3000\u795e\u7684\u56fd\u7684\u4eba\u3002\u8fd9\u56fd\u662f\u3000\u795e\u5e94\u8bb8\u8d50\u7ed9\u7231\u4ed6\u7684\u4eba\u7684\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u7136\u800c\u4f60\u4eec\u5374\u4fae\u8fb1\u7a77\u4eba\u3002\u5176\u5b9e\uff0c\u90a3\u4e9b\u6b3a\u538b\u4f60\u4eec\uff0c\u62c9\u4f60\u4eec\u4e0a\u6cd5\u5ead\u7684\uff0c\u4e0d\u5c31\u662f\u5bcc\u8db3\u7684\u4eba\u5417\uff1f\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u96be\u9053\u4e0d\u662f\u4ed6\u4eec\u4eb5\u6e0e\u90a3\u53ec\u4f60\u4eec\u7684\u5c0a\u540d\u5417\uff1f\uff08\u6216\u8bd1\uff1a\u201c\u96be\u9053\u4e0d\u662f\u4ed6\u4eec\u6bc1\u8c24\u4f60\u4eec\u8499\u53ec\u7684\u7f8e\u540d\u5417\uff1f\u201d\uff09\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u4eec\u82e5\u7167\u7740\u5723\u7ecf\u6240\u8bf4\u201c\u8981\u7231\u4eba\u5982\u5df1\u201d\u8fd9\u8bdd\uff0c\u53bb\u5b8c\u6210\u8fd9\u81f3\u5c0a\u7684\u5f8b\u6cd5\uff0c\u4f60\u4eec\u5c31\u4f5c\u5bf9\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5982\u679c\u4f60\u4eec\u51ed\u5916\u8c8c\u5f85\u4eba\uff0c\u5c31\u662f\u72af\u7f6a\uff0c\u5f8b\u6cd5\u5c31\u8981\u88c1\u5b9a\u4f60\u4eec\u662f\u72af\u6cd5\u7684\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u4e3a\u51e1\u662f\u9075\u5b88\u5168\u90e8\u5f8b\u6cd5\u7684\uff0c\u53ea\u8981\u5728\u4e00\u6761\u4e0a\u5931\u8db3\uff0c\u5c31\u8fdd\u72af\u6240\u6709\u7684\u4e86\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5c31\u50cf\u90a3\u8bf4\u201c\u4e0d\u53ef\u5978\u6deb\u201d\u7684\uff0c\u4e5f\u8bf4\u201c\u4e0d\u53ef\u6740\u4eba\u201d\uff1b\u4f60\u7eb5\u7136\u4e0d\u5978\u6deb\uff0c\u5374\u6740\u4eba\uff0c\u8fd8\u662f\u72af\u6cd5\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4f60\u4eec\u65e2\u7136\u6309\u7740\u4f7f\u4eba\u81ea\u7531\u7684\u5f8b\u6cd5\u53d7\u5ba1\u5224\uff0c\u5c31\u5e94\u7167\u7740\u8fd9\u5f8b\u6cd5\u8bf4\u8bdd\u884c\u4e8b\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u56e0\u4e3a\u5bf9\u4e0d\u884c\u601c\u60af\u7684\u4eba\uff0c\u5ba1\u5224\u4ed6\u4eec\u7684\u65f6\u5019\u5c31\u6ca1\u6709\u601c\u60af\uff1b\u601c\u60af\u80dc\u8fc7\u5ba1\u5224\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4fe1\u5fc3\u6ca1\u6709\u884c\u4e3a\u662f\u6b7b\u7684\u6211\u7684\u5f1f\u5144\u4eec\uff0c\u4eba\u82e5\u8bf4\u4ed6\u6709\u4fe1\u5fc3\uff0c\u5374\u6ca1\u6709\u884c\u4e3a\uff0c\u6709\u4ec0\u4e48\u76ca\u5904\u5462\uff1f\u8fd9\u4fe1\u5fc3\u80fd\u6551\u4ed6\u5417\uff1f\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5982\u679c\u6709\u5f1f\u5144\u6216\u59ca\u59b9\u7f3a\u8863\u5c11\u98df\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u800c\u4f60\u4eec\u4e2d\u95f4\u6709\u4eba\u5bf9\u4ed6\u4eec\u8bf4\uff1a\u201c\u5e73\u5e73\u5b89\u5b89\u5730\u53bb\u5427\uff01\u613f\u4f60\u4eec\u7a7f\u5f97\u6696\uff0c\u5403\u5f97\u9971\u3002\u201d\u5374\u4e0d\u7ed9\u4ed6\u4eec\u8eab\u4f53\u6240\u9700\u7528\u7684\uff0c\u90a3\u6709\u4ec0\u4e48\u7528\u5904\u5462\uff1f\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u7167\u6837\uff0c\u5982\u679c\u53ea\u6709\u4fe1\u5fc3\uff0c\u6ca1\u6709\u884c\u4e3a\uff0c\u8fd9\u4fe1\u5fc3\u5c31\u662f\u6b7b\u7684\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
