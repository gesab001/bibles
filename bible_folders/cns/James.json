            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8b66\u544a\u4e0d\u4e49\u7684\u6709\u94b1\u4eba\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4f60\u4eec\u7684\u8d22\u7269\u673d\u574f\u4e86\uff0c\u4f60\u4eec\u7684\u8863\u670d\u7ed9\u86c0\u4e86\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u4eec\u7684\u91d1\u94f6\u751f\u9508\uff0c\u8fd9\u9508\u8981\u6210\u4e3a\u63a7\u544a\u4f60\u4eec\u7684\u94c1\u8bc1\uff0c\u53c8\u8981\u50cf\u706b\u4e00\u6837\u541e\u5403\u4f60\u4eec\u7684\u8089\u3002\u4f60\u4eec\u7adf\u7136\u5728\u8fd9\u672b\u4e16\u79ef\u805a\u8d22\u5b9d\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u770b\u54ea\uff0c\u5de5\u4eba\u4e3a\u4f60\u4eec\u6536\u5272\u5e84\u7a3c\uff0c\u4f60\u4eec\u7adf\u7136\u514b\u6263\u4ed6\u4eec\u7684\u5de5\u8d44\uff1b\u90a3\u5de5\u8d44\u5fc5\u4e3a\u4ed6\u4eec\u547c\u51a4\uff1b\u6536\u5272\u8005\u7684\u547c\u58f0\uff0c\u5df2\u7ecf\u8fbe\u5230\u4e07\u519b\u4e4b\u4e3b\u7684\u8033\u4e2d\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u4eec\u5728\u4e16\u4e0a\u7a77\u5962\u6781\u4f88\uff0c\u517b\u80a5\u4e86\u81ea\u5df1\uff0c\u7adf\u4e0d\u77e5\u5c60\u5bb0\u7684\u65e5\u5b50\u5230\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u4eec\u628a\u4e49\u4eba\u5b9a\u7f6a\u6740\u5bb3\uff0c\u4f46\u4ed6\u5e76\u6ca1\u6709\u53cd\u6297\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5fcd\u8010\u7684\u4eba\u6709\u798f\u4e86\u6240\u4ee5\uff0c\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u5e94\u5f53\u5fcd\u8010\uff0c\u76f4\u5230\u4e3b\u6765\u3002\u770b\u54ea\uff0c\u519c\u592b\u7b49\u5f85\u7740\u5730\u91cc\u5b9d\u8d35\u7684\u51fa\u4ea7\uff0c\u4e3a\u5b83\u5fcd\u8010\uff0c\u76f4\u5230\u83b7\u5f97\u79cb\u9716\u6625\u96e8\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u4eec\u4e5f\u5e94\u5f53\u5fcd\u8010\uff0c\u575a\u5b9a\u81ea\u5df1\u7684\u5fc3\uff1b\u56e0\u4e3a\u4e3b\u518d\u6765\u7684\u65e5\u5b50\u8fd1\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5f1f\u5144\u4eec\uff0c\u4e0d\u8981\u5f7c\u6b64\u62b1\u6028\uff0c\u514d\u5f97\u4f60\u4eec\u53d7\u5ba1\u5224\u3002\u770b\u54ea\uff0c\u5ba1\u5224\u7684\u4e3b\u5df2\u7ecf\u7ad9\u5728\u95e8\u524d\u4e86\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u5e94\u5f53\u6548\u6cd5\u5949\u4e3b\u7684\u540d\u8bf4\u8bdd\u7684\u5148\u77e5\uff0c\u4ee5\u4ed6\u4eec\u4e3a\u53d7\u82e6\u5fcd\u8010\u7684\u699c\u6837\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u770b\u54ea\uff0c\u90a3\u4e9b\u5fcd\u8010\u7684\u4eba\uff0c\u6211\u4eec\u79f0\u4ed6\u4eec\u662f\u6709\u798f\u7684\uff1b\u4f60\u4eec\u542c\u8fc7\u7ea6\u4f2f\u7684\u5fcd\u8010\uff0c\u4e5f\u770b\u89c1\u4e86\u4e3b\u8d50\u7ed9\u4ed6\u7684\u7ed3\u5c40\uff0c\u77e5\u9053\u4e3b\u662f\u6ee1\u6709\u601c\u60af\u548c\u4ec1\u6148\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u7684\u5f1f\u5144\u4eec\uff0c\u6700\u8981\u7d27\u7684\u662f\u4e0d\u53ef\u8d77\u8a93\uff1a\u4e0d\u53ef\u6307\u7740\u5929\u8d77\u8a93\uff0c\u4e5f\u4e0d\u53ef\u6307\u7740\u5730\u8d77\u8a93\uff0c\u4efb\u4f55\u7684\u8a93\u90fd\u5f53\u7981\u7edd\u3002\u4f60\u4eec\u7684\u8bdd\uff0c\u662f\u5c31\u8bf4\u662f\uff0c\u4e0d\u662f\u5c31\u8bf4\u4e0d\u662f\uff1b\u514d\u5f97\u4f60\u4eec\u843d\u5728\u5ba1\u5224\u4e4b\u4e0b\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4e49\u4eba\u7684\u7948\u7977\u5927\u6709\u529f\u6548\u4f60\u4eec\u4e2d\u95f4\u6709\u4eba\u53d7\u82e6\u5417\uff1f\u4ed6\u5c31\u5e94\u8be5\u7977\u544a\u3002\u6709\u4eba\u5fc3\u60c5\u6109\u5feb\u5417\uff1f\u4ed6\u5c31\u5e94\u8be5\u6b4c\u9882\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u4eec\u4e2d\u95f4\u6709\u4eba\u60a3\u75c5\u5417\uff1f\u4ed6\u5c31\u5e94\u8be5\u8bf7\u6559\u4f1a\u7684\u957f\u8001\u6765\uff0c\u8ba9\u4ed6\u4eec\u5949\u4e3b\u7684\u540d\u4e3a\u4ed6\u62b9\u6cb9\u7948\u7977\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u51fa\u4e8e\u4fe1\u5fc3\u7684\u7948\u7977\uff0c\u53ef\u4ee5\u4f7f\u75c5\u4eba\u5eb7\u590d\uff0c\u4e3b\u5fc5\u53eb\u4ed6\u8d77\u6765\uff1b\u4ed6\u82e5\u72af\u4e86\u7f6a\uff0c\u4e5f\u5fc5\u8499\u8d66\u514d\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6240\u4ee5\u4f60\u4eec\u5e94\u5f53\u5f7c\u6b64\u8ba4\u7f6a\uff0c\u4e92\u76f8\u4ee3\u6c42\uff0c\u8fd9\u6837\u4f60\u4eec\u5c31\u53ef\u4ee5\u75ca\u6108\u3002\u4e49\u4eba\u7948\u7977\u6240\u53d1\u51fa\u7684\u529b\u91cf\uff0c\u662f\u5927\u6709\u529f\u6548\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ee5\u5229\u4e9a\u662f\u4e0e\u6211\u4eec\u6027\u60c5\u76f8\u540c\u7684\u4eba\uff1b\u4ed6\u6073\u5207\u7948\u6c42\u4e0d\u8981\u4e0b\u96e8\uff0c\u5730\u4e0a\u5c31\u4e09\u5e74\u96f6\u516d\u4e2a\u6708\u6ca1\u6709\u96e8\uff1b\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6\u53c8\u7948\u7977\uff0c\u5929\u5c31\u4e0b\u96e8\uff0c\u5730\u5c31\u751f\u51fa\u571f\u4ea7\u6765\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6211\u7684\u5f1f\u5144\u4eec\uff0c\u4f60\u4eec\u4e2d\u95f4\u82e5\u6709\u4eba\u88ab\u9a97\u79bb\u5f00\u4e86\u771f\u9053\uff0c\u5982\u679c\u6709\u4eba\u4f7f\u4ed6\u56de\u5934\uff0c\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4f60\u4eec\u5e94\u8be5\u77e5\u9053\uff0c\u90a3\u4f7f\u7f6a\u4eba\u4ece\u6b67\u9014\u4e0a\u8f6c\u56de\u7684\uff0c\u5c31\u4f1a\u62ef\u6551\u4ed6\u7684\u7075\u9b42\u8131\u79bb\u6b7b\u4ea1\uff0c\u4e5f\u4f1a\u906e\u76d6\u8bb8\u591a\u7f6a\u6076\u3002\n"
                }
            }
        }
    }
}