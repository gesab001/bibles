                    "verse": "\u4f60\u4eec\u8981\u6101\u82e6\u3001\u60b2\u54c0\u3001\u54ed\u6ce3\uff0c\u628a\u6b22\u7b11\u53d8\u4e3a\u4f24\u75db\uff0c\u628a\u5feb\u4e50\u53d8\u4e3a\u5fe7\u6101\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f60\u4eec\u52a1\u8981\u5728\u4e3b\u9762\u524d\u8c26\u5351\uff0c\u4ed6\u5c31\u4f7f\u4f60\u4eec\u9ad8\u5347\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4e0d\u53ef\u5f7c\u6b64\u6279\u8bc4\u5f1f\u5144\u4eec\uff0c\u4e0d\u8981\u4e92\u76f8\u6bc1\u8c24\uff1b\u4eba\u82e5\u6bc1\u8c24\u5f1f\u5144\uff0c\u6216\u5224\u65ad\u5f1f\u5144\uff0c\u5c31\u662f\u6bc1\u8c24\u5f8b\u6cd5\u3001\u5224\u65ad\u5f8b\u6cd5\u4e86\u3002\u5982\u679c\u4f60\u5224\u65ad\u5f8b\u6cd5\uff0c\u5c31\u4e0d\u662f\u5b9e\u884c\u5f8b\u6cd5\u7684\u4eba\uff0c\u800c\u662f\u5ba1\u5224\u5b98\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u7acb\u6cd5\u7684\uff0c\u5ba1\u5224\u7684\uff0c\u53ea\u6709\u4e00\u4f4d\uff0c\u5c31\u662f\u90a3\u80fd\u62ef\u6551\u4eba\uff0c\u4e5f\u80fd\u6bc1\u706d\u4eba\u7684\u3000\u795e\uff1b\u4f60\u8fd9\u5224\u65ad\u90bb\u820d\u7684\uff0c\u4f60\u662f\u8c01\u5462\uff1f\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4e0d\u53ef\u81ea\u5938\u81ea\u6043\u4f60\u4eec\u8bf4\uff1a\u201c\u4eca\u5929\u6216\u660e\u5929\uff0c\u6211\u4eec\u8981\u5230\u67d0\u57ce\u53bb\uff0c\u5728\u90a3\u91cc\u4f4f\u4e00\u5e74\uff0c\u4f5c\u751f\u610f\u8d5a\u94b1\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5176\u5b9e\u660e\u5929\u600e\u6837\uff0c\u4f60\u4eec\u5e76\u4e0d\u77e5\u9053\u3002\u4f60\u4eec\u7684\u751f\u547d\u662f\u4ec0\u4e48\u5462\uff1f\u4f60\u4eec\u672c\u6765\u662f\u8fc7\u773c\u4e91\u70df\uff0c\u8f6c\u77ac\u4e4b\u95f4\u5c31\u6d88\u901d\u4e86\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u4eec\u5012\u4e0d\u5982\u8bf4\uff1a\u201c\u4e3b\u82e5\u613f\u610f\uff0c\u6211\u4eec\u5c31\u53ef\u4ee5\u6d3b\u7740\uff0c\u4f5c\u8fd9\u4e8b\u6216\u4f5c\u90a3\u4e8b\u3002\u201d\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f46\u73b0\u5728\u4f60\u4eec\u7adf\u7136\u5f20\u72c2\u81ea\u5938\uff1b\u8fd9\u4e00\u5207\u81ea\u5938\uff0c\u90fd\u662f\u90aa\u6076\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4eba\u82e5\u77e5\u9053\u8be5\u884c\u5584\u4e8b\uff0c\u5374\u4e0d\u53bb\u884c\uff0c\u8fd9\u5c31\u662f\u4ed6\u7684\u7f6a\u4e86\u3002\n"
                }
            }
        },
        "5": {
