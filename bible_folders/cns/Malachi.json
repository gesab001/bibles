                "2": {
                    "verse_nr": 2,
                    "verse": "\u53ef\u662f\uff0c\u5bf9\u4f60\u4eec\u656c\u754f\u6211\u540d\u7684\u4eba\uff0c\u5fc5\u6709\u516c\u4e49\u7684\u592a\u9633\u5347\u8d77\u6765\uff1b\u5b83\u7684\u5149\u7ebf\u6709\u533b\u6cbb\u7684\u529f\u80fd\uff1b\u4f60\u4eec\u5fc5\u51fa\u6765\u8df3\u8dc3\uff0c\u50cf\u680f\u91cc\u7684\u80a5\u725b\u728a\u3002\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u4f60\u4eec\u5fc5\u8df5\u8e0f\u6076\u4eba\uff1b\u5728\u6211\u65bd\u884c\u4f5c\u4e3a\u7684\u65e5\u5b50\uff0c\u4ed6\u4eec\u5fc5\u5728\u4f60\u4eec\u811a\u638c\u4e4b\u4e0b\u6210\u4e3a\u5c18\u571f\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u201c\u4f60\u4eec\u8981\u8c28\u8bb0\u6211\u4ec6\u4eba\u6469\u897f\u7684\u5f8b\u6cd5\uff0c\u5c31\u662f\u6211\u5728\u4f55\u70c8\u5c71\u4e3a\u4ee5\u8272\u5217\u4f17\u4eba\u6240\u5429\u5490\u4ed6\u7684\u5f8b\u4f8b\u548c\u5178\u7ae0\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9884\u8a00\u5dee\u6d3e\u4ee5\u5229\u4e9a\u6765\u4e34\u201c\u770b\u54ea\uff01\u5728\u8036\u548c\u534e\u5927\u800c\u53ef\u754f\u7684\u65e5\u5b50\u6765\u5230\u4ee5\u5148\uff0c\u6211\u5fc5\u5dee\u6d3e\u4ee5\u5229\u4e9a\u5148\u77e5\u5230\u4f60\u4eec\u90a3\u91cc\u53bb\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u8981\u4f7f\u7236\u4eb2\u7684\u5fc3\u8f6c\u5411\u513f\u5973\uff0c\u513f\u5973\u7684\u5fc3\u8f6c\u5411\u7236\u4eb2\uff0c\u514d\u5f97\u6211\u6765\u51fb\u6253\u8fd9\u5730\uff0c\u4ee5\u81f3\u5b8c\u5168\u6bc1\u706d\u3002\u201d\uff08\u672c\u7ae0\u7b2c1-6\u8282\u5728\u300a\u9a6c\u7d22\u62c9\u6284\u672c\u300b\u4e3a3:19-24\uff09\n"
                }
