{
    "version": "cns",
    "book_name": "Jonah",
    "book_nr": "32",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u7ea6\u62ff\u8fdd\u547d\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u201c\u8d77\u6765\uff01\u5230\u5c3c\u5c3c\u5fae\u5927\u57ce\u53bb\uff0c\u8b66\u544a\u5176\u4e2d\u7684\u5c45\u6c11\uff0c\u56e0\u4e3a\u4ed6\u4eec\u7684\u6076\u884c\u5df2\u7ecf\u8fbe\u5230\u6211\u9762\u524d\u3002\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u53ef\u662f\u7ea6\u62ff\u5374\u8d77\u6765\uff0c\u8981\u9003\u5f80\u4ed6\u65bd\u53bb\uff0c\u8eb2\u907f\u8036\u548c\u534e\uff1b\u4ed6\u4e0b\u5230\u7ea6\u5e15\uff0c\u627e\u5230\u4e86\u4e00\u53ea\u8981\u5f00\u5f80\u4ed6\u65bd\u53bb\u7684\u8239\uff1b\u4ed6\u7ed9\u4e86\u94b1\uff0c\u5c31\u4e0a\u8239\uff08\u201c\u4e0a\u8239\u201d\u539f\u6587\u4f5c\u201c\u4e0b\u8239\u201d\uff09\uff0c\u8981\u548c\u4ed6\u4eec\u540c\u5f80\u4ed6\u65bd\u53bb\uff0c\u79bb\u5f00\u8036\u548c\u534e\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6d77\u8d77\u5927\u98ce\u4f46\u662f\u8036\u548c\u534e\u5728\u6d77\u4e0a\u5ffd\u7136\u522e\u8d77\u5927\u98ce\uff0c\u4e8e\u662f\u6d77\u4e2d\u72c2\u98ce\u5927\u4f5c\uff0c\u8239\u51e0\u4e4e\u8981\u7834\u88c2\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6c34\u624b\u90fd\u5f88\u60e7\u6015\uff0c\u5404\u4eba\u5411\u81ea\u5df1\u7684\u795e\u5927\u58f0\u547c\u6c42\uff1b\u4ed6\u4eec\u4e3a\u4e86\u51cf\u8f7b\u8239\u7684\u91cd\u91cf\uff0c\u5c31\u628a\u8239\u4e0a\u7684\u4e1c\u897f\u62cb\u5728\u6d77\u91cc\uff0c\u53ef\u662f\u7ea6\u62ff\u5374\u4e0b\u4e86\u8239\u8231\uff0c\u8eba\u7740\u719f\u7761\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u8239\u957f\u8d70\u5230\u4ed6\u9762\u524d\uff0c\u5bf9\u4ed6\u8bf4\uff1a\u201c\u4f60\u600e\u4e48\u5566\uff01\u7761\u5f97\u90a3\u4e48\u719f\uff1f\u5feb\u8d77\u6765\uff0c\u6c42\u544a\u4f60\u7684\u3000\u795e\u5427\uff01\u6216\u8005\u3000\u795e\u4f1a\u8bb0\u5ff5\u6211\u4eec\uff0c\u4f7f\u6211\u4eec\u4e0d\u81f3\u4e8e\u4e27\u547d\uff01\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u7ea6\u62ff\u53d7\u7f5a\u6c34\u624b\u5c31\u5f7c\u6b64\u8bf4\uff1a\u201c\u6765\u5427\uff01\u8ba9\u6211\u4eec\u62bd\u7b7e\uff0c\u597d\u77e5\u9053\u8fd9\u573a\u707e\u7978\u4e34\u5230\u6211\u4eec\uff0c\u662f\u56e0\u8c01\u7684\u7f18\u6545\u3002\u201d\u4e8e\u662f\u4ed6\u4eec\u62bd\u7b7e\uff0c\u7ed3\u679c\u62bd\u4e2d\u4e86\u7ea6\u62ff\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ed6\u4eec\u5c31\u5bf9\u7ea6\u62ff\u8bf4\uff1a\u201c\u8bf7\u544a\u8bc9\u6211\u4eec\uff0c\u8fd9\u573a\u707e\u7978\u4e34\u5230\u6211\u4eec\uff0c\u662f\u56e0\u8c01\u7684\u7f18\u6545\uff1f\u4f60\u662f\u4f5c\u4ec0\u4e48\u7684\uff1f\u4f60\u4ece\u54ea\u91cc\u6765\uff1f\u4f60\u662f\u54ea\u4e00\u56fd\u7684\u4eba\uff1f\u54ea\u4e00\u65cf\u7684\u4eba\uff1f\u201d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u56de\u7b54\u8bf4\uff1a\u201c\u6211\u662f\u5e0c\u4f2f\u6765\u4eba\uff0c\u6211\u656c\u754f\u8036\u548c\u534e\u5929\u4e0a\u7684\u3000\u795e\uff0c\u5c31\u662f\u90a3\u521b\u9020\u6d77\u6d0b\u548c\u9646\u5730\u7684\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f17\u4eba\u5c31\u5927\u5927\u60e7\u6015\uff0c\u5bf9\u4ed6\u8bf4\uff1a\u201c\u4f60\u4e3a\u4ec0\u4e48\u8fd9\u6837\u4f5c\u5462\uff1f\uff08\u201c\u4f60\u4e3a\u4ec0\u4e48\u8fd9\u6837\u4f5c\u5462\uff1f\u201d\u6216\u8bd1\uff1a\u201c\u4f60\u4f5c\u7684\u662f\u4ec0\u4e48\u5462\uff1f\u201d\uff09\u201d\u90a3\u4e9b\u4eba\u77e5\u9053\u4ed6\u8eb2\u907f\u8036\u548c\u534e\uff0c\u539f\u6765\u7ea6\u62ff\u544a\u8bc9\u4e86\u4ed6\u4eec\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56e0\u4e3a\u6d77\u6d6a\u8d8a\u6765\u8d8a\u6c79\u6d8c\uff0c\u4ed6\u4eec\u53c8\u5bf9\u4ed6\u8bf4\uff1a\u201c\u6211\u4eec\u8be5\u600e\u6837\u5904\u7f6e\u4f60\uff0c\u624d\u80fd\u53eb\u6d77\u6d6a\u5e73\u9759\u5462\uff1f\u201d\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4ed6\u56de\u7b54\u8bf4\uff1a\u201c\u4f60\u4eec\u628a\u6211\u62ac\u8d77\u6765\uff0c\u6295\u5728\u6d77\u91cc\uff0c\u6d77\u6d6a\u5c31\u4f1a\u5e73\u9759\uff0c\u56e0\u4e3a\u6211\u77e5\u9053\uff0c\u8fd9\u573a\u5927\u98ce\u66b4\u4e34\u5230\u4f60\u4eec\uff0c\u662f\u4e3a\u4e86\u6211\u7684\u7f18\u6545\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u90a3\u4e9b\u4eba\u867d\u7136\u7834\u6d6a\u6323\u624e\uff0c\u8981\u628a\u8239\u9760\u5cb8\uff0c\u5374\u662f\u4e0d\u80fd\uff0c\u56e0\u4e3a\u6d77\u6d6a\u4e0d\u65ad\u6c79\u6d8c\u6251\u6765\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4ed6\u4eec\u5c31\u547c\u6c42\u8036\u548c\u534e\uff0c\u8bf4\uff1a\u201c\u8036\u548c\u534e\u554a\uff01\u6211\u4eec\u6073\u6c42\u4f60\uff0c\u4e0d\u8981\u56e0\u8fd9\u4eba\u53eb\u6211\u4eec\u4e27\u547d\uff1b\u4e0d\u8981\u628a\u65e0\u8f9c\u4eba\u7684\u8840\u5f52\u5728\u6211\u4eec\u8eab\u4e0a\uff0c\u56e0\u4e3a\u4f60\u8036\u548c\u534e\u662f\u7167\u81ea\u5df1\u6240\u559c\u60a6\u7684\u884c\u4e8b\u3002\u201d\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4e8e\u662f\u4ed6\u4eec\u628a\u7ea6\u62ff\u62ac\u8d77\u6765\uff0c\u6295\u5728\u6d77\u91cc\uff0c\u6012\u6d9b\u5c31\u5e73\u606f\u4e86\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u90a3\u4e9b\u4eba\u5c31\u5927\u5927\u656c\u754f\u8036\u548c\u534e\uff0c\u5411\u4ed6\u732e\u796d\u548c\u8bb8\u613f\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8036\u548c\u534e\u5374\u5b89\u6392\u4e86\u4e00\u6761\u5927\u9c7c\uff0c\u628a\u7ea6\u62ff\u541e\u4e0b\u3002\u7ea6\u62ff\u5c31\u5728\u9c7c\u8179\u4e2d\u4e09\u65e5\u4e09\u591c\u3002\uff08\u672c\u8282\u5728\u300a\u9a6c\u7d22\u62c9\u6284\u672c\u300b\u4e3a2:1\uff09\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u7ea6\u62ff\u7977\u544a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8bf4\uff1a\u201c\u6211\u4ece\u60a3\u96be\u4e2d\u6c42\u544a\u8036\u548c\u534e\uff0c\u4ed6\u5c31\u5e94\u5141\u6211\uff1b\u6211\u4ece\u9634\u95f4\u7684\u6df1\u5e9c\u547c\u6c42\uff0c\u4f60\u5c31\u5782\u542c\u6211\u7684\u58f0\u97f3\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u628a\u6211\u6295\u5165\u6df1\u6d77\uff0c\u5728\u6d77\u6d0b\u7684\u6df1\u5904\uff0c\u5927\u6c34\u73af\u7ed5\u6211\uff1b\u4f60\u7684\u6d2a\u6d9b\u3001\u4f60\u7684\u6ce2\u6d6a\uff0c\u90fd\u6f2b\u8fc7\u6211\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u8bf4\uff1a\u2018\u6211\u867d\u4ece\u4f60\u773c\u524d\u88ab\u8d76\u9010\uff0c\u6211\u4ecd\u8981\u4ef0\u671b\u4f60\u7684\u5723\u6bbf\u3002\u2019\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f17\u6c34\u5305\u56f4\u6211\uff0c\u51e0\u4e4e\u7f6e\u6211\u4e8e\u6b7b\u5730\uff1b\u6df1\u6e0a\u73af\u7ed5\u6211\uff1b\u6d77\u8349\u7f20\u88f9\u6211\u7684\u5934\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u4e0b\u6c89\u76f4\u5230\u5c71\u9e93\uff0c\u5927\u5730\u7684\u95e8\u95e9\u628a\u6211\u6c38\u8fdc\u5173\u95ed\uff1b\u8036\u548c\u534e\u6211\u7684\u3000\u795e\u554a\uff01\u4f60\u5374\u628a\u6211\u7684\u6027\u547d\u4ece\u5751\u4e2d\u62c9\u4e0a\u6765\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5f53\u6211\u5fc3\u7075\u75b2\u5f31\u65f6\uff0c\u6211\u5c31\u601d\u5ff5\u8036\u548c\u534e\uff1b\u6211\u7684\u7977\u544a\u76f4\u8fbe\u4f60\u9762\u524d\uff0c\u8fdb\u5165\u4f60\u7684\u5723\u6bbf\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u90a3\u656c\u5949\u865a\u5984\u4e4b\u5076\u50cf\u7684\u4eba\uff0c\u5b9e\u5728\u662f\u79bb\u5f03\u4e86\u601c\u7231\u4ed6\u4eec\u7684\u3000\u795e\uff08\u201c\u601c\u7231\u4ed6\u4eec\u7684\u3000\u795e\u201d\u539f\u6587\u4f5c\u201c\u4ed6\u4eec\u7684\u6069\u5178\u201d\uff09\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u81f3\u4e8e\u6211\uff0c\u6211\u8981\u5e26\u7740\u611f\u8c22\u7684\u58f0\u97f3\u5411\u4f60\u732e\u796d\uff1b\u6211\u6240\u8bb8\u7684\u613f\uff0c\u6211\u5fc5\u507f\u8fd8\u3002\u6551\u6069\u662f\u5c5e\u4e8e\u8036\u548c\u534e\u7684\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4e8e\u662f\u8036\u548c\u534e\u5429\u5490\u90a3\u9c7c\uff0c\u9c7c\u5c31\u628a\u7ea6\u62ff\u5410\u5728\u9646\u5730\u4e0a\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u7ea6\u62ff\u9075\u547d\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u201c\u8d77\u6765\uff01\u5230\u5c3c\u5c3c\u5fae\u5927\u57ce\u53bb\uff0c\u5411\u5176\u4e2d\u7684\u5c45\u6c11\u5ba3\u544a\u6211\u6240\u5429\u5490\u4f60\u7684\u8bdd\u3002\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u7ea6\u62ff\u5c31\u52a8\u8eab\uff0c\u7167\u8036\u548c\u534e\u7684\u8bdd\u5f80\u5c3c\u5c3c\u5fae\u53bb\u4e86\uff0c\u8fd9\u5c3c\u5c3c\u5fae\u662f\u4e00\u5ea7\u6781\u5927\u7684\u57ce\uff08\u201c\u662f\u4e00\u5ea7\u6781\u5927\u7684\u57ce\u201d\u6216\u8bd1\uff1a\u201c\u5728\u3000\u795e\u9762\u524d\u662f\u4e00\u5ea7\u5927\u57ce\u201d\uff09\uff0c\u8981\u4e09\u5929\u624d\u80fd\u8d70\u5b8c\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5c3c\u5c3c\u5fae\u4eba\u6094\u6539\u5f97\u6551\u7ea6\u62ff\u8fdb\u57ce\uff0c\u8d70\u4e86\u4e00\u5929\uff0c\u5c31\u5ba3\u544a\u8bf4\uff1a\u201c\u518d\u8fc7\u56db\u5341\u5929\uff0c\u5c3c\u5c3c\u5fae\u5c31\u8981\u6bc1\u706d\u4e86\u3002\u201d\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5c3c\u5c3c\u5fae\u4eba\u4fe1\u4e86\uff08\u201c\u4fe1\u4e86\u201d\u6216\u8bd1\uff1a\u201c\u4fe1\u670d\u201d\uff09\u3000\u795e\uff0c\u5c31\u5ba3\u544a\u7981\u98df\uff0c\u4ece\u5927\u5230\u5c0f\uff0c\u90fd\u7a7f\u4e0a\u9ebb\u8863\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u8fd9\u4e8b\u4f20\u5230\u5c3c\u5c3c\u5fae\u738b\u90a3\u91cc\uff0c\u4ed6\u5c31\u8d77\u6765\uff0c\u79bb\u5f00\u5b9d\u5ea7\uff0c\u8131\u53bb\u738b\u888d\uff0c\u62ab\u4e0a\u9ebb\u8863\uff0c\u5750\u5728\u7089\u7070\u4e2d\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u53c8\u4f20\u8c15\u5168\u5c3c\u5c3c\u5fae\uff0c\u8bf4\uff1a\u201c\u738b\u548c\u5927\u81e3\u6709\u4ee4\uff1a\u2018\u4eba\u3001\u755c\u3001\u725b\u3001\u7f8a\uff0c\u90fd\u4e0d\u51c6\u5c1d\u4ec0\u4e48\uff0c\u4e0d\u51c6\u5403\uff0c\u4e5f\u4e0d\u51c6\u559d\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4e0d\u8bba\u4eba\u755c\uff0c\u90fd\u8981\u62ab\u4e0a\u9ebb\u8863\uff1b\u5404\u4eba\u8981\u6073\u5207\u547c\u6c42\u3000\u795e\uff0c\u6094\u6539\uff0c\u79bb\u5f00\u6076\u884c\uff0c\u6452\u5f03\u624b\u4e2d\u7684\u5f3a\u66b4\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6216\u8005\u3000\u795e\u56de\u5fc3\u8f6c\u610f\uff0c\u4e0d\u53d1\u70c8\u6012\uff0c\u4f7f\u6211\u4eec\u4e0d\u81f4\u706d\u4ea1\u3002\u2019\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u3000\u795e\u770b\u89c1\u4ed6\u4eec\u6240\u4f5c\u7684\uff0c\u5c31\u662f\u6094\u6539\u79bb\u5f00\u6076\u884c\uff0c\u3000\u795e\u5c31\u8f6c\u610f\uff0c\u4e0d\u628a\u6240\u8bf4\u7684\u707e\u7978\u964d\u5728\u4ed6\u4eec\u8eab\u4e0a\u4e86\u3002\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u7ea6\u62ff\u4e0d\u60a6\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5c31\u5411\u8036\u548c\u534e\u7977\u544a\uff0c\u8bf4\uff1a\u201c\u8036\u548c\u534e\u554a\uff01\u6211\u8fd8\u5728\u672c\u56fd\u7684\u65f6\u5019\uff0c\u4e0d\u662f\u8fd9\u6837\u8bf4\u8fc7\u5417\uff1f\u6211\u77e5\u9053\u4f60\u662f\u6709\u6069\u5178\u6709\u601c\u60af\u7684\u3000\u795e\uff0c\u4e0d\u8f7b\u6613\u53d1\u6012\uff0c\u5e76\u4e14\u6709\u4e30\u76db\u7684\u6148\u7231\uff0c\u8f6c\u610f\u4e0d\u964d\u707e\u7978\uff0c\u6240\u4ee5\u6211\u624d\u6025\u5fd9\u9003\u5f80\u4ed6\u65bd\u53bb\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
