                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u4eec\u82e5\u63a5\u53d7\u4eba\u7684\u89c1\u8bc1\uff0c\u3000\u795e\u7684\u89c1\u8bc1\u5c31\u66f4\u5f3a\u800c\u6709\u529b\u4e86\uff0c\u56e0\u4e3a\u8fd9\u662f\u3000\u795e\u4e3a\u4ed6\u7684\u513f\u5b50\u4f5c\u7684\u89c1\u8bc1\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4fe1\u3000\u795e\u7684\u513f\u5b50\u7684\uff0c\u5c31\u6709\u8fd9\u89c1\u8bc1\u5728\u4ed6\u5fc3\u91cc\uff1b\u4e0d\u4fe1\u3000\u795e\u7684\uff0c\u5c31\u662f\u628a\u3000\u795e\u5f53\u4f5c\u8bf4\u8c0e\u7684\uff0c\u56e0\u4e3a\u4ed6\u4e0d\u4fe1\u3000\u795e\u4e3a\u4ed6\u513f\u5b50\u6240\u4f5c\u7684\u89c1\u8bc1\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8fd9\u89c1\u8bc1\u5c31\u662f\u3000\u795e\u5df2\u7ecf\u628a\u6c38\u8fdc\u7684\u751f\u547d\u8d50\u7ed9\u6211\u4eec\uff0c\u8fd9\u751f\u547d\u662f\u5728\u4ed6\u513f\u5b50\u91cc\u9762\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u51e1\u6709\u3000\u795e\u513f\u5b50\u7684\uff0c\u5c31\u6709\u751f\u547d\uff1b\u6ca1\u6709\u3000\u795e\u513f\u5b50\u7684\uff0c\u5c31\u6ca1\u6709\u751f\u547d\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8981\u7167\u3000\u795e\u7684\u65e8\u610f\u7948\u6c42\u6211\u628a\u8fd9\u4e9b\u4e8b\u5199\u7ed9\u4f60\u4eec\u4fe1\u3000\u795e\u7684\u513f\u5b50\u4e4b\u540d\u7684\u4eba\uff0c\u662f\u8981\u4f60\u4eec\u77e5\u9053\u81ea\u5df1\u6709\u6c38\u751f\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5982\u679c\u6211\u4eec\u7167\u7740\u3000\u795e\u7684\u65e8\u610f\u7948\u6c42\uff0c\u4ed6\u5fc5\u542c\u6211\u4eec\uff1b\u8fd9\u5c31\u662f\u6211\u4eec\u5bf9\u3000\u795e\u6240\u5b58\u7684\u5766\u7136\u65e0\u60e7\u7684\u5fc3\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u65e2\u7136\u4ed6\u542c\u6211\u4eec\u7684\u7948\u6c42\uff0c\u6211\u4eec\u5c31\u77e5\u9053\uff0c\u6211\u4eec\u65e0\u8bba\u6c42\u4ec0\u4e48\uff0c\u4ed6\u5fc5\u8d50\u7ed9\u6211\u4eec\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5982\u679c\u6709\u4eba\u770b\u89c1\u5f1f\u5144\u72af\u4e86\u4e0d\u81f3\u4e8e\u6b7b\u7684\u7f6a\uff0c\u4ed6\u5c31\u8981\u7948\u6c42\uff0c\u3000\u795e\u5fc5\u56e0\u4ed6\u7684\u7f18\u6545\uff0c\u628a\u751f\u547d\u8d50\u7ed9\u90a3\u4e9b\u72af\u4e86\u4e0d\u81f3\u4e8e\u6b7b\u7684\u7f6a\u7684\u4eba\uff1b\u6709\u81f3\u4e8e\u6b7b\u7684\u7f6a\uff0c\u6211\u4e0d\u8bf4\u4ed6\u5e94\u5f53\u4e3a\u90a3\u7f6a\u7948\u6c42\u3002\n"
                },
                "17": {
