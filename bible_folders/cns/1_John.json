                "5": {
                    "verse_nr": 5,
                    "verse": "\u80dc\u8fc7\u4e16\u754c\u7684\u662f\u8c01\u5462\uff1f\u4e0d\u5c31\u662f\u90a3\u4fe1\u8036\u7a23\u662f\u3000\u795e\u7684\u513f\u5b50\u7684\u5417\uff1f\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u3000\u795e\u4e3a\u4ed6\u513f\u5b50\u4f5c\u7684\u89c1\u8bc1\u90a3\u501f\u7740\u6c34\u548c\u8840\u6765\u7684\u5c31\u662f\u8036\u7a23\u57fa\u7763\uff0c\u4e0d\u662f\u5355\u7528\u6c34\uff0c\u800c\u662f\u7528\u6c34\u53c8\u7528\u8840\uff1b\u4f5c\u89c1\u8bc1\u7684\u662f\u5723\u7075\uff0c\u56e0\u4e3a\u5723\u7075\u5c31\u662f\u771f\u7406\u3002\n"
