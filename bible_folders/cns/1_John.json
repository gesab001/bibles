    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u751f\u547d\u4e4b\u9053\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8fd9\u751f\u547d\u5df2\u7ecf\u663e\u73b0\u51fa\u6765\uff0c\u6211\u4eec\u89c1\u8fc7\u4e86\uff0c\u73b0\u5728\u4e5f\u4f5c\u89c1\u8bc1\uff0c\u5e76\u4e14\u5411\u4f60\u4eec\u5ba3\u626c\u8fd9\u672c\u6765\u4e0e\u7236\u540c\u5728\uff0c\u53c8\u5411\u6211\u4eec\u663e\u73b0\u8fc7\u7684\u6c38\u8fdc\u7684\u751f\u547d\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u4eec\u628a\u6240\u770b\u89c1\u6240\u542c\u89c1\u7684\u5411\u4f60\u4eec\u5ba3\u626c\uff0c\u4f7f\u4f60\u4eec\u4e5f\u53ef\u4ee5\u548c\u6211\u4eec\u5fc3\u7075\u76f8\u901a\u3002\u6211\u4eec\u662f\u4e0e\u7236\u548c\u4ed6\u7684\u513f\u5b50\u8036\u7a23\u57fa\u7763\u5fc3\u7075\u76f8\u901a\u7684\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u4eec\u5199\u8fd9\u4e9b\u4e8b\uff0c\u662f\u8981\u4f7f\u6211\u4eec\u7684\u559c\u4e50\u5145\u8db3\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u3000\u795e\u662f\u5149\u3000\u795e\u662f\u5149\uff0c\u5728\u4ed6\u91cc\u9762\u6beb\u65e0\u9ed1\u6697\uff1b\u8fd9\u5c31\u662f\u6211\u4eec\u4ece\u4ed6\u90a3\u91cc\u542c\u89c1\uff0c\u73b0\u5728\u4f20\u7ed9\u4f60\u4eec\u7684\u4fe1\u606f\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u4eec\u82e5\u8bf4\u81ea\u5df1\u4e0e\u4ed6\u5fc3\u7075\u76f8\u901a\uff0c\u5374\u884c\u5728\u9ed1\u6697\u91cc\uff0c\u5c31\u662f\u8bf4\u8c0e\u8bdd\uff0c\u4e0d\u5b9e\u884c\u771f\u7406\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6211\u4eec\u82e5\u884c\u5728\u5149\u4e2d\uff0c\u50cf\u4ed6\u5728\u5149\u4e2d\u4e00\u6837\uff0c\u5c31\u5f7c\u6b64\u5fc3\u7075\u76f8\u901a\uff0c\u4ed6\u513f\u5b50\u8036\u7a23\u7684\u8840\u4e5f\u6d01\u51c0\u6211\u4eec\u8131\u79bb\u4e00\u5207\u7f6a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u4eec\u82e5\u8bf4\u81ea\u5df1\u6ca1\u6709\u7f6a\uff0c\u5c31\u662f\u81ea\u6b3a\uff0c\u771f\u7406\u5c31\u4e0d\u5728\u6211\u4eec\u91cc\u9762\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u4eec\u82e5\u627f\u8ba4\u81ea\u5df1\u7684\u7f6a\uff0c\u3000\u795e\u662f\u4fe1\u5b9e\u7684\u3001\u516c\u4e49\u7684\uff0c\u5fc5\u5b9a\u8d66\u514d\u6211\u4eec\u7684\u7f6a\uff0c\u6d01\u51c0\u6211\u4eec\u8131\u79bb\u4e00\u5207\u4e0d\u4e49\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u4eec\u82e5\u8bf4\u81ea\u5df1\u6ca1\u6709\u72af\u8fc7\u7f6a\uff0c\u5c31\u662f\u628a\u3000\u795e\u5f53\u4f5c\u8bf4\u8c0e\u7684\uff0c\u4ed6\u7684\u9053\u5c31\u4e0d\u5728\u6211\u4eec\u5fc3\u91cc\u4e86\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6211\u4eec\u7684\u7ef4\u62a4\u8005\u57fa\u7763\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4ed6\u4e3a\u6211\u4eec\u7684\u7f6a\u4f5c\u4e86\u8d4e\u7f6a\u796d\uff0c\u4e0d\u4ec5\u4e3a\u6211\u4eec\u7684\u7f6a\uff0c\u4e5f\u4e3a\u5168\u4eba\u7c7b\u7684\u7f6a\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u4eec\u82e5\u9075\u5b88\u3000\u795e\u7684\u547d\u4ee4\uff0c\u8fd9\u6837\uff0c\u5c31\u77e5\u9053\u6211\u4eec\u5df2\u7ecf\u8ba4\u8bc6\u4ed6\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u51e1\u662f\u8bf4\u201c\u6211\u5df2\u7ecf\u8ba4\u8bc6\u4ed6\u201d\uff0c\u5374\u4e0d\u9075\u5b88\u4ed6\u547d\u4ee4\u7684\uff0c\u5c31\u662f\u8bf4\u8c0e\u7684\u4eba\uff0c\u771f\u7406\u5c31\u4e0d\u5728\u4ed6\u91cc\u9762\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7136\u800c\u51e1\u662f\u9075\u5b88\u4ed6\u7684\u9053\u7684\uff0c\u4ed6\u7231\u3000\u795e\u7684\u5fc3\u5c31\u7684\u786e\u5728\u4ed6\u91cc\u9762\u5b8c\u5168\u4e86\u3002\u8fd9\u6837\uff0c\u6211\u4eec\u5c31\u77e5\u9053\u6211\u4eec\u662f\u5728\u3000\u795e\u91cc\u9762\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u51e1\u8bf4\u81ea\u5df1\u662f\u4f4f\u5728\u4ed6\u91cc\u9762\u7684\uff0c\u5c31\u5e94\u8be5\u7167\u7740\u4ed6\u6240\u884c\u7684\u53bb\u884c\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u7231\u5f1f\u5144\u7684\u5c31\u662f\u4f4f\u5728\u5149\u660e\u4e2d\u4eb2\u7231\u7684\uff0c\u6211\u5199\u7ed9\u4f60\u4eec\u7684\uff0c\u4e0d\u662f\u4e00\u6761\u65b0\u547d\u4ee4\uff0c\u800c\u662f\u4f60\u4eec\u672c\u6765\u5c31\u6709\u7684\u65e7\u547d\u4ee4\u3002\u8fd9\u65e7\u547d\u4ee4\u5c31\u662f\u4f60\u4eec\u542c\u8fc7\u7684\u9053\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u7136\u800c\uff0c\u56e0\u4e3a\u9ed1\u6697\u6e10\u6e10\u6d88\u901d\uff0c\u771f\u5149\u5df2\u5728\u7167\u8000\uff0c\u6211\u5199\u7ed9\u4f60\u4eec\u7684\u4e5f\u662f\u4e00\u6761\u65b0\u547d\u4ee4\uff0c\u8fd9\u5728\u3000\u795e\u548c\u5728\u4f60\u4eec\u6765\u8bf4\uff0c\u90fd\u662f\u771f\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u51e1\u662f\u8bf4\u81ea\u5df1\u5728\u5149\u660e\u4e2d\uff0c\u5374\u6068\u4ed6\u7684\u5f1f\u5144\u7684\uff0c\u4ed6\u5230\u73b0\u5728\u8fd8\u5728\u9ed1\u6697\u91cc\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u51e1\u662f\u7231\u5f1f\u5144\u7684\uff0c\u5c31\u662f\u4f4f\u5728\u5149\u660e\u4e2d\uff0c\u5728\u5149\u660e\u4e2d\u4ed6\u5c31\u4e0d\u4f1a\u8dcc\u5012\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u51e1\u662f\u6068\u5f1f\u5144\u7684\uff0c\u5c31\u662f\u5728\u9ed1\u6697\u91cc\uff0c\u4e5f\u5728\u9ed1\u6697\u91cc\u884c\u4e8b\u3002\u4ed6\u4e0d\u77e5\u9053\u5f80\u54ea\u91cc\u53bb\uff0c\u56e0\u4e3a\u9ed1\u6697\u5f04\u778e\u4e86\u4ed6\u7684\u773c\u775b\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5b69\u5b50\u4eec\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u7684\u7f6a\u56e0\u7740\u4e3b\u7684\u540d\u5df2\u7ecf\u5f97\u5230\u8d66\u514d\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u7236\u8001\u4eec\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u8ba4\u8bc6\u592a\u521d\u5c31\u5b58\u5728\u7684\u90a3\u4e00\u4f4d\u3002\u9752\u5e74\u4eec\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u5df2\u7ecf\u80dc\u8fc7\u90a3\u6076\u8005\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5b69\u5b50\u4eec\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u8ba4\u8bc6\u7236\u3002\u7236\u8001\u4eec\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u8ba4\u8bc6\u592a\u521d\u5c31\u5b58\u5728\u7684\u90a3\u4e00\u4f4d\u3002\u9752\u5e74\u4eec\uff0c\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u56e0\u4e3a\u4f60\u4eec\u5f3a\u58ee\uff0c\u3000\u795e\u7684\u9053\u5728\u4f60\u4eec\u5fc3\u91cc\uff0c\u4f60\u4eec\u4e5f\u80dc\u8fc7\u4e86\u90a3\u6076\u8005\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4e0d\u8981\u7231\u4e16\u754c\u548c\u4e16\u4e0a\u7684\u4e1c\u897f\u3002\u4eba\u82e5\u7231\u4e16\u754c\uff0c\u7231\u7236\u7684\u5fc3\u5c31\u4e0d\u5728\u4ed6\u91cc\u9762\u4e86\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u539f\u6765\u4e16\u4e0a\u7684\u4e00\u5207\uff0c\u5c31\u5982\u8089\u4f53\u7684\u79c1\u6b32\uff0c\u773c\u76ee\u7684\u79c1\u6b32\u548c\u4eca\u751f\u7684\u9a84\u50b2\uff0c\u90fd\u4e0d\u662f\u51fa\u4e8e\u7236\uff0c\u800c\u662f\u4ece\u4e16\u754c\u6765\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8fd9\u4e16\u754c\u548c\u4e16\u4e0a\u7684\u79c1\u6b32\u90fd\u8981\u6e10\u6e10\u8fc7\u53bb\uff0c\u4f46\u90a3\u9075\u884c\u3000\u795e\u65e8\u610f\u7684\u5374\u5b58\u5230\u6c38\u8fdc\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u654c\u57fa\u7763\u8005\u5426\u8ba4\u8036\u7a23\u662f\u57fa\u7763\u5b69\u5b50\u4eec\uff0c\u73b0\u5728\u662f\u672b\u4e16\u7684\u65f6\u5019\u4e86\u3002\u4f60\u4eec\u542c\u8fc7\u654c\u57fa\u7763\u8005\u8981\u6765\uff0c\u73b0\u5728\u5df2\u7ecf\u6709\u4e0d\u5c11\u654c\u57fa\u7763\u8d77\u6765\u4e86\uff1b\u56e0\u6b64\u6211\u4eec\u5c31\u77e5\u9053\u8fd9\u662f\u672b\u4e16\u7684\u65f6\u5019\u4e86\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u4ed6\u4eec\u4ece\u6211\u4eec\u4e2d\u95f4\u79bb\u53bb\uff0c\u8fd9\u5c31\u8868\u660e\u4ed6\u4eec\u662f\u4e0d\u5c5e\u4e8e\u6211\u4eec\u7684\u3002\u5176\u5b9e\u4ed6\u4eec\u5e76\u4e0d\u5c5e\u4e8e\u6211\u4eec\uff0c\u5982\u679c\u771f\u7684\u5c5e\u4e8e\u6211\u4eec\uff0c\u5c31\u4e00\u5b9a\u4f1a\u7559\u5728\u6211\u4eec\u4e2d\u95f4\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4f60\u4eec\u4ece\u90a3\u5723\u8005\u5f97\u7740\u818f\u62b9\uff0c\u8fd9\u662f\u4f60\u4eec\u90fd\u77e5\u9053\u7684\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211\u5199\u4fe1\u7ed9\u4f60\u4eec\uff0c\u4e0d\u662f\u56e0\u4e3a\u4f60\u4eec\u4e0d\u660e\u767d\u771f\u7406\uff0c\u800c\u662f\u56e0\u4e3a\u4f60\u4eec\u660e\u767d\uff0c\u53c8\u56e0\u4e3a\u8c0e\u8a00\u7edd\u4e0d\u4f1a\u51fa\u4e8e\u771f\u7406\u7684\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u90a3\u4e48\uff0c\u8c01\u662f\u8bf4\u8c0e\u7684\u5462\uff1f\u4e0d\u5c31\u662f\u90a3\u5426\u8ba4\u8036\u7a23\u662f\u57fa\u7763\u7684\u5417\uff1f\u5426\u8ba4\u7236\u548c\u5b50\u7684\uff0c\u4ed6\u5c31\u662f\u654c\u57fa\u7763\u7684\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u51e1\u5426\u8ba4\u5b50\u7684\uff0c\u5c31\u6ca1\u6709\u7236\uff1b\u627f\u8ba4\u5b50\u7684\uff0c\u8fde\u7236\u4e5f\u6709\u4e86\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u81f3\u4e8e\u4f60\u4eec\uff0c\u603b\u8981\u628a\u8d77\u521d\u6240\u542c\u89c1\u7684\u5b58\u8bb0\u5728\u4f60\u4eec\u5fc3\u91cc\uff1b\u4f60\u4eec\u82e5\u628a\u8d77\u521d\u6240\u542c\u89c1\u7684\u5b58\u8bb0\u5728\u5fc3\u91cc\uff0c\u4f60\u4eec\u4e5f\u5c31\u4f4f\u5728\u5b50\u548c\u7236\u91cc\u9762\u4e86\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u4ed6\u5e94\u8bb8\u7ed9\u6211\u4eec\u7684\uff0c\u5c31\u662f\u6c38\u8fdc\u7684\u751f\u547d\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u6211\u628a\u8fd9\u4e9b\u4e8b\u5199\u7ed9\u4f60\u4eec\uff0c\u662f\u6307\u7740\u90a3\u4e9b\u6b3a\u9a97\u4f60\u4eec\u7684\u4eba\u8bf4\u7684\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u4f60\u4eec\u65e2\u7136\u5728\u4f60\u4eec\u91cc\u9762\u6709\u4ece\u4e3b\u9886\u53d7\u7684\u818f\u62b9\uff0c\u5c31\u4e0d\u9700\u8981\u522b\u4eba\u6559\u5bfc\u4f60\u4eec\u4e86\uff0c\u56e0\u4e3a\u5728\u4e00\u5207\u4e8b\u4e0a\u6709\u4e3b\u7684\u818f\u62b9\u6559\u5bfc\u4f60\u4eec\uff1b\u8fd9\u818f\u62b9\u662f\u771f\u7684\uff0c\u4e0d\u662f\u5047\u7684\uff0c\u4f60\u4eec\u5e94\u8be5\u6309\u7740\u4ed6\u6240\u6559\u5bfc\u7684\u4f4f\u5728\u4ed6\u91cc\u9762\u3002\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\u3000\u795e\u7684\u513f\u5973\u5c31\u4e0d\u72af\u7f6a\u5b69\u5b50\u4eec\uff0c\u4f60\u4eec\u8981\u4f4f\u5728\u4e3b\u91cc\u9762\u3002\u8fd9\u6837\uff0c\u4ed6\u82e5\u663e\u73b0\uff0c\u6211\u4eec\u5c31\u53ef\u4ee5\u5766\u7136\u65e0\u60e7\uff1b\u4ed6\u6765\u7684\u65f6\u5019\uff0c\u6211\u4eec\u4e5f\u4e0d\u81f3\u60ed\u6127\u5730\u8eb2\u907f\u4ed6\u4e86\u3002\n"
                },
                "29": {
                    "verse_nr": 29,
                    "verse": "\u4f60\u4eec\u65e2\u7136\u6653\u5f97\u3000\u795e\u662f\u516c\u4e49\u7684\uff0c\u5c31\u5e94\u5f53\u77e5\u9053\u6240\u6709\u884c\u516c\u4e49\u7684\u90fd\u662f\u4ece\u4ed6\u751f\u7684\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
