        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5199\u4fe1\u7ed9\u4eb2\u7231\u7684\u513f\u5b50\u63d0\u6469\u592a\uff0c\u613f\u6069\u60e0\u3001\u601c\u60af\u3001\u5e73\u5b89\u4ece\u7236\u3000\u795e\u548c\u6211\u4eec\u7684\u4e3b\u57fa\u7763\u8036\u7a23\u4e34\u5230\u4f60\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e3a\u4e3b\u4f5c\u89c1\u8bc1\u4e0d\u8981\u5f53\u4f5c\u7f9e\u803b\u6211\u611f\u8c22\u3000\u795e\uff0c\u5c31\u662f\u6211\u50cf\u6211\u7956\u5148\u4e00\u6837\uff0c\u4ee5\u6e05\u6d01\u7684\u826f\u5fc3\u6240\u4e8b\u5949\u7684\u3000\u795e\uff0c\u5728\u7977\u544a\u4e2d\u663c\u591c\u4e0d\u65ad\u5730\u8bb0\u5ff5\u4f60\uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4e00\u60f3\u8d77\u4f60\u6d41\u7684\u773c\u6cea\uff0c\u6211\u5c31\u6e34\u671b\u89c1\u4f60\uff0c\u597d\u53eb\u6211\u6ee1\u6709\u559c\u4e50\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6211\u8bb0\u5f97\u4f60\u5fc3\u91cc\u65e0\u4f2a\u7684\u4fe1\u5fc3\uff0c\u8fd9\u4fe1\u5fc3\u539f\u5148\u662f\u5728\u4f60\u5916\u7956\u6bcd\u7f57\u4ee5\u548c\u4f60\u6bcd\u4eb2\u53cb\u5c3c\u57fa\u5fc3\u91cc\u7684\uff0c\u6211\u6df1\u4fe1\u4e5f\u5728\u4f60\u7684\u5fc3\u91cc\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4e3a\u4e86\u8fd9\u7f18\u6545\uff0c\u6211\u63d0\u9192\u4f60\uff0c\u8981\u628a\u3000\u795e\u501f\u7740\u6211\u6309\u624b\u7ed9\u4f60\u7684\u6069\u8d50\uff0c\u50cf\u706b\u4e00\u6837\u518d\u6311\u65fa\u8d77\u6765\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u4e3a\u3000\u795e\u6240\u8d50\u7ed9\u6211\u4eec\u7684\uff0c\u4e0d\u662f\u80c6\u602f\u7684\u7075\uff0c\u800c\u662f\u6709\u80fd\u529b\u3001\u4ec1\u7231\u3001\u81ea\u5f8b\u7684\u7075\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6240\u4ee5\uff0c\u4e0d\u8981\u4ee5\u7ed9\u6211\u4eec\u7684\u4e3b\u4f5c\u89c1\u8bc1\u5f53\u4f5c\u7f9e\u803b\uff0c\u4e5f\u4e0d\u8981\u4ee5\u6211\u8fd9\u4e3a\u4e3b\u88ab\u56da\u7684\u5f53\u4f5c\u7f9e\u803b\uff0c\u5374\u8981\u4e3a\u4e86\u798f\u97f3\u7684\u7f18\u6545\uff0c\u9760\u7740\u3000\u795e\u7684\u5927\u80fd\uff0c\u4e0e\u6211\u540c\u53d7\u78e8\u96be\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u3000\u795e\u6551\u4e86\u6211\u4eec\uff0c\u4ee5\u5723\u53ec\u547c\u53ec\u6211\u4eec\uff0c\u4e0d\u662f\u6309\u7167\u6211\u4eec\u7684\u884c\u4e3a\uff0c\u5374\u662f\u6309\u7167\u4ed6\u81ea\u5df1\u7684\u8ba1\u5212\u548c\u6069\u5178\uff1b\u8fd9\u6069\u5178\u662f\u5728\u6c38\u4e16\u4e4b\u5148\uff0c\u5728\u57fa\u7763\u8036\u7a23\u91cc\u8d50\u7ed9\u4e86\u6211\u4eec\u7684\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f46\u73b0\u5728\u501f\u7740\u6211\u4eec\u6551\u4e3b\u57fa\u7763\u8036\u7a23\u7684\u663e\u73b0\uff0c\u624d\u8868\u660e\u51fa\u6765\u3002\u4ed6\u5e9f\u6389\u4e86\u6b7b\u4ea1\uff0c\u501f\u7740\u798f\u97f3\u628a\u751f\u547d\u548c\u4e0d\u673d\u5f70\u663e\u51fa\u6765\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4e3a\u8fd9\u798f\u97f3\uff0c\u6211\u88ab\u6d3e\u4f5c\u4f20\u9053\u7684\u3001\u4f7f\u5f92\u548c\u6559\u5e08\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4e3a\u4e86\u8fd9\u7f18\u6545\uff0c\u6211\u4e5f\u53d7\u8fd9\u4e9b\u82e6\uff0c\u4f46\u6211\u4e0d\u4ee5\u4e3a\u803b\uff0c\u56e0\u4e3a\u6211\u77e5\u9053\u6211\u6240\u4fe1\u7684\u662f\u8c01\uff0c\u4e5f\u6df1\u4fe1\u4ed6\u80fd\u4fdd\u5b88\u6211\u6240\u4ea4\u6258\u4ed6\u7684\uff0c\u76f4\u5230\u90a3\u65e5\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u8981\u9760\u7740\u5728\u57fa\u7763\u8036\u7a23\u91cc\u7684\u4fe1\u5fc3\u548c\u7231\u5fc3\uff0c\u5e38\u5e38\u575a\u5b88\u4ece\u6211\u8fd9\u91cc\u542c\u8fc7\u7684\u8bdd\uff0c\u4f5c\u4e3a\u7eaf\u6b63\u8bdd\u8bed\u7684\u6a21\u8303\uff1b\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u53c8\u8981\u9760\u7740\u90a3\u4f4f\u5728\u6211\u4eec\u91cc\u9762\u7684\u5723\u7075\uff0c\u4fdd\u5b88\u6240\u4ea4\u6258\u4f60\u7684\u5584\u9053\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u77e5\u9053\u6240\u6709\u5728\u4e9a\u897f\u4e9a\u7684\u4eba\u90fd\u79bb\u5f03\u4e86\u6211\uff0c\u4ed6\u4eec\u5f53\u4e2d\u6709\u8153\u5409\u8def\u548c\u9ed1\u6469\u5176\u5c3c\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u613f\u4e3b\u8d50\u601c\u60af\u7ed9\u963f\u5c3c\u8272\u5f17\u4e00\u5bb6\u7684\u4eba\uff0c\u56e0\u4e3a\u4ed6\u591a\u6b21\u4f7f\u6211\u7545\u5feb\uff0c\u4e5f\u4e0d\u4ee5\u6211\u7684\u9501\u94fe\u4e3a\u803b\uff1b\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ed6\u7adf\u7136\u6765\u5230\u7f57\u9a6c\uff0c\u8feb\u5207\u5730\u5bfb\u627e\u6211\uff0c\u7ed3\u679c\u627e\u5230\u4e86\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\uff08\u613f\u4e3b\u5728\u90a3\u65e5\u4f7f\u4ed6\u4ece\u4e3b\u90a3\u91cc\u5f97\u7740\u601c\u60af\u3002\uff09\u4f60\u4e5f\u6e05\u695a\u77e5\u9053\u4ed6\u5728\u4ee5\u5f17\u6240\u662f\u600e\u6837\u591a\u65b9\u670d\u4e8b\u6211\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4f5c\u57fa\u7763\u7684\u7cbe\u5175\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u53c8\u5e94\u5f53\u628a\u4f60\u5728\u8bb8\u591a\u89c1\u8bc1\u4eba\u9762\u524d\u4ece\u6211\u8fd9\u91cc\u542c\u89c1\u7684\uff0c\u4ea4\u6258\u7ed9\u90a3\u4e9b\u53c8\u5fe0\u5fc3\u53c8\u80fd\u591f\u6559\u5bfc\u522b\u4eba\u7684\u4eba\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u5e94\u5f53\u548c\u6211\u540c\u53d7\u78e8\u96be\uff0c\u597d\u50cf\u57fa\u7763\u8036\u7a23\u7684\u7cbe\u5175\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5f53\u5175\u7684\u4eba\u4e0d\u8ba9\u4e16\u52a1\u7f20\u8eab\uff0c\u4e3a\u8981\u4f7f\u90a3\u62db\u5175\u7684\u4eba\u6b22\u559c\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7ade\u8d5b\u7684\u4eba\u82e5\u4e0d\u9075\u5b88\u89c4\u5219\uff0c\u5c31\u4e0d\u80fd\u5f97\u51a0\u5195\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u52b3\u529b\u7684\u519c\u592b\u7406\u5f53\u5148\u5c1d\u679c\u5b9e\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u8981\u60f3\u60f3\u6211\u7684\u8bdd\uff0c\u56e0\u4e3a\u51e1\u4e8b\u4e3b\u5fc5\u7ed9\u4f60\u9886\u609f\u529b\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u8981\u8bb0\u5f97\u90a3\u4ece\u6b7b\u4eba\u4e2d\u590d\u6d3b\u7684\u8036\u7a23\u57fa\u7763\uff0c\u4ed6\u662f\u5927\u536b\u7684\u540e\u88d4\u6240\u751f\u7684\uff0c\u8fd9\u5c31\u662f\u6211\u6240\u4f20\u7684\u798f\u97f3\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u4e3a\u4e86\u8fd9\u798f\u97f3\u53d7\u4e86\u78e8\u96be\uff0c\u751a\u81f3\u50cf\u72af\u4eba\u4e00\u6837\u88ab\u6346\u7ed1\u8d77\u6765\uff1b\u53ef\u662f\uff0c\u3000\u795e\u7684\u8bdd\u5374\u4e0d\u88ab\u6346\u7ed1\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u6b64\uff0c\u6211\u4e3a\u4e86\u9009\u6c11\u5fcd\u53d7\u4e00\u5207\uff0c\u597d\u53eb\u4ed6\u4eec\u4e5f\u53ef\u4ee5\u5f97\u7740\u5728\u57fa\u7763\u8036\u7a23\u91cc\u7684\u6551\u6069\uff0c\u548c\u6c38\u8fdc\u7684\u8363\u8000\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u201c\u6211\u4eec\u82e5\u4e0e\u57fa\u7763\u540c\u6b7b\uff0c\u5c31\u5fc5\u4e0e\u4ed6\u540c\u6d3b\uff1b\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u4eec\u82e5\u80fd\u575a\u5fcd\uff0c\u5c31\u5fc5\u4e0e\u4ed6\u4e00\u540c\u4f5c\u738b\uff1b\u6211\u4eec\u82e5\u4e0d\u8ba4\u4ed6\uff0c\u4ed6\u5fc5\u4e0d\u8ba4\u6211\u4eec\uff1b\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u4eec\u7eb5\u7136\u4e0d\u4fe1\uff0c\u4ed6\u4ecd\u7136\u662f\u4fe1\u5b9e\u7684\uff0c\u56e0\u4e3a\u4ed6\u4e0d\u80fd\u5426\u5b9a\u81ea\u5df1\u3002\u201d\u8fd9\u8bdd\u662f\u53ef\u4fe1\u7684\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f5c\u65e0\u6127\u7684\u5de5\u4eba\u4f60\u8981\u5728\u3000\u795e\u9762\u524d\u628a\u8fd9\u4e9b\u4e8b\u63d0\u9192\u4f17\u4eba\uff0c\u53ee\u5631\u4ed6\u4eec\u4e0d\u8981\u4f5c\u65e0\u76ca\u7684\u4e89\u8fa9\uff1b\u8fd9\u53ea\u80fd\u8d25\u574f\u542c\u89c1\u7684\u4eba\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u5e94\u5f53\u7aed\u529b\u5728\u3000\u795e\u9762\u524d\u4f5c\u4e00\u4e2a\u8499\u79f0\u8bb8\u3001\u65e0\u6127\u7684\u5de5\u4eba\uff0c\u6b63\u786e\u5730\u8bb2\u89e3\u771f\u7406\u7684\u9053\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u603b\u8981\u8fdc\u907f\u4e16\u4fd7\u7684\u7a7a\u8c08\uff0c\u56e0\u4e3a\u8fd9\u4e9b\u5fc5\u4f1a\u5f15\u4eba\u8fdb\u5230\u66f4\u4e0d\u656c\u8654\u7684\u5730\u6b65\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ed6\u4eec\u7684\u8bdd\u597d\u50cf\u6bd2\u7624\u4e00\u6837\u8513\u5ef6\uff1b\u4ed6\u4eec\u5f53\u4e2d\u6709\u8bb8\u7c73\u4e43\u548c\u8153\u7406\u5f92\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6\u4eec\u504f\u79bb\u4e86\u771f\u9053\uff0c\u8bf4\u590d\u6d3b\u7684\u4e8b\u5df2\u7ecf\u8fc7\u53bb\u4e86\uff0c\u4e8e\u662f\u6bc1\u574f\u4e86\u4e00\u4e9b\u4eba\u7684\u4fe1\u5fc3\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u7136\u800c\uff0c\u3000\u795e\u575a\u56fa\u7684\u6839\u57fa\u5df2\u7ecf\u7acb\u5b9a\uff0c\u4e0a\u9762\u523b\u7740\u8fd9\u6837\u7684\u5370\uff1a\u201c\u4e3b\u8ba4\u8bc6\u5c5e\u4e8e\u4ed6\u7684\u4eba\u201d\u548c\u201c\u51e1\u79f0\u547c\u4e3b\u540d\u7684\u4eba\u90fd\u5e94\u5f53\u79bb\u5f00\u4e0d\u4e49\u201d\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5728\u5bcc\u8d35\u4eba\u7684\u5bb6\u91cc\uff08\u201c\u5bcc\u8d35\u4eba\u7684\u5bb6\u91cc\u201d\u539f\u6587\u4f5c\u201c\u5927\u623f\u5b50\u201d\uff09\uff0c\u4e0d\u4f46\u6709\u91d1\u5668\u3001\u94f6\u5668\uff0c\u4e5f\u6709\u6728\u5668\u3001\u74e6\u5668\uff1b\u6709\u8d35\u91cd\u7684\uff0c\u4e5f\u6709\u5351\u8d31\u7684\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u4eba\u82e5\u81ea\u6d01\uff0c\u79bb\u5f00\u5351\u8d31\u7684\u4e8b\uff0c\u5c31\u5fc5\u4f5c\u8d35\u91cd\u7684\u5668\u76bf\uff0c\u6210\u4e3a\u5723\u6d01\uff0c\u5408\u4e3b\u4f7f\u7528\uff0c\u9884\u5907\u884c\u5404\u6837\u7684\u5584\u4e8b\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4f60\u5e94\u5f53\u9003\u907f\u5e74\u8f7b\u4eba\u7684\u79c1\u6b32\uff0c\u8981\u548c\u90a3\u4e9b\u4ee5\u6e05\u6d01\u7684\u5fc3\u6c42\u544a\u4e3b\u7684\u4eba\uff0c\u4e00\u540c\u8ffd\u6c42\u516c\u4e49\u3001\u4fe1\u5fc3\u3001\u7231\u5fc3\u3001\u548c\u5e73\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u4f60\u8981\u62d2\u7edd\u611a\u8822\u65e0\u77e5\u7684\u95ee\u96be\uff1b\u4f60\u77e5\u9053\u8fd9\u4e9b\u4e8b\u4f1a\u5f15\u8d77\u4e89\u8bba\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u4f46\u4e3b\u7684\u4ec6\u4eba\u5374\u4e0d\u53ef\u4e89\u8bba\uff0c\u603b\u8981\u5f85\u4eba\u6e29\u548c\uff0c\u5584\u4e8e\u6559\u5bfc\uff0c\u5b58\u5fc3\u5fcd\u8010\uff0c\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u4ee5\u6e29\u67d4\u529d\u5bfc\u90a3\u4e9b\u5bf9\u6297\u7684\u4eba\uff0c\u6216\u8bb8\u3000\u795e\u7ed9\u4ed6\u4eec\u6094\u6539\u7684\u5fc3\uff0c\u53ef\u4ee5\u8ba4\u8bc6\u771f\u7406\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u4ed6\u4eec\u867d\u7136\u66fe\u7ecf\u88ab\u9b54\u9b3c\u63b3\u53bb\uff0c\u968f\u4ece\u4ed6\u7684\u610f\u601d\u800c\u884c\uff0c\u6216\u8bb8\u4e5f\u80fd\u9192\u609f\u8fc7\u6765\uff0c\u8131\u79bb\u9b54\u9b3c\u7684\u9677\u9631\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u672b\u4e16\u7684\u60c5\u51b5\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u90a3\u65f6\uff0c\u4eba\u4f1a\u4e13\u7231\u81ea\u5df1\u3001\u8d2a\u7231\u94b1\u8d22\u3001\u81ea\u5938\u3001\u9ad8\u50b2\u3001\u4eb5\u6e0e\u3001\u6096\u9006\u7236\u6bcd\u3001\u5fd8\u6069\u8d1f\u4e49\u3001\u4e0d\u5723\u6d01\u3001\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6ca1\u6709\u4eb2\u60c5\u3001\u4e0d\u80af\u548c\u89e3\u3001\u6076\u8a00\u4e2d\u4f24\u3001\u4e0d\u80fd\u81ea\u5f8b\u3001\u6a2a\u86ee\u51f6\u66b4\u3001\u4e0d\u7231\u826f\u5584\u3001\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5356\u4e3b\u5356\u53cb\u3001\u5bb9\u6613\u51b2\u52a8\u3001\u50b2\u6162\u81ea\u5927\u3001\u7231\u4eab\u4e50\u8fc7\u4e8e\u7231\u3000\u795e\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6709\u656c\u8654\u7684\u5f62\u5f0f\uff0c\u5374\u5426\u5b9a\u656c\u8654\u7684\u80fd\u529b\uff1b\u8fd9\u4e9b\u4eba\u4f60\u5e94\u5f53\u907f\u5f00\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u4eec\u4e2d\u95f4\u6709\u4eba\u6f5c\u5165\u522b\u4eba\u7684\u5bb6\u91cc\uff0c\u64cd\u7eb5\u65e0\u77e5\u7684\u5987\u5973\u3002\u8fd9\u4e9b\u5987\u5973\u88ab\u7f6a\u538b\u4f4f\uff0c\u53c8\u88ab\u5404\u79cd\u79c1\u6b32\u62d6\u7d2f\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u867d\u7136\u5e38\u5e38\u5b66\u4e60\uff0c\u603b\u4e0d\u80fd\u5145\u5206\u660e\u767d\u771f\u7406\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ece\u524d\u96c5\u5c3c\u548c\u6768\u84d3\u600e\u6837\u53cd\u5bf9\u6469\u897f\uff0c\u8fd9\u4e9b\u4eba\u4e5f\u600e\u6837\u53cd\u5bf9\u771f\u7406\u3002\u4ed6\u4eec\u7684\u5fc3\u601d\u8d25\u574f\uff0c\u4fe1\u4ef0\u4e5f\u7ecf\u4e0d\u8d77\u8003\u9a8c\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7136\u800c\u4ed6\u4eec\u4e0d\u80fd\u518d\u8fd9\u6837\u4e0b\u53bb\u4e86\uff0c\u56e0\u4e3a\u4ed6\u4eec\u7684\u611a\u6627\u7ec8\u5fc5\u5728\u4f17\u4eba\u9762\u524d\u663e\u9732\u51fa\u6765\uff0c\u597d\u50cf\u90a3\u4e24\u4e2a\u4eba\u4e00\u6837\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5723\u7ecf\u90fd\u662f\u3000\u795e\u9ed8\u793a\u7684\u4f46\u662f\uff0c\u4f60\u5df2\u7ecf\u4e86\u89e3\u6211\u7684\u6559\u8bad\u3001\u54c1\u884c\u3001\u5fd7\u5411\u3001\u4fe1\u5fc3\u3001\u5bbd\u5bb9\u3001\u7231\u5fc3\u548c\u5fcd\u8010\uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ee5\u53ca\u6211\u5728\u5b89\u63d0\u963f\u3001\u4ee5\u54e5\u5ff5\u3001\u8def\u53f8\u5f97\u6240\u53d7\u7684\u82e6\u548c\u8feb\u5bb3\u3002\u6211\u6240\u5fcd\u53d7\u7684\u662f\u600e\u6837\u7684\u8feb\u5bb3\uff1b\u8fd9\u4e00\u5207\u4e3b\u90fd\u6551\u6211\u8131\u79bb\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5176\u5b9e\uff0c\u6240\u6709\u7acb\u5fd7\u5728\u57fa\u7763\u8036\u7a23\u91cc\u8fc7\u656c\u8654\u751f\u6d3b\u7684\uff0c\u90fd\u5fc5\u906d\u53d7\u8feb\u5bb3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f46\u6076\u4eba\u548c\u9a97\u5b50\u5fc5\u8d8a\u6765\u8d8a\u574f\uff0c\u4ed6\u4eec\u6b3a\u9a97\u4eba\uff0c\u4e5f\u5fc5\u53d7\u6b3a\u9a97\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u5e94\u5f53\u9075\u5b88\u6240\u5b66\u548c\u786e\u4fe1\u7684\uff1b\u4f60\u77e5\u9053\u662f\u8ddf\u8c01\u5b66\u7684\uff0c\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5e76\u4e14\u77e5\u9053\u81ea\u5df1\u4ece\u5c0f\u5c31\u660e\u767d\u5723\u7ecf\uff0c\u8fd9\u5723\u7ecf\u80fd\u591f\u4f7f\u4f60\u6709\u667a\u6167\uff0c\u53ef\u4ee5\u56e0\u4fe1\u57fa\u7763\u8036\u7a23\u5f97\u7740\u6551\u6069\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5168\u90e8\u5723\u7ecf\u90fd\u662f\u3000\u795e\u6240\u9ed8\u793a\u7684\uff0c\u5728\u6559\u8bad\u3001\u8d23\u5907\u3001\u77eb\u6b63\u548c\u516c\u4e49\u7684\u8bad\u7ec3\u5404\u65b9\u9762\uff0c\u90fd\u662f\u6709\u76ca\u7684\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4e3a\u8981\u4f7f\u5c5e\u3000\u795e\u7684\u4eba\u88c5\u5907\u597d\uff0c\u53ef\u4ee5\u5b8c\u6210\u5404\u6837\u7684\u5584\u5de5\u3002\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u52a1\u8981\u4f20\u9053\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u52a1\u8981\u4f20\u9053\uff1b\u65e0\u8bba\u65f6\u673a\u662f\u5426\u9002\u5408\uff0c\u90fd\u8981\u5e38\u4f5c\u51c6\u5907\uff1b\u8981\u4ee5\u591a\u65b9\u7684\u5fcd\u8010\u548c\u6559\u8bad\u8d23\u5907\u4eba\u3001\u8b66\u6212\u4eba\u3001\u529d\u52c9\u4eba\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56e0\u4e3a\u65f6\u5019\u5feb\u8981\u5230\u4e86\uff0c\u4eba\u5fc5\u5bb9\u4e0d\u4e0b\u7eaf\u6b63\u7684\u9053\u7406\uff0c\u53cd\u800c\u8033\u6735\u53d1\u75d2\uff0c\u968f\u7740\u81ea\u5df1\u7684\u79c1\u6b32\uff0c\u589e\u6dfb\u8bb8\u591a\u6559\u5e08\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u800c\u4e14\u8f6c\u79bb\u4e0d\u542c\u771f\u7406\uff0c\u53cd\u5012\u8d8b\u5411\u65e0\u7a3d\u4e4b\u8c08\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u5374\u8981\u51e1\u4e8b\u8c28\u614e\uff0c\u5fcd\u53d7\u78e8\u96be\u3001\u4f5c\u4f20\u798f\u97f3\u8005\u7684\u5de5\u4f5c\uff0c\u5b8c\u6210\u4f60\u7684\u804c\u52a1\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u5df2\u7ecf\u88ab\u6d47\u5960\uff1b\u6211\u79bb\u4e16\u7684\u65f6\u5019\u4e5f\u5230\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u90a3\u7f8e\u597d\u7684\u4ed7\u6211\u5df2\u7ecf\u6253\u8fc7\u4e86\uff0c\u5f53\u8dd1\u7684\u8def\u6211\u5df2\u7ecf\u8dd1\u5c3d\u4e86\uff0c\u6240\u6301\u7684\u4fe1\u4ef0\u6211\u5df2\u7ecf\u5b88\u4f4f\u4e86\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ece\u6b64\u4ee5\u540e\uff0c\u6709\u516c\u4e49\u7684\u51a0\u5195\u4e3a\u6211\u5b58\u7559\uff0c\u5c31\u662f\u6309\u516c\u4e49\u5ba1\u5224\u7684\u4e3b\u5728\u90a3\u65e5\u8981\u8d4f\u7ed9\u6211\u7684\uff1b\u4e0d\u4f46\u8d4f\u7ed9\u6211\uff0c\u4e5f\u8d4f\u7ed9\u6240\u6709\u7231\u6155\u4ed6\u663e\u73b0\u7684\u4eba\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6700\u540e\u7684\u5429\u5490\u4f60\u8981\u5c3d\u5feb\u5230\u6211\u8fd9\u91cc\u6765\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u4e3a\u5e95\u9a6c\u8d2a\u7231\u73b0\u4eca\u7684\u4e16\u754c\uff0c\u5c31\u79bb\u5f03\u6211\u5230\u5e16\u6492\u7f57\u5c3c\u8fe6\u53bb\u4e86\u3002\u9769\u52d2\u58eb\u53bb\u4e86\u52a0\u62c9\u592a\uff0c\u63d0\u591a\u53bb\u4e86\u8fbe\u9a6c\u592a\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u53ea\u6709\u8def\u52a0\u5728\u6211\u8fd9\u91cc\u3002\u4f60\u627e\u5230\u4e86\u9a6c\u53ef\uff0c\u5c31\u628a\u4ed6\u5e26\u6765\uff0c\u56e0\u4e3a\u4ed6\u5728\u5723\u5de5\u4e0a\u5bf9\u6211\u6709\u76ca\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u5dee\u6d3e\u4e86\u63a8\u57fa\u53e4\u5230\u4ee5\u5f17\u6240\u53bb\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u5728\u7279\u7f57\u4e9a\u7559\u5728\u52a0\u5e03\u90a3\u91cc\u7684\u5916\u8863\uff0c\u4f60\u6765\u7684\u65f6\u5019\u8981\u628a\u5b83\u5e26\u6765\uff0c\u8fd8\u6709\u90a3\u4e9b\u4e66\u5377\uff0c\u7279\u522b\u662f\u90a3\u4e9b\u7f8a\u76ae\u5377\uff0c\u4e5f\u8981\u5e26\u6765\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u94dc\u5320\u4e9a\u5386\u5c71\u5927\u4f5c\u4e86\u8bb8\u591a\u6076\u4e8b\u9677\u5bb3\u6211\uff0c\u4e3b\u5fc5\u6309\u7740\u4ed6\u6240\u884c\u7684\u62a5\u5e94\u4ed6\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u4e5f\u8981\u63d0\u9632\u4ed6\uff0c\u56e0\u4e3a\u4ed6\u6781\u529b\u62b5\u6321\u6211\u4eec\u7684\u8bdd\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6211\u7b2c\u4e00\u6b21\u7533\u8fa9\u7684\u65f6\u5019\uff0c\u6ca1\u6709\u4e00\u4e2a\u4eba\u652f\u6301\u6211\uff0c\u53cd\u800c\u79bb\u5f03\u6211\u3002\u4f46\u613f\u8fd9\u7f6a\u4e0d\u8981\u5f52\u5728\u4ed6\u4eec\u8eab\u4e0a\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u7136\u800c\u4e3b\u7ad9\u5728\u6211\u65c1\u8fb9\uff0c\u52a0\u7ed9\u6211\u529b\u91cf\uff0c\u4f7f\u798f\u97f3\u7684\u4fe1\u606f\u501f\u7740\u6211\u5c3d\u90fd\u4f20\u5f00\uff0c\u4e07\u56fd\u7684\u4eba\u90fd\u53ef\u4ee5\u542c\u89c1\uff1b\u4ed6\u5e76\u4e14\u628a\u6211\u4ece\u72ee\u5b50\u53e3\u4e2d\u6551\u51fa\u6765\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4e3b\u5fc5\u6551\u6211\u8131\u79bb\u4e00\u5207\u90aa\u6076\u7684\u4e8b\uff0c\u4e5f\u5fc5\u6551\u6211\u8fdb\u5165\u4ed6\u5929\u4e0a\u7684\u56fd\u3002\u613f\u8363\u8000\u5f52\u7ed9\u4ed6\uff0c\u76f4\u5230\u6c38\u6c38\u8fdc\u8fdc\u3002\u963f\u4eec\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u95ee\u5019\u7684\u8bdd\u95ee\u5019\u767e\u57fa\u62c9\u3001\u4e9a\u5c45\u62c9\u548c\u963f\u5c3c\u8272\u5f17\u4e00\u5bb6\u7684\u4eba\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4ee5\u62c9\u90fd\u5728\u54e5\u6797\u591a\u4f4f\u4e0b\u4e86\u3002\u7279\u7f57\u975e\u6469\u75c5\u4e86\uff0c\u6211\u5c31\u7559\u4ed6\u5728\u7c73\u5229\u90fd\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u4f60\u8981\u8d76\u7d27\u5728\u51ac\u5929\u4ee5\u524d\u5230\u6211\u8fd9\u91cc\u6765\u3002\u53cb\u5e03\u7f57\u3001\u5e03\u7530\u3001\u5229\u5974\u3001\u9769\u8001\u5e95\u4e9a\u548c\u4f17\u5f1f\u5144\u90fd\u95ee\u5019\u4f60\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4e3b\u4e0e\u4f60\u540c\u5728\u3002\u613f\u6069\u60e0\u4e0e\u4f60\u4eec\u540c\u5728\u3002\n"
                }
            }
        }
    }
}