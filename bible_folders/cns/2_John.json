{
    "version": "cns",
    "book_name": "2 John",
    "book_nr": "63",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\u6211\u8fd9\u4f5c\u957f\u8001\u7684\uff0c\u5199\u4fe1\u7ed9\u8499\u62e3\u9009\u7684\u592b\u4eba\u548c\u5979\u7684\u513f\u5973\u3002\u4f60\u4eec\u662f\u6211\u5728\u771f\u7406\u4e2d\u6240\u7231\u7684\uff1b\u4e0d\u5355\u662f\u6211\uff0c\u51e1\u8ba4\u8bc6\u771f\u7406\u7684\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4e5f\u56e0\u4e3a\u771f\u7406\u7684\u7f18\u6545\u7231\u4f60\u4eec\u3002\u8fd9\u771f\u7406\u5728\u6211\u4eec\u91cc\u9762\uff0c\u4e5f\u5fc5\u4e0e\u6211\u4eec\u540c\u5728\uff0c\u76f4\u5230\u6c38\u8fdc\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u613f\u6069\u60e0\u3001\u601c\u60af\u3001\u5e73\u5b89\uff0c\u4ece\u7236\u3000\u795e\u548c\u4ed6\u513f\u5b50\u8036\u7a23\u57fa\u7763\u5728\u771f\u7406\u548c\u7231\u4e2d\u4e0e\u6211\u4eec\u540c\u5728\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8981\u6309\u771f\u7406\u51ed\u7231\u5fc3\u884c\u4e8b\u6211\u770b\u89c1\u4f60\u7684\u513f\u5973\u4e2d\uff0c\u6709\u4eba\u7167\u7740\u6211\u4eec\u4ece\u7236\u9886\u53d7\u7684\u547d\u4ee4\u5728\u771f\u7406\u4e2d\u884c\u4e8b\uff0c\u6211\u5c31\u975e\u5e38\u6b23\u6170\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u592b\u4eba\u54ea\uff0c\u6211\u73b0\u5728\u8bf7\u6c42\u4f60\uff0c\u6211\u4eec\u8981\u5f7c\u6b64\u76f8\u7231\u3002\u6211\u5199\u7ed9\u4f60\u7684\uff0c\u4e0d\u662f\u4e00\u6761\u65b0\u547d\u4ee4\uff0c\u800c\u662f\u6211\u4eec\u4ece\u8d77\u521d\u5c31\u6709\u7684\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u4eec\u8981\u7167\u7740\u4ed6\u7684\u547d\u4ee4\u884c\u4e8b\uff0c\u8fd9\u5c31\u662f\u7231\u3002\u4f60\u4eec\u4ece\u8d77\u521d\u6240\u542c\u89c1\u7684\u90a3\u547d\u4ee4\uff0c\u5c31\u662f\u8981\u4f60\u4eec\u51ed\u7740\u7231\u5fc3\u884c\u4e8b\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6709\u8bb8\u591a\u6b3a\u9a97\u4eba\u7684\u5df2\u7ecf\u5728\u4e16\u4e0a\u51fa\u73b0\uff0c\u4ed6\u4eec\u5426\u8ba4\u8036\u7a23\u57fa\u7763\u662f\u6210\u4e86\u8089\u8eab\u6765\u7684\uff1b\u8fd9\u5c31\u662f\u90a3\u9a97\u4eba\u7684\u548c\u654c\u57fa\u7763\u7684\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u4eec\u8981\u5c0f\u5fc3\uff0c\u4e0d\u8981\u62c6\u6bc1\u6211\u4eec\u5df2\u7ecf\u5b8c\u6210\u7684\u5de5\u4f5c\uff0c\u5374\u8981\u5f97\u7740\u7f8e\u6ee1\u7684\u8d4f\u8d50\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u51e1\u8d8a\u8fc7\u57fa\u7763\u7684\u6559\u8bad\u53c8\u4e0d\u6301\u5b88\u7684\uff0c\u5c31\u6ca1\u6709\u3000\u795e\uff1b\u6301\u5b88\u8fd9\u6559\u8bad\u7684\uff0c\u5c31\u6709\u7236\u548c\u5b50\u4e86\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5982\u679c\u6709\u4eba\u5230\u4f60\u4eec\u90a3\u91cc\uff0c\u4e0d\u4f20\u8fd9\u6559\u8bad\uff0c\u4f60\u4eec\u5c31\u4e0d\u8981\u63a5\u5f85\u4ed6\u5230\u5bb6\u91cc\uff0c\u4e5f\u4e0d\u8981\u95ee\u5019\u4ed6\uff1b\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56e0\u4e3a\u95ee\u5019\u4ed6\u7684\uff0c\u5c31\u5728\u4ed6\u7684\u6076\u4e8b\u4e0a\u6709\u5206\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u95ee\u5019\u7684\u8bdd\u6211\u8fd8\u6709\u8bb8\u591a\u8bdd\u8981\u5199\u7ed9\u4f60\u4eec\uff0c\u53ef\u662f\u6211\u4e0d\u60f3\u501f\u7528\u7eb8\u58a8\uff0c\u53ea\u662f\u76fc\u671b\u5230\u4f60\u4eec\u90a3\u91cc\u5f53\u9762\u8c08\u8c08\uff0c\u597d\u8ba9\u6211\u4eec\u7684\u559c\u4e50\u5145\u8db3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u90a3\u8499\u62e3\u9009\u7684\u59ca\u59b9\u7684\u513f\u5973\u90fd\u95ee\u5019\u4f60\u3002\n"
                }
            }
        }
    }
}