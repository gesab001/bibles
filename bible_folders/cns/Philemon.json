{
    "version": "cns",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\u4e3a\u57fa\u7763\u8036\u7a23\u88ab\u56da\u7981\u7684\u4fdd\u7f57\uff0c\u548c\u63d0\u6469\u592a\u5f1f\u5144\uff0c\u5199\u4fe1\u7ed9\u6211\u4eec\u6240\u7231\u7684\uff0c\u53c8\u4e00\u540c\u4f5c\u5de5\u7684\u8153\u5229\u95e8\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u548c\u4e9a\u8153\u4e9a\u59ca\u59b9\uff0c\u5e76\u6211\u4eec\u7684\u6218\u53cb\u4e9a\u57fa\u5e03\uff0c\u4ee5\u53ca\u5728\u4f60\u5bb6\u91cc\u7684\u6559\u4f1a\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u613f\u6069\u60e0\u5e73\u5b89\u4ece\u6211\u4eec\u7684\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u4e34\u5230\u4f60\u4eec\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u79f0\u8d5e\u8153\u5229\u95e8\u6211\u542c\u89c1\u4f60\u5bf9\u4e3b\u8036\u7a23\u548c\u4f17\u5723\u5f92\u6709\u7231\u5fc3\u548c\u4fe1\u5fc3\uff0c\u6211\u6bcf\u9022\u7977\u544a\u63d0\u5230\u4f60\u7684\u65f6\u5019\uff0c\u5c31\u5e38\u5e38\u611f\u8c22\u6211\u7684\u3000\u795e\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": " \n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u613f\u4f60\u4e0e\u4f17\u4eba\u5206\u4eab\u4f60\u7684\u4fe1\u5fc3\u7684\u65f6\u5019\uff0c\u4f1a\u4ea7\u751f\u529f\u6548\uff0c\u4f7f\u6211\u4eec\u53ef\u4ee5\u77e5\u9053\u5728\u6211\u4eec\u4e2d\u95f4\u7684\u4e00\u5207\u5584\u4e8b\uff0c\u90fd\u662f\u4e3a\u57fa\u7763\u4f5c\u7684\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5f1f\u5144\u554a\uff0c\u4f17\u5723\u5f92\u7684\u5fc3\u65e2\u7136\u4ece\u4f60\u5f97\u5230\u8212\u7545\uff0c\u6211\u4e5f\u56e0\u4f60\u7684\u7231\u5fc3\uff0c\u5f97\u5230\u66f4\u5927\u7684\u559c\u4e50\u548c\u5b89\u6170\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4e3a\u6b27\u5c3c\u897f\u6155\u8bf7\u6c42\u6211\u5728\u57fa\u7763\u91cc\u867d\u7136\u53ef\u4ee5\u653e\u80c6\u5429\u5490\u4f60\u4f5c\u5e94\u4f5c\u7684\u4e8b\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7136\u800c\u50cf\u6211\u8fd9\u4e0a\u4e86\u5e74\u7eaa\u7684\u4fdd\u7f57\uff0c\u73b0\u5728\u53c8\u662f\u4e3a\u57fa\u7763\u8036\u7a23\u88ab\u56da\u7981\u7684\uff0c\u5b81\u613f\u51ed\u7740\u7231\u5fc3\u8bf7\u6c42\u4f60\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5c31\u662f\u4e3a\u6211\u5728\u56da\u7981\u65f6\u6240\u751f\u7684\u513f\u5b50\u6b27\u5c3c\u897f\u6155\u6c42\u4f60\uff1b\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6\u4ece\u524d\u5bf9\u4f60\u6ca1\u6709\u4ec0\u4e48\u597d\u5904\uff0c\u4f46\u73b0\u5728\u5bf9\u4f60\u6211\u90fd\u6709\u597d\u5904\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u73b0\u5728\u6253\u53d1\u4ed6\u4eb2\u81ea\u56de\u5230\u4f60\u90a3\u91cc\u53bb\uff1b\u4ed6\u662f\u6211\u6240\u5fc3\u7231\u7684\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u672c\u6765\u60f3\u628a\u4ed6\u7559\u5728\u6211\u8fd9\u91cc\uff0c\u4f7f\u4ed6\u5728\u6211\u4e3a\u798f\u97f3\u88ab\u56da\u7981\u65f6\uff0c\u53ef\u4ee5\u66ff\u4f60\u670d\u4e8b\u6211\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f46\u8fd8\u6ca1\u6709\u5f97\u5230\u4f60\u7684\u540c\u610f\uff0c\u6211\u5c31\u4e0d\u613f\u610f\u8fd9\u6837\u4f5c\uff0c\u597d\u53eb\u4f60\u7684\u5584\u884c\u4e0d\u662f\u51fa\u4e8e\u52c9\u5f3a\uff0c\u800c\u662f\u51fa\u4e8e\u7518\u5fc3\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4e5f\u8bb8\u4ed6\u6682\u65f6\u79bb\u5f00\u4f60\uff0c\u6b63\u662f\u4e3a\u4e86\u4f7f\u4f60\u6c38\u8fdc\u5f97\u7740\u4ed6\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4e0d\u518d\u662f\u5974\u4ec6\uff0c\u800c\u662f\u9ad8\u8fc7\u5974\u4ec6\uff0c\u662f\u4eb2\u7231\u7684\u5f1f\u5144\u3002\u5bf9\u6211\u56fa\u7136\u662f\u8fd9\u6837\uff0c\u5bf9\u4f60\u6765\u8bf4\uff0c\u4e0d\u8bba\u6309\u8089\u8eab\u6216\u5728\u4e3b\u5185\u7684\u5173\u7cfb\uff0c\u66f4\u662f\u8fd9\u6837\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u6240\u4ee5\uff0c\u4f60\u8981\u662f\u628a\u6211\u770b\u4f5c\u540c\u4f34\uff0c\u5c31\u63a5\u7eb3\u4ed6\u597d\u50cf\u63a5\u7eb3\u6211\u4e00\u6837\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5982\u679c\u4ed6\u4f7f\u4f60\u53d7\u4e86\u635f\u5931\uff0c\u6216\u6b20\u4f60\u4ec0\u4e48\uff0c\u90fd\u8bb0\u5728\u6211\u7684\u5e10\u4e0a\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u201c\u6211\u5fc5\u507f\u8fd8\u201d\uff0c\u8fd9\u662f\u6211\u4fdd\u7f57\u4eb2\u624b\u5199\u7684\u3002\u7528\u4e0d\u7740\u6211\u8bf4\uff0c\u751a\u81f3\u4f60\u7684\u751f\u547d\uff0c\u4f60\u4e5f\u662f\u6b20\u6211\u7684\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u6240\u4ee5\u5f1f\u5144\u554a\uff01\u8ba9\u6211\u5728\u4e3b\u91cc\u5f97\u5230\u4f60\u7684\u5e2e\u52a9\uff0c\u4f7f\u6211\u7684\u5fc3\u5728\u57fa\u7763\u91cc\u5f97\u7740\u7545\u5feb\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211\u6df1\u4fe1\u4f60\u4f1a\u542c\u4ece\uff0c\u4e5f\u77e5\u9053\u4f60\u6240\u4f5c\u7684\u5fc5\u8d85\u8fc7\u6211\u6240\u8bf4\u7684\uff0c\u56e0\u6b64\u624d\u5199\u4fe1\u7ed9\u4f60\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u540c\u65f6\uff0c\u8fd8\u8bf7\u4f60\u4e3a\u6211\u9884\u5907\u4f4f\u7684\u5730\u65b9\uff0c\u56e0\u4e3a\u6211\u76fc\u671b\u501f\u7740\u4f60\u4eec\u7684\u7977\u544a\uff0c\u53ef\u4ee5\u83b7\u5f97\u91ca\u653e\u5230\u4f60\u4eec\u90a3\u91cc\u53bb\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u95ee\u5019\u7684\u8bdd\u4e3a\u57fa\u7763\u8036\u7a23\u7684\u7f18\u6545\u548c\u6211\u4e00\u540c\u5750\u76d1\u7684\u4ee5\u5df4\u5f17\uff0c\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u4ee5\u53ca\u6211\u7684\u540c\u5de5\u9a6c\u53ef\u3001\u4e9a\u91cc\u8fbe\u53e4\u3001\u5e95\u9a6c\u3001\u8def\u52a0\u90fd\u95ee\u5019\u4f60\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u613f\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u6069\u60e0\u5e38\u4e0e\u4f60\u4eec\u540c\u5728\u3002\uff08\u201c\u4e0e\u4f60\u4eec\u540c\u5728\u201d\u539f\u6587\u4f5c\u201c\u4e0e\u4f60\u4eec\u7684\u5fc3\u7075\u540c\u5728\u201d\uff1b\u597d\u4e9b\u6284\u672c\u5728\u8fd9\u4e66\u672b\u6709\u201c\u963f\u4eec\u201d\u4e00\u8bcd\u3002\uff09\n"
                }
            }
        }
    }
}