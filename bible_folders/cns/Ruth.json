{
    "version": "cns",
    "book_name": "Ruth",
    "book_nr": "8",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4ee5\u5229\u7c73\u52d2\u4e0e\u59bb\u513f\u8fc1\u5c45\u6469\u62bc\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8fd9\u4e2a\u4eba\u540d\u53eb\u4ee5\u5229\u7c73\u52d2\uff0c\u59bb\u5b50\u540d\u53eb\u62ff\u4fc4\u7c73\uff0c\u4e24\u4e2a\u513f\u5b50\u53eb\u739b\u4f26\u548c\u57fa\u8fde\uff0c\u90fd\u662f\u72b9\u5927\u4f2f\u5229\u6052\u7684\u4ee5\u6cd5\u4ed6\u4eba\u3002\u4ed6\u4eec\u5230\u4e86\u6469\u62bc\u5730\uff0c\u5c31\u5728\u90a3\u91cc\u4f4f\u4e0b\u6765\u4e86\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u540e\u6765\u62ff\u4fc4\u7c73\u7684\u4e08\u592b\u4ee5\u5229\u7c73\u52d2\u53bb\u4e16\u4e86\uff0c\u7559\u4e0b\u5979\u548c\u4e24\u4e2a\u513f\u5b50\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u4eec\u5a36\u4e86\u6469\u62bc\u5973\u5b50\u4f5c\u59bb\u5b50\uff0c\u4e00\u4e2a\u540d\u53eb\u4fc4\u8033\u5df4\uff0c\u4e00\u4e2a\u540d\u53eb\u8def\u5f97\u3002\u4ed6\u4eec\u5728\u90a3\u91cc\u4f4f\u4e86\u7ea6\u6709\u5341\u5e74\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u540e\u6765\uff0c\u739b\u4f26\u548c\u57fa\u8fde\u4e8c\u4eba\u4e5f\u6b7b\u4e86\uff0c\u7559\u4e0b\u62ff\u4fc4\u7c73\uff0c\u65e2\u6ca1\u6709\u4e86\u4e24\u4e2a\u513f\u5b50\uff0c\u53c8\u6ca1\u6709\u4e86\u4e08\u592b\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u62ff\u4fc4\u7c73\u4e0e\u8def\u5f97\u540c\u8fd4\u4f2f\u5229\u6052\u62ff\u4fc4\u7c73\u8fd8\u5728\u6469\u62bc\u5730\u7684\u65f6\u5019\uff0c\u56e0\u4e3a\u542c\u8bf4\u8036\u548c\u534e\u7737\u987e\u4ed6\u7684\u5b50\u6c11\uff0c\u8d50\u7cae\u98df\u7ed9\u4ed6\u4eec\uff0c\u5c31\u5e26\u7740\u4e24\u4e2a\u5ab3\u5987\u52a8\u8eab\u4ece\u6469\u62bc\u5730\u56de\u6765\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5979\u79bb\u5f00\u5c45\u4f4f\u7684\u5730\u65b9\uff0c\u4e24\u4e2a\u5ab3\u5987\u4e5f\u968f\u5979\u51fa\u53d1\uff0c\u8981\u56de\u5230\u72b9\u5927\u5730\u53bb\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u62ff\u4fc4\u7c73\u5bf9\u4e24\u4e2a\u5ab3\u5987\u8bf4\uff1a\u201c\u53bb\u5427\uff0c\u4f60\u4eec\u8fd8\u662f\u5404\u81ea\u56de\u5a18\u5bb6\u5427\u3002\u613f\u8036\u548c\u534e\u6069\u5f85\u4f60\u4eec\uff0c\u50cf\u4f60\u4eec\u5584\u5f85\u5df2\u6b7b\u7684\u4eba\u548c\u6211\u4e00\u6837\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u613f\u8036\u548c\u534e\u4f7f\u4f60\u4eec\u5404\u4eba\u6709\u673a\u4f1a\u518d\u5ac1\uff08\u201c\u6709\u673a\u4f1a\u518d\u5ac1\u201d\u539f\u6587\u4f5c\u201c\u5728\u4e08\u592b\u5bb6\u91cc\u201d\uff09\uff0c\u627e\u5230\u5f52\u5bbf\u3002\u201d\u8ddf\u7740\u4eb2\u543b\u5979\u4eec\uff0c\u5979\u4eec\u5c31\u53d1\u58f0\u5927\u54ed\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8bf4\uff1a\u201c\u4e0d\uff0c\u6211\u4eec\u4e00\u5b9a\u8981\u8ddf\u4f60\u4e00\u8d77\u56de\u5230\u4f60\u540c\u80de\u90a3\u91cc\u53bb\u3002\u201d\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u62ff\u4fc4\u7c73\u8bf4\uff1a\u201c\u6211\u5973\u513f\u554a\uff0c\u4f60\u4eec\u56de\u53bb\u5427\uff0c\u4e3a\u4ec0\u4e48\u8981\u8ddf\u6211\u53bb\u5462\uff1f\u6211\u809a\u5b50\u91cc\u8fd8\u6709\u513f\u5b50\u4f5c\u4f60\u4eec\u7684\u4e08\u592b\u5417\uff1f\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u8d70\u5427\uff0c\u6211\u5973\u513f\u554a\uff0c\u4f60\u4eec\u56de\u53bb\u5427\uff0c\u56e0\u4e3a\u6211\u592a\u8001\uff0c\u4e0d\u80fd\u5ac1\u4eba\u4e86\uff0c\u5c31\u7b97\u6211\u8bf4\u6211\u8fd8\u6709\u5e0c\u671b\u4eca\u5929\u665a\u4e0a\u6709\u4e2a\u4e08\u592b\uff0c\u53c8\u751f\u513f\u5b50\uff0c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u4eec\u53ef\u4ee5\u7b49\u5230\u4ed6\u4eec\u957f\u5927\u5417\uff1f\u4f60\u4eec\u53ef\u4ee5\u4e3a\u4ed6\u4eec\u5b88\u8282\u800c\u4e0d\u5ac1\u4eba\u5417\uff1f\u4e0d\u884c\uff0c\u6211\u5973\u513f\u554a\uff0c\u6211\u4e3a\u4f60\u4eec\u975e\u5e38\u96be\u8fc7\uff0c\u56e0\u4e3a\u8036\u548c\u534e\u4f38\u624b\u5bf9\u4ed8\u6211\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5979\u4eec\u53c8\u653e\u58f0\u5927\u54ed\u3002\u4fc4\u8033\u5df4\u5c31\u548c\u5a46\u5a46\u543b\u522b\uff0c\u8def\u5f97\u5374\u820d\u4e0d\u5f97\u79bb\u5f00\u62ff\u4fc4\u7c73\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u62ff\u4fc4\u7c73\u8bf4\uff1a\u201c\u4f60\u770b\uff0c\u4f60\u5ac2\u5ac2\u5df2\u7ecf\u56de\u5230\u5979\u540c\u80de\u548c\u5979\u7684\u795e\u90a3\u91cc\u53bb\u4e86\u3002\u4f60\u4e5f\u8ddf\u7740\u5ac2\u5ac2\u56de\u53bb\u5427\u3002\u201d\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u8def\u5f97\u8bf4\uff1a\u201c\u8bf7\u4e0d\u8981\u903c\u6211\u79bb\u5f00\u4f60\u56de\u53bb\uff0c\u4e0d\u8ddf\u4ece\u4f60\u3002\u4f60\u5230\u54ea\u91cc\u53bb\uff0c\u6211\u4e5f\u8981\u5230\u54ea\u91cc\u53bb\uff1b\u4f60\u5728\u54ea\u91cc\u8fc7\u591c\uff0c\u6211\u4e5f\u8981\u5728\u54ea\u91cc\u8fc7\u591c\uff1b\u4f60\u7684\u540c\u80de\u5c31\u662f\u6211\u7684\u540c\u80de\uff0c\u4f60\u7684\u3000\u795e\u5c31\u662f\u6211\u7684\u3000\u795e\uff1b\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60\u6b7b\u5728\u54ea\u91cc\uff0c\u6211\u4e5f\u8981\u6b7b\u5728\u54ea\u91cc\uff0c\u846c\u5728\u54ea\u91cc\u3002\u9664\u975e\u6b7b\u4ea1\u628a\u4f60\u6211\u5206\u79bb\uff1b\u5982\u679c\u6211\u79bb\u5f00\u4f60\uff0c\u613f\u8036\u548c\u534e\u52a0\u500d\u60e9\u7f5a\u6211\u3002\u201d\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u62ff\u4fc4\u7c73\u770b\u8def\u5f97\u575a\u51b3\u8981\u8ddf\u5979\u8d70\uff0c\u5c31\u4e0d\u518d\u591a\u8d39\u5507\u820c\u4e86\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u5979\u4eec\u53cc\u53cc\u4e0a\u8def\uff0c\u6765\u5230\u4f2f\u5229\u6052\u3002\u5979\u4eec\u5230\u8fbe\u7684\u65f6\u5019\uff0c\u5168\u57ce\u90fd\u56e0\u5979\u4eec\u7684\u7f18\u6545\u9a9a\u52a8\u8d77\u6765\u3002\u5987\u5973\u4eec\u8bf4\uff1a\u201c\u8fd9\u662f\u62ff\u4fc4\u7c73\u5417\uff1f\u201d\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u62ff\u4fc4\u7c73\u5bf9\u5979\u4eec\u8bf4\uff1a\u201c\u4e0d\u8981\u53eb\u6211\u62ff\u4fc4\u7c73\uff0c\u53eb\u6211\u739b\u62c9\u5427\uff0c\u56e0\u4e3a\u5168\u80fd\u8005\u4f7f\u6211\u5403\u5c3d\u4e86\u82e6\u5934\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211\u6ee1\u6ee1\u5730\u51fa\u53bb\uff0c\u8036\u548c\u534e\u5374\u4f7f\u6211\u7a7a\u7a7a\u5730\u56de\u6765\uff0c\u4f60\u4eec\u4e3a\u4ec0\u4e48\u8fd8\u53eb\u6211\u62ff\u4fc4\u7c73\u5462\uff1f\u8036\u548c\u534e\u6298\u78e8\u6211\uff0c\u5168\u80fd\u8005\u52a0\u5bb3\u4e8e\u6211\u3002\u201d\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u62ff\u4fc4\u7c73\u5c31\u8fd9\u6837\u56de\u6765\u4e86\u3002\u5979\u5ab3\u5987\u6469\u62bc\u5973\u5b50\u8def\u5f97\u968f\u7740\u5979\u4ece\u6469\u62bc\u5730\u56de\u6765\u3002\u5979\u4eec\u5230\u8fbe\u4f2f\u5229\u6052\uff0c\u6b63\u662f\u5f00\u59cb\u6536\u5272\u5927\u9ea6\u7684\u65f6\u5019\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8def\u5f97\u5728\u6ce2\u963f\u65af\u7684\u7530\u91cc\u6361\u62fe\u9ea6\u7a57\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6469\u62bc\u5973\u5b50\u8def\u5f97\u5bf9\u62ff\u4fc4\u7c73\u8bf4\uff1a\u201c\u8ba9\u6211\u5230\u7530\u91cc\u53bb\uff0c\u6211\u5728\u8c01\u7684\u773c\u524d\u8499\u6069\uff0c\u6211\u5c31\u8ddf\u5728\u8c01\u7684\u80cc\u540e\u6361\u9ea6\u7a57\u3002\u201d\u62ff\u4fc4\u7c73\u8bf4\uff1a\u201c\u5973\u513f\u554a\uff0c\u4f60\u5c3d\u7ba1\u53bb\u3002\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u8def\u5f97\u5c31\u53bb\u4e86\uff0c\u6765\u5230\u7530\u91cc\uff0c\u5728\u6536\u5272\u7684\u4eba\u80cc\u540e\u6361\u9ea6\u7a57\u3002\u5979\u6070\u5de7\u6765\u5230\u4ee5\u5229\u7c73\u52d2\u540c\u65cf\u7684\u4eba\u6ce2\u963f\u65af\u90a3\u5757\u7530\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6ce2\u963f\u65af\u521a\u597d\u4ece\u4f2f\u5229\u6052\u6765\uff0c\u5bf9\u6536\u5272\u7684\u4eba\u8bf4\uff1a\u201c\u613f\u8036\u548c\u534e\u4e0e\u4f60\u4eec\u540c\u5728\u3002\u201d\u4ed6\u4eec\u56de\u7b54\uff1a\u201c\u613f\u8036\u548c\u534e\u8d50\u798f\u4e0e\u4f60\u3002\u201d\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6ce2\u963f\u65af\u95ee\u90a3\u76d1\u7763\u6536\u5272\u7684\u4ec6\u4eba\uff1a\u201c\u90a3\u662f\u8c01\u5bb6\u7684\u59d1\u5a18\u5462\uff1f\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u76d1\u7763\u6536\u5272\u7684\u4ec6\u4eba\u56de\u7b54\uff1a\u201c\u5979\u662f\u8ddf\u62ff\u4fc4\u7c73\u4ece\u6469\u62bc\u5730\u56de\u6765\u7684\u6469\u62bc\u5973\u5b50\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5979\u8bf4\uff1a\u2018\u8bf7\u8ba9\u6211\u6361\u4e00\u70b9\u9ea6\u7a57\u5427\u3002\u8ba9\u6211\u8ddf\u5728\u6536\u5272\u7684\u4eba\u80cc\u540e\uff0c\u5728\u79be\u6346\u5806\u4e2d\u6361\u4e9b\u96f6\u788e\u7684\u7a57\u5b50\u3002\u2019\u4e8e\u662f\u5979\u6765\u4e86\u3002\u9664\u4e86\u5728\u623f\u5b50\u91cc\u4f11\u606f\u4e86\u4e00\u4f1a\u513f\u4e4b\u5916\uff0c\u4ece\u65e9\u6668\u5230\u73b0\u5728\u4e00\u76f4\u90fd\u7559\u5728\u8fd9\u91cc\u3002\u201d\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6ce2\u963f\u65af\u539a\u5f85\u8def\u5f97\u6ce2\u963f\u65af\u5bf9\u8def\u5f97\u8bf4\uff1a\u201c\u5973\u513f\u554a\uff0c\u542c\u6211\u8bf4\uff0c\u4e0d\u8981\u5230\u522b\u4eba\u7530\u91cc\u53bb\u6361\u9ea6\u7a57\uff0c\u4e5f\u4e0d\u8981\u79bb\u5f00\u8fd9\u91cc\uff0c\u8981\u5e38\u5e38\u4e0e\u6211\u7684\u5973\u4f63\u4eba\u5728\u4e00\u8d77\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u770b\u4ed6\u4eec\u5728\u54ea\u5757\u7530\u91cc\u6536\u5272\uff0c\u5c31\u8ddf\u7740\u5973\u4f63\u4eba\u53bb\u3002\u6211\u4e0d\u662f\u5df2\u7ecf\u5429\u5490\u4ec6\u4eba\u4e0d\u53ef\u6b3a\u8d1f\u4f60\u5417\uff1f\u5047\u5982\u4f60\u6e34\u4e86\uff0c\u5c31\u5230\u6c34\u7f38\u90a3\u91cc\u53bb\uff0c\u559d\u4ec6\u4eba\u6253\u6765\u7684\u6c34\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8def\u5f97\u5c31\u4fef\u4f0f\u5728\u5730\u53e9\u62dc\uff0c\u5bf9\u4ed6\u8bf4\uff1a\u201c\u6211\u662f\u4e2a\u5916\u65cf\u4eba\uff0c\u4e3a\u4ec0\u4e48\u4f1a\u5728\u4f60\u773c\u524d\u8499\u6069\uff0c\u8499\u4f60\u5173\u7167\u5462\uff1f\u201d\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6ce2\u963f\u65af\u56de\u7b54\u5979\u8bf4\uff1a\u201c\u81ea\u4ece\u4f60\u4e08\u592b\u53bb\u4e16\u4ee5\u540e\uff0c\u4f60\u5bf9\u5a46\u5a46\u6240\u884c\u7684\uff0c\u548c\u4f60\u600e\u6837\u79bb\u5f00\u7236\u6bcd\u4e0e\u51fa\u751f\u4e4b\u5730\uff0c\u6765\u5230\u7d20\u6765\u4e0d\u8ba4\u8bc6\u7684\u4eba\u4e2d\u95f4\uff0c\u8fd9\u4e00\u5207\u6211\u90fd\u77e5\u9053\u5f97\u5f88\u6e05\u695a\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u613f\u8036\u548c\u534e\u7167\u4f60\u6240\u4f5c\u7684\u62a5\u7b54\u4f60\uff1b\u4f60\u6765\u6295\u9760\u5728\u8036\u548c\u534e\u4ee5\u8272\u5217\u3000\u795e\u7684\u7fc5\u8180\u4e0b\uff0c\u613f\u4ed6\u5145\u5145\u8db3\u8db3\u916c\u62a5\u4f60\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8def\u5f97\u8bf4\uff1a\u201c\u4e3b\u4eba\u554a\uff0c\u4f46\u613f\u6211\u5728\u4f60\u773c\u524d\u8499\u6069\uff0c\u867d\u7136\u6211\u8fde\u4f60\u7684\u5a62\u5973\u90fd\u4e0d\u662f\uff0c\u4f60\u8fd8\u662f\u5b89\u6170\u6211\uff0c\u5bf9\u6211\u8fd9\u4e48\u597d\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5230\u4e86\u5403\u996d\u7684\u65f6\u5019\uff0c\u6ce2\u963f\u65af\u5bf9\u8def\u5f97\u8bf4\uff1a\u201c\u4f60\u8fc7\u6765\uff0c\u5403\u4e00\u70b9\u997c\u5427\uff0c\u62ff\u997c\u5757\u8638\u5728\u918b\u91cc\u3002\u201d\u8def\u5f97\u5c31\u5728\u6536\u5272\u7684\u4eba\u65c1\u8fb9\u5750\u4e0b\u6765\u3002\u6ce2\u963f\u65af\u628a\u4e00\u4e9b\u70d8\u597d\u7684\u9ea6\u7a57\u9012\u7ed9\u5979\uff0c\u5979\u5403\u9971\u4e86\uff0c\u8fd8\u6709\u5269\u4e0b\u7684\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5979\u518d\u8d77\u6765\u53bb\u6361\u9ea6\u7a57\u7684\u65f6\u5019\uff0c\u6ce2\u963f\u65af\u5429\u5490\u4ec6\u4eba\u8bf4\uff1a\u201c\u5c31\u7b97\u5979\u5728\u79be\u6346\u5806\u4e2d\u6361\u9ea6\u7a57\uff0c\u4e5f\u4e0d\u53ef\u4ee5\u8fb1\u9a82\u5979\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u751a\u81f3\u8981\u6545\u610f\u4e3a\u5979\u4ece\u79be\u6346\u4e2d\u62bd\u4e9b\u51fa\u6765\uff0c\u7559\u7ed9\u5979\u53bb\u6361\uff0c\u5343\u4e07\u4e0d\u53ef\u8d23\u5907\u5979\u3002\u201d\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8def\u5f97\u628a\u4e00\u5207\u7ecf\u8fc7\u544a\u8bc9\u62ff\u4fc4\u7c73\u8def\u5f97\u5c31\u8fd9\u6837\u5728\u7530\u91cc\u6361\u9ea6\u7a57\uff0c\u76f4\u5230\u508d\u665a\u3002\u5979\u628a\u6361\u6765\u7684\u9ea6\u7a57\u6253\u4e86\uff0c\u7ea6\u6709\u4e8c\u5341\u4e8c\u516c\u5347\u5927\u9ea6\uff0c\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u7136\u540e\u5e26\u56de\u57ce\u91cc\u53bb\u3002\u8def\u5f97\u628a\u6361\u6765\u7684\u9ea6\u5b50\u62ff\u7ed9\u5a46\u5a46\u770b\uff0c\u628a\u5403\u5269\u7684\u7ed9\u4e86\u5a46\u5a46\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u5a46\u5a46\u95ee\u5979\uff1a\u201c\u4f60\u4eca\u5929\u5728\u54ea\u91cc\u6361\u9ea6\u7a57\uff1f\u5728\u54ea\u91cc\u5de5\u4f5c\u5462\uff1f\u613f\u90a3\u5173\u7167\u4f60\u7684\u4eba\u8499\u798f\u3002\u201d\u8def\u5f97\u5c31\u544a\u8bc9\u5a46\u5a46\u5979\u5728\u8c01\u90a3\u91cc\u5de5\u4f5c\uff0c\u8bf4\uff1a\u201c\u6211\u4eca\u5929\u5728\u4e00\u4e2a\u540d\u53eb\u6ce2\u963f\u65af\u7684\u4eba\u90a3\u91cc\u5de5\u4f5c\u3002\u201d\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u62ff\u4fc4\u7c73\u5bf9\u5ab3\u5987\u8bf4\uff1a\u201c\u613f\u4e0d\u65ad\u65bd\u6148\u7231\u7ed9\u6d3b\u4eba\u548c\u6b7b\u4eba\u7684\u8036\u548c\u534e\u8d50\u798f\u7ed9\u4ed6\u3002\u201d\u62ff\u4fc4\u7c73\u53c8\u5bf9\u5979\u8bf4\uff1a\u201c\u8fd9\u4e2a\u4eba\u662f\u6211\u4eec\u7684\u4eb2\u4eba\uff0c\u6709\u4e70\u8d4e\u6743\u7684\u4e00\u4f4d\u8fd1\u4eb2\u3002\u201d\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6469\u62bc\u5973\u5b50\u8def\u5f97\u8bf4\uff1a\u201c\u4ed6\u8fd8\u544a\u8bc9\u6211\uff1a\u2018\u7d27\u8ddf\u7740\u6211\u7684\u4ec6\u4eba\u6361\u9ea6\u7a57\uff0c\u76f4\u5230\u5168\u90e8\u6536\u5272\u5b8c\u6bd5\u3002\u2019\u201d\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u62ff\u4fc4\u7c73\u5bf9\u5ab3\u5987\u8def\u5f97\u8bf4\uff1a\u201c\u6211\u5973\u513f\u554a\uff0c\u8fd9\u6837\u624d\u597d\uff0c\u8981\u4e0e\u4ed6\u7684\u5973\u4f63\u4eba\u4e00\u8d77\u53bb\uff0c\u514d\u5f97\u5728\u522b\u4eba\u7684\u7530\u91cc\u906d\u53d7\u654c\u89c6\u3002\u201d\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u5979\u5c31\u7d27\u8ddf\u7740\u6ce2\u963f\u65af\u7684\u5973\u4f63\u4eba\u6361\u9ea6\u7a57\uff0c\u76f4\u5230\u5927\u9ea6\u548c\u5c0f\u9ea6\u90fd\u6536\u5272\u597d\u4e86\u3002\u8def\u5f97\u4e00\u76f4\u4e0e\u5a46\u5a46\u4f4f\u5728\u4e00\u8d77\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u62ff\u4fc4\u7c73\u529d\u8def\u5f97\u6539\u5ac1\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4f60\u5e38\u5e38\u548c\u4ed6\u7684\u5973\u4f63\u4eba\u5728\u4e00\u8d77\u7684\u6ce2\u963f\u65af\u4e0d\u662f\u6211\u4eec\u7684\u4eb2\u621a\u5417\uff1f\u4ed6\u4eca\u5929\u665a\u4e0a\u8981\u5728\u79be\u573a\u4e0a\u7c38\u5927\u9ea6\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u6d17\u4e2a\u6fa1\uff0c\u62b9\u4e0a\u9999\u818f\uff0c\u7a7f\u4e0a\u6597\u7bf7\uff0c\u5c31\u4e0b\u5230\u79be\u573a\u53bb\u3002\u4e0d\u8fc7\uff0c\u90a3\u4e2a\u4eba\u8fd8\u6ca1\u6709\u5403\u559d\u5b8c\u6bd5\uff0c\u522b\u8ba9\u4ed6\u8ba4\u51fa\u4f60\u6765\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u7b49\u5230\u4ed6\u8eba\u4e0b\u6765\uff0c\u4f60\u8981\u5f04\u6e05\u695a\u4ed6\u6240\u8eba\u7684\u5730\u65b9\uff0c\u8fdb\u53bb\u6380\u9732\u4ed6\u7684\u811a\uff0c\u7136\u540e\u8eba\u4e0b\u53bb\u3002\u4ed6\u5c31\u4f1a\u544a\u8bc9\u4f60\u8be5\u4f5c\u4ec0\u4e48\u3002\u201d\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8def\u5f97\u5bf9\u5979\u8bf4\uff1a\u201c\u4f60\u5429\u5490\u7684\uff0c\u6211\u5c31\u53bb\u4f5c\u3002\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u8def\u5f97\u72ec\u5f80\u79be\u573a\u591c\u8bbf\u6ce2\u963f\u65af\u4e8e\u662f\uff0c\u5979\u4e0b\u5230\u79be\u573a\u53bb\uff0c\u7167\u7740\u5a46\u5a46\u5429\u5490\u7684\u4e00\u5207\u53bb\u4f5c\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6ce2\u963f\u65af\u5403\u559d\u5b8c\u4e86\uff0c\u5fc3\u91cc\u8212\u7545\uff0c\u5c31\u53bb\u8eba\u5728\u9ea6\u5806\u7684\u65c1\u8fb9\u3002\u8def\u5f97\u6084\u6084\u5730\u6765\uff0c\u6380\u9732\u4ed6\u7684\u811a\uff0c\u8eba\u5367\u4e0b\u53bb\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5230\u4e86\u591c\u534a\uff0c\u6ce2\u963f\u65af\u60ca\u9192\u8fc7\u6765\uff0c\u4e00\u7ffb\u8eab\u5c31\u770b\u5230\u4e00\u4e2a\u5973\u5b50\u8eba\u5728\u4ed6\u7684\u811a\u8fb9\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5c31\u8bf4\uff1a\u201c\u4f60\u662f\u8c01\uff1f\u201d\u5979\u56de\u7b54\uff1a\u201c\u6211\u662f\u4f60\u7684\u5a62\u5973\u8def\u5f97\uff0c\u8bf7\u7528\u4f60\u7684\u8863\u895f\u906e\u76d6\u4f60\u7684\u5a62\u5973\uff0c\u56e0\u4e3a\u4f60\u662f\u6211\u7684\u6709\u4e70\u8d4e\u6743\u7684\u8fd1\u4eb2\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6ce2\u963f\u65af\u8bf4\uff1a\u201c\u6211\u5973\u513f\uff0c\u613f\u4f60\u8499\u8036\u548c\u534e\u8d50\u798f\u3002\u4f60\u672b\u540e\u8868\u73b0\u7684\u7231\u5fc3\u6bd4\u8d77\u521d\u66f4\u5927\uff0c\u56e0\u4e3a\u5e74\u8f7b\u4eba\u65e0\u8bba\u8d2b\u5bcc\uff0c\u4f60\u90fd\u6ca1\u6709\u8ddf\u4ece\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6211\u5973\u513f\uff0c\u73b0\u5728\u4f60\u4e0d\u8981\u6015\uff0c\u4f60\u6240\u8bf4\u7684\uff0c\u6211\u4e00\u5b9a\u53bb\u4f5c\u3002\u672c\u57ce\u7684\u4eba\u90fd\u77e5\u9053\u4f60\u662f\u4e2a\u8d24\u6167\u7684\u5973\u4eba\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4e0d\u9519\uff0c\u6211\u662f\u4f60\u90a3\u6709\u4e70\u8d4e\u6743\u7684\u8fd1\u4eb2\uff0c\u53ef\u60dc\u8fd8\u6709\u4e00\u4e2a\u6709\u4e70\u8d4e\u6743\u7684\u8fd1\u4eb2\u6bd4\u6211\u66f4\u4eb2\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4eca\u665a\u4f60\u5c31\u5728\u8fd9\u91cc\u8fc7\u591c\uff0c\u660e\u65e9\u5982\u679c\u4ed6\u80af\u5c3d\u4e70\u8d4e\u4f60\u7684\u672c\u5206\uff0c\u597d\uff0c\u5c31\u7531\u4ed6\u6765\u5c65\u884c\uff1b\u5047\u5982\u4ed6\u4e0d\u613f\u610f\u8fd9\u6837\u4f5c\uff0c\u6211\u6307\u7740\u6c38\u6d3b\u7684\u8036\u548c\u534e\u8d77\u8a93\uff0c\u6211\u4e00\u5b9a\u5c3d\u4e70\u8d4e\u4f60\u7684\u672c\u5206\u3002\u4f60\u53ea\u7ba1\u8eba\u5230\u5929\u4eae\u5427\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u8def\u5f97\u8eba\u5728\u6ce2\u963f\u65af\u811a\u8fb9\u76f4\u5230\u65e9\u6668\u3002\u56e0\u4e3a\u6ce2\u963f\u65af\u8bf4\u8fc7\u4e0d\u53ef\u4ee5\u8ba9\u4eba\u77e5\u9053\u6709\u5973\u4eba\u6765\u8fc7\u79be\u573a\uff0c\u8def\u5f97\u5c31\u8d81\u5929\u8272\u672a\u660e\uff0c\u4eba\u4eec\u5f7c\u6b64\u65e0\u6cd5\u8fa8\u8ba4\u7684\u65f6\u5019\u5c31\u8d77\u6765\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4ed6\u53c8\u8bf4\uff1a\u201c\u62ff\u4f60\u6240\u62ab\u7684\u5916\u8863\u6765\uff0c\u628a\u5b83\u6253\u5f00\u3002\u201d\u5979\u6253\u5f00\u4e86\uff0c\u4ed6\u5c31\u91cf\u4e86\u516d\u7c38\u7b95\u5927\u9ea6\uff0c\u653e\u5728\u5979\u7684\u80a9\u4e0a\u3002\u8def\u5f97\u5c31\u8fdb\u57ce\u53bb\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u8def\u5f97\u5c06\u4e00\u5207\u4e8b\u60c5\u544a\u8bc9\u62ff\u4fc4\u7c73\u5230\u5979\u5a46\u5a46\u90a3\u91cc\uff0c\u5a46\u5a46\u5c31\u95ee\uff1a\u201c\u6211\u5973\u513f\uff0c\u600e\u6837\u4e86\uff1f\u201d\u8def\u5f97\u5c31\u628a\u90a3\u4eba\u5bf9\u5979\u6240\u4f5c\u7684\u4e00\u5207\u90fd\u544a\u8bc9\u4e86\u5a46\u5a46\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u53c8\u8bf4\uff1a\u201c\u4ed6\u7ed9\u6211\u8fd9\u516d\u7c38\u7b95\u5927\u9ea6\uff0c\u5bf9\u6211\u8bf4\uff1a\u2018\u4e0d\u8981\u7a7a\u624b\u53bb\u89c1\u4f60\u5a46\u5a46\u3002\u2019\u201d\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u62ff\u4fc4\u7c73\u8bf4\uff1a\u201c\u6211\u5973\u513f\uff0c\u4f60\u5c3d\u7ba1\u5b89\u9759\u7b49\u5019\uff0c\u76f4\u5230\u4f60\u77e5\u9053\u4e8b\u60c5\u600e\u6837\u4e86\u7ed3\uff0c\u56e0\u4e3a\u90a3\u4eba\u4eca\u5929\u4e0d\u628a\u4e8b\u60c5\u529e\u59a5\uff0c\u7edd\u4e0d\u6b47\u606f\u3002\u201d\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6ce2\u963f\u65af\u5a36\u8def\u5f97\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6ce2\u963f\u65af\u53c8\u9080\u8bf7\u57ce\u91cc\u7684\u5341\u4e2a\u957f\u8001\uff0c\u5bf9\u4ed6\u4eec\u8bf4\uff1a\u201c\u8bf7\u5750\u5728\u8fd9\u91cc\u3002\u201d\u4ed6\u4eec\u5c31\u5750\u4e0b\u6765\u4e86\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4ed6\u5bf9\u90a3\u4e2a\u6709\u4e70\u8d4e\u6743\u7684\u8fd1\u4eb2\u8bf4\uff1a\u201c\u4ece\u6469\u62bc\u5730\u56de\u6765\u7684\u62ff\u4fc4\u7c73\uff0c\u73b0\u5728\u8981\u5356\u6211\u4eec\u65cf\u5144\u4ee5\u5229\u7c73\u52d2\u7684\u90a3\u5757\u5730\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u60f3\u628a\u8fd9\u4e00\u4ef6\u4e8b\u544a\u8bc9\u4f60\uff0c\u8bf7\u4f60\u5728\u4f17\u6c11\u7684\u957f\u8001\uff0c\u8fd8\u6709\u5728\u5ea7\u7684\u5404\u4f4d\u9762\u524d\uff0c\u628a\u5b83\u4e70\u4e0b\u6765\u5427\u3002\u5982\u679c\u4f60\u80af\u8d4e\u5c31\u8d4e\uff0c\u5047\u5982\u4e0d\u80af\uff0c\u5c31\u8bf4\u51fa\u6765\uff0c\u8ba9\u6211\u77e5\u9053\uff1b\u56e0\u4e3a\u9664\u4e86\u4f60\u5f53\u8d4e\u5b83\u4ee5\u5916\uff0c\u6211\u662f\u7b2c\u4e00\u5019\u8865\u3002\u201d\u90a3\u4eba\u8bf4\uff1a\u201c\u6211\u80af\u8d4e\u3002\u201d\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6ce2\u963f\u65af\u8bf4\uff1a\u201c\u4f60\u4ece\u62ff\u4fc4\u7c73\u624b\u4e2d\u4e70\u4e0b\u90a3\u5757\u5730\u7684\u65f6\u5019\uff0c\u4f60\u4e5f\u5f97\u5a36\uff08\u201c\u5a36\u201d\u539f\u6587\u4f5c\u201c\u4e70\u201d\uff09\u5df2\u6b7b\u7684\u4eba\u7684\u59bb\u5b50\u6469\u62bc\u5973\u5b50\u8def\u5f97\uff0c\u4f7f\u5df2\u6b7b\u7684\u4eba\u7684\u540d\u5b57\u7559\u5728\u4ed6\u7684\u4ea7\u4e1a\u4e0a\u3002\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u90a3\u6709\u4e70\u8d4e\u6743\u7684\u8fd1\u4eb2\u8bf4\uff1a\u201c\u90a3\u6211\u5c31\u4e0d\u80fd\u8d4e\u5b83\u4e86\uff0c\u53ea\u6015\u635f\u5bb3\u4e86\u6211\u7684\u4ea7\u4e1a\u3002\u4f60\u53ef\u4ee5\u8d4e\u6211\u5f53\u8d4e\u7684\uff0c\u56e0\u4e3a\u6211\u6ca1\u6709\u529e\u6cd5\u8d4e\u4e86\u3002\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ece\u524d\u5728\u4ee5\u8272\u5217\uff0c\u65e0\u8bba\u4e70\u8d4e\u6216\u662f\u4ea4\u6613\uff0c\u51b3\u5b9a\u4ec0\u4e48\u4e8b\uff0c\u5f53\u4e8b\u4eba\u5c31\u8981\u8131\u978b\u7ed9\u5bf9\u65b9\u3002\u8fd9\u662f\u4ee5\u8272\u5217\u4e2d\u4f5c\u8bc1\u7684\u65b9\u5f0f\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
