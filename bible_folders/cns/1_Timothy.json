                "11": {
                    "verse_nr": 11,
                    "verse": "\u5973\u4eba\u5e94\u8be5\u5b89\u9759\u800c\u53c8\u5b8c\u5168\u987a\u670d\u5730\u5b66\u4e60\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u4e0d\u51c6\u5973\u4eba\u6559\u8bad\u7537\u4eba\uff0c\u8f96\u5236\u7537\u4eba\uff1b\u5973\u4eba\u603b\u8981\u5b89\u9759\u3002\n"
