                "13": {
                    "verse_nr": 13,
                    "verse": "\u56e0\u4e3a\u5148\u9020\u7684\u662f\u4e9a\u5f53\uff0c\u540e\u9020\u7684\u662f\u590f\u5a03\uff1b\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e0d\u662f\u4e9a\u5f53\u53d7\u4e86\u5f15\u8bf1\uff0c\u800c\u662f\u5973\u4eba\u53d7\u4e86\u5f15\u8bf1\uff0c\u9677\u5728\u8fc7\u72af\u91cc\u9762\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u7136\u800c\u5973\u4eba\u8981\u662f\u5e38\u5e38\u5b58\u7740\u4fe1\u5fc3\u3001\u7231\u5fc3\u3001\u5723\u6d01\u81ea\u5f8b\uff0c\u5728\u5979\u751f\u80b2\u7684\u4e8b\u4e0a\u5fc5\u5b9a\u5f97\u6551\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4f5c\u76d1\u7763\u7684\u8d44\u683c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6240\u4ee5\u4f5c\u76d1\u7763\u7684\uff0c\u5fc5\u987b\u65e0\u53ef\u6307\u6458\uff0c\u53ea\u4f5c\u4e00\u4e2a\u59bb\u5b50\u7684\u4e08\u592b\uff0c\u6709\u8282\u5236\uff0c\u81ea\u5f8b\uff0c\u5e84\u91cd\uff0c\u4e50\u610f\u63a5\u5f85\u5ba2\u65c5\uff0c\u5584\u4e8e\u6559\u5bfc\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e0d\u597d\u9152\uff0c\u4e0d\u6253\u4eba\uff0c\u53ea\u8981\u6e29\u548c\uff0c\u4e0e\u4eba\u65e0\u4e89\uff0c\u4e0d\u8d2a\u8d22\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u597d\u597d\u7ba1\u7406\u81ea\u5df1\u7684\u5bb6\uff0c\u4f7f\u513f\u5973\u51e1\u4e8b\u656c\u91cd\u987a\u670d\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\uff08\u4eba\u82e5\u4e0d\u77e5\u9053\u600e\u6837\u7ba1\u7406\u81ea\u5df1\u7684\u5bb6\uff0c\u600e\u80fd\u7167\u6599\u3000\u795e\u7684\u6559\u4f1a\u5462\uff1f\uff09\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u521d\u4fe1\u4e3b\u7684\u4e0d\u53ef\u4f5c\u76d1\u7763\uff0c\u6050\u6015\u4ed6\u9a84\u50b2\uff0c\u5c31\u843d\u5728\u9b54\u9b3c\u6240\u53d7\u7684\u5211\u7f5a\u91cc\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f5c\u76d1\u7763\u7684\u4e5f\u5fc5\u987b\u5728\u6559\u5916\u6709\u597d\u58f0\u8a89\uff0c\u514d\u5f97\u4ed6\u88ab\u4eba\u6bc1\u8c24\uff0c\u5c31\u843d\u5728\u9b54\u9b3c\u7684\u9677\u9631\u91cc\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f5c\u6267\u4e8b\u7684\u8d44\u683c\u7167\u6837\uff0c\u6267\u4e8b\u4e5f\u5fc5\u987b\u5e84\u91cd\uff0c\u4e0d\u4e00\u53e3\u4e24\u820c\uff0c\u4e0d\u9157\u9152\uff0c\u4e0d\u8d2a\u4e0d\u4e49\u4e4b\u8d22\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7528\u6e05\u6d01\u7684\u826f\u5fc3\u6301\u5b88\u4fe1\u4ef0\u7684\u5965\u79d8\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4ed6\u4eec\u4e5f\u5fc5\u987b\u5148\u53d7\u8003\u9a8c\uff0c\u82e5\u6ca1\u6709\u53ef\u8d23\u4e4b\u5904\uff0c\u7136\u540e\u624d\u8ba9\u4ed6\u4eec\u4f5c\u6267\u4e8b\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u7167\u6837\uff0c\u4ed6\u4eec\u7684\u59bb\u5b50\uff08\u201c\u4ed6\u4eec\u7684\u59bb\u5b50\u201d\u53ef\u89e3\u4f5c\u201c\u5973\u6267\u4e8b\u201d\uff09\u4e5f\u8981\u5e84\u91cd\uff0c\u4e0d\u8bf4\u8c17\u8a00\uff0c\u6709\u8282\u5236\uff0c\u51e1\u4e8b\u5fe0\u5fc3\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6267\u4e8b\u53ea\u53ef\u4ee5\u4f5c\u4e00\u4e2a\u59bb\u5b50\u7684\u4e08\u592b\uff0c\u5584\u4e8e\u7ba1\u7406\u513f\u5973\u548c\u81ea\u5df1\u7684\u5bb6\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u90a3\u5584\u4e8e\u4f5c\u6267\u4e8b\u7684\uff0c\u5c31\u4e3a\u81ea\u5df1\u5f97\u4e86\u597d\u7684\u4f4d\u5206\uff0c\u4e5f\u56e0\u7740\u76f8\u4fe1\u57fa\u7763\u8036\u7a23\u5f97\u5230\u5927\u5927\u7684\u80c6\u91cf\u3002\n"
