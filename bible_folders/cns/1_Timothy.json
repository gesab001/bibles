                "2": {
                    "verse_nr": 2,
                    "verse": "\u5974\u4ec6\u6709\u4fe1\u4e3b\u7684\u4e3b\u4eba\uff0c\u4e0d\u53ef\u56e0\u4e3a\u4ed6\u4eec\u662f\u5f1f\u5144\u800c\u8f7b\u770b\u4ed6\u4eec\uff1b\u5012\u8981\u52a0\u610f\u670d\u4e8b\u4ed6\u4eec\uff0c\u56e0\u4e3a\u8fd9\u4e9b\u53d7\u5230\u670d\u4e8b\u7684\u76ca\u5904\u7684\uff0c\u662f\u4fe1\u4e3b\u8499\u7231\u7684\u4eba\u3002\u4f60\u8981\u628a\u8fd9\u4e9b\u4e8b\u6559\u5bfc\u4eba\uff0c\u529d\u52c9\u4eba\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u656c\u8654\u4e0e\u77e5\u8db3\u5982\u679c\u6709\u4eba\u4f20\u522b\u7684\u6559\u4e49\uff0c\u4e0d\u63a5\u53d7\u6211\u4eec\u7684\u4e3b\u8036\u7a23\u57fa\u7763\u7eaf\u6b63\u7684\u8bdd\u8bed\uff0c\u548c\u90a3\u656c\u8654\u7684\u9053\u7406\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u662f\u81ea\u9ad8\u81ea\u5927\uff0c\u4e00\u65e0\u6240\u77e5\uff0c\u53cd\u800c\u4e13\u597d\u95ee\u96be\u4e89\u8fa9\uff0c\u7531\u6b64\u4ea7\u751f\u5992\u5fcc\u3001\u7eb7\u4e89\u3001\u6bc1\u8c24\u3001\u6076\u610f\u7684\u731c\u7591\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5c31\u548c\u90a3\u4e9b\u5fc3\u672f\u8d25\u574f\uff0c\u4e27\u5931\u771f\u7406\u7684\u4eba\u4e0d\u65ad\u5730\u4e89\u7ade\u3002\u4ed6\u4eec\u89c6\u656c\u8654\u4e3a\u5f97\u5229\u7684\u95e8\u8def\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5176\u5b9e\u656c\u8654\u800c\u53c8\u77e5\u8db3\uff0c\u5c31\u662f\u5f97\u5927\u5229\u7684\u9014\u5f84\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u4e3a\u6211\u4eec\u6ca1\u6709\u5e26\u4ec0\u4e48\u5230\u4e16\u4e0a\u6765\uff0c\u4e5f\u4e0d\u80fd\u5e26\u4ec0\u4e48\u53bb\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u53ea\u8981\u6709\u8863\u6709\u98df\uff0c\u5c31\u5e94\u5f53\u77e5\u8db3\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f46\u90a3\u4e9b\u60f3\u8981\u53d1\u8d22\u7684\u4eba\uff0c\u5c31\u843d\u5728\u8bd5\u63a2\u4e2d\u548c\u9677\u9631\u91cc\uff1b\u53c8\u843d\u5728\u8bb8\u591a\u65e0\u77e5\u800c\u6709\u5bb3\u7684\u79c1\u6b32\u91cc\uff0c\u4f7f\u4eba\u6c89\u6ca6\u5728\u8d25\u574f\u548c\u706d\u4ea1\u4e2d\uff1b\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u4e3a\u8d2a\u8d22\u662f\u4e07\u6076\u4e4b\u6839\u3002\u6709\u4eba\u8d2a\u7231\u94b1\u8d22\uff0c\u5c31\u88ab\u5f15\u8bf1\u79bb\u5f00\u771f\u9053\uff0c\u7528\u8bb8\u591a\u75db\u82e6\u628a\u81ea\u5df1\u523a\u900f\u4e86\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8981\u4e3a\u4fe1\u4ef0\u6253\u90a3\u7f8e\u597d\u7684\u4ed7\u4f46\u4f60\u8fd9\u5c5e\u3000\u795e\u7684\u4eba\u554a\uff0c\u5e94\u8be5\u9003\u907f\u8fd9\u4e9b\u4e8b\uff0c\u8981\u8ffd\u6c42\u516c\u4e49\u3001\u656c\u8654\u3001\u4fe1\u5fc3\u3001\u7231\u5fc3\u3001\u5fcd\u8010\u548c\u6e29\u67d4\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u8981\u4e3a\u4fe1\u4ef0\u6253\u90a3\u7f8e\u597d\u7684\u4ed7\uff0c\u6301\u5b9a\u6c38\u751f\uff1b\u4f60\u662f\u4e3a\u8fd9\u6c38\u751f\u800c\u8499\u53ec\u7684\uff0c\u53c8\u5728\u8bb8\u591a\u7684\u8bc1\u4eba\u9762\u524d\u627f\u8ba4\u8fc7\u7f8e\u597d\u7684\u4fe1\u4ef0\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u5728\u8d50\u751f\u547d\u7ed9\u4e07\u7269\u7684\u3000\u795e\u9762\u524d\uff0c\u5e76\u90a3\u5728\u672c\u4e22\uff0e\u5f7c\u62c9\u591a\u9762\u524d\u89c1\u8bc1\u8fc7\u7f8e\u597d\u7684\u4fe1\u4ef0\u7684\u57fa\u7763\u8036\u7a23\u9762\u524d\u5631\u5490\u4f60\uff0c\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u5f53\u6beb\u65e0\u73b7\u6c61\uff0c\u65e0\u53ef\u6307\u6458\u5730\u6301\u5b88\u8fd9\u547d\u4ee4\uff0c\u76f4\u5230\u6211\u4eec\u4e3b\u8036\u7a23\u57fa\u7763\u7684\u663e\u73b0\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5230\u4e86\u9002\u5f53\u7684\u65f6\u5019\uff0c\u90a3\u53ef\u79f0\u9882\u7684\u3001\u72ec\u4e00\u7684\u5168\u80fd\u8005\uff0c\u4e07\u738b\u4e4b\u738b\uff0c\u4e07\u4e3b\u4e4b\u4e3b\uff0c\u5fc5\u628a\u57fa\u7763\u7684\u663e\u73b0\u8868\u660e\u51fa\u6765\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u53ea\u6709\u4ed6\u6c38\u8fdc\u4e0d\u6b7b\uff0c\u4f4f\u5728\u4e0d\u80fd\u63a5\u8fd1\u7684\u5149\u91cc\uff0c\u6ca1\u6709\u4eba\u89c1\u8fc7\u4ed6\uff0c\u4eba\u4e5f\u4e0d\u80fd\u770b\u89c1\u4ed6\u3002\u613f\u5c0a\u8363\u548c\u6c38\u8fdc\u7684\u6743\u80fd\u90fd\u5f52\u7ed9\u4ed6\u3002\u963f\u4eec\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60\u8981\u5631\u5490\u90a3\u4e9b\u4eca\u4e16\u5bcc\u6709\u7684\u4eba\uff0c\u53eb\u4ed6\u4eec\u4e0d\u8981\u5fc3\u9ad8\u6c14\u50b2\uff0c\u4e5f\u4e0d\u8981\u5bc4\u671b\u5728\u6d6e\u52a8\u7684\u8d22\u5bcc\u4e0a\uff0c\u5374\u8981\u4ef0\u671b\u90a3\u539a\u8d50\u767e\u7269\u7ed9\u6211\u4eec\u4eab\u7528\u7684\u3000\u795e\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u53c8\u8981\u5631\u5490\u4ed6\u4eec\u884c\u5584\uff0c\u5728\u5584\u4e8b\u4e0a\u5bcc\u8db3\uff0c\u6177\u6168\u597d\u65bd\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8fd9\u6837\uff0c\u5c31\u4e3a\u81ea\u5df1\u5728\u6765\u4e16\u79ef\u805a\u8d22\u5bcc\uff0c\u4f5c\u7f8e\u597d\u7684\u57fa\u7840\uff0c\u597d\u53eb\u4ed6\u4eec\u80fd\u591f\u5f97\u7740\u90a3\u771f\u6b63\u7684\u751f\u547d\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u63d0\u6469\u592a\u554a\uff0c\u4f60\u8981\u4fdd\u5b88\u6240\u4ea4\u6258\u4f60\u7684\uff0c\u907f\u514d\u4e16\u4fd7\u7684\u7a7a\u8c08\u548c\u5192\u79f0\u662f\u77e5\u8bc6\u7684\u90a3\u79cd\u53cd\u8c03\uff1b\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6709\u4e9b\u4eba\u81ea\u79f0\u6709\u8fd9\u77e5\u8bc6\uff0c\u5c31\u504f\u79bb\u4e86\u771f\u9053\u3002\u613f\u6069\u60e0\u4e0e\u4f60\u4eec\u540c\u5728\u3002\n"
                }
            }
        }
    }
}