                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5199\u4fe1\u7ed9\u56e0\u4fe1\u4e3b\u800c\u4f5c\u6211\u771f\u513f\u5b50\u7684\u63d0\u6469\u592a\u3002\u613f\u6069\u60e0\u3001\u601c\u60af\u3001\u5e73\u5b89\u4ece\u7236\u3000\u795e\u548c\u6211\u4eec\u7684\u4e3b\u57fa\u7763\u8036\u7a23\u4e34\u5230\u4f60\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u63d0\u9632\u522b\u7684\u6559\u4e49\u6211\u5f80\u9a6c\u5176\u987f\u53bb\u7684\u65f6\u5019\uff0c\u66fe\u7ecf\u529d\u4f60\u7559\u5728\u4ee5\u5f17\u6240\uff0c\u4e3a\u8981\u5631\u5490\u67d0\u4e9b\u4eba\uff0c\u4e0d\u53ef\u4f20\u522b\u7684\u6559\u4e49\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4e5f\u4e0d\u53ef\u6c89\u8ff7\u4e8e\u65e0\u7a3d\u4e4b\u8c08\u548c\u65e0\u7a77\u7684\u5bb6\u8c31\uff1b\u8fd9\u4e9b\u4e8b\u53ea\u80fd\u5f15\u8d77\u4e89\u8bba\uff0c\u5bf9\u4e8e\u3000\u795e\u5728\u4fe1\u4ef0\u4e0a\u6240\u5b9a\u7684\u8ba1\u5212\u662f\u6beb\u65e0\u5e2e\u52a9\u7684\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8fd9\u5631\u5490\u7684\u76ee\u7684\u662f\u51fa\u4e8e\u7231\uff1b\u8fd9\u7231\u662f\u53d1\u81ea\u7eaf\u6d01\u7684\u5fc3\u3001\u65e0\u6127\u7684\u826f\u5fc3\u548c\u65e0\u4f2a\u7684\u4fe1\u5fc3\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6709\u4e9b\u4eba\u504f\u79bb\u4e86\u8fd9\u4e9b\uff0c\u5c31\u8f6c\u5411\u65e0\u610f\u4e49\u7684\u8fa9\u8bba\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u60f3\u8981\u4f5c\u5f8b\u6cd5\u6559\u5e08\uff0c\u5374\u4e0d\u660e\u767d\u81ea\u5df1\u8bb2\u7684\u662f\u4ec0\u4e48\uff0c\u4e3b\u5f20\u7684\u53c8\u662f\u4ec0\u4e48\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u4eec\u77e5\u9053\u5f8b\u6cd5\u662f\u597d\u7684\uff0c\u53ea\u8981\u5e94\u7528\u5f97\u6070\u5f53\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u8981\u77e5\u9053\u5f8b\u6cd5\u672c\u6765\u4e0d\u662f\u4e3a\u4e49\u4eba\u8bbe\u7acb\u7684\uff0c\u800c\u662f\u4e3a\u90a3\u4e9b\u65e0\u6cd5\u65e0\u5929\u548c\u653e\u8361\u4e0d\u7f81\u7684\u3001\u4e0d\u656c\u8654\u548c\u72af\u7f6a\u7684\u3001\u4e0d\u5723\u6d01\u548c\u4e16\u4fd7\u7684\u3001\u5f12\u7236\u6bcd\u548c\u6740\u4eba\u7684\u3001\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6deb\u4e71\u7684\u3001\u4eb2\u7537\u8272\u7684\u3001\u62d0\u5e26\u4eba\u53e3\u7684\u3001\u8bf4\u8c0e\u8bdd\u7684\u3001\u53d1\u5047\u8a93\u7684\uff0c\u4ee5\u53ca\u4e3a\u5176\u4ed6\u62b5\u6321\u7eaf\u6b63\u6559\u8bad\u7684\u4eba\u8bbe\u7acb\u7684\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8fd9\u662f\u7167\u7740\u53ef\u79f0\u9882\u7684\u3001\u8363\u8000\u7684\u3000\u795e\u6240\u4ea4\u6258\u6211\u7684\u798f\u97f3\u8bf4\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u57fa\u7763\u8036\u7a23\u964d\u4e16\u4e3a\u8981\u62ef\u6551\u7f6a\u4eba\u6211\u611f\u8c22\u90a3\u8d50\u6211\u80fd\u529b\u7684\u3001\u6211\u4eec\u7684\u4e3b\u57fa\u7763\u8036\u7a23\uff0c\u56e0\u4e3a\u4ed6\u8ba4\u4e3a\u6211\u6709\u5fe0\u5fc3\uff0c\u6d3e\u6211\u670d\u4e8b\u4ed6\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u4ece\u524d\u662f\u4eb5\u6e0e\u3000\u795e\u7684\u3001\u8feb\u5bb3\u4eba\u7684\u3001\u51cc\u8fb1\u4eba\u7684\uff0c\u7136\u800c\u6211\u8fd8\u8499\u4e86\u601c\u60af\uff0c\u56e0\u4e3a\u6211\u662f\u5728\u4e0d\u4fe1\u7684\u65f6\u5019\uff0c\u7531\u4e8e\u65e0\u77e5\u800c\u4f5c\u7684\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6211\u4eec\u4e3b\u7684\u6069\u5178\uff0c\u968f\u7740\u5728\u57fa\u7763\u8036\u7a23\u91cc\u7684\u4fe1\u5fc3\u548c\u7231\u5fc3\uff0c\u5728\u6211\u8eab\u4e0a\u8d8a\u53d1\u589e\u52a0\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u201c\u57fa\u7763\u8036\u7a23\u964d\u4e16\uff0c\u4e3a\u8981\u62ef\u6551\u7f6a\u4eba\u3002\u201d\u8fd9\u8bdd\u662f\u53ef\u4fe1\u7684\uff0c\u662f\u503c\u5f97\u5b8c\u5168\u63a5\u7eb3\u7684\u3002\u5728\u7f6a\u4eba\u4e2d\u6211\u662f\u4e2a\u7f6a\u9b41\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u53ef\u662f\uff0c\u6211\u7adf\u7136\u8499\u4e86\u601c\u60af\uff0c\u597d\u8ba9\u57fa\u7763\u8036\u7a23\u5728\u6211\u8fd9\u4e2a\u7f6a\u9b41\u8eab\u4e0a\uff0c\u663e\u660e\u4ed6\u5b8c\u5168\u7684\u5fcd\u8010\uff0c\u7ed9\u540e\u6765\u4fe1\u4ed6\u5f97\u6c38\u751f\u7684\u4eba\u4f5c\u699c\u6837\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f46\u613f\u5c0a\u8d35\u8363\u8000\u5f52\u7ed9\u4e07\u4e16\u7684\u541b\u738b\uff0c\u5c31\u662f\u90a3\u4e0d\u673d\u574f\u3001\u4eba\u4e0d\u80fd\u89c1\u3001\u72ec\u4e00\u7684\u3000\u795e\uff0c\u76f4\u5230\u6c38\u6c38\u8fdc\u8fdc\u3002\u963f\u4eec\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u6211\u513f\u63d0\u6469\u592a\u554a\uff01\u6211\u7167\u7740\u4ece\u524d\u5173\u4e8e\u4f60\u7684\u9884\u8a00\uff0c\u628a\u8fd9\u547d\u4ee4\u4ea4\u6258\u4f60\uff0c\u4e3a\u7684\u662f\u8981\u53eb\u4f60\u501f\u7740\u8fd9\u4e9b\u9884\u8a00\u6253\u90a3\u7f8e\u597d\u7684\u4ed7\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u5e38\u5e38\u5b58\u7740\u4fe1\u5fc3\u548c\u65e0\u6127\u7684\u826f\u5fc3\u3002\u6709\u4e9b\u4eba\u4e22\u5f03\u826f\u5fc3\uff0c\u5c31\u5728\u4fe1\u4ef0\u4e0a\u5931\u843d\u4e86\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4ed6\u4eec\u5f53\u4e2d\u6709\u8bb8\u7c73\u4e43\u548c\u4e9a\u5386\u5c71\u5927\uff0c\u6211\u5df2\u7ecf\u628a\u4ed6\u4eec\u4ea4\u7ed9\u6492\u4f46\uff0c\u4f7f\u4ed6\u4eec\u53d7\u7ba1\u6559\u4e0d\u518d\u4eb5\u6e0e\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
