                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u820d\u4e86\u81ea\u5df1\u4f5c\u4e07\u4eba\u7684\u8d4e\u4ef7\uff0c\u5230\u4e86\u9002\u5f53\u7684\u65f6\u5019\uff0c\u8fd9\u4e8b\u5c31\u8bc1\u5b9e\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4e3a\u4e86\u8fd9\u4e8b\uff0c\u6211\u4e5f\u88ab\u6d3e\u4f5c\u4f20\u9053\u7684\u548c\u4f7f\u5f92\uff08\u6211\u8bf4\u7684\u662f\u771f\u8bdd\uff0c\u4e0d\u662f\u8c0e\u8a00\uff09\uff0c\u5728\u4fe1\u4ef0\u548c\u771f\u7406\u4e0a\u4f5c\u5916\u65cf\u4eba\u7684\u6559\u5e08\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u656c\u754f\u3000\u795e\u7684\u5973\u4eba\u56e0\u6b64\uff0c\u6211\u613f\u610f\u7537\u4eba\u6ca1\u6709\u5fff\u6012\uff0c\u6ca1\u6709\u4e89\u8bba\uff0c\u4e3e\u8d77\u5723\u6d01\u7684\u624b\u968f\u5904\u7977\u544a\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7167\u6837\uff0c\u6211\u4e5f\u613f\u610f\u5973\u4eba\u4ee5\u7aef\u6b63\u3001\u5a34\u6dd1\u3001\u81ea\u5f8b\u4e3a\u88c5\u9970\uff1b\u4e0d\u8981\u4ee5\u9b08\u53d1\u3001\u91d1\u9970\u3001\u73e0\u5b9d\uff0c\u6216\u540d\u8d35\u8863\u88f3\u4e3a\u88c5\u9970\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5374\u8981\u4ee5\u5584\u884c\u4f5c\u88c5\u9970\uff0c\u8fd9\u624d\u914d\u79f0\u4e3a\u656c\u754f\u3000\u795e\u7684\u5973\u4eba\u3002\n"
