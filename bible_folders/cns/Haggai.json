    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8d23\u5907\u4eba\u6c11\u62d6\u5ef6\u91cd\u5efa\u5723\u6bbf\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u201c\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8fd9\u6837\u8bf4\uff1a\u2018\u8fd9\u6c11\u8bf4\uff0c\u91cd\u5efa\u8036\u548c\u534e\u6bbf\u5b87\u7684\u65f6\u5019\u8fd8\u6ca1\u6709\u5230\u3002\u2019\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e8e\u662f\uff0c\u8036\u548c\u534e\u7684\u8bdd\u4e34\u5230\u54c8\u8be5\u5148\u77e5\u8bf4\uff1a\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u201c\u8fd9\u6bbf\u5b87\u4ecd\u7136\u8352\u5e9f\uff0c\u5c82\u662f\u4f60\u4eec\u4f4f\u5728\u6709\u5929\u82b1\u677f\u7684\u623f\u5c4b\u7684\u65f6\u5019\u5417\uff1f\u201d\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u73b0\u5728\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8fd9\u6837\u8bf4\uff1a\u201c\u4f60\u4eec\u8981\u53cd\u7701\u81ea\u5df1\u7684\u884c\u4e3a\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u4eec\u6492\u7684\u79cd\u591a\uff0c\u6536\u5272\u7684\u5374\u5c11\uff1b\u4f60\u4eec\u5403\uff0c\u5374\u5403\u4e0d\u9971\uff1b\u4f60\u4eec\u559d\uff0c\u5374\u559d\u4e0d\u8db3\uff1b\u4f60\u4eec\u7a7f\u8863\u670d\uff0c\u5374\u7a7f\u4e0d\u6696\uff1b\u5f97\u5de5\u94b1\u7684\uff0c\u5374\u628a\u5de5\u94b1\u653e\u5728\u7834\u888b\u91cc\u3002\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u529d\u52c9\u4eba\u6c11\u52a8\u5de5\u91cd\u5efa\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8fd9\u6837\u8bf4\uff1a\u201c\u4f60\u4eec\u8981\u53cd\u7701\u81ea\u5df1\u7684\u884c\u4e3a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u4eec\u8981\u4e0a\u5c71\u53d6\u6728\u6750\uff0c\u5efa\u9020\u6bbf\u5b87\uff1b\u6211\u5fc5\u56e0\u6b64\u559c\u60a6\uff0c\u5e76\u4e14\u5f97\u8363\u8000\u3002\u201d\u8036\u548c\u534e\u8bf4\uff1a\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u201c\u4f60\u4eec\u671f\u671b\u4e30\u6536\uff0c\u53cd\u5012\u6b49\u6536\uff1b\u4f60\u4eec\u6536\u5230\u5bb6\u91cc\u7684\uff0c\u6211\u5c31\u5439\u53bb\u3002\u201d\u4e3a\u4ec0\u4e48\u5462\uff1f\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u56e0\u4e3a\u6211\u7684\u6bbf\u5b87\u8352\u5e9f\uff0c\u4f60\u4eec\u5404\u4eba\u5374\u4e3a\u81ea\u5df1\u7684\u623f\u5c4b\u5954\u9a70\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6240\u4ee5\uff0c\u56e0\u4f60\u4eec\u7684\u7f18\u6545\uff0c\u5929\u5c31\u4e0d\u964d\u7518\u9732\uff0c\u5730\u4e5f\u4e0d\u51fa\u571f\u4ea7\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6211\u53eb\u5e72\u65f1\u4e34\u5230\u5927\u5730\u3001\u7fa4\u5c71\u3001\u4e94\u8c37\u3001\u65b0\u9152\u3001\u65b0\u6cb9\u3001\u5730\u4e0a\u7684\u51fa\u4ea7\u3001\u4eba\u755c\u548c\u4eba\u624b\u52b3\u788c\u5f97\u6765\u7684\u4e00\u5207\u3002\u201d\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u529d\u52c9\u7684\u679c\u6548\u90a3\u65f6\uff0c\u6492\u62c9\u94c1\u7684\u513f\u5b50\u6240\u7f57\u5df4\u4f2f\u3001\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u548c\u6240\u6709\u4f59\u5269\u7684\u5b50\u6c11\uff0c\u90fd\u542c\u4ece\u4e86\u8036\u548c\u534e\u4ed6\u4eec\u3000\u795e\u7684\u8bdd\uff0c\u542c\u4ece\u4e86\u8036\u548c\u534e\u4ed6\u4eec\u3000\u795e\u6240\u5dee\u6765\u7684\u54c8\u8be5\u5148\u77e5\u7684\u8bdd\u3002\u4f17\u6c11\u5728\u8036\u548c\u534e\u9762\u524d\u90fd\u5b58\u7740\u656c\u754f\u7684\u5fc3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8036\u548c\u534e\u7684\u4f7f\u8005\u54c8\u8be5\uff0c\u5949\u8036\u548c\u534e\u7684\u547d\uff0c\u5bf9\u4f17\u6c11\u8bf4\uff1a\u201c\u8036\u548c\u534e\u8bf4\uff1a\u2018\u6211\u4e0e\u4f60\u4eec\u540c\u5728\u3002\u2019\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e8e\u662f\u8036\u548c\u534e\u6fc0\u53d1\u4e86\u6492\u62c9\u94c1\u7684\u513f\u5b50\u3001\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\u7684\u5fc3\uff0c\u548c\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u7684\u5fc3\uff0c\u4ee5\u53ca\u6240\u6709\u4f59\u5269\u5b50\u6c11\u7684\u5fc3\uff0c\u4ed6\u4eec\u5c31\u6765\u5174\u5de5\u5efa\u9020\u4e07\u519b\u4e4b\u8036\u548c\u534e\u4ed6\u4eec\u3000\u795e\u7684\u6bbf\u5b87\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u90a3\u65f6\u662f\u5927\u5229\u4e4c\u738b\u7b2c\u4e8c\u5e74\u516d\u6708\u4e8c\u5341\u56db\u65e5\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u65b0\u6bbf\u7684\u8363\u8000\u80dc\u8fc7\u524d\u6bbf\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u201c\u4f60\u8981\u5bf9\u6492\u62c9\u94c1\u7684\u513f\u5b50\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\uff0c\u548c\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\uff0c\u4ee5\u53ca\u6240\u6709\u4f59\u5269\u7684\u5b50\u6c11\uff0c\u8bf4\uff1a\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u2018\u4f60\u4eec\u4e2d\u95f4\u5269\u4e0b\u7684\u4eba\uff0c\u6709\u8c01\u89c1\u8fc7\u8fd9\u6bbf\u5b87\u4ece\u524d\u7684\u8363\u8000\u5462\uff1f\u73b0\u5728\u4f60\u4eec\u53c8\u770b\u5b83\u600e\u6837\uff1f\u5c82\u4e0d\u662f\u89c6\u5982\u65e0\u7269\u4e00\u6837\u5417\uff1f\u2019\u201d\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a\u201c\u6240\u7f57\u5df4\u4f2f\u554a\uff01\u73b0\u5728\u4f60\u8981\u521a\u5f3a\u3002\u201d\u8036\u548c\u534e\u8bf4\uff1a\u201c\u7ea6\u6492\u7b54\u7684\u513f\u5b50\u5927\u796d\u53f8\u7ea6\u4e66\u4e9a\u554a\uff01\u4f60\u8981\u521a\u5f3a\uff1b\u8fd9\u5730\u7684\u4e00\u5207\u5b50\u6c11\u554a\uff01\u4f60\u4eec\u4e5f\u8981\u521a\u5f3a\uff0c\u5e76\u4e14\u8981\u4f5c\u5de5\u3002\u201d\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u56e0\u4e3a\u6211\u4e0e\u4f60\u4eec\u540c\u5728\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8fd9\u5c31\u662f\u4f60\u4eec\u4ece\u57c3\u53ca\u51fa\u6765\u7684\u65f6\u5019\uff0c\u6211\u4e0e\u4f60\u4eec\u7acb\u7ea6\u7684\u8bdd\u3002\u73b0\u5728\u6211\u7684\u7075\u4f4f\u5728\u4f60\u4eec\u4e2d\u95f4\uff1b\u4f60\u4eec\u4e0d\u8981\u60e7\u6015\u3002\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u56e0\u4e3a\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8fd9\u6837\u8bf4\uff1a\u201c\u518d\u8fc7\u4e0d\u591a\u7684\u65f6\u5019\uff0c\u6211\u8981\u9707\u52a8\u5929\u3001\u5730\u3001\u6d77\u6d0b\u548c\u65f1\u5730\u3002\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u6211\u8981\u9707\u52a8\u4e07\u56fd\uff0c\u4e07\u56fd\u7684\u73cd\u5b9d\u5c31\u5fc5\u8fd0\u6765\uff1b\u6211\u8981\u4f7f\u8fd9\u6bbf\u5b87\u5145\u6ee1\u8363\u8000\u3002\u201d\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u94f6\u5b50\u662f\u6211\u7684\uff0c\u91d1\u5b50\u4e5f\u662f\u6211\u7684\u3002\u201d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u8fd9\u6bbf\u5b87\u540e\u6765\u7684\u8363\u8000\uff0c\u5fc5\u5927\u8fc7\u5148\u524d\u7684\u8363\u8000\u3002\u201d\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8bf4\uff1a\u201c\u5728\u8fd9\u5730\u65b9\uff0c\u6211\u5fc5\u8d50\u4e0b\u5e73\u5b89\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6307\u51fa\u4eba\u6c11\u7684\u6076\u884c\u5927\u5229\u4e4c\u738b\u7b2c\u4e8c\u5e74\u4e5d\u6708\u4e8c\u5341\u56db\u65e5\uff0c\u8036\u548c\u534e\u7684\u8bdd\u4e34\u5230\u54c8\u8be5\u5148\u77e5\u8bf4\uff1a\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u201c\u4e07\u519b\u4e4b\u8036\u548c\u534e\u8fd9\u6837\u8bf4\uff1a\u4f60\u8981\u5411\u796d\u53f8\u8be2\u95ee\u6709\u5173\u5f8b\u6cd5\u7684\u4e8b\u8bf4\uff1a\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4eba\u82e5\u7528\u81ea\u5df1\u7684\u8863\u895f\u76db\u8f7d\u5723\u8089\uff0c\u540e\u6765\u53c8\u4f7f\u81ea\u5df1\u7684\u8863\u895f\u89e6\u7740\u997c\u3001\u6c64\u3001\u9152\u3001\u6cb9\u6216\u522b\u7684\u98df\u7269\uff0c\u8fd9\u5c31\u7b97\u4e3a\u5723\u5417\uff1f\u201d\u796d\u53f8\u90fd\u56de\u7b54\u8bf4\uff1a\u201c\u4e0d\u7b97\u4e3a\u5723\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u54c8\u8be5\u53c8\u8bf4\uff1a\u201c\u4eba\u82e5\u56e0\u89e6\u7740\u5c38\u4f53\u800c\u6210\u4e3a\u4e0d\u6d01\uff0c\u53c8\u89e6\u7740\u8fd9\u4e9b\u4e1c\u897f\u7684\u4efb\u4f55\u4e00\u6837\uff0c\u8fd9\u5c31\u7b97\u4e3a\u4e0d\u6d01\u5417\uff1f\u201d\u796d\u53f8\u90fd\u56de\u7b54\u8bf4\uff1a\u201c\u8981\u7b97\u4e3a\u4e0d\u6d01\u51c0\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e8e\u662f\u54c8\u8be5\u56de\u7b54\uff0c\u8bf4\uff1a\u201c\u8036\u548c\u534e\u8bf4\uff1a\u2018\u8fd9\u6c11\u4e5f\u662f\u8fd9\u6837\uff0c\u8fd9\u56fd\u5728\u6211\u9762\u524d\u4e5f\u662f\u8fd9\u6837\uff1b\u4ed6\u4eec\u624b\u91cc\u7684\u4e00\u5207\u5de5\u4f5c\uff0c\u90fd\u662f\u8fd9\u6837\uff1b\u4ed6\u4eec\u5728\u90a3\u91cc\u6240\u732e\u7684\uff0c\u90fd\u662f\u4e0d\u6d01\u51c0\u7684\u3002\u2019\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8036\u548c\u534e\u5e94\u8bb8\u8d50\u798f\u201c\u73b0\u5728\u4f60\u4eec\u8981\u53cd\u7701\uff0c\u4ece\u4eca\u65e5\u8d77\u56de\u60f3\u8fc7\u5f80\uff0c\u5c31\u662f\u5728\u8036\u548c\u534e\u7684\u6bbf\u8fd8\u6ca1\u6709\u4e00\u5757\u77f3\u5934\u53e0\u5728\u53e6\u4e00\u5757\u77f3\u5934\u4e0a\u9762\u4ee5\u524d\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u90a3\u65f6\u4f60\u4eec\u600e\u6837\u5462\uff1f\u6709\u4eba\u5230\u8c37\u5806\u6765\uff0c\u60f3\u5f97\u4e24\u767e\u516c\u65a4\u8c37\uff0c\u5374\u53ea\u5f97\u4e00\u767e\u516c\u65a4\uff1b\u6709\u4eba\u5230\u69a8\u9152\u6c60\u53bb\uff0c\u60f3\u4ece\u9152\u69fd\u5f97\u4e00\u767e\u516c\u5347\u9152\uff0c\u5374\u53ea\u5f97\u56db\u5341\u516c\u5347\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a\u2018\u6211\u7528\u65f1\u98ce\u3001\u9709\u70c2\u548c\u51b0\u96f9\u51fb\u6253\u4f60\u4eec\uff0c\u548c\u4f60\u4eec\u624b\u91cc\u7684\u4e00\u5207\u5de5\u4f5c\uff0c\u4f60\u4eec\u8fd8\u662f\u4e0d\u5f52\u5411\u6211\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4f60\u4eec\u8981\u53cd\u7701\uff0c\u4ece\u4eca\u65e5\u8d77\u56de\u60f3\u8fc7\u5f80\uff0c\u5c31\u662f\u81ea\u4e5d\u6708\u4e8c\u5341\u56db\u65e5\u8d77\uff0c\u4ece\u8036\u548c\u534e\u6bbf\u7684\u5960\u57fa\u65e5\u5f00\u59cb\uff0c\u4f60\u4eec\u8981\u53cd\u7701\uff1a\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u4ed3\u91cc\u8fd8\u6709\u79cd\u5b50\u5417\uff1f\u8461\u8404\u6811\u3001\u65e0\u82b1\u679c\u6811\u3001\u77f3\u69b4\u6811\u548c\u6a44\u6984\u6811\uff0c\u5c82\u662f\u8fd8\u6ca1\u6709\u7ed3\u679c\u5b50\u5417\uff1f\u4ece\u4eca\u65e5\u8d77\uff0c\u6211\u5fc5\u8d50\u798f\u3002\u2019\u201d\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5e94\u8bb8\u62e3\u9009\u6240\u7f57\u5df4\u4f2f\u540c\u6708\u4e8c\u5341\u56db\u65e5\uff0c\u8036\u548c\u534e\u7684\u8bdd\u518d\u6b21\u4e34\u5230\u54c8\u8be5\uff0c\u8bf4\uff1a\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u201c\u4f60\u8981\u5411\u72b9\u5927\u7701\u957f\u6240\u7f57\u5df4\u4f2f\u8bf4\uff1a\u2018\u6211\u8981\u9707\u52a8\u5929\u5730\uff0c\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u6211\u5fc5\u503e\u8986\u5217\u56fd\u7684\u5b9d\u5ea7\uff0c\u6d88\u706d\u5916\u65cf\u4e07\u56fd\u7684\u52bf\u529b\uff0c\u6211\u5fc5\u63a8\u7ffb\u6218\u8f66\u548c\u9a7e\u8f66\u7684\u4eba\uff1b\u9a6c\u5339\u548c\u9a91\u9a6c\u7684\u90fd\u5fc5\u8dcc\u5012\uff1b\u5404\u4eba\u5fc5\u6b7b\u5728\u81ea\u5df1\u5144\u5f1f\u7684\u5200\u4e0b\u3002\u2019\n"
                },
                "23": {
