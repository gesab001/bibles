                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5e94\u5f53\u6beb\u65e0\u5fe7\u8651\uff0c\u53ea\u8981\u51e1\u4e8b\u501f\u7740\u7977\u544a\u7948\u6c42\uff0c\u5e26\u7740\u611f\u6069\u7684\u5fc3\uff0c\u628a\u4f60\u4eec\u6240\u8981\u7684\u544a\u8bc9\u3000\u795e\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u8fd9\u6837\uff0c\u3000\u795e\u6240\u8d50\u8d85\u8fc7\u4eba\u80fd\u4e86\u89e3\u7684\u5e73\u5b89\uff0c\u5fc5\u5728\u57fa\u7763\u8036\u7a23\u91cc\uff0c\u4fdd\u5b88\u4f60\u4eec\u7684\u5fc3\u601d\u610f\u5ff5\u3002\n"
