    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u613f\u6069\u60e0\u5e73\u5b89\u4ece\u6211\u4eec\u7684\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a23\u57fa\u7763\u4e34\u5230\u4f60\u4eec\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e3a\u4fe1\u5f92\u7948\u7977\u6211\u6bcf\u9022\u60f3\u5230\u4f60\u4eec\uff0c\u5c31\u611f\u8c22\u6211\u7684\u3000\u795e\uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6bcf\u6b21\u4e3a\u4f60\u4eec\u5927\u5bb6\u7948\u6c42\u7684\u65f6\u5019\uff0c\u603b\u662f\u6b22\u6b22\u559c\u559c\u5730\u7948\u6c42\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4e3a\u4e86\u4f60\u4eec\u4ece\u5934\u4e00\u5929\u76f4\u5230\u73b0\u5728\u90fd\u5728\u798f\u97f3\u7684\u4e8b\u5de5\u4e0a\u6709\u5206\uff0c\u6211\u5c31\u611f\u8c22\u6211\u7684\u3000\u795e\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u6df1\u4fe1\u90a3\u5728\u4f60\u4eec\u4e2d\u95f4\u5f00\u59cb\u4e86\u7f8e\u597d\u5de5\u4f5c\u7684\uff0c\u5230\u4e86\u57fa\u7763\u8036\u7a23\u7684\u65e5\u5b50\uff0c\u5fc5\u6210\u5168\u8fd9\u5de5\u4f5c\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4e3a\u4f60\u4eec\u4f17\u4eba\u6211\u6709\u8fd9\u6837\u7684\u610f\u5ff5\u662f\u5f88\u6070\u5f53\u7684\uff0c\u56e0\u4e3a\u4f60\u4eec\u5e38\u5e38\u5728\u6211\u7684\u5fc3\u91cc\uff0c\u65e0\u8bba\u6211\u662f\u5728\u6346\u9501\u4e4b\u4e2d\uff0c\u6216\u662f\u5728\u8fa9\u62a4\u548c\u8bc1\u5b9e\u798f\u97f3\u7684\u65f6\u5019\uff0c\u4f60\u4eec\u90fd\u548c\u6211\u4e00\u540c\u5206\u4eab\u3000\u795e\u7684\u6069\u5178\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u662f\u600e\u6837\u4ee5\u57fa\u7763\u8036\u7a23\u7684\u5fc3\u80a0\uff0c\u5207\u5207\u5730\u60f3\u5ff5\u4f60\u4eec\u4f17\u4eba\uff0c\u8fd9\u662f\u3000\u795e\u53ef\u4ee5\u4e3a\u6211\u4f5c\u8bc1\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u6240\u7977\u544a\u7684\uff0c\u662f\u8981\u4f60\u4eec\u7684\u7231\u5fc3\uff0c\u5728\u5145\u8db3\u7684\u77e5\u8bc6\u548c\u5404\u6837\u7684\u89c1\u8bc6\u4e0a\uff0c\u591a\u800c\u53c8\u591a\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f7f\u4f60\u4eec\u53ef\u4ee5\u8fa8\u522b\u662f\u975e\uff0c\u6210\u4e3a\u771f\u8bda\u65e0\u53ef\u6307\u6458\u7684\u4eba\uff0c\u76f4\u5230\u57fa\u7763\u7684\u65e5\u5b50\uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u9760\u7740\u8036\u7a23\u57fa\u7763\u7ed3\u6ee1\u4e86\u516c\u4e49\u7684\u679c\u5b50\uff0c\u4f7f\u3000\u795e\u5f97\u7740\u8363\u8000\u548c\u8d5e\u7f8e\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6d3b\u7740\u5c31\u662f\u57fa\u7763\u5f1f\u5144\u4eec\uff0c\u6211\u613f\u610f\u4f60\u4eec\u77e5\u9053\uff0c\u6211\u6240\u906d\u9047\u7684\u53cd\u800c\u4f7f\u798f\u97f3\u66f4\u52a0\u5174\u65fa\u4e86\uff0c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4ee5\u81f4\u5bab\u91cc\u7684\u536b\u961f\u548c\u5176\u4f59\u7684\u4eba\uff0c\u90fd\u77e5\u9053\u6211\u662f\u4e3a\u4e86\u57fa\u7763\u624d\u53d7\u6346\u9501\u7684\uff1b\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u800c\u4e14\u5927\u591a\u6570\u4e3b\u5185\u7684\u5f1f\u5144\uff0c\u56e0\u6211\u6240\u53d7\u7684\u6346\u9501\uff0c\u5c31\u7b03\u4fe1\u4e0d\u7591\uff0c\u6beb\u65e0\u754f\u60e7\uff0c\u66f4\u52c7\u6562\u5730\u4f20\u8bb2\u3000\u795e\u7684\u9053\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u6709\u4e9b\u4eba\u4f20\u626c\u57fa\u7763\u662f\u51fa\u4e8e\u5ac9\u5992\u548c\u7eb7\u4e89\uff0c\u4f46\u4e5f\u6709\u4e9b\u4eba\u662f\u51fa\u4e8e\u597d\u610f\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u8fd9\u4e9b\u4eba\u662f\u51fa\u4e8e\u7231\u5fc3\uff0c\u77e5\u9053\u6211\u662f\u6d3e\u6765\u4e3a\u798f\u97f3\u8fa9\u62a4\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u90a3\u4e9b\u4eba\u4f20\u8bb2\u57fa\u7763\u5374\u662f\u51fa\u4e8e\u81ea\u79c1\uff0c\u52a8\u673a\u5e76\u4e0d\u7eaf\u6b63\uff0c\u53ea\u60f3\u52a0\u91cd\u6211\u5728\u6346\u9501\u4e2d\u7684\u70e6\u607c\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u90a3\u6709\u4ec0\u4e48\u5173\u7cfb\u5462\uff1f\u771f\u5fc3\u4e5f\u597d\uff0c\u5047\u610f\u4e5f\u597d\uff0c\u65e0\u8bba\u600e\u4e48\u6837\uff0c\u57fa\u7763\u603b\u88ab\u4f20\u5f00\u4e86\uff0c\u4e3a\u6b64\u6211\u5c31\u6b22\u559c\uff1b\u5e76\u4e14\u6211\u8fd8\u8981\u6b22\u559c\uff0c\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u56e0\u4e3a\u6211\u77e5\u9053\uff0c\u501f\u7740\u4f60\u4eec\u7684\u7948\u6c42\u548c\u8036\u7a23\u57fa\u7763\u7684\u7075\u7684\u5e2e\u52a9\uff0c\u6211\u4e00\u5b9a\u4f1a\u5f97\u5230\u91ca\u653e\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u6211\u6240\u70ed\u5207\u671f\u5f85\u548c\u76fc\u671b\u7684\uff0c\u5c31\u662f\u5728\u51e1\u4e8b\u4e0a\u6211\u90fd\u4e0d\u4f1a\u7f9e\u6127\uff0c\u53ea\u8981\u6ee1\u6709\u80c6\u91cf\uff0c\u4e0d\u8bba\u751f\u6b7b\uff0c\u603b\u8981\u8ba9\u57fa\u7763\u5728\u6211\u8eab\u4e0a\u7167\u5e38\u88ab\u5c0a\u4e3a\u5927\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u56e0\u4e3a\u6211\u6d3b\u7740\u5c31\u662f\u57fa\u7763\uff0c\u6211\u6b7b\u4e86\u5c31\u6709\u76ca\u5904\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4f46\u5982\u679c\u6211\u4ecd\u5728\u4e16\u4e0a\u6d3b\u7740\uff0c\u80fd\u591f\u4f7f\u6211\u7684\u5de5\u4f5c\u6709\u6210\u679c\uff0c\u6211\u5c31\u4e0d\u77e5\u9053\u5e94\u8be5\u600e\u6837\u9009\u62e9\u4e86\uff01\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u6211\u5904\u4e8e\u4e24\u96be\u4e4b\u95f4\uff0c\u60c5\u613f\u79bb\u4e16\u4e0e\u57fa\u7763\u540c\u5728\uff0c\u56e0\u4e3a\u90a3\u662f\u597d\u5f97\u65e0\u6bd4\u7684\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u53ef\u662f\u4e3a\u4e86\u4f60\u4eec\uff0c\u6211\u66f4\u9700\u8981\u6d3b\u5728\u4e16\u4e0a\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u6211\u65e2\u7136\u8fd9\u6837\u6df1\u4fe1\uff0c\u5c31\u77e5\u9053\u8fd8\u8981\u6d3b\u4e0b\u53bb\uff0c\u5e76\u4e14\u8981\u7ee7\u7eed\u548c\u4f60\u4eec\u5927\u5bb6\u5728\u4e00\u8d77\uff0c\u4f7f\u4f60\u4eec\u5728\u4fe1\u5fc3\u4e0a\u6709\u957f\u8fdb\uff0c\u6709\u559c\u4e50\uff0c\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u4ee5\u81f4\u4f60\u4eec\u56e0\u4e3a\u6211\u8981\u518d\u5230\u4f60\u4eec\u90a3\u91cc\u53bb\uff0c\u5c31\u5728\u57fa\u7763\u8036\u7a23\u91cc\u66f4\u52a0\u4ee5\u6211\u4e3a\u8363\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u4e0d\u8fc7\uff0c\u4f60\u4eec\u884c\u4e8b\u4e3a\u4eba\u5e94\u5f53\u548c\u57fa\u7763\u7684\u798f\u97f3\u76f8\u914d\u3002\u8fd9\u6837\uff0c\u65e0\u8bba\u6211\u6765\u89c1\u4f60\u4eec\u6216\u662f\u4e0d\u5728\u4f60\u4eec\u4e2d\u95f4\uff0c\u90fd\u53ef\u4ee5\u542c\u5230\u4f60\u4eec\u7684\u60c5\u51b5\uff0c\u5c31\u662f\u4f60\u4eec\u6709\u540c\u4e00\u7684\u5fc3\u5fd7\uff0c\u7ad9\u7acb\u5f97\u7a33\uff0c\u4e3a\u4e86\u798f\u97f3\u7684\u4fe1\u4ef0\u9f50\u5fc3\u52aa\u529b\uff0c\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\u4ec0\u4e48\u4e8b\u90fd\u4e0d\u6015\u6709\u53cd\u5bf9\u4f60\u4eec\u7684\u4eba\uff0c\u8fd9\u5c31\u8bc1\u660e\u4ed6\u4eec\u8981\u706d\u4ea1\uff0c\u4f60\u4eec\u8981\u5f97\u6551\uff0c\u8fd9\u90fd\u662f\u51fa\u4e8e\u3000\u795e\u3002\n"
                },
                "29": {
                    "verse_nr": 29,
                    "verse": "\u56e0\u4e3a\u3000\u795e\u4e3a\u4e86\u57fa\u7763\u7684\u7f18\u6545\u8d50\u6069\u7ed9\u4f60\u4eec\uff0c\u4f7f\u4f60\u4eec\u4e0d\u5355\u662f\u4fe1\u57fa\u7763\uff0c\u4e5f\u662f\u8981\u4e3a\u4ed6\u53d7\u82e6\uff1b\n"
                },
                "30": {
                    "verse_nr": 30,
                    "verse": "\u4f60\u4eec\u9762\u5bf9\u7684\u4e89\u6218\uff0c\u548c\u4f60\u4eec\u5728\u6211\u8eab\u4e0a\u6240\u89c1\u8fc7\uff0c\u73b0\u5728\u53c8\u542c\u5230\u7684\u662f\u4e00\u6837\u7684\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6548\u6cd5\u57fa\u7763\u7684\u8c26\u5351\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5c31\u5e94\u5f53\u6709\u540c\u6837\u7684\u601d\u60f3\uff0c\u540c\u6837\u7684\u7231\u5fc3\uff0c\u8981\u5fc3\u5fd7\u76f8\u540c\uff0c\u601d\u60f3\u4e00\u81f4\uff0c\u4f7f\u6211\u5145\u6ee1\u559c\u4e50\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e0d\u8981\u81ea\u79c1\u81ea\u5229\uff0c\u4e5f\u4e0d\u8981\u8d2a\u56fe\u865a\u8363\uff0c\u53ea\u8981\u8c26\u5351\uff0c\u770b\u522b\u4eba\u6bd4\u81ea\u5df1\u5f3a\uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5404\u4eba\u4e0d\u8981\u5355\u987e\u81ea\u5df1\u7684\u4e8b\uff0c\u4e5f\u8981\u987e\u522b\u4eba\u7684\u4e8b\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u4eec\u5e94\u5f53\u6709\u8fd9\u6837\u7684\u610f\u5ff5\uff0c\u8fd9\u4e5f\u662f\u57fa\u7763\u8036\u7a23\u7684\u610f\u5ff5\u3002\uff08\u5168\u8282\u6216\u8bd1\uff1a\u201c\u4f60\u4eec\u5f53\u4ee5\u57fa\u7763\u8036\u7a23\u7684\u5fc3\u4e3a\u5fc3\u3002\u201d\uff09\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u672c\u6765\u6709\u3000\u795e\u7684\u5f62\u8c61\uff0c\u5374\u4e0d\u575a\u6301\u81ea\u5df1\u4e0e\u3000\u795e\u5e73\u7b49\u7684\u5730\u4f4d\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u53cd\u800c\u5012\u7a7a\u81ea\u5df1\uff0c\u53d6\u4e86\u5974\u4ec6\u7684\u5f62\u8c61\uff0c\u6210\u4e3a\u4eba\u7684\u6837\u5f0f\uff1b\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u65e2\u7136\u6709\u4eba\u7684\u6837\u5b50\uff0c\u5c31\u81ea\u7518\u5351\u5fae\uff0c\u987a\u670d\u81f3\u6b7b\uff0c\u800c\u4e14\u6b7b\u5728\u5341\u5b57\u67b6\u4e0a\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u56e0\u6b64\u3000\u795e\u628a\u4ed6\u5347\u4e3a\u81f3\u9ad8\uff0c\u5e76\u4e14\u8d50\u7ed9\u4ed6\u8d85\u8fc7\u4e07\u540d\u4e4b\u4e0a\u7684\u540d\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f7f\u5929\u4e0a\u3001\u5730\u4e0a\u548c\u5730\u5e95\u4e0b\u7684\u4e00\u5207\uff0c\u56e0\u7740\u8036\u7a23\u7684\u540d\uff0c\u90fd\u8981\u5c48\u819d\uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5e76\u4e14\u53e3\u91cc\u627f\u8ba4\u8036\u7a23\u57fa\u7763\u4e3a\u4e3b\uff0c\u4f7f\u8363\u8000\u5f52\u7ed9\u7236\u3000\u795e\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u663e\u51fa\u751f\u547d\u4e4b\u9053\u8fd9\u6837\u770b\u6765\uff0c\u6211\u6240\u4eb2\u7231\u7684\uff0c\u4f60\u4eec\u7d20\u6765\u662f\u987a\u670d\u7684\uff0c\uff08\u4e0d\u4f46\u6211\u5728\u4f60\u4eec\u90a3\u91cc\u7684\u65f6\u5019\u662f\u8fd9\u6837\uff0c\u73b0\u4eca\u6211\u4e0d\u5728\uff0c\u4f60\u4eec\u66f4\u8981\u987a\u670d\uff0c\uff09\u5c31\u5e94\u5f53\u6050\u60e7\u6218\u5162\u5730\u4f5c\u6210\u81ea\u5df1\u7684\u6551\u6069\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u3000\u795e\u4e3a\u4e86\u6210\u5168\u81ea\u5df1\u7684\u7f8e\u610f\uff0c\u5c31\u5728\u4f60\u4eec\u91cc\u9762\u52a8\u5de5\uff0c\u4f7f\u4f60\u4eec\u53ef\u4ee5\u7acb\u5fd7\u548c\u884c\u4e8b\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u65e0\u8bba\u4f5c\u4ec0\u4e48\uff0c\u90fd\u4e0d\u8981\u53d1\u6028\u8a00\u3001\u8d77\u4e89\u8bba\uff0c\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u597d\u4f7f\u4f60\u4eec\u65e0\u53ef\u6307\u6458\u3001\u7eaf\u771f\u65e0\u90aa\uff0c\u5728\u8fd9\u5f2f\u66f2\u4e56\u8c2c\u7684\u4e16\u4ee3\u4e2d\uff0c\u4f5c\u3000\u795e\u6ca1\u6709\u7455\u75b5\u7684\u513f\u5973\uff1b\u4f60\u4eec\u8981\u5728\u8fd9\u4e16\u4ee3\u4e2d\u53d1\u5149\uff0c\u597d\u50cf\u5929\u4e0a\u7684\u5149\u4f53\u4e00\u6837\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
