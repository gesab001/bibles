                "7": {
                    "verse_nr": 7,
                    "verse": "\u9521\u5b89\u7684\u8d35\u65cf\u7d20\u6765\u6bd4\u96ea\u66f4\u660e\u4eae\uff0c\u6bd4\u5976\u66f4\u6d01\u767d\uff1b\u4ed6\u4eec\u7684\u8eab\u4f53\u6bd4\u7ea2\u5b9d\u77f3\u66f4\u7ea2\u6da6\uff0c\u4ed6\u4eec\u7684\u6837\u8c8c\u50cf\u84dd\u5b9d\u77f3\u4e00\u6837\u7f8e\u4e3d\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u73b0\u5728\u4ed6\u4eec\u7684\u9762\u8c8c\u6bd4\u7164\u70ad\u8fd8\u9ed1\uff0c\u5728\u8857\u4e0a\u518d\u6ca1\u6709\u4eba\u8ba4\u5f97\u4ed6\u4eec\uff1b\u4ed6\u4eec\u76ae\u5305\u9aa8\u5934\uff0c\u67af\u7626\u5982\u67f4\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u90a3\u4e9b\u6b7b\u4e8e\u5200\u4e0b\u7684\u6bd4\u997f\u6b7b\u7684\u8fd8\u597d\uff1b\u997f\u6b7b\u7684\u56e0\u7f3a\u4e4f\u7530\u95f4\u7684\u51fa\u4ea7\uff0c\u5c31\u65e5\u6e10\u6d88\u7626\u800c\u6b7b\u4ea1\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6148\u5fc3\u7684\u5987\u4eba\u4eb2\u624b\u70f9\u716e\u81ea\u5df1\u6240\u751f\u7684\u5b69\u5b50\uff1b\u5728\u6211\u4f17\u6c11\uff08\u201c\u6211\u4f17\u6c11\u201d\u539f\u6587\u4f5c\u201c\u6211\u5b50\u6c11\u7684\u5973\u5b50\u201d\uff09\u906d\u6bc1\u706d\u7684\u65f6\u5019\uff0c\u5b69\u5b50\u7adf\u6210\u4e86\u6bcd\u4eb2\u7684\u98df\u7269\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8036\u548c\u534e\u5927\u53d1\u9707\u6012\uff0c\u503e\u5012\u4ed6\u7684\u70c8\u6012\uff1b\u4ed6\u5728\u9521\u5b89\u653e\u706b\uff0c\u541e\u706d\u4e86\u9521\u5b89\u7684\u6839\u57fa\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5730\u4e0a\u7684\u541b\u738b\uff0c\u4ee5\u53ca\u4e16\u4e0a\u6240\u6709\u7684\u5c45\u6c11\u90fd\u4e0d\u76f8\u4fe1\uff0c\u4f1a\u6709\u4efb\u4f55\u654c\u4eba\u653b\u8fdb\u8036\u8def\u6492\u51b7\u7684\u57ce\u95e8\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u60e9\u7f5a\u4e0d\u4e49\u7684\u9886\u8896\u56e0\u4e3a\u9521\u5b89\u5148\u77e5\u7684\u7f6a\u6076\uff0c\u548c\u796d\u53f8\u7684\u7f6a\u5b7d\uff0c\u5c31\u662f\u4ed6\u4eec\u5728\u57ce\u4e2d\u6d41\u4e86\u4e49\u4eba\u7684\u8840\uff0c\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6240\u4ee5\uff0c\u4ed6\u4eec\u50cf\u778e\u5b50\u5728\u8857\u4e0a\u6d41\u8361\uff0c\u4ed6\u4eec\u88ab\u8840\u73b7\u6c61\uff0c\u6ca1\u6709\u4eba\u53ef\u4ee5\u89e6\u6478\u4ed6\u4eec\u7684\u8863\u670d\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u6709\u4eba\u5411\u4ed6\u4eec\u558a\u53eb\uff1a\u201c\u8d70\u5f00\uff01\u4f60\u4eec\u4e0d\u6d01\u7684\u4eba\uff1b\u8d70\u5f00\uff01\u8d70\u5f00\uff01\u4e0d\u8981\u89e6\u6478\u6211\u3002\u201d\u4ed6\u4eec\u5230\u5904\u9003\u4ea1\u98d8\u6d41\u7684\u65f6\u5019\uff0c\u5217\u56fd\u7684\u4eba\u5c31\u8bf4\uff1a\u201c\u4ed6\u4eec\u518d\u4e0d\u80fd\u5728\u8fd9\u91cc\u5bc4\u5c45\u3002\u201d\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u8036\u548c\u534e\u4eb2\u4e34\u5206\u6563\u4ed6\u4eec\uff0c\u4e0d\u518d\u770b\u987e\u4ed6\u4eec\uff1b\u4eba\u4e0d\u518d\u5c0a\u91cd\u796d\u53f8\uff0c\u4e5f\u4e0d\u539a\u5f85\u957f\u8001\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u6211\u4eec\u5f92\u7136\u7b49\u5f85\u5e2e\u52a9\uff0c\u7b49\u5230\u53cc\u76ee\u5931\u53bb\u89c6\u529b\uff1b\u6211\u4eec\u5728\u77ad\u671b\u53f0\u4e0a\u671f\u671b\u4e00\u4e2a\u4e0d\u80fd\u62ef\u6551\u6211\u4eec\u7684\u56fd\u5bb6\u6765\u4e34\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u654c\u4eba\u8ffd\u8e2a\u6211\u4eec\uff0c\u4f7f\u6211\u4eec\u4e0d\u6562\u5728\u81ea\u5df1\u7684\u8857\u9053\u4e0a\u884c\u8d70\u3002\u6211\u4eec\u7684\u7ed3\u5c40\u8fd1\u4e86\uff0c\u6211\u4eec\u7684\u65e5\u5b50\u6ee1\u4e86\uff1b\u6211\u4eec\u7684\u7ed3\u5c40\u5df2\u7ecf\u5230\u4e86\u3002\n"
                },
                "19": {
