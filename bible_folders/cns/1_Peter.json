                "7": {
                    "verse_nr": 7,
                    "verse": "\u6309\u7167\u6069\u8d50\u5f7c\u6b64\u670d\u4e8b\u4e07\u7269\u7684\u7ed3\u5c40\u8fd1\u4e86\uff0c\u6240\u4ee5\u4f60\u4eec\u8981\u8c28\u614e\u8b66\u9192\u5730\u7977\u544a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6700\u91cd\u8981\u7684\u662f\u8981\u5f7c\u6b64\u5207\u5b9e\u76f8\u7231\uff0c\u56e0\u4e3a\u7231\u80fd\u906e\u76d6\u8bb8\u591a\u7684\u7f6a\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u4eec\u8981\u4e92\u76f8\u63a5\u5f85\uff0c\u4e0d\u53d1\u6028\u8a00\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f60\u4eec\u8981\u4f5c\u3000\u795e\u5404\u6837\u6069\u8d50\u7684\u597d\u7ba1\u5bb6\uff0c\u5404\u4eba\u7167\u7740\u6240\u9886\u53d7\u7684\u6069\u8d50\u5f7c\u6b64\u670d\u4e8b\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8bb2\u9053\u7684\uff0c\u5e94\u5f53\u6309\u7740\u3000\u795e\u7684\u5723\u8a00\u8bb2\uff1b\u670d\u4e8b\u7684\uff0c\u5e94\u5f53\u6309\u7740\u3000\u795e\u6240\u8d50\u7684\u529b\u91cf\u670d\u4e8b\uff0c\u4e3a\u8981\u5728\u4e00\u5207\u4e8b\u4e0a\uff0c\u4f7f\u3000\u795e\u501f\u8036\u7a23\u57fa\u7763\u5f97\u8363\u8000\u3002\u8363\u8000\u3001\u6743\u80fd\u90fd\u662f\u4ed6\u7684\uff0c\u76f4\u5230\u6c38\u6c38\u8fdc\u8fdc\u3002\u963f\u4eec\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4e3a\u57fa\u7763\u53d7\u82e6\u7684\u6709\u798f\u4e86\u4eb2\u7231\u7684\uff0c\u6709\u706b\u70bc\u7684\u8bd5\u9a8c\u4e34\u5230\u4f60\u4eec\uff0c\u4e0d\u8981\u4ee5\u4e3a\u5947\u602a\uff0c\u597d\u50cf\u662f\u906d\u9047\u975e\u5e38\u7684\u4e8b\uff0c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u5012\u8981\u6b22\u559c\uff0c\u56e0\u4e3a\u4f60\u4eec\u65e2\u7136\u5728\u57fa\u7763\u7684\u53d7\u82e6\u4e0a\u6709\u5206\uff0c\u5c31\u5728\u4ed6\u8363\u8000\u663e\u73b0\u7684\u65f6\u5019\uff0c\u53ef\u4ee5\u6b22\u559c\u5feb\u4e50\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u4eec\u8981\u662f\u4e3a\u57fa\u7763\u7684\u540d\u53d7\u8fb1\u9a82\uff0c\u5c31\u6709\u798f\u4e86\uff01\u56e0\u4e3a\u3000\u795e\u8363\u8000\u7684\u7075\uff0c\u4f4f\u5728\u4f60\u4eec\u8eab\u4e0a\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u4eec\u4e2d\u95f4\u4e0d\u53ef\u6709\u4eba\u56e0\u4e3a\u6740\u4eba\u3001\u6216\u5077\u7a83\u3001\u6216\u884c\u6076\u3001\u6216\u597d\u7ba1\u95f2\u4e8b\u800c\u53d7\u82e6\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5982\u679c\u56e0\u4e3a\u4f5c\u57fa\u7763\u5f92\u800c\u53d7\u82e6\uff0c\u4e0d\u8981\u4ee5\u4e3a\u7f9e\u803b\uff0c\u5012\u8981\u501f\u7740\u8fd9\u540d\u5b57\u8363\u8000\u3000\u795e\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u56e0\u4e3a\u5ba1\u5224\u4ece\u3000\u795e\u7684\u5bb6\u5f00\u59cb\uff0c\u5c31\u5728\u8fd9\u65f6\u5019\u4e86\u3002\u5982\u679c\u5148\u4ece\u6211\u4eec\u8d77\u5934\uff0c\u90a3\u4e0d\u4fe1\u4ece\u3000\u795e\u798f\u97f3\u7684\u4eba\uff0c\u7ed3\u5c40\u5c06\u8981\u600e\u6837\u5462\uff1f\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u201c\u5982\u679c\u4e49\u4eba\u4ec5\u4ec5\u5f97\u6551\uff0c\u4e0d\u656c\u8654\u548c\u72af\u7f6a\u7684\u4eba\uff0c\u53c8\u4f1a\u53d8\u6210\u600e\u6837\u5462\uff1f\u201d\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6240\u4ee5\u90a3\u987a\u7740\u3000\u795e\u7684\u65e8\u610f\u800c\u53d7\u82e6\u7684\u4eba\uff0c\u8981\u7ee7\u7eed\u5730\u884c\u5584\uff0c\u628a\u81ea\u5df1\u7684\u751f\u547d\u4ea4\u6258\u90a3\u4fe1\u5b9e\u7684\u521b\u9020\u8005\u3002\n"
                }
            }
        },
        "5": {
            "chapter_nr": 5,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u52a1\u8981\u7267\u517b\u3000\u795e\u7684\u7f8a\u7fa4\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u52a1\u8981\u7267\u517b\u4f60\u4eec\u4e2d\u95f4\u7684\u7f8a\u7fa4\uff0c\u6309\u7740\u3000\u795e\u7684\u65e8\u610f\u7167\u987e\u4ed6\u4eec\u3002\u4e0d\u662f\u51fa\u4e8e\u52c9\u5f3a\uff0c\u800c\u662f\u51fa\u4e8e\u7518\u5fc3\uff1b\u4e0d\u662f\u56e0\u4e3a\u8d2a\u8d22\uff0c\u800c\u662f\u51fa\u4e8e\u70ed\u8bda\uff1b\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e5f\u4e0d\u662f\u8981\u8f96\u5236\u6258\u4ed8\u4f60\u4eec\u7684\u7f8a\u7fa4\uff0c\u800c\u662f\u4f5c\u4ed6\u4eec\u7684\u699c\u6837\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5230\u4e86\u7267\u957f\u663e\u73b0\u7684\u65f6\u5019\uff0c\u4f60\u4eec\u5fc5\u5b9a\u5f97\u7740\u90a3\u6c38\u4e0d\u8870\u6b8b\u7684\u8363\u8000\u51a0\u5195\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7167\u6837\uff0c\u4f60\u4eec\u9752\u5e74\u4eba\u8981\u987a\u670d\u5e74\u957f\u7684\u3002\u5c31\u662f\u4f60\u4eec\u5404\u4eba\u4e5f\u8981\u5f7c\u6b64\u4ee5\u8c26\u5351\u4e3a\u88c5\u675f\uff0c\u56e0\u4e3a\u201c\u3000\u795e\u654c\u6321\u9a84\u50b2\u7684\u4eba\uff0c\u8d50\u6069\u7ed9\u8c26\u5351\u7684\u4eba\u3002\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6240\u4ee5\u4f60\u4eec\u8981\u8c26\u5351\uff0c\u670d\u5728\u3000\u795e\u5927\u80fd\u7684\u624b\u4e0b\uff0c\u5230\u4e86\u65f6\u5019\uff0c\u4ed6\u5fc5\u53eb\u4f60\u4eec\u5347\u9ad8\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u4eec\u8981\u628a\u4e00\u5207\u5fe7\u8651\u5378\u7ed9\u3000\u795e\uff0c\u56e0\u4e3a\u4ed6\u987e\u5ff5\u4f60\u4eec\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u4eec\u8981\u8c28\u5b88\u3001\u8b66\u9192\u3002\u4f60\u4eec\u7684\u4ec7\u654c\u9b54\u9b3c\uff0c\u597d\u50cf\u543c\u53eb\u7684\u72ee\u5b50\u8d70\u6765\u8d70\u53bb\uff0c\u5bfb\u627e\u53ef\u4ee5\u541e\u5403\u7684\u4eba\uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u4eec\u8981\u7528\u575a\u5f3a\u7684\u4fe1\u5fc3\u62b5\u6321\u4ed6\uff0c\u56e0\u4e3a\u77e5\u9053\u4f60\u4eec\u5728\u4e16\u4e0a\u7684\u5f1f\u5144\uff0c\u4e5f\u7ecf\u5386\u8fc7\u540c\u6837\u7684\u82e6\u96be\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f46\u6ee1\u6709\u6069\u5178\u7684\u3000\u795e\uff0c\u5c31\u662f\u5728\u57fa\u7763\u91cc\u53ec\u4f60\u4eec\u8fdb\u5165\u4ed6\u6c38\u8fdc\u8363\u8000\u7684\u90a3\u4e00\u4f4d\uff0c\u5728\u4f60\u4eec\u53d7\u4e86\u77ed\u6682\u7684\u82e6\u96be\u4e4b\u540e\uff0c\u5fc5\u5b9a\u4eb2\u81ea\u6210\u5168\u4f60\u4eec\uff0c\u575a\u56fa\u4f60\u4eec\uff0c\u8d50\u529b\u91cf\u7ed9\u4f60\u4eec\uff0c\u5efa\u7acb\u4f60\u4eec\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u613f\u6743\u80fd\u5f52\u7ed9\u4ed6\uff0c\u76f4\u5230\u6c38\u8fdc\u3002\u963f\u4eec\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u529d\u52c9\u548c\u795d\u798f\u6211\u501f\u7740\u6211\u8ba4\u4e3a\u5fe0\u5fc3\u7684\u5f1f\u5144\u897f\u62c9\uff0c\u7b80\u7565\u5730\u5199\u4e86\u8fd9\u5c01\u4fe1\u529d\u52c9\u4f60\u4eec\uff0c\u53c8\u5411\u4f60\u4eec\u8bc1\u5b9e\u8fd9\u662f\u3000\u795e\u771f\u6b63\u7684\u6069\u5178\uff1b\u4f60\u4eec\u8981\u5728\u8fd9\u6069\u5178\u4e0a\u7ad9\u7acb\u5f97\u4f4f\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u5728\u5df4\u6bd4\u4f26\u4e0e\u4f60\u4eec\u540c\u8499\u62e3\u9009\u7684\uff0c\u548c\u6211\u513f\u5b50\u9a6c\u53ef\u90fd\u95ee\u5019\u4f60\u4eec\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u4eec\u8981\u7528\u7231\u5fc3\u5f7c\u6b64\u4eb2\u5634\u95ee\u5b89\u3002\u613f\u5e73\u5b89\u5f52\u4e0e\u4f60\u4eec\u6240\u6709\u5728\u57fa\u7763\u91cc\u7684\u4eba\u3002\n"
                }
            }
        }
    }
}