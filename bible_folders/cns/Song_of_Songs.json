                    "verse": "\u6211\u4e0b\u5230\u6838\u6843\u56ed\u53bb\uff0c\u770b\u770b\u8c37\u4e2d\u9752\u7eff\u7684\u690d\u7269\uff0c\u770b\u770b\u8461\u8404\u6811\u53d1\u82bd\u6ca1\u6709\uff0c\u77f3\u69b4\u6811\u5f00\u82b1\u6ca1\u6709\uff1b\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4e0d\u77e5\u4e0d\u89c9\u4e4b\u95f4\uff0c\u6211\u7684\u5fc3\u628a\u6211\u7f6e\u4e8e\u6211\u5c0a\u8d35\u7684\u6c11\u7684\u8f66\u4e2d\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u56de\u6765\u5427\uff0c\u56de\u6765\u5427\uff0c\u4e66\u62c9\u5bc6\u5973\u554a\uff01\u56de\u6765\u5427\uff0c\u56de\u6765\u5427\uff0c\u8ba9\u6211\u4eec\u770b\u770b\u4f60\u3002\u4f60\u4eec\u4e3a\u4ec0\u4e48\u8981\u770b\u4e66\u62c9\u5bc6\u5973\uff0c\u50cf\u770b\u4e24\u961f\u5175\u8df3\u821e\u5462\uff1f\uff08\u672c\u8282\u5728\u300a\u9a6c\u7d22\u62c9\u6284\u672c\u300b\u4e3a7:1\uff09\n"
                }
            }
        },
        "7": {
            "chapter_nr": 7,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u518d\u79f0\u8d5e\u4f73\u5076\u4e30\u59ff\u79c0\u4e3d\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4f60\u7684\u809a\u8110\u5982\u5706\u676f\uff0c\u6c38\u4e0d\u7f3a\u4e4f\u8c03\u548c\u7684\u9152\uff1b\u4f60\u7684\u809a\u8179\u50cf\u4e00\u5806\u9ea6\u5b50\uff0c\u5468\u56f4\u6709\u767e\u5408\u82b1\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u7684\u4e24\u4e73\u50cf\u4e00\u5bf9\u5c0f\u9e7f\uff0c\u50cf\u53cc\u751f\u7684\u6bcd\u7f9a\u7f8a\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4f60\u7684\u9888\u9879\u5982\u8c61\u7259\u5854\uff1b\u4f60\u7684\u773c\u50cf\u5e0c\u5b9e\u672c\u57ce\u5185\uff0c\u5df4\u7279\uff0e\u62c9\u5e76\u95e8\u65c1\u7684\u6c34\u6c60\uff1b\u4f60\u7684\u9f3b\u5b50\u597d\u50cf\u9ece\u5df4\u5ae9\u7684\u9ad8\u5854\uff0c\u5c45\u9ad8\u4e34\u4e0b\u671d\u7740\u5927\u9a6c\u58eb\u9769\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u7684\u5934\u5728\u4f60\u8eab\u4e0a\u50cf\u8fe6\u5bc6\u5c71\uff1b\u4f60\u5934\u4e0a\u7684\u79c0\u53d1\u662f\u7d2b\u9ed1\u8272\u7684\uff1b\u541b\u738b\u7684\u5fc3\u7adf\u7ed9\u8fd9\u4e9b\u53d1\u7efa\u7cfb\u4f4f\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4eb2\u7231\u7684\u554a\uff01\u4f7f\u4eba\u559c\u60a6\u7684\u554a\uff01\u4f60\u591a\u4e48\u7f8e\u4e3d\uff0c\u591a\u4e48\u53ef\u7231\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u7684\u8eab\u91cf\u50cf\u4e00\u68f5\u68d5\u6811\uff1b\u4f60\u7684\u4e24\u4e73\u5982\u6811\u4e0a\u7d2f\u7d2f\u7684\u679c\u5b50\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u8bf4\uff1a\u201c\u6211\u8981\u722c\u4e0a\u8fd9\u68f5\u68d5\u6811\uff0c\u6211\u8981\u6293\u4f4f\u5b83\u7684\u679c\u5b50\u3002\u201d\u613f\u4f60\u7684\u4e24\u4e73\u5982\u4e00\u6302\u4e00\u6302\u7684\u8461\u8404\u4e32\uff1b\u4f60\u9f3b\u5b50\u7684\u9999\u6c14\u5982\u82f9\u679c\u82ac\u82b3\uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u7684\u53e3\u50cf\u7f8e\u9152\u3002\u4e66\u62c9\u5bc6\u5973\u8bf4\uff1a\u201c\u76f4\u6d41\u5165\u6211\u826f\u4eba\u7684\u53e3\u91cc\uff0c\u8f7b\u8f7b\u6d41\u8fc7\u5507\u9f7f\u4e4b\u95f4\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u5c5e\u6211\u7684\u826f\u4eba\uff0c\u4ed6\u4e5f\u604b\u6155\u6211\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6211\u7684\u826f\u4eba\u54ea\uff01\u6765\u5427\uff01\u8ba9\u6211\u4eec\u5f80\u7530\u95f4\u53bb\uff1b\u8ba9\u6211\u4eec\u5728\u51e4\u4ed9\u82b1\u4e1b\u4e2d\u8fc7\u591c\u5427\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u4eec\u65e9\u6668\u8d77\u6765\u5f80\u8461\u8404\u56ed\u53bb\uff0c\u53bb\u770b\u8461\u8404\u6811\u53d1\u82bd\u6ca1\u6709\uff0c\u82b1\u6735\u5f00\u653e\u6ca1\u6709\uff1b\u4e5f\u770b\u770b\u77f3\u69b4\u5f00\u82b1\u6ca1\u6709\u3002\u6211\u5728\u90a3\u91cc\u8981\u628a\u6211\u7684\u7231\u7ed9\u4f60\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u98ce\u8304\u6b63\u5728\u6563\u53d1\u9999\u6c14\uff1b\u5728\u6211\u4eec\u7684\u95e8\u65c1\u6709\u5404\u6837\u4f73\u7f8e\u7684\u679c\u5b50\uff0c\u65b0\u7684\u65e7\u7684\u90fd\u6709\u3002\u6211\u7684\u826f\u4eba\u54ea\uff0c\u8fd9\u4e9b\u90fd\u662f\u6211\u4e3a\u4f60\u4fdd\u5b58\u7684\u3002\n"
                }
            }
        },
        "8": {
            "chapter_nr": 8,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u7231\u7684\u529b\u91cf\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6211\u8981\u5f15\u9886\u4f60\uff0c\u9886\u4f60\u5230\u6211\u6bcd\u4eb2\u5bb6\u91cc\u53bb\uff1b\u4ece\u5c0f\u5979\u5c31\u6559\u5bfc\u6211\u3002\u6211\u8981\u7ed9\u4f60\u559d\u9999\u9152\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4ed6\u7684\u5de6\u624b\u6258\u4f4f\u6211\u7684\u5934\uff0c\u4ed6\u7684\u53f3\u624b\u62e5\u62b1\u7740\u6211\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u4f17\u5973\u5b50\u554a\uff01\u6211\u6073\u6c42\u4f60\u4eec\uff0c\u4e0d\u8981\u60ca\u52a8\u4ed6\uff0c\u4e0d\u8981\u6311\u8d77\u4ed6\u7684\u7231\u60c5\uff0c\u7b49\u5b83\u81ea\u53d1\u5427\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u90a3\u4ece\u65f7\u91ce\u4e0a\u6765\uff0c\u9760\u5728\u5979\u7231\u4eba\u8eab\u65c1\u7684\u662f\u8c01\u5462\uff1f\u6211\u5728\u82f9\u679c\u6811\u4e0b\u5524\u9192\u4e86\u4f60\uff1b\u5728\u90a3\u91cc\u4f60\u7684\u6bcd\u4eb2\u6000\u4e86\u4f60\uff1b\u5728\u90a3\u91cc\u751f\u80b2\u4f60\u7684\u4e3a\u4f60\u53d7\u751f\u4ea7\u7684\u75db\u82e6\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6c42\u4f60\u628a\u6211\u653e\u5728\u4f60\u5fc3\u4e0a\u5982\u5370\u8bb0\uff0c\u628a\u6211\u5e26\u5728\u4f60\u81c2\u4e0a\u5982\u6233\u5370\uff1b\u56e0\u4e3a\u7231\u60c5\u50cf\u6b7b\u4ea1\u4e00\u822c\u7684\u575a\u5f3a\uff1b\u5ac9\u5992\u5982\u9634\u95f4\u4e00\u822c\u7684\u575a\u7a33\uff1b\u5b83\u7684\u70c8\u7130\u662f\u706b\u7684\u70c8\u7130\uff0c\u662f\u975e\u5e38\u731b\u70c8\u7684\u706b\u7130\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u7231\u60c5\uff0c\u4f17\u6c34\u4e0d\u80fd\u7184\u706d\uff0c\u6d2a\u6d41\u4e0d\u80fd\u6df9\u6ca1\uff1b\u5982\u679c\u6709\u4eba\u62ff\u81ea\u5df1\u5bb6\u91cc\u6240\u6709\u7684\u8d22\u4ea7\u53bb\u6362\u53d6\u7231\u60c5\uff0c\u5c31\u5fc5\u88ab\u85d0\u89c6\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u4eec\u6709\u4e00\u4e2a\u5c0f\u59b9\u59b9\uff0c\u5979\u7684\u4e24\u4e73\u8fd8\u672a\u53d1\u80b2\u5b8c\u5168\uff1b\u6709\u4eba\u6765\u63d0\u4eb2\u7684\u65f6\u5019\uff0c\u6211\u4eec\u5f53\u4e3a\u5979\u600e\u6837\u529e\u7406\u5462\uff1f\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5979\u82e5\u662f\u5899\uff0c\u6211\u4eec\u5c31\u8981\u5728\u5979\u4e0a\u9762\u5efa\u9020\u94f6\u5854\uff1b\u5979\u82e5\u662f\u95e8\uff0c\u6211\u4eec\u5c31\u8981\u7528\u9999\u67cf\u6728\u677f\u56f4\u62a4\u5979\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u662f\u5899\uff0c\u6211\u7684\u4e24\u4e73\u50cf\u5176\u4e0a\u7684\u5854\uff1b\u6211\u5728\u4ed6\u7684\u773c\u4e2d\u50cf\u5bfb\u5f97\u5b89\u4e50\u7684\u4eba\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6240\u7f57\u95e8\u5728\u5df4\u529b\uff0e\u54c8\u4eec\u6709\u4e2a\u8461\u8404\u56ed\uff1b\u4ed6\u628a\u8fd9\u8461\u8404\u56ed\u79df\u7ed9\u770b\u5b88\u7684\u4eba\uff0c\u5404\u4eba\u8981\u4e3a\u8461\u8404\u56ed\u6240\u5f97\u7684\u679c\u5b50\u4ed8\u4e00\u4e07\u4e00\u5343\u56db\u767e\u514b\u94f6\u5b50\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u81ea\u5df1\u7684\u8461\u8404\u56ed\u5374\u5728\u6211\u9762\u524d\uff0c\u6240\u7f57\u95e8\u54ea\uff0c\u4e00\u4e07\u4e00\u5343\u56db\u767e\u514b\u5f52\u7ed9\u4f60\uff0c\u4e8c\u5343\u4e8c\u767e\u516b\u5341\u514b\u5f52\u7ed9\u90a3\u4e9b\u770b\u5b88\u679c\u5b50\u7684\u4eba\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u8fd9\u4f4f\u5728\u56ed\u4e2d\u7684\u554a\uff01\u6211\u7684\u540c\u4f34\u90fd\u5728\u7ec6\u542c\u4f60\u7684\u58f0\u97f3\uff1b\u613f\u4f60\u4f7f\u6211\u4e5f\u542c\u89c1\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6211\u7684\u826f\u4eba\u54ea\uff01\u613f\u4f60\u5feb\u6765\uff0c\u50cf\u7f9a\u7f8a\u6216\u5c0f\u9e7f\uff0c\u5728\u9999\u8349\u5c71\u4e0a\uff01\n"
                }
            }
        }
    }
