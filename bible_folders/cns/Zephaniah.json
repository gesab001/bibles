{
    "version": "cns",
    "book_name": "Zephaniah",
    "book_nr": "36",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u534e\u5ba3\u5e03\u5ba1\u5224\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a\u201c\u6211\u5fc5\u628a\u4e07\u7269\u4ece\u5730\u4e0a\u5b8c\u5168\u9664\u706d\u3002\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a\u201c\u6211\u5fc5\u9664\u706d\u4eba\u7c7b\u3001\u7272\u755c\u3001\u7a7a\u4e2d\u98de\u9e1f\u3001\u6d77\u91cc\u7684\u9c7c\u3002\u6211\u5fc5\u4f7f\u6076\u4eba\u8dcc\u5012\uff0c\u6211\u5fc5\u628a\u4eba\u7c7b\u4ece\u5730\u4e0a\u526a\u9664\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u5fc5\u4f38\u624b\u653b\u51fb\u72b9\u5927\uff0c\u653b\u51fb\u8036\u8def\u6492\u51b7\u6240\u6709\u7684\u5c45\u6c11\u3002\u6211\u5fc5\u4ece\u8fd9\u5730\u65b9\u9664\u6389\u5df4\u529b\u6240\u4f59\u4e0b\u7684\uff0c\u9664\u6389\u62dc\u5076\u50cf\u7684\u796d\u53f8\u7684\u540d\u5b57\uff0c\u4ee5\u53ca\u8036\u548c\u534e\u7684\u796d\u53f8\uff1b\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5c31\u662f\u90a3\u4e9b\u5728\u623f\u9876\u4e0a\u656c\u62dc\u5929\u4e0a\u4e07\u8c61\u7684\uff0c\u90a3\u4e9b\u656c\u62dc\u8036\u548c\u534e\uff0c\u6307\u7740\u4ed6\u8d77\u8a93\uff0c\u53c8\u6307\u7740\u739b\u52d2\u516c\u8d77\u8a93\u7684\uff0c\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u90a3\u4e9b\u8f6c\u79bb\u4e0d\u8ddf\u4ece\u8036\u548c\u534e\u7684\uff0c\u4e0d\u5bfb\u627e\u8036\u548c\u534e\uff0c\u4e5f\u4e0d\u6c42\u95ee\u4ed6\u7684\u3002\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u8036\u548c\u534e\u7684\u65e5\u5b50\u4f60\u8981\u5728\u6211\u4e3b\u8036\u548c\u534e\u9762\u524d\u8083\u9759\uff01\u56e0\u4e3a\u8036\u548c\u534e\u7684\u65e5\u5b50\u4e34\u8fd1\u4e86\uff0c\u56e0\u4e3a\u8036\u548c\u534e\u5df2\u7ecf\u9884\u5907\u4e86\u796d\u7269\uff0c\u628a\u81ea\u5df1\u6240\u8bf7\u6765\u7684\u4eba\u5206\u522b\u4e3a\u5723\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u201c\u5230\u4e86\u6211\u8036\u548c\u534e\u732e\u796d\u7684\u65e5\u5b50\uff0c\u6211\u5fc5\u60e9\u7f5a\u9996\u9886\u548c\u738b\u5b50\uff0c\u60e9\u7f5a\u6240\u6709\u7a7f\u5916\u65cf\u8863\u670d\u7684\u4eba\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5230\u90a3\u65e5\uff0c\u6211\u5fc5\u60e9\u7f5a\u6240\u6709\u8df3\u8fc7\u95e8\u69db\u7684\uff0c\u6240\u6709\u4f7f\u5f3a\u66b4\u548c\u6b3a\u8bc8\u5145\u6ee1\u4e3b\u4eba\u623f\u5c4b\u7684\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8036\u548c\u534e\u8bf4\uff1a\u201c\u5230\u90a3\u65e5\uff0c\u5fc5\u6709\u547c\u53eb\u7684\u58f0\u97f3\u4ece\u9c7c\u95e8\u53d1\u51fa\u6765\uff0c\u5fc5\u6709\u54c0\u53f7\u4ece\u65b0\u533a\u53d1\u51fa\u6765\uff0c\u5fc5\u6709\u5d29\u88c2\u5de8\u54cd\u4ece\u5c71\u95f4\u53d1\u51fa\u6765\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u739b\u9769\u63d0\u65bd\u7684\u5c45\u6c11\u54ea\uff01\u4f60\u4eec\u8981\u54c0\u53f7\uff0c\u56e0\u4e3a\u6240\u6709\u7684\u5546\u4eba\u90fd\u706d\u4ea1\u4e86\uff1b\u6240\u6709\u79f0\u94f6\u5b50\u7684\u90fd\u88ab\u526a\u9664\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u90a3\u65f6\uff0c\u6211\u5fc5\u63d0\u706f\u641c\u67e5\u8036\u8def\u6492\u51b7\uff0c\u60e9\u7f5a\u90a3\u4e9b\u6c89\u6dc0\u5728\u9152\u6e23\u4e0a\u7684\u4eba\u3002\u8fd9\u4e9b\u4eba\u5fc3\u91cc\u8bf4\uff1a\u2018\u8036\u548c\u534e\u5fc5\u4e0d\u8d50\u798f\uff0c\u4e5f\u4e0d\u964d\u7978\u3002\u2019\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4ed6\u4eec\u7684\u8d22\u5b9d\u5fc5\u6210\u4e3a\u63a0\u7269\uff0c\u4ed6\u4eec\u7684\u623f\u5c4b\u5fc5\u53d8\u4e3a\u5e9f\u589f\u3002\u4ed6\u4eec\u5efa\u9020\u623f\u5c4b\uff0c\u5374\u4e0d\u5f97\u4f4f\u5728\u5176\u4e2d\uff1b\u4ed6\u4eec\u683d\u690d\u8461\u8404\u56ed\uff0c\u5374\u4e0d\u5f97\u559d\u6240\u51fa\u7684\u9152\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u90a3\u65e5\u5b50\u7684\u60c5\u5f62\u8036\u548c\u534e\u7684\u5927\u65e5\u5b50\u4e34\u8fd1\u4e86\uff0c\u4e34\u8fd1\u4e86\uff0c\u6765\u5f97\u975e\u5e38\u5feb\u901f\u3002\u542c\uff01\u8036\u548c\u534e\u7684\u65e5\u5b50\u7684\u58f0\u97f3\u3002\u5728\u90a3\u65e5\uff0c\u52c7\u58eb\u8981\u54c0\u75db\u5730\u53f7\u53eb\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u90a3\u65e5\u662f\u5fff\u6012\u7684\u65e5\u5b50\uff0c\u662f\u56f0\u82e6\u8270\u96be\u7684\u65e5\u5b50\uff0c\u662f\u8352\u5e9f\u51c4\u51c9\u7684\u65e5\u5b50\uff0c\u662f\u9ed1\u6697\u5e7d\u51a5\u7684\u65e5\u5b50\uff0c\u662f\u5bc6\u4e91\u6f06\u9ed1\u7684\u65e5\u5b50\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u662f\u5439\u53f7\u5436\u558a\uff0c\u8981\u653b\u6253\u575a\u57ce\uff0c\u8fdb\u51fb\u57ce\u697c\u7684\u65e5\u5b50\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u6211\u8981\u4f7f\u4eba\u53d7\u56f0\u82e6\uff0c\u4ee5\u81f4\u4ed6\u4eec\u50cf\u778e\u5b50\u884c\u8d70\uff0c\u56e0\u4e3a\u4ed6\u4eec\u5f97\u7f6a\u8036\u548c\u534e\u3002\u4ed6\u4eec\u7684\u9c9c\u8840\u5fc5\u5012\u51fa\uff0c\u5982\u5012\u7070\u5c18\u4e00\u6837\uff0c\u4ed6\u4eec\u7684\u8eab\u4f53\u5fc5\u88ab\u9057\u5f03\u50cf\u7caa\u571f\u4e00\u822c\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5728\u8036\u548c\u534e\u53d1\u6012\u7684\u65e5\u5b50\uff0c\u4ed6\u4eec\u7684\u91d1\u94f6\u4e5f\u4e0d\u80fd\u6551\u4ed6\u4eec\uff0c\u5168\u5730\u90fd\u8981\u88ab\u4ed6\u7684\u5992\u706b\u541e\u706d\uff1b\u56e0\u4e3a\u4ed6\u8981\u5f7b\u5e95\u6bc1\u706d\u5730\u4e0a\u6240\u6709\u7684\u5c45\u6c11\uff0c\u771f\u662f\u53ef\u6015\u7684\u6bc1\u706d\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u529d\u53ca\u65f6\u6094\u6539\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8d81\u547d\u4ee4\u8fd8\u6ca1\u6709\u53d1\u51fa\uff0c\u65e5\u5b50\u8fd8\u6ca1\u6709\u50cf\u98ce\u524d\u7684\u7ce0\u79d5\u4e00\u822c\u5439\u8fc7\uff0c\u8036\u548c\u534e\u7684\u70c8\u6012\u8fd8\u6ca1\u6709\u4e34\u5230\u4f60\u4eec\uff0c\u8036\u548c\u534e\u5fff\u6012\u7684\u65e5\u5b50\u8fd8\u6ca1\u6709\u4e34\u5230\u4f60\u4eec\u4ee5\u524d\uff0c\u4f60\u4eec\u8981\u805a\u96c6\u8d77\u6765\uff01\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5730\u4e0a\u6240\u6709\u8c26\u5351\u7684\u4eba\uff0c\u5c31\u662f\u9075\u5b88\u8036\u548c\u534e\u5178\u7ae0\u7684\u4eba\u54ea\uff01\u4f60\u4eec\u8981\u5bfb\u6c42\u8036\u548c\u534e\uff0c\u4f60\u4eec\u8981\u5bfb\u6c42\u516c\u4e49\uff0c\u5bfb\u6c42\u8c26\u5351\u3002\u5728\u8036\u548c\u534e\u5fff\u6012\u7684\u65e5\u5b50\uff0c\u4f60\u4eec\u6216\u8bb8\u5f97\u4ee5\u9690\u85cf\u8d77\u6765\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5ba1\u5224\u975e\u5229\u58eb\u56e0\u4e3a\u8fe6\u8428\u5fc5\u88ab\u62cb\u5f03\uff0c\u4e9a\u5b9e\u57fa\u4f26\u5fc5\u6210\u8352\u573a\uff0c\u4e9a\u5b9e\u7a81\u7684\u5c45\u6c11\u5fc5\u5728\u664c\u5348\u88ab\u8d76\u8d70\uff1b\u4ee5\u9769\u4f26\u5fc5\u8fde\u6839\u62d4\u8d77\u6765\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f4f\u5728\u6cbf\u6d77\u5730\u5e26\u7684\u4eba\uff0c\u57fa\u5217\u63d0\u7684\u56fd\u6c11\u54ea\uff01\u4f60\u4eec\u6709\u7978\u4e86\u3002\u8fd9\u662f\u8036\u548c\u534e\u653b\u51fb\u4f60\u4eec\u7684\u8bdd\uff1a\u201c\u975e\u5229\u58eb\u4eba\u4e4b\u5730\u8fe6\u5357\u554a\uff01\u6211\u5fc5\u6bc1\u706d\u4f60\uff0c\u4f7f\u4f60\u90a3\u91cc\u6ca1\u6709\u4eba\u5c45\u4f4f\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6cbf\u6d77\u7684\u5730\u5fc5\u6210\u4e3a\u8349\u573a\u3001\u7267\u4eba\u7684\u6d1e\u7a74\u3001\u7f8a\u7fa4\u7684\u5708\u680f\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u8fd9\u5730\u5fc5\u5f52\u7ed9\u72b9\u5927\u5bb6\u7684\u4f59\u6c11\uff1b\u4ed6\u4eec\u8981\u5728\u90a3\u91cc\u653e\u7267\uff0c\u665a\u4e0a\u4ed6\u4eec\u8eba\u5367\u5728\u4e9a\u5b9e\u57fa\u4f26\u7684\u623f\u820d\u4e2d\uff1b\u56e0\u4e3a\u8036\u548c\u534e\u4ed6\u4eec\u7684\u3000\u795e\u5fc5\u7737\u987e\u4ed6\u4eec\uff0c\u4f7f\u4ed6\u4eec\u4ece\u88ab\u63b3\u4e4b\u5730\u5f52\u56de\u3002\u201d\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5ba1\u5224\u6469\u62bc\u4e0e\u4e9a\u626a\u6211\u542c\u89c1\u4e86\u6469\u62bc\u4eba\u7684\u8fb1\u9a82\u4e4b\u8a00\uff0c\u542c\u89c1\u4e86\u4e9a\u626a\u4eba\u7684\u6bc1\u8c24\u4e4b\u8bed\uff0c\u4ed6\u4eec\u8fb1\u9a82\u6211\u7684\u5b50\u6c11\uff0c\u53c8\u81ea\u5938\u81ea\u5927\u4fb5\u72af\u4ed6\u4eec\u7684\u5883\u754c\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u56e0\u6b64\uff0c\u4e07\u519b\u4e4b\u8036\u548c\u534e\u4ee5\u8272\u5217\u7684\u3000\u795e\u8bf4\uff1a\u201c\u6211\u6307\u7740\u6211\u7684\u6c38\u751f\u8d77\u8a93\uff1a\u6469\u62bc\u4eba\u5fc5\u8981\u50cf\u6240\u591a\u739b\uff0c\u4e9a\u626a\u4eba\u5fc5\u5b9a\u50cf\u86fe\u6469\u62c9\uff0c\u6210\u4e3a\u523a\u8349\u548c\u76d0\u5751\u4e4b\u6240\uff0c\u6c38\u8fdc\u8352\u51c9\u4e4b\u5730\u3002\u6211\u4f59\u4e0b\u7684\u5b50\u6c11\u5fc5\u63b3\u63a0\u4ed6\u4eec\uff0c\u6211\u56fd\u4e2d\u6240\u4f59\u7684\u8981\u5f97\u7740\u4ed6\u4eec\u4e3a\u4e1a\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u7531\u4e8e\u4ed6\u4eec\u7684\u72c2\u50b2\uff0c\u8fd9\u4e8b\u5c31\u4e34\u5230\u4ed6\u4eec\uff1b\u56e0\u4e3a\u4ed6\u4eec\u81ea\u5938\u81ea\u5927\uff0c\u8fb1\u9a82\u4e07\u519b\u4e4b\u8036\u548c\u534e\u7684\u5b50\u6c11\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5bf9\u4e8e\u4ed6\u4eec\uff0c\u8036\u548c\u534e\u662f\u53ef\u754f\u60e7\u7684\uff0c\u56e0\u4e3a\u4ed6\u8981\u6d88\u706d\u5730\u4e0a\u4e00\u5207\u795e\u7947\uff1b\u90a3\u65f6\uff0c\u5217\u56fd\u6d77\u5c9b\u4e0a\u7684\u5c45\u6c11\uff0c\u90fd\u5404\u5728\u81ea\u5df1\u7684\u5730\u65b9\u656c\u62dc\u4ed6\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5ba1\u5224\u53e4\u5b9e\u4e0e\u4e9a\u8ff0\u53e4\u5b9e\u4eba\u54ea\uff01\u4f60\u4eec\u4e5f\u5fc5\u88ab\u6211\u7684\u5200\u5251\u6240\u6740\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8036\u548c\u534e\u5fc5\u4f38\u624b\u653b\u51fb\u5317\u65b9\uff0c\u6bc1\u706d\u4e9a\u8ff0\uff0c\u4f7f\u5c3c\u5c3c\u5fae\u53d8\u6210\u8352\u573a\uff0c\u5e72\u65f1\u50cf\u65f7\u91ce\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u7fa4\u755c\u548c\u5404\u7c7b\u7684\u8d70\u517d\u5fc5\u8eba\u5367\u5728\u5176\u4e2d\uff1b\u9e48\u9e55\u548c\u7bad\u732a\u6816\u5bbf\u5728\u67f1\u9876\u4e0a\uff1b\u7a97\u5185\u6709\u9e23\u53eb\u7684\u58f0\u97f3\uff0c\u95e8\u69db\u4e0a\u5fc5\u6709\u8352\u51c9\uff1b\u56e0\u4e3a\u9999\u67cf\u6728\u90fd\u8981\u66b4\u9732\u51fa\u6765\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8fd9\u5c31\u662f\u90a3\u6b22\u4e50\u5b89\u5c45\u4e4b\u57ce\uff0c\u5979\u5fc3\u91cc\u66fe\u8bf4\uff1a\u201c\u53ea\u6709\u6211\uff0c\u9664\u6211\u4ee5\u5916\uff0c\u518d\u6ca1\u6709\u522b\u7684\u4e86\u3002\u201d\u73b0\u5728\u600e\u4e48\u7adf\u6210\u4e86\u8352\u573a\u3001\u8d70\u517d\u8eba\u5367\u4e4b\u5904\u5462\uff01\u51e1\u4ece\u90a3\u91cc\u7ecf\u8fc7\u7684\uff0c\u90fd\u5fc5\u821e\u624b\u55e4\u7b11\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6307\u6458\u8036\u8def\u6492\u51b7\u7684\u7f6a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5979\u4e0d\u542c\u8bdd\uff0c\u4e5f\u4e0d\u53d7\u7ba1\u6559\uff1b\u5979\u4e0d\u501a\u9760\u8036\u548c\u534e\uff0c\u4e5f\u4e0d\u4eb2\u8fd1\u5979\u7684\u3000\u795e\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u57ce\u4e2d\u7684\u9886\u8896\u90fd\u662f\u5486\u54ee\u7684\u72ee\u5b50\uff0c\u57ce\u4e2d\u7684\u5ba1\u5224\u5b98\u662f\u591c\u95f4\u7684\u8c7a\u72fc\uff0c\u4ec0\u4e48\u90fd\u4e0d\u7559\u5230\u65e9\u6668\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u57ce\u4e2d\u7684\u5148\u77e5\u90fd\u662f\u8f7b\u6d6e\u8be1\u8bc8\u7684\u4eba\uff1b\u57ce\u4e2d\u7684\u796d\u53f8\u4eb5\u6e0e\u5723\u7269\uff0c\u8fdd\u72af\u5f8b\u6cd5\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8036\u548c\u534e\u5728\u57ce\u4e2d\u662f\u516c\u4e49\u7684\uff0c\u51b3\u4e0d\u884c\u4e0d\u4e49\u7684\u4e8b\uff1b\u4ed6\u6bcf\u65e5\u65e9\u6668\u90fd\u663e\u660e\u516c\u6b63\uff0c\u6c38\u4e0d\u6b62\u606f\u3002\u4e0d\u516c\u4e49\u7684\u4eba\u5374\u4e0d\u77e5\u5ec9\u803b\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u8036\u548c\u534e\u5df2\u7ecf\u526a\u9664\u5217\u56fd\uff0c\u4ed6\u4eec\u7684\u57ce\u697c\u88ab\u6bc1\uff1b\u6211\u5df2\u7ecf\u4f7f\u4ed6\u4eec\u7684\u8857\u9053\u8352\u51c9\uff0c\u4ee5\u81f4\u6773\u65e0\u4eba\u8ff9\uff1b\u4ed6\u4eec\u7684\u57ce\u5e02\u8352\u5e9f\uff0c\u4ee5\u81f4\u65e0\u4eba\u5b58\u7559\uff0c\u65e0\u4eba\u5c45\u4f4f\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6211\u8bf4\uff1a\u201c\u5979\u5b9a\u8981\u656c\u754f\u6211\uff0c\u63a5\u53d7\u7ba1\u6559\u3002\u201d\u8fd9\u6837\uff0c\u5979\u7684\u4f4f\u6240\u5fc5\u4e0d\u4f1a\u7167\u7740\u6211\u539f\u5b9a\u5bf9\u5979\u7684\u60e9\u7f5a\u88ab\u526a\u9664\u3002\u53ef\u662f\u4ed6\u4eec\u6bb7\u52e4\u884c\u6076\uff0c\u8d25\u574f\u4e86\u81ea\u5df1\u4e00\u5207\u6240\u4f5c\u7684\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u8036\u8def\u6492\u51b7\u5fc5\u8499\u62ef\u6551\u8036\u548c\u534e\u8bf4\uff1a\u201c\u56e0\u6b64\uff0c\u4f60\u4eec\u8981\u7b49\u5019\u6211\uff0c\u7b49\u5019\u6211\u8d77\u6765\u6307\u8bc1\u7684\u65e5\u5b50\uff0c\u6211\u5df2\u7ecf\u51b3\u5b9a\u96c6\u5408\u4e07\u90a6\uff0c\u53ec\u805a\u5217\u56fd\uff0c\u628a\u6211\u7684\u76db\u6012\uff0c\u5c31\u662f\u6211\u7684\u4e00\u5207\u70c8\u6012\uff0c\u90fd\u503e\u5012\u5728\u4ed6\u4eec\u8eab\u4e0a\uff1b\u56e0\u4e3a\u5168\u5730\u90fd\u5fc5\u88ab\u6211\u7684\u5992\u706b\u541e\u706d\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u90a3\u65f6\u6211\u8981\u4f7f\u4e07\u6c11\u6709\u6d01\u51c0\u7684\u5634\u5507\uff0c\u4ed6\u4eec\u5168\u90fd\u53ef\u4ee5\u547c\u6c42\u8036\u548c\u534e\u7684\u540d\uff0c\u540c\u5fc3\u5408\u610f\u4e8b\u5949\u8036\u548c\u534e\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u656c\u62dc\u6211\u7684\u4eba\uff0c\u5c31\u662f\u6211\u6240\u5206\u6563\u7684\u4eba\uff0c\u5fc5\u4ece\u53e4\u5b9e\u6cb3\u5916\u800c\u6765\uff0c\u7ed9\u6211\u732e\u4e0a\u793c\u7269\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5230\u4e86\u90a3\u65e5\uff0c\u4f60\u5fc5\u4e0d\u56e0\u4f60\u80cc\u53db\u6211\u6240\u4f5c\u7684\u4e00\u5207\u611f\u5230\u7f9e\u6127\uff0c\u56e0\u4e3a\u90a3\u65f6\u6211\u5fc5\u4ece\u4f60\u4e2d\u95f4\u9664\u6389\u90a3\u4e9b\u81ea\u9ad8\u81ea\u5927\u7684\u4eba\u3002\u5728\u6211\u7684\u5723\u5c71\u4e0a\uff0c\u4f60\u5fc5\u4e0d\u518d\u9ad8\u50b2\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4f46\u6211\u8981\u5728\u4f60\u4e2d\u95f4\u7559\u4e0b\u8c26\u865a\u5351\u5fae\u7684\u4eba\uff1b\u4ed6\u4eec\u5fc5\u6295\u9760\u8036\u548c\u534e\u7684\u540d\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4ee5\u8272\u5217\u7684\u4f59\u6c11\u5fc5\u4e0d\u518d\u884c\u4e0d\u4e49\uff0c\u4e5f\u5fc5\u4e0d\u8bf4\u8c0e\uff1b\u4ed6\u4eec\u7684\u53e3\u91cc\u4e5f\u6ca1\u6709\u8be1\u8bc8\u7684\u820c\u5934\u3002\u4ed6\u4eec\u5403\u559d\u8eba\u5367\uff0c\u6ca1\u6709\u4eba\u60ca\u5413\u4ed6\u4eec\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6b22\u4e50\u4e4b\u6b4c\u9521\u5b89\u7684\u5c45\u6c11\uff08\u201c\u5c45\u6c11\u201d\u539f\u6587\u4f5c\u201c\u5973\u5b50\u201d\uff09\u54ea\uff01\u4f60\u4eec\u8981\u9ad8\u6b4c\u3002\u4ee5\u8272\u5217\u554a\uff01\u4f60\u4eec\u8981\u6b22\u547c\u3002\u8036\u8def\u6492\u51b7\u7684\u5c45\u6c11\u54ea\uff01\u4f60\u4eec\u8981\u6ee1\u5fc3\u6b22\u559c\u5feb\u4e50\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8036\u548c\u534e\u5df2\u7ecf\u9664\u53bb\u4f60\u4eec\u7684\u5211\u7f5a\uff0c\u8d76\u8d70\u4f60\u4eec\u7684\u4ec7\u654c\u3002\u4ee5\u8272\u5217\u7684\u738b\uff0c\u8036\u548c\u534e\u5728\u4f60\u4eec\u4e2d\u95f4\uff0c\u4f60\u4eec\u5fc5\u4e0d\u518d\u60e7\u6015\u707e\u7978\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5230\u4e86\u90a3\u65e5\uff0c\u5fc5\u6709\u4eba\u5bf9\u8036\u8def\u6492\u51b7\u8bf4\uff1a\u201c\u9521\u5b89\u54ea\uff01\u4e0d\u8981\u60e7\u6015\uff1b\u4f60\u7684\u624b\u4e0d\u8981\u4e0b\u5782\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8036\u548c\u534e\u4f60\u7684\u3000\u795e\u5728\u4f60\u4e2d\u95f4\uff0c\u4ed6\u662f\u65bd\u884c\u62ef\u6551\u7684\u5927\u80fd\u8005\uff0c\u5fc5\u56e0\u4f60\u6b22\u6b23\u5feb\u4e50\uff0c\u5fc5\u9ed8\u7136\u7231\u4f60\uff0c\u800c\u4e14\u5fc5\u56e0\u4f60\u559c\u4e50\u6b22\u5531\u3002\u201d\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u201c\u90a3\u4e9b\u5c5e\u4f60\uff0c\u4e3a\u4e86\u5207\u6155\u5927\u4f1a\u800c\u5fe7\u6101\u7684\u4eba\uff0c\u4ed6\u4eec\u62c5\u5f53\u4e86\u7f9e\u8fb1\uff1b\u6211\u5fc5\u62db\u805a\u4ed6\u4eec\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u770b\u54ea\uff01\u90a3\u65f6\u6211\u5fc5\u5bf9\u4ed8\u4e00\u5207\u82e6\u5f85\u4f60\u7684\u4eba\uff1b\u6211\u5fc5\u62ef\u6551\u90a3\u4e9b\u7638\u817f\u7684\uff0c\u805a\u96c6\u90a3\u4e9b\u88ab\u8d76\u6563\u7684\uff1b\u5728\u5168\u5730\u53d7\u7f9e\u8fb1\u7684\uff0c\u6211\u5fc5\u4f7f\u4ed6\u4eec\u5f97\u79f0\u8d5e\uff0c\u6709\u540d\u58f0\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u90a3\u65f6\u6211\u5fc5\u628a\u4f60\u4eec\u9886\u56de\uff0c\u90a3\u65f6\u6211\u5fc5\u628a\u4f60\u4eec\u9f50\u96c6\uff1b\u6211\u4f7f\u4f60\u4eec\u88ab\u63b3\u7684\u4eba\u5f52\u56de\u7684\u65f6\u5019\uff0c\u5fc5\u5728\u4f60\u4eec\u773c\u524d\uff0c\u53eb\u4f60\u4eec\u5728\u5730\u4e0a\u7684\u4e07\u6c11\u4e2d\u5f97\u79f0\u8d5e\uff0c\u6709\u540d\u58f0\u3002\u201d\u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002\n"
                }
            }
        }
    }
}