{
    "version": "cns",
    "book_name": "Joel",
    "book_nr": "29",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4e25\u91cd\u7684\u8757\u707e\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8001\u5e74\u4eba\u54ea\uff01\u4f60\u4eec\u8981\u542c\u8fd9\u8bdd\u3002\u8fd9\u5730\u6240\u6709\u7684\u5c45\u6c11\u554a\uff01\u4f60\u4eec\u8981\u7559\u5fc3\u542c\u3002\u5728\u4f60\u4eec\u7684\u65e5\u5b50\uff0c\u6216\u5728\u4f60\u4eec\u7956\u5148\u7684\u65e5\u5b50\uff0c\u66fe\u6709\u8fd9\u6837\u7684\u4e8b\u5417\uff1f\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u4eec\u8981\u628a\u8fd9\u4e8b\u4f20\u4e0e\u5b50\uff0c\u5b50\u4f20\u4e0e\u5b59\uff0c\u5b59\u4f20\u4e0e\u540e\u4ee3\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u526a\u866b\u5269\u4e0b\u7684\uff0c\u8757\u866b\u5403\u4e86\uff1b\u8757\u866b\u5269\u4e0b\u7684\uff0c\u877b\u5b50\u5403\u4e86\uff1b\u877b\u5b50\u5269\u4e0b\u7684\uff0c\u8682\u86b1\u5403\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9152\u9189\u7684\u4eba\u54ea\uff01\u4f60\u4eec\u8981\u9192\u8fc7\u6765\uff1b\u8981\u54c0\u54ed\uff01\u6240\u6709\u55dc\u9152\u7684\u4eba\u54ea\uff01\u4f60\u4eec\u90fd\u8981\u4e3a\u751c\u9152\u54c0\u53f7\uff0c\u56e0\u4e3a\u751c\u9152\u4ece\u4f60\u4eec\u7684\u53e3\u91cc\u65ad\u7edd\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u56e0\u4e3a\u6709\u4e00\u6c11\u65cf\u4e0a\u6765\u4fb5\u72af\u6211\u7684\u56fd\u571f\uff0c\u4ed6\u4eec\u5f3a\u5927\u53c8\u65e0\u6570\uff1b\u4ed6\u4eec\u7684\u7259\u9f7f\u50cf\u72ee\u5b50\u7684\u7259\u9f7f\uff0c\u4ed6\u4eec\u6709\u6bcd\u72ee\u7684\u5927\u7259\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u4eec\u4f7f\u6211\u7684\u8461\u8404\u6811\u8352\u51c9\uff0c\u6298\u65ad\u4e86\u6211\u7684\u65e0\u82b1\u679c\u6811\uff0c\u628a\u6811\u76ae\u5265\u5c3d\uff0c\u4e22\u5728\u4e00\u65c1\uff0c\u4f7f\u679d\u6761\u9732\u767d\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u529d\u4eba\u6c11\u54c0\u54ed\u4f60\u4eec\u8981\u54c0\u53f7\uff0c\u50cf\u5c11\u5973\u8170\u675f\u9ebb\u5e03\uff0c\u4e3a\u5979\u5e7c\u5e74\u65f6\u8bb8\u914d\u7684\u4e08\u592b\u54c0\u53f7\u4e00\u6837\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7d20\u796d\u548c\u5960\u796d\u90fd\u4ece\u8036\u548c\u534e\u7684\u6bbf\u91cc\u65ad\u7edd\u4e86\uff0c\u4e8b\u5949\u8036\u548c\u534e\u7684\u796d\u53f8\u4e5f\u90fd\u60b2\u54c0\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u7530\u91ce\u8352\u5e9f\uff0c\u571f\u5730\u51c4\u51c9\uff0c\u56e0\u4e3a\u4e94\u8c37\u6bc1\u574f\uff0c\u65b0\u9152\u5e72\u7aed\uff0c\u6cb9\u4e5f\u7f3a\u4e4f\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u519c\u592b\u554a\uff01\u4f60\u4eec\u8981\u4e3a\u5927\u9ea6\u548c\u5c0f\u9ea6\u7f9e\u6127\u3002\u4fee\u7406\u8461\u8404\u56ed\u7684\u554a\uff01\u4f60\u4eec\u8981\u54c0\u53f7\uff0c\u56e0\u4e3a\u7530\u95f4\u7684\u5e84\u7a3c\u90fd\u7834\u574f\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u8461\u8404\u6811\u67af\u5e72\uff0c\u65e0\u82b1\u679c\u6811\u8870\u6b8b\uff1b\u77f3\u69b4\u6811\u3001\u68d5\u6811\u3001\u82f9\u679c\u6811\uff0c\u7530\u91ce\u6240\u6709\u7684\u6811\u6728\u90fd\u67af\u5e72\uff0c\u56e0\u6b64\u6b22\u4e50\u4ece\u4eba\u95f4\u6d88\u5931\u4e86\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u547c\u5401\u4eba\u6c11\u7981\u98df\u7977\u544a\u796d\u53f8\u554a\uff01\u4f60\u4eec\u8981\u8170\u675f\u9ebb\u5e03\uff0c\u5e76\u4e14\u75db\u54ed\u3002\u5728\u796d\u575b\u4f8d\u5019\u7684\u554a\uff01\u4f60\u4eec\u8981\u54c0\u53f7\u3002\u4e8b\u5949\u6211\u7684\u3000\u795e\u7684\u554a\uff01\u4f60\u4eec\u62ab\u4e0a\u9ebb\u5e03\u8fdb\u53bb\u8fc7\u591c\u5427\uff0c\u56e0\u4e3a\u7d20\u796d\u548c\u5960\u796d\uff0c\u90fd\u4ece\u4f60\u4eec\u7684\u3000\u795e\u7684\u6bbf\u4e2d\u6b62\u606f\u4e86\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u4eec\u8981\u628a\u7981\u98df\u7684\u65e5\u5b50\u5206\u522b\u4e3a\u5723\uff0c\u53ec\u5f00\u4e25\u8083\u4f1a\uff0c\u805a\u96c6\u4f17\u957f\u8001\u548c\u56fd\u4e2d\u6240\u6709\u7684\u5c45\u6c11\uff0c\u5230\u4f60\u4eec\u7684\u3000\u795e\u8036\u548c\u534e\u7684\u6bbf\u91cc\uff0c\u5411\u8036\u548c\u534e\u547c\u6c42\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u54c0\u54c9\u90a3\u65e5\uff01\u56e0\u4e3a\u8036\u548c\u534e\u7684\u65e5\u5b50\u4e34\u8fd1\u4e86\uff1b\u90a3\u65e5\u6765\u5230\uff0c\u597d\u50cf\u6bc1\u706d\u4ece\u5168\u80fd\u8005\u4e34\u5230\u4e00\u6837\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u7cae\u98df\u4e0d\u662f\u5728\u6211\u4eec\u7684\u773c\u524d\u65ad\u7edd\u4e86\u5417\uff1f\u6b22\u559c\u4e0e\u5feb\u4e50\u4e0d\u662f\u4ece\u6211\u4eec\u7684\u3000\u795e\u7684\u6bbf\u4e2d\u6b62\u606f\u4e86\u5417\uff1f\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8c37\u79cd\u5728\u571f\u5757\u5e95\u4e0b\u673d\u70c2\uff0c\u4ed3\u5e93\u8352\u51c9\uff0c\u8c37\u4ed3\u7834\u70c2\u88ab\u62c6\u6bc1\uff0c\u56e0\u4e3a\u4e94\u8c37\u90fd\u67af\u5e72\u4e86\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u7272\u755c\u53d1\u51fa\u54c0\u9e23\uff0c\u725b\u7fa4\u5230\u5904\u6d41\u8361\uff0c\u56e0\u4e3a\u6ca1\u6709\u8349\u573a\u7ed9\u5b83\u4eec\uff0c\u7f8a\u7fa4\u4e5f\u540c\u6837\u53d7\u82e6\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8036\u548c\u534e\u554a\uff01\u6211\u5411\u4f60\u547c\u6c42\uff0c\u56e0\u4e3a\u706b\u541e\u706d\u4e86\u65f7\u91ce\u7684\u8349\u573a\uff0c\u706b\u7130\u70e7\u5c3d\u4e86\u7530\u95f4\u6240\u6709\u7684\u6811\u6728\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u7530\u91ce\u7684\u8d70\u517d\u4e5f\u5411\u4f60\u53d1\u5598\uff0c\u56e0\u4e3a\u6eaa\u6c34\u90fd\u5e72\u6db8\u4e86\uff0c\u706b\u541e\u706d\u4e86\u65f7\u91ce\u7684\u8349\u573a\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u534e\u7684\u65e5\u5b50\u5feb\u6765\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u90a3\u65e5\u662f\u9ed1\u6697\u5e7d\u51a5\u7684\u65e5\u5b50\uff0c\u662f\u5bc6\u4e91\u6f06\u9ed1\u7684\u65e5\u5b50\u3002\u6709\u4e00\u961f\u4f17\u591a\u5f3a\u76db\u7684\u6c11\u6765\u5230\uff0c\u597d\u50cf\u6668\u5149\u6ee1\u5e03\u7fa4\u5c71\uff1b\u8fd9\u6837\u7684\u4e8b\u4ece\u524d\u6ca1\u6709\u53d1\u751f\u8fc7\uff0c\u4ee5\u540e\u76f4\u5230\u4e07\u4ee3\uff0c\u4e5f\u4e0d\u4f1a\u53d1\u751f\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5728\u4ed6\u4eec\u524d\u9762\u6709\u541e\u706d\u7684\u706b\uff0c\u5728\u4ed6\u4eec\u540e\u9762\u6709\u71c3\u70e7\u7684\u706b\u7130\uff1b\u4ed6\u4eec\u672a\u5230\u4ee5\u524d\uff0c\u5927\u5730\u50cf\u4f0a\u7538\u56ed\uff0c\u4ed6\u4eec\u8fc7\u53bb\u4ee5\u540e\uff0c\u5927\u5730\u6210\u4e86\u8352\u51c9\u7684\u65f7\u91ce\uff0c\u6ca1\u6709\u4e00\u6837\u80fd\u9003\u907f\u4ed6\u4eec\u7684\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u4eec\u7684\u5f62\u72b6\u50cf\u9a6c\u7684\u5f62\u72b6\uff0c\u6218\u9a6c\u600e\u6837\u5954\u8dd1\uff0c\u4ed6\u4eec\u4e5f\u600e\u6837\u5954\u8dd1\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4ed6\u4eec\u5728\u7fa4\u5c71\u9876\u4e0a\u8df3\u8dc3\u7684\u58f0\u97f3\uff0c\u5982\u6218\u8f66\u7684\u54cd\u58f0\uff0c\u53c8\u5982\u706b\u7130\u541e\u706d\u788e\u79f8\u7684\u54cd\u58f0\uff0c\u50cf\u5f3a\u76db\u7684\u6c11\u6446\u9635\u9884\u5907\u4f5c\u6218\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5728\u4ed6\u4eec\u9762\u524d\uff0c\u4e07\u6c11\u82e6\u607c\uff0c\u4eba\u4eba\u8138\u7ea2\u8033\u8d64\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u4eec\u5954\u8dd1\u50cf\u52c7\u58eb\uff0c\u53c8\u50cf\u6218\u58eb\u722c\u4e0a\u57ce\u5899\uff1b\u4ed6\u4eec\u5404\u6309\u81ea\u5df1\u7684\u8def\u524d\u884c\uff0c\u5e76\u4e0d\u504f\u79bb\u8def\u7ebf\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ed6\u4eec\u5f7c\u6b64\u4e0d\u62e5\u6324\uff0c\u5404\u6309\u81ea\u5df1\u7684\u9053\u524d\u884c\uff1b\u751a\u81f3\u4ed6\u4eec\u6301\u7740\u5175\u5668\u8dcc\u5012\uff0c\u4e5f\u4e0d\u4f1a\u88ab\u526a\u9664\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u4eec\u51b2\u5165\u57ce\u9547\uff0c\u5954\u4e0a\u57ce\u5899\uff0c\u722c\u8fdb\u623f\u5c4b\uff0c\u50cf\u76d7\u8d3c\u4e00\u6837\u4ece\u7a97\u6237\u722c\u8fdb\u53bb\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5728\u4ed6\u4eec\u9762\u524d\uff0c\u5927\u5730\u9707\u52a8\uff0c\u8bf8\u5929\u6447\u64bc\uff0c\u65e5\u6708\u660f\u6697\uff0c\u661f\u661f\u65e0\u5149\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8036\u548c\u534e\u5728\u4ed6\u7684\u519b\u961f\u524d\u5934\u53d1\u51fa\u58f0\u97f3\uff1b\u4ed6\u7684\u961f\u4f0d\u6781\u5927\uff0c\u6210\u5c31\u4ed6\u8bdd\u8bed\u7684\uff0c\u662f\u5f3a\u76db\u7684\u4eba\u3002\u8036\u548c\u534e\u7684\u65e5\u5b50\u4f1f\u5927\uff0c\u975e\u5e38\u53ef\u754f\uff0c\u8c01\u80fd\u5fcd\u53d7\u5f97\u4f4f\u5462\uff1f\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u547c\u5401\u4eba\u6c11\u6094\u6539\u5f52\u5411\u3000\u795e\u8036\u548c\u534e\u8bf4\uff1a\u201c\u73b0\u5728\u867d\u7136\u5982\u6b64\uff0c\u4f60\u4eec\u4ecd\u8981\u5168\u5fc3\u5168\u610f\uff0c\u4ee5\u7981\u98df\u3001\u54ed\u6ce3\u3001\u54c0\u53f7\uff0c\u5f52\u5411\u6211\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u4eec\u8981\u6495\u88c2\u4f60\u4eec\u7684\u5fc3\u80a0\uff0c\u4e0d\u8981\u6495\u88c2\u4f60\u4eec\u7684\u8863\u670d\u3002\u5e76\u8981\u5f52\u5411\u8036\u548c\u534e\u4f60\u4eec\u7684\u3000\u795e\uff0c\u56e0\u4e3a\u4ed6\u6709\u6069\u5178\u6709\u601c\u60af\uff0c\u4e0d\u8f7b\u6613\u53d1\u6012\uff0c\u5e76\u4e14\u6709\u4e30\u76db\u7684\u6148\u7231\uff0c\u968f\u65f6\u8f6c\u610f\u4e0d\u964d\u707e\u7978\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6709\u8c01\u77e5\u9053\u6216\u8bb8\u4ed6\u4f1a\u8f6c\u610f\uff0c\u5e76\u4e14\u65bd\u6148\u7231\uff0c\u7559\u4e0b\u4f59\u798f\uff0c\u5c31\u662f\u7559\u4e0b\u732e\u7ed9\u8036\u548c\u534e\u4f60\u4eec\u7684\u3000\u795e\u7684\u7d20\u796d\u548c\u5960\u796d\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u4eec\u8981\u5728\u9521\u5b89\u5439\u53f7\u89d2\uff0c\u628a\u7981\u98df\u7684\u65e5\u5b50\u5206\u522b\u4e3a\u5723\uff0c\u53ec\u5f00\u4e25\u8083\u4f1a\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f60\u4eec\u8981\u53ec\u96c6\u6c11\u4f17\uff0c\u4f7f\u4f1a\u4f17\u5206\u522b\u4e3a\u5723\uff1b\u62db\u805a\u8001\u4eba\uff0c\u805a\u96c6\u5b69\u7ae5\uff0c\u548c\u5728\u6000\u4e2d\u5403\u5976\u7684\u5a74\u5b69\uff1b\u8ba9\u65b0\u90ce\u4ece\u65b0\u623f\u51fa\u6765\uff0c\u65b0\u5987\u4ece\u5185\u5ba4\u51fa\u6765\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8ba9\u4e8b\u5949\u8036\u548c\u534e\u7684\u796d\u53f8\u5728\u5723\u6bbf\u7684\u5eca\u5b50\u548c\u796d\u575b\u4e4b\u95f4\u54ed\u6ce3\uff0c\u8ba9\u4ed6\u4eec\u8bf4\uff1a\u201c\u8036\u548c\u534e\u554a\uff01\u6c42\u4f60\u601c\u6064\u4f60\u7684\u5b50\u6c11\u3002\u4e0d\u8981\u4f7f\u4f60\u7684\u4ea7\u4e1a\u53d7\u7f9e\u8fb1\uff0c\u4f7f\u4ed6\u4eec\u5728\u5217\u56fd\u4e2d\u6210\u4e3a\u7b11\u8c08\u3002\u4e3a\u4ec0\u4e48\u5bb9\u4eba\u5728\u4e07\u6c11\u4e2d\u8ba5\u7b11\u8bf4\uff1a\u2018\u4ed6\u4eec\u7684\u3000\u795e\u5728\u54ea\u91cc\u5462\uff1f\u2019\u201d\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u518d\u8d50\u6069\u798f\u4e8e\u662f\u8036\u548c\u534e\u4e3a\u4e86\u81ea\u5df1\u7684\u5730\u53d1\u70ed\u5fc3\uff0c\u601c\u6064\u4ed6\u7684\u5b50\u6c11\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8036\u548c\u534e\u5e94\u5141\u4ed6\u7684\u5b50\u6c11\uff0c\u5bf9\u4ed6\u4eec\u8bf4\uff1a\u201c\u770b\u54ea\uff01\u6211\u5fc5\u8d50\u7ed9\u4f60\u4eec\u4e94\u8c37\u3001\u65b0\u9152\u548c\u65b0\u6cb9\uff0c\u4f60\u4eec\u5fc5\u5403\u5f97\u9971\u8db3\uff0c\u6211\u5fc5\u4e0d\u518d\u4f7f\u4f60\u4eec\u6210\u4e3a\u5217\u56fd\u4e2d\u7684\u7f9e\u8fb1\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u6211\u8981\u4f7f\u5317\u65b9\u6765\u7684\u519b\u961f\u8fdc\u79bb\u4f60\u4eec\uff1b\u6211\u8981\u628a\u4ed6\u4eec\u9a71\u9010\u5230\u5e72\u65f1\u8352\u51c9\u4e4b\u5730\uff0c\u4ed6\u4eec\u7684\u524d\u961f\u8d76\u5165\u4e1c\u6d77\uff0c\u4ed6\u4eec\u7684\u540e\u961f\u8d76\u5165\u897f\u6d77\uff1b\u5e76\u4e14\u4ed6\u4eec\u7684\u81ed\u6c14\u4e0a\u5347\uff0c\u4ed6\u4eec\u7684\u8165\u5473\u4e0a\u817e\uff0c\u56e0\u4e3a\u8036\u548c\u534e\u884c\u4e86\u5927\u4e8b\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u5730\u571f\u554a\uff01\u4f60\u4e0d\u8981\u60e7\u6015\uff1b\u4f60\u8981\u6b22\u559c\u5feb\u4e50\uff0c\u56e0\u4e3a\u8036\u548c\u534e\u884c\u4e86\u5927\u4e8b\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u7530\u91ce\u7684\u8d70\u517d\u554a\uff0c\u4f60\u4eec\u4e0d\u8981\u60e7\u6015\uff0c\u56e0\u4e3a\u65f7\u91ce\u7684\u8349\u573a\u53c8\u53d1\u9752\u4e86\uff0c\u6811\u6728\u53c8\u7ed3\u679c\u5b50\u4e86\uff0c\u65e0\u82b1\u679c\u6811\u548c\u8461\u8404\u6811\u4e5f\u90fd\u6548\u529b\u4e86\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u9521\u5b89\u7684\u4eba\u6c11\u54ea\uff01\u4f60\u4eec\u8981\u6b22\u559c\uff0c\u8981\u9760\u8036\u548c\u534e\u4f60\u4eec\u7684\u3000\u795e\u5feb\u4e50\uff0c\u56e0\u4e3a\u4ed6\u8d50\u7ed9\u4e86\u4f60\u4eec\u5408\u65f6\u7684\u79cb\u96e8\u3002\u4ed6\u7ed9\u4f60\u4eec\u964d\u4e0b\u65f6\u96e8\uff0c\u5c31\u662f\u79cb\u96e8\u6625\u96e8\uff0c\u50cf\u4ee5\u524d\u4e00\u6837\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u79be\u573a\u4e0a\u5fc5\u6ee1\u4e86\u4e94\u8c37\uff1b\u69a8\u9152\u6c60\u4e0e\u6cb9\u91a1\u5fc5\u6ea2\u51fa\u65b0\u9152\u4e0e\u65b0\u6cb9\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u6211\u5dee\u9063\u5230\u4f60\u4eec\u4e2d\u95f4\u53bb\u7684\uff0c\u6211\u7684\u5927\u519b\u961f\uff0c\u5c31\u662f\u8757\u866b\u3001\u877b\u5b50\u3001\u8682\u86b1\u548c\u526a\u866b\uff0c\u5728\u90a3\u4e9b\u5e74\u95f4\u5403\u5c3d\u7684\uff0c\u6211\u5fc5\u8865\u7ed9\u4f60\u4eec\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u4f60\u4eec\u5fc5\u5403\u5f97\u4e30\u5bcc\uff0c\u5e76\u4e14\u9971\u8db3\uff0c\u4f60\u4eec\u5c31\u8d5e\u7f8e\u8036\u548c\u534e\u4f60\u4eec\u7684\u3000\u795e\u7684\u540d\uff0c\u56e0\u4e3a\u4ed6\u4e3a\u4f60\u4eec\u884c\u4e86\u5947\u5999\u7684\u4e8b\u3002\u6211\u7684\u5b50\u6c11\u5fc5\u6c38\u8fdc\u4e0d\u4f1a\u7f9e\u6127\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u8fd9\u6837\uff0c\u4f60\u4eec\u5fc5\u77e5\u9053\u6211\u662f\u5728\u4ee5\u8272\u5217\u4e2d\u95f4\uff0c\u53c8\u77e5\u9053\u6211\u662f\u8036\u548c\u534e\u4f60\u4eec\u7684\u3000\u795e\uff0c\u9664\u4e86\u6211\u518d\u6ca1\u6709\u522b\u7684\u795e\u3002\u6211\u7684\u5b50\u6c11\u5fc5\u6c38\u8fdc\u4e0d\u4f1a\u7f9e\u6127\u3002\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\u5e94\u8bb8\u8d50\u4e0b\u5723\u7075\u201c\u4ee5\u540e\uff0c\u6211\u8981\u628a\u6211\u7684\u7075\u6d47\u704c\u6240\u6709\u7684\u4eba\u3002\u4f60\u4eec\u7684\u513f\u5973\u8981\u8bf4\u9884\u8a00\uff0c\u4f60\u4eec\u7684\u8001\u5e74\u4eba\u8981\u4f5c\u5f02\u68a6\uff0c\u4f60\u4eec\u7684\u5c11\u5e74\u4eba\u8981\u89c1\u5f02\u8c61\u3002\n"
                },
                "29": {
                    "verse_nr": 29,
                    "verse": "\u5728\u90a3\u4e9b\u65e5\u5b50\uff0c\u6211\u4e5f\u8981\u628a\u6211\u7684\u7075\u6d47\u704c\u6211\u7684\u4ec6\u4eba\u548c\u5a62\u5973\u3002\n"
                },
                "30": {
                    "verse_nr": 30,
                    "verse": "\u6211\u8981\u5728\u5929\u4e0a\u5730\u4e0b\u663e\u51fa\u795e\u8ff9\u5947\u4e8b\uff0c\u6709\u8840\u3001\u6709\u706b\u3001\u6709\u70df\u67f1\u3002\n"
                },
                "31": {
                    "verse_nr": 31,
                    "verse": "\u592a\u9633\u5c06\u53d8\u4e3a\u9ed1\u6697\uff0c\u6708\u4eae\u5c06\u53d8\u4e3a\u8840\u7ea2\u3002\u5728\u8036\u548c\u534e\u4f1f\u5927\u53ef\u754f\u7684\u65e5\u5b50\u4e34\u5230\u4ee5\u524d\uff0c\u8fd9\u4e00\u5207\u90fd\u8981\u53d1\u751f\u3002\n"
                },
                "32": {
                    "verse_nr": 32,
                    "verse": "\u90a3\u65f6\uff0c\u6c42\u544a\u8036\u548c\u534e\u7684\u540d\u7684\uff0c\u90fd\u5fc5\u5f97\u6551\uff1b\u56e0\u4e3a\u6b63\u5982\u8036\u548c\u534e\u6240\u8bf4\u7684\uff0c\u5728\u9521\u5b89\u5c71\u548c\u8036\u8def\u6492\u51b7\u5fc5\u6709\u5f97\u6551\u7684\u4eba\uff0c\u5728\u52ab\u540e\u4f59\u751f\u7684\u4eba\u4e2d\uff0c\u5fc5\u6709\u8036\u548c\u534e\u6240\u547c\u53ec\u7684\u3002\u201d\uff08\u672c\u7ae0\u7b2c28-32\u8282\u5728\u300a\u9a6c\u7d22\u62c9\u6284\u672c\u300b\u4e3a3:1-5\uff09\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5ba1\u5224\u4e07\u56fd\u201c\u770b\u54ea\uff01\u5230\u4e86\u90a3\u4e9b\u65e5\u5b50\uff0c\u5728\u90a3\u4e2a\u65f6\u671f\uff0c\u6211\u4f7f\u72b9\u5927\u548c\u8036\u8def\u6492\u51b7\u88ab\u63b3\u7684\u4eba\u5f52\u56de\uff0c\uff08\u672c\u8282\u5728\u300a\u9a6c\u7d22\u62c9\u6284\u672c\u300b\u4e3a4:1\uff09\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6211\u5fc5\u62db\u805a\u5217\u56fd\uff0c\u9886\u4ed6\u4eec\u4e0b\u5230\u7ea6\u6c99\u6cd5\u8c37\uff0c\u4e3a\u4e86\u6211\u7684\u5b50\u6c11\uff0c\u5c31\u662f\u6211\u7684\u4ea7\u4e1a\u4ee5\u8272\u5217\uff0c\u6211\u8981\u5728\u90a3\u91cc\u4eb2\u4e34\u5ba1\u5224\u4ed6\u4eec\uff0c\u56e0\u4e3a\u4ed6\u4eec\u628a\u6211\u7684\u5b50\u6c11\u5206\u6563\u5728\u5217\u56fd\u4e2d\uff0c\u53c8\u5206\u53d6\u4e86\u6211\u7684\u5730\u571f\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4ed6\u4eec\u4e3a\u6211\u7684\u5b50\u6c11\u62bd\u7b7e\uff0c\u7528\u7537\u7ae5\u4ea4\u6362\u5993\u5973\uff0c\u5973\u7ae5\u88ab\u5356\u6362\u9152\u559d\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u201c\u63a8\u7f57\u3001\u897f\u987f\u548c\u975e\u5229\u58eb\u56db\u5883\u7684\u4eba\u54ea\uff01\u4f60\u4eec\u60f3\u5bf9\u6211\u4f5c\u4ec0\u4e48\u5462\uff1f\u4f60\u4eec\u60f3\u5411\u6211\u62a5\u590d\u5417\uff1f\u4f60\u4eec\u82e5\u5411\u6211\u62a5\u590d\uff0c\u6211\u5f88\u5feb\u5c31\u4f7f\u62a5\u5e94\u5f52\u5728\u4f60\u4eec\u7684\u5934\u4e0a\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u4eec\u62ff\u53bb\u4e86\u6211\u7684\u91d1\u94f6\uff0c\u53c8\u628a\u6211\u7684\u73cd\u5b9d\u5e26\u8fdb\u4e86\u4f60\u4eec\u7684\u5e99\u5b87\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u4eec\u628a\u72b9\u5927\u4eba\u548c\u8036\u8def\u6492\u51b7\u4eba\u5356\u7ed9\u5e0c\u814a\u4eba\uff0c\u4f7f\u4ed6\u4eec\u8fdc\u79bb\u81ea\u5df1\u7684\u5883\u754c\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u770b\u54ea\uff01\u6211\u8981\u6fc0\u52a8\u4ed6\u4eec\u79bb\u5f00\u88ab\u4f60\u4eec\u5356\u5230\u7684\u5730\u65b9\uff0c\u6211\u4e5f\u8981\u4f7f\u62a5\u5e94\u5f52\u5230\u4f60\u4eec\u7684\u5934\u4e0a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u5fc5\u628a\u4f60\u4eec\u7684\u513f\u5973\u5356\u5728\u72b9\u5927\u4eba\u7684\u624b\u4e2d\uff0c\u72b9\u5927\u4eba\u5fc5\u628a\u4ed6\u4eec\u5356\u7ed9\u8fdc\u65b9\u793a\u5df4\u56fd\u7684\u4eba\uff0c\u56e0\u4e3a\u8fd9\u662f\u8036\u548c\u534e\u8bf4\u7684\u3002\u201d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u4eec\u8981\u5728\u5217\u56fd\u4e2d\u5ba3\u544a\u4ee5\u4e0b\u8fd9\u8bdd\uff1a\u201c\u4f60\u4eec\u8981\u9884\u5907\u4e89\u6218\uff0c\u8981\u6fc0\u52a8\u52c7\u58eb\u3002\u8ba9\u6240\u6709\u7684\u6218\u58eb\u90fd\u8fd1\u524d\u6765\uff0c\u8ba9\u4ed6\u4eec\u90fd\u4e0a\u53bb\u653b\u51fb\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8981\u628a\u7281\u5934\u6253\u6210\u5200\u5251\uff0c\u628a\u9570\u5200\u6253\u6210\u77db\u67aa\uff1b\u8f6f\u5f31\u7684\u8981\u8bf4\uff1a\u2018\u6211\u662f\u52c7\u58eb\u3002\u2019\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56db\u56f4\u7684\u5217\u56fd\u554a\uff01\u4f60\u4eec\u5feb\u6765\u5e2e\u52a9\uff0c\u5728\u90a3\u91cc\u4e00\u540c\u805a\u96c6\u3002\u8036\u548c\u534e\u554a\uff01\u6c42\u4f60\u53eb\u4f60\u7684\u52c7\u58eb\u4e0b\u6765\u5427\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5728\u7ea6\u6c99\u6cd5\u8c37\u5ba1\u5224\u4e07\u6c11\u201c\u4e07\u56fd\u90fd\u5f53\u594b\u8d77\uff0c\u4e0a\u5230\u7ea6\u6c99\u6cd5\u8c37\u53bb\u3002\u56e0\u4e3a\u6211\u5fc5\u5750\u5728\u90a3\u91cc\uff0c\u5ba1\u5224\u56db\u56f4\u7684\u5217\u56fd\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u4eec\u8981\u4f38\u51fa\u9570\u5200\uff0c\u56e0\u4e3a\u5e84\u7a3c\u5df2\u7ecf\u719f\u4e86\u3002\u6765\u8df5\u8e0f\u5427\uff0c\u56e0\u4e3a\u538b\u9152\u6c60\u6ee1\u4e86\uff0c\u9152\u6c60\u76c8\u6ea2\uff0c\u56e0\u4e3a\u4ed6\u4eec\u7684\u7f6a\u6076\u6781\u5927\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u65e0\u6570\u7684\u4eba\u7fa4\u9f50\u96c6\u5728\u5224\u51b3\u8c37\uff0c\u56e0\u4e3a\u8036\u548c\u534e\u7684\u65e5\u5b50\u4e34\u8fd1\u4e86\u5224\u51b3\u8c37\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u65e5\u6708\u660f\u6697\uff0c\u661f\u661f\u65e0\u5149\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u8036\u548c\u534e\u4ece\u9521\u5b89\u543c\u53eb\uff0c\u5e76\u4ece\u8036\u8def\u6492\u51b7\u53d1\u58f0\uff0c\u5929\u5730\u5c31\u9707\u52a8\u3002\u8036\u548c\u534e\u5374\u8981\u4f5c\u4ed6\u5b50\u6c11\u7684\u907f\u96be\u6240\uff0c\u4f5c\u4ee5\u8272\u5217\u4f17\u5b50\u7684\u4fdd\u969c\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60\u4eec\u77e5\u9053\u6211\u662f\u8036\u548c\u534e\u4f60\u4eec\u7684\u3000\u795e\uff0c\u662f\u4f4f\u5728\u9521\u5b89\u6211\u7684\u5723\u5c71\u4e0a\u3002\u90a3\u65f6\uff0c\u8036\u8def\u6492\u51b7\u5fc5\u6210\u4e3a\u5723\u3002\u5916\u65cf\u4eba\u5fc5\u4e0d\u5f97\u4ece\u5176\u4e2d\u7ecf\u8fc7\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5230\u90a3\u65e5\uff0c\u5927\u5c71\u90fd\u5fc5\u6ef4\u4e0b\u751c\u9152\uff0c\u5c0f\u5c71\u90fd\u5fc5\u6d41\u51fa\u5976\uff0c\u72b9\u5927\u7684\u4e00\u5207\u6eaa\u6d41\uff0c\u90fd\u5fc5\u6c34\u6d41\u4e0d\u606f\u3002\u5fc5\u6709\u6cc9\u6e90\u4ece\u8036\u548c\u534e\u7684\u6bbf\u91cc\u6d41\u51fa\u6765\uff0c\u704c\u6e89\u6cb3\u8c37\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u57c3\u53ca\u5c06\u4f1a\u8352\u51c9\uff0c\u4ee5\u4e1c\u5fc5\u53d8\u6210\u51c4\u51c9\u7684\u65f7\u91ce\uff0c\u90fd\u56e0\u4e3a\u4ed6\u4eec\u5411\u72b9\u5927\u4eba\u6240\u884c\u7684\u5f3a\u66b4\uff0c\u53c8\u56e0\u4e3a\u5728\u4ed6\u4eec\u7684\u56fd\u4e2d\u6d41\u4e86\u65e0\u8f9c\u4eba\u8840\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u72b9\u5927\u5fc5\u6c38\u5b58\u201c\u4f46\u72b9\u5927\u5fc5\u5b58\u5230\u6c38\u8fdc\uff0c\u8036\u8def\u6492\u51b7\u4e5f\u5fc5\u5b58\u5230\u4e07\u4ee3\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u73b0\u5728\u6211\u8981\u8ffd\u8ba8\u6211\u8fd8\u672a\u6709\u8ffd\u8ba8\u7684\u6d41\u4eba\u8840\u7684\u7f6a\u3002\u8036\u548c\u534e\u5728\u9521\u5b89\u5c45\u4f4f\u3002\u201d\n"
                }
            }
        }
    }
}