        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u95ee\u5b89\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8fd9\u90fd\u662f\u6839\u636e\u6c38\u751f\u7684\u76fc\u671b\uff0c\u8fd9\u6c38\u751f\u5c31\u662f\u6ca1\u6709\u8c0e\u8a00\u7684\u3000\u795e\u5728\u4e07\u4e16\u4ee5\u524d\u6240\u5e94\u8bb8\u7684\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5230\u4e86\u9002\u5f53\u7684\u65f6\u5019\uff0c\u3000\u795e\u5c31\u501f\u7740\u798f\u97f3\u7684\u5ba3\u8bb2\uff0c\u628a\u4ed6\u7684\u9053\u663e\u660e\u51fa\u6765\u3002\u7167\u7740\u3000\u795e\u6211\u4eec\u6551\u4e3b\u7684\u547d\u4ee4\uff0c\u8fd9\u5ba3\u8bb2\u7684\u4f7f\u547d\u4ea4\u6258\u4e86\u6211\uff09\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5199\u4fe1\u7ed9\u63d0\u591a\uff0c\u5c31\u662f\u90a3\u5728\u5171\u540c\u7684\u4fe1\u4ef0\u4e0a\u4f5c\u6211\u771f\u513f\u5b50\u7684\u3002\u613f\u6069\u60e0\u5e73\u5b89\u4ece\u7236\u3000\u795e\u548c\u6211\u4eec\u7684\u6551\u4e3b\u57fa\u7763\u8036\u7a23\u4e34\u5230\u4f60\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u63d0\u591a\u5728\u514b\u91cc\u7279\u7684\u5de5\u4f5c\u6211\u4ece\u524d\u7559\u4f60\u5728\u514b\u91cc\u7279\u5c9b\uff0c\u662f\u8981\u4f60\u529e\u597d\u6ca1\u6709\u529e\u5b8c\u7684\u4e8b\uff0c\u5e76\u4e14\u7167\u6211\u6240\u5429\u5490\u7684\uff0c\u5728\u5404\u57ce\u8bbe\u7acb\u957f\u8001\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5982\u679c\u6709\u4eba\u65e0\u53ef\u6307\u6458\uff0c\u53ea\u4f5c\u4e00\u4e2a\u59bb\u5b50\u7684\u4e08\u592b\uff0c\u513f\u5973\u90fd\u4fe1\u4e3b\uff0c\u4e5f\u6ca1\u6709\u4eba\u63a7\u544a\u4ed6\u4eec\u653e\u8361\u6216\u4e0d\u53d7\u7ea6\u675f\uff0c\u624d\u53ef\u4ee5\u4f5c\u957f\u8001\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u4e3a\u76d1\u7763\u662f\u3000\u795e\u7684\u7ba1\u5bb6\uff0c\u6240\u4ee5\u5fc5\u987b\u65e0\u53ef\u6307\u6458\u3001\u4e0d\u4efb\u6027\u3001\u4e0d\u968f\u4fbf\u52a8\u6012\u3001\u4e0d\u597d\u9152\u3001\u4e0d\u6253\u4eba\u3001\u4e0d\u8d2a\u4e0d\u4e49\u4e4b\u8d22\uff1b\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5374\u8981\u63a5\u5f85\u5ba2\u65c5\u3001\u559c\u7231\u826f\u5584\u3001\u81ea\u5f8b\u3001\u516c\u6b63\u3001\u5723\u6d01\u3001\u81ea\u5236\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u575a\u5b88\u90a3\u5408\u4e4e\u6559\u4e49\u3001\u53ef\u9760\u7684\u771f\u9053\uff0c\u597d\u4f7f\u4ed6\u80fd\u591f\u7528\u7eaf\u6b63\u7684\u9053\u7406\u529d\u52c9\u4eba\uff0c\u5e76\u4e14\u80fd\u591f\u6298\u670d\u53cd\u5bf9\u7684\u4eba\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u4e3a\u6709\u8bb8\u591a\u4e0d\u53d7\u7ea6\u675f\u3001\u597d\u8bb2\u7a7a\u8bdd\u548c\u6b3a\u9a97\u4eba\u7684\uff0c\u5c24\u5176\u662f\u90a3\u4e9b\u5b88\u5272\u793c\u7684\u4eba\uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6\u4eec\u4e3a\u4e86\u53ef\u803b\u7684\u5229\u76ca\uff0c\u6559\u8bad\u4e00\u4e9b\u4e0d\u5e94\u8be5\u6559\u5bfc\u7684\u4e8b\uff0c\u8d25\u574f\u4eba\u7684\u5168\u5bb6\uff0c\u4f60\u52a1\u8981\u5835\u4f4f\u4ed6\u4eec\u7684\u5634\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6709\u4e00\u4e2a\u514b\u91cc\u7279\u672c\u5730\u7684\u5148\u77e5\u8bf4\u8fc7\uff1a\u201c\u514b\u91cc\u7279\u4eba\u662f\u5e38\u5e38\u8bf4\u8c0e\u7684\uff0c\u662f\u6076\u517d\uff0c\u597d\u5403\u61d2\u4f5c\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8fd9\u89c1\u8bc1\u662f\u771f\u7684\u3002\u4e3a\u4e86\u8fd9\u7f18\u6545\uff0c\u4f60\u8981\u4e25\u5389\u5730\u8d23\u5907\u4ed6\u4eec\uff0c\u597d\u4f7f\u4ed6\u4eec\u5728\u4fe1\u4ef0\u4e0a\u5065\u5168\uff0c\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e0d\u518d\u7406\u4f1a\u72b9\u592a\u4eba\u7684\u65e0\u7a3d\u4e4b\u8c08\u548c\u504f\u79bb\u6b63\u9053\u7684\u4eba\u7684\u89c4\u6761\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5728\u6e05\u6d01\u7684\u4eba\uff0c\u4e00\u5207\u90fd\u662f\u6e05\u6d01\u7684\uff1b\u4f46\u5728\u6c61\u79fd\u548c\u4e0d\u4fe1\u7684\u4eba\uff0c\u6ca1\u6709\u4e00\u6837\u662f\u6e05\u6d01\u7684\uff0c\u8fde\u4ed6\u4eec\u7684\u610f\u5ff5\u548c\u826f\u5fc3\u90fd\u6c61\u79fd\u4e86\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4ed6\u4eec\u58f0\u79f0\u8ba4\u8bc6\u3000\u795e\uff0c\u5374\u5728\u884c\u4e3a\u4e0a\u5426\u8ba4\u4ed6\u3002\u4ed6\u4eec\u662f\u53ef\u618e\u7684\u3001\u6096\u9006\u7684\uff0c\u5728\u5404\u6837\u7684\u5584\u4e8b\u4e0a\uff0c\u662f\u6beb\u65e0\u7528\u5904\u7684\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8981\u4f20\u8bb2\u7eaf\u6b63\u7684\u9053\u7406\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u529d\u8001\u5e74\u4eba\u8981\u6709\u8282\u5236\u3001\u5e84\u91cd\u3001\u81ea\u5f8b\uff0c\u5728\u4fe1\u5fc3\u3001\u7231\u5fc3\u3001\u5fcd\u8010\u4e0a\u90fd\u8981\u5065\u5168\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u7167\u6837\uff0c\u529d\u5e74\u8001\u7684\u5987\u5973\u8981\u751f\u6d3b\u656c\u8654\uff0c\u4e0d\u8bf4\u8c17\u8a00\uff0c\u4e0d\u88ab\u9152\u5974\u5f79\uff0c\u7528\u5584\u9053\u6559\u5bfc\u4eba\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u597d\u63d0\u9192\u5e74\u8f7b\u7684\u5987\u5973\u7231\u4e08\u592b\u7231\u513f\u5973\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5e76\u4e14\u81ea\u5f8b\u3001\u8d1e\u6d01\u3001\u7406\u5bb6\u3001\u5584\u826f\u3001\u987a\u4ece\u81ea\u5df1\u7684\u4e08\u592b\uff0c\u514d\u5f97\u3000\u795e\u7684\u9053\u53d7\u6bc1\u8c24\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u7167\u6837\uff0c\u529d\u5e74\u8f7b\u7684\u7537\u5b50\u8981\u81ea\u5f8b\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u65e0\u8bba\u5728\u4ec0\u4e48\u4e8b\u4e0a\u4f60\u90fd\u8981\u663e\u51fa\u597d\u884c\u4e3a\u7684\u699c\u6837\uff0c\u5728\u6559\u5bfc\u4e0a\u8981\u7eaf\u5168\uff0c\u8981\u5e84\u91cd\uff0c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u8a00\u8bcd\u8981\u7eaf\u6b63\uff0c\u65e0\u53ef\u6307\u6458\uff0c\u4f7f\u53cd\u5bf9\u7684\u4eba\u56e0\u4e3a\u65e0\u4ece\u6bc1\u8c24\uff0c\u5c31\u81ea\u89c9\u60ed\u6127\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u529d\u4ec6\u4eba\u8981\u51e1\u4e8b\u987a\u670d\u81ea\u5df1\u7684\u4e3b\u4eba\uff0c\u8ba8\u4ed6\u6b22\u559c\uff0c\u4e0d\u8981\u9876\u5634\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4e0d\u8981\u79c1\u53d6\u8d22\u7269\uff0c\u5374\u8981\u663e\u793a\u7edd\u5bf9\u7684\u8bda\u5b9e\uff0c\u597d\u4f7f\u6211\u4eec\u6551\u4e3b\u3000\u795e\u7684\u9053\u7406\uff0c\u5728\u51e1\u4e8b\u4e0a\u90fd\u5f97\u7740\u5c0a\u8363\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u3000\u795e\u62ef\u6551\u4e07\u4eba\u7684\u6069\u5178\u5df2\u7ecf\u663e\u660e\u51fa\u6765\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u8fd9\u6069\u5178\u8bad\u7ec3\u6211\u4eec\u9664\u53bb\u4e0d\u656c\u8654\u7684\u5fc3\uff0c\u548c\u5c5e\u4e16\u7684\u79c1\u6b32\uff0c\u5728\u4eca\u751f\u8fc7\u7740\u81ea\u5f8b\u3001\u516c\u6b63\u3001\u656c\u8654\u7684\u751f\u6d3b\uff0c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u7b49\u5019\u90a3\u6709\u798f\u7684\u76fc\u671b\uff0c\u5c31\u662f\u6211\u4eec\u4f1f\u5927\u7684\u3000\u795e\uff0c\u6551\u4e3b\u8036\u7a23\u57fa\u7763\u8363\u8000\u7684\u663e\u73b0\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4ed6\u4e3a\u6211\u4eec\u820d\u5df1\uff0c\u4e3a\u7684\u662f\u8981\u6551\u8d4e\u6211\u4eec\u8131\u79bb\u4e00\u5207\u4e0d\u6cd5\u7684\u4e8b\uff0c\u5e76\u4e14\u6d01\u51c0\u6211\u4eec\u4f5c\u4ed6\u81ea\u5df1\u7684\u5b50\u6c11\uff0c\u70ed\u5fc3\u5584\u5de5\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u8981\u4f20\u8bb2\u8fd9\u4e9b\u4e8b\uff0c\u8fd0\u7528\u5404\u6837\u7684\u6743\u67c4\u53bb\u529d\u6212\u4eba\uff0c\u8d23\u5907\u4eba\uff1b\u4e0d\u8981\u8ba9\u4eba\u8f7b\u770b\u4f60\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8981\u5e38\u5e38\u7559\u5fc3\u4f5c\u5584\u5de5\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4e0d\u53ef\u6bc1\u8c24\u4eba\uff0c\u8981\u4e0e\u4eba\u65e0\u4e89\uff0c\u8c26\u606d\u6709\u793c\uff0c\u5411\u4f17\u4eba\u8868\u73b0\u5145\u5206\u6e29\u67d4\u7684\u5fc3\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u4eec\u4ece\u524d\u4e5f\u662f\u65e0\u77e5\u3001\u4e0d\u987a\u670d\u3001\u53d7\u4e86\u8ff7\u60d1\u3001\u88ab\u5404\u79cd\u79c1\u6b32\u548c\u9038\u4e50\u6240\u5974\u5f79\uff0c\u751f\u6d3b\u5728\u6076\u6bd2\u548c\u5ac9\u5992\u4e4b\u4e2d\uff0c\u662f\u53ef\u618e\u53ef\u6076\u7684\uff0c\u5e76\u4e14\u4e92\u76f8\u4ec7\u89c6\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u7136\u800c\uff0c\u5230\u4e86\u3000\u795e\u6211\u4eec\u7684\u6551\u4e3b\u663e\u660e\u4ed6\u7684\u6069\u6148\u548c\u601c\u7231\u7684\u65f6\u5019\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4ed6\u5c31\u6551\u4e86\u6211\u4eec\uff0c\u5e76\u4e0d\u662f\u7531\u4e8e\u6211\u4eec\u6240\u884c\u7684\u4e49\uff0c\u800c\u662f\u7167\u7740\u4ed6\u7684\u601c\u60af\uff0c\u501f\u7740\u91cd\u751f\u7684\u6d17\u548c\u5723\u7075\u7684\u66f4\u65b0\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5723\u7075\u5c31\u662f\u3000\u795e\u501f\u7740\u6211\u4eec\u7684\u6551\u4e3b\u8036\u7a23\u57fa\u7763\u4e30\u4e30\u5bcc\u5bcc\u6d47\u704c\u5728\u6211\u4eec\u8eab\u4e0a\u7684\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f7f\u6211\u4eec\u65e2\u7136\u56e0\u7740\u4ed6\u7684\u6069\u5178\u5f97\u79f0\u4e3a\u4e49\uff0c\u5c31\u53ef\u4ee5\u51ed\u7740\u6c38\u751f\u7684\u76fc\u671b\u6210\u4e3a\u540e\u55e3\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u8fd9\u8bdd\u662f\u53ef\u4fe1\u7684\uff0c\u6211\u613f\u4f60\u786e\u5b9e\u5730\u5f3a\u8c03\u8fd9\u4e9b\u4e8b\uff0c\u4f7f\u4fe1\u3000\u795e\u7684\u4eba\u5e38\u5e38\u7559\u5fc3\u4f5c\u5584\u5de5\uff1b\u8fd9\u4e9b\u90fd\u662f\u7f8e\u4e8b\uff0c\u5e76\u4e14\u662f\u5bf9\u4eba\u6709\u76ca\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u8981\u8fdc\u907f\u611a\u6627\u7684\u8fa9\u8bba\u3001\u5bb6\u8c31\u3001\u7eb7\u4e89\u548c\u5f8b\u6cd5\u4e0a\u7684\u4e89\u6267\uff0c\u56e0\u4e3a\u8fd9\u90fd\u662f\u865a\u5984\u65e0\u76ca\u7684\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5206\u95e8\u7ed3\u515a\u7684\u4eba\uff0c\u8b66\u6212\u4e00\u4e24\u6b21\u4e4b\u540e\uff0c\u5c31\u8981\u548c\u4ed6\u7edd\u4ea4\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4f60\u77e5\u9053\u8fd9\u79cd\u4eba\u5df2\u7ecf\u80cc\u9053\uff0c\u5e38\u5e38\u72af\u7f6a\uff0c\u5b9a\u4e86\u81ea\u5df1\u7684\u7f6a\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5631\u5490\u548c\u95ee\u5b89\u6211\u6d3e\u4e9a\u63d0\u9a6c\u6216\u63a8\u57fa\u53e4\u5230\u4f60\u90a3\u91cc\u53bb\u7684\u65f6\u5019\uff0c\u4f60\u8981\u8d76\u5feb\u5230\u5c3c\u54e5\u6ce2\u7acb\u6765\u89c1\u6211\uff0c\u56e0\u4e3a\u6211\u5df2\u51b3\u5b9a\u5728\u90a3\u91cc\u8fc7\u51ac\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u8981\u5c3d\u529b\u8d44\u52a9\u897f\u7eb3\u5f8b\u5e08\u548c\u4e9a\u6ce2\u7f57\u7684\u65c5\u7a0b\uff0c\u4f7f\u4ed6\u4eec\u4e0d\u81f4\u7f3a\u4e4f\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6211\u4eec\u81ea\u5df1\u7684\u4eba\u4e5f\u5e94\u5f53\u5b66\u4e60\u4f5c\u5584\u5de5\uff0c\u4f9b\u5e94\u65e5\u5e38\u7684\u9700\u8981\uff0c\u514d\u5f97\u4e0d\u7ed3\u679c\u5b50\u3002\n"
                },
