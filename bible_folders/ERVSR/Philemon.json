{
    "version": "ERVSR",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u041f\u0430\u0432\u043b\u0435, \u0441\u0443\u0436\u0430\u045a \u0425\u0440\u0438\u0441\u0442\u0430 \u0418\u0441\u0443\u0441\u0430, \u0438 \u0431\u0440\u0430\u0442 \u0422\u0438\u043c\u043e\u0442\u0435\u0458, \u0434\u0440\u0430\u0433\u043e\u043c \u0424\u0438\u043b\u0438\u043c\u043e\u043d\u0443, \u043d\u0430\u0448\u0435\u043c \u0441\u0430\u0440\u0430\u0434\u043d\u0438\u043a\u0443,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0441\u0435\u0441\u0442\u0440\u0438 \u0410\u043f\u0444\u0438\u0458\u0438, \u0410\u0440\u0445\u0438\u043f\u0443, \u043d\u0430\u0448\u0435\u043c \u0441\u0430\u0431\u043e\u0440\u0446\u0443, \u0438 \u0446\u0440\u043a\u0432\u0438 \u0443 \u0442\u0432\u043e\u0458\u043e\u0458 \u043a\u0443\u045b\u0438:"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u043c\u0438\u043b\u043e\u0441\u0442 \u0432\u0430\u043c \u0438 \u043c\u0438\u0440 \u043e\u0434 \u0411\u043e\u0433\u0430, \u043d\u0430\u0448\u0435\u0433 \u041e\u0446\u0430, \u0438 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0417\u0430\u0445\u0432\u0430\u0459\u0438\u0432\u0430\u045a\u0435 \u0438 \u043c\u043e\u043b\u0438\u0442\u0432\u0430 - \u0423\u0432\u0435\u043a \u0437\u0430\u0445\u0432\u0430\u0459\u0443\u0458\u0435\u043c \u0441\u0432\u043e\u043c\u0435 \u0411\u043e\u0433\u0443, \u0441\u0435\u045b\u0430\u0458\u0443\u045b\u0438 \u0442\u0435 \u0441\u0435 \u0443 \u0441\u0432\u043e\u0458\u0438\u043c \u043c\u043e\u043b\u0438\u0442\u0432\u0430\u043c\u0430,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0458\u0435\u0440 \u0447\u0443\u0458\u0435\u043c \u0437\u0430 \u0442\u0432\u043e\u0458\u0443 \u0459\u0443\u0431\u0430\u0432 \u043f\u0440\u0435\u043c\u0430 \u0441\u0432\u0438\u043c\u0430 \u0441\u0432\u0435\u0442\u0438\u043c\u0430 \u0438 \u0437\u0430 \u0442\u0432\u043e\u0458\u0443 \u0432\u0435\u0440\u0443 \u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0441\u0443\u0441\u0430."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u041d\u0435\u043a\u0430 \u0442\u0432\u043e\u0458\u0435 \u0443\u0447\u0435\u0441\u0442\u0432\u043e\u0432\u0430\u045a\u0435 \u0443 \u0432\u0435\u0440\u0438 \u0431\u0443\u0434\u0435 \u0434\u0435\u043b\u043e\u0442\u0432\u043e\u0440\u043d\u043e, \u0434\u0430 \u0441\u043f\u043e\u0437\u043d\u0430\u0448 \u0441\u0432\u0430\u043a\u043e \u0434\u043e\u0431\u0440\u043e \u043a\u043e\u0458\u0435 \u0438\u043c\u0430\u043c\u043e \u0443 \u0425\u0440\u0438\u0441\u0442\u0443."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0422\u0432\u043e\u0458\u0430 \u0459\u0443\u0431\u0430\u0432 \u043c\u0435 \u0458\u0435 \u0432\u0435\u043e\u043c\u0430 \u043e\u0431\u0440\u0430\u0434\u043e\u0432\u0430\u043b\u0430 \u0438 \u043e\u0431\u043e\u0434\u0440\u0438\u043b\u0430, \u0458\u0435\u0440 \u0441\u0438 \u0442\u0438, \u0431\u0440\u0430\u0442\u0435, \u043e\u043a\u0440\u0435\u043f\u0438\u043e \u0441\u0440\u0446\u0435 \u0441\u0432\u0435\u0442\u0438\u0445."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u041f\u0430\u0432\u043b\u0435 \u0441\u0435 \u0437\u0430\u0443\u0437\u0438\u043c\u0430 \u0437\u0430 \u041e\u043d\u0438\u0441\u0438\u043c\u0430 - \u0421\u0442\u043e\u0433\u0430, \u0438\u0430\u043a\u043e \u0431\u0438\u0445 \u0443 \u0425\u0440\u0438\u0441\u0442\u0443 \u0438\u043c\u0430\u043e \u0441\u043c\u0435\u043b\u043e\u0441\u0442\u0438 \u0434\u0430 \u0442\u0438 \u043d\u0430\u0440\u0435\u0434\u0438\u043c \u0448\u0442\u0430 \u0442\u0440\u0435\u0431\u0430 \u0434\u0430 \u0440\u0430\u0434\u0438\u0448,"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0437\u0431\u043e\u0433 \u0459\u0443\u0431\u0430\u0432\u0438 \u0440\u0430\u0434\u0438\u0458\u0435 \u043c\u043e\u043b\u0438\u043c, \u0442\u0430\u043a\u0430\u0432 \u043a\u0430\u043a\u0430\u0432 \u0441\u0430\u043c, \u041f\u0430\u0432\u043b\u0435, \u0441\u0442\u0430\u0440\u0430\u0446, \u0430 \u0441\u0430\u0434\u0430 \u0438 \u0441\u0443\u0436\u0430\u045a \u0425\u0440\u0438\u0441\u0442\u0430 \u0418\u0441\u0443\u0441\u0430:"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u043c\u043e\u043b\u0438\u043c \u0442\u0435 \u0437\u0430 \u0441\u0432\u043e\u0433\u0430 \u0441\u0438\u043d\u0430 \u041e\u043d\u0438\u0441\u0438\u043c\u0430, \u043a\u043e\u0433\u0430 \u0441\u0430\u043c \u0440\u043e\u0434\u0438\u043e \u0443 \u043e\u043a\u043e\u0432\u0438\u043c\u0430."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u041e\u043d \u0442\u0438 \u0458\u0435 \u0440\u0430\u043d\u0438\u0458\u0435 \u0431\u0438\u043e \u0431\u0435\u0441\u043a\u043e\u0440\u0438\u0441\u0442\u0430\u043d, \u0430 \u0441\u0430\u0434\u0430 \u0458\u0435 \u043a\u043e\u0440\u0438\u0441\u0442\u0430\u043d \u043d\u0435 \u0441\u0430\u043c\u043e \u0442\u0435\u0431\u0438 \u043d\u0435\u0433\u043e \u0438 \u043c\u0435\u043d\u0438."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0428\u0430\u0459\u0435\u043c \u0442\u0438 \u0433\u0430 \u043d\u0430\u0437\u0430\u0434, \u045a\u0435\u0433\u0430, \u0442\u043e \u0458\u0435\u0441\u0442 \u0441\u0432\u043e\u0458\u0435 \u0441\u043e\u043f\u0441\u0442\u0432\u0435\u043d\u043e \u0441\u0440\u0446\u0435."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0425\u0442\u0435\u043e \u0441\u0430\u043c \u0434\u0430 \u0433\u0430 \u0437\u0430\u0434\u0440\u0436\u0438\u043c \u043a\u043e\u0434 \u0441\u0435\u0431\u0435, \u0434\u0430 \u043c\u0438 \u0443\u043c\u0435\u0441\u0442\u043e \u0442\u0435\u0431\u0435 \u0441\u043b\u0443\u0436\u0438 \u0434\u043e\u043a \u0441\u0430\u043c \u0443 \u043e\u043a\u043e\u0432\u0438\u043c\u0430 \u0437\u0430 \u0435\u0432\u0430\u043d\u0452\u0435\u0459\u0435,"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0430\u043b\u0438 \u043d\u0438\u0441\u0430\u043c \u043d\u0438\u0448\u0442\u0430 \u0445\u0442\u0435\u043e \u0434\u0430 \u0443\u0447\u0438\u043d\u0438\u043c \u0431\u0435\u0437 \u0442\u0432\u043e\u0433 \u0437\u043d\u0430\u045a\u0430, \u0434\u0430 \u0442\u0432\u043e\u0458\u0435 \u0434\u043e\u0431\u0440\u043e \u0434\u0435\u043b\u043e \u043d\u0435 \u0431\u0443\u0434\u0435 \u0438\u0437\u043d\u0443\u0452\u0435\u043d\u043e, \u043d\u0435\u0433\u043e \u0434\u0440\u0430\u0433\u043e\u0432\u043e\u0459\u043d\u043e."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u041c\u043e\u0436\u0434\u0430 \u0458\u0435 \u0431\u0430\u0448 \u0437\u0430\u0442\u043e \u0431\u0438\u043e \u043d\u0430\u043a\u0440\u0430\u0442\u043a\u043e \u043e\u0434 \u0442\u0435\u0431\u0435 \u043e\u0434\u0432\u043e\u0458\u0435\u043d \u0434\u0430 \u0431\u0438 \u0433\u0430 \u0442\u0438 \u0437\u0430\u0443\u0432\u0435\u043a \u0434\u043e\u0431\u0438\u043e"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0438 \u0442\u043e \u043d\u0435 \u0432\u0438\u0448\u0435 \u043a\u0430\u043e \u0440\u043e\u0431\u0430, \u043d\u0435\u0433\u043e \u043a\u0430\u043e \u0432\u0438\u0448\u0435 \u043e\u0434 \u0440\u043e\u0431\u0430 \u2013 \u043a\u0430\u043e \u0434\u0440\u0430\u0433\u043e\u0433 \u0431\u0440\u0430\u0442\u0430. \u041e\u043d \u043c\u0438 \u0458\u0435 \u0432\u0435\u043e\u043c\u0430 \u0434\u0440\u0430\u0433, \u0430 \u043a\u043e\u043b\u0438\u043a\u043e \u043b\u0438 \u0458\u0435 \u0442\u0435\u043a \u0434\u0440\u0430\u0436\u0438 \u0442\u0435\u0431\u0438, \u043d\u0435 \u0441\u0430\u043c\u043e \u043a\u0430\u043e \u0447\u043e\u0432\u0435\u043a \u043d\u0435\u0433\u043e \u0438 \u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u0443."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0410\u043a\u043e \u043c\u0435, \u0434\u0430\u043a\u043b\u0435, \u0441\u043c\u0430\u0442\u0440\u0430\u0448 \u0437\u0430 \u0434\u0440\u0443\u0433\u0430, \u043f\u0440\u0438\u043c\u0438 \u0433\u0430 \u043a\u0430\u043e \u043c\u0435\u043d\u0435."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0410 \u0430\u043a\u043e \u0442\u0438 \u0458\u0435 \u043d\u0435\u0448\u0442\u043e \u0441\u043a\u0440\u0438\u0432\u0438\u043e \u0438\u043b\u0438 \u0442\u0438 \u043d\u0435\u0448\u0442\u043e \u0434\u0443\u0433\u0443\u0458\u0435, \u0443\u043f\u0438\u0448\u0438 \u0442\u043e \u043d\u0430 \u043c\u043e\u0458 \u0440\u0430\u0447\u0443\u043d."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0408\u0430, \u041f\u0430\u0432\u043b\u0435, \u043f\u0438\u0448\u0435\u043c \u043e\u0432\u043e \u0441\u0432\u043e\u0458\u043e\u043c \u0440\u0443\u043a\u043e\u043c: \u043f\u043b\u0430\u0442\u0438\u045b\u0443. \u0414\u0430 \u043d\u0435 \u043f\u043e\u043c\u0438\u045a\u0435\u043c \u0434\u0430 \u0442\u0438 \u043c\u0435\u043d\u0438 \u0438 \u0441\u0430\u043c\u043e\u0433\u0430 \u0441\u0435\u0431\u0435 \u0434\u0443\u0433\u0443\u0458\u0435\u0448."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0414\u0430, \u0431\u0440\u0430\u0442\u0435, \u0432\u043e\u043b\u0435\u043e \u0431\u0438\u0445 \u0434\u0430 \u043e\u0434 \u0442\u0435\u0431\u0435 \u0432\u0438\u0434\u0438\u043c \u043d\u0435\u043a\u0443 \u043a\u043e\u0440\u0438\u0441\u0442 \u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u0443. \u041e\u043a\u0440\u0435\u043f\u0438 \u043c\u0438 \u0441\u0440\u0446\u0435 \u0443 \u0425\u0440\u0438\u0441\u0442\u0443."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u041f\u0438\u0448\u0435\u043c \u0442\u0438, \u0443\u0432\u0435\u0440\u0435\u043d \u0443 \u0442\u0432\u043e\u0458\u0443 \u043f\u043e\u0441\u043b\u0443\u0448\u043d\u043e\u0441\u0442, \u0458\u0435\u0440 \u0437\u043d\u0430\u043c \u0434\u0430 \u045b\u0435\u0448 \u0443\u0447\u0438\u043d\u0438\u0442\u0438 \u0438 \u0432\u0438\u0448\u0435 \u043d\u0435\u0433\u043e \u0448\u0442\u043e \u043a\u0430\u0436\u0435\u043c."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u0418 \u0458\u043e\u0448 \u043d\u0435\u0448\u0442\u043e: \u0441\u043f\u0440\u0435\u043c\u0438 \u043c\u0438 \u0433\u043e\u0441\u0442\u0438\u043d\u0441\u043a\u0443 \u0441\u043e\u0431\u0443, \u0458\u0435\u0440 \u0441\u0435 \u043d\u0430\u0434\u0430\u043c \u0434\u0430 \u045b\u0435 \u043c\u0435 \u0411\u043e\u0433 \u043f\u043e\u0441\u043b\u0430\u0442\u0438 \u043a \u0432\u0430\u043c\u0430 \u0437\u0431\u043e\u0433 \u0432\u0430\u0448\u0438\u0445 \u043c\u043e\u043b\u0438\u0442\u0432\u0438."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u041f\u043e\u0437\u0434\u0440\u0430\u0432\u0459\u0430\u0458\u0443 \u0442\u0435 \u0415\u043f\u0430\u0444\u0440\u0430\u0441, \u043a\u043e\u0458\u0438 \u0458\u0435 \u0441\u0430 \u043c\u043d\u043e\u043c \u0441\u0443\u0436\u0430\u045a \u0443 \u0425\u0440\u0438\u0441\u0442\u0443 \u0418\u0441\u0443\u0441\u0443,"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u041c\u0430\u0440\u043a\u043e, \u0410\u0440\u0438\u0441\u0442\u0430\u0440\u0445, \u0414\u0438\u043c\u0430\u0441 \u0438 \u041b\u0443\u043a\u0430, \u043c\u043e\u0458\u0438 \u0441\u0430\u0440\u0430\u0434\u043d\u0438\u0446\u0438."
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u041c\u0438\u043b\u043e\u0441\u0442 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430 \u0441\u0430 \u0432\u0430\u0448\u0438\u043c \u0434\u0443\u0445\u043e\u043c."
                }
            }
        }
    }
}