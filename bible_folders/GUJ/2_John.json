{
    "version": "GUJ",
    "book_name": "2 John",
    "book_nr": "63",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0aaa\u0ab8\u0a82\u0aa6 \u0a95\u0ab0\u0ac7\u0ab2\u0ac0 \u0aac\u0ab9\u0ac7\u0aa8\u0aa8\u0ac7 \u0aa4\u0aa5\u0abe \u0aa4\u0ac7\u0aa8\u0abe\u0a82 \u0aac\u0abe\u0ab3\u0a95\u0acb\u0aa8\u0ac7 \u0ab2\u0a96\u0aa8\u0abe\u0ab0 \u0ab5\u0aa1\u0ac0\u0ab2."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0a9c\u0ac7 \u0ab8\u0aa4\u0acd\u0aaf \u0a86\u0aaa\u0aa3\u0abe\u0aae\u0abe\u0a82 \u0ab0\u0ab9\u0ac7 \u0a9b\u0ac7, \u0aa4\u0ac7 \u0ab8\u0ab0\u0acd\u0ab5\u0a95\u0abe\u0ab3 \u0a9f\u0a95\u0ab5\u0abe\u0aa8\u0ac1\u0a82 \u0a9b\u0ac7 \u0aa4\u0ac7\u0aa8\u0ac7 \u0ab2\u0ac0\u0aa7\u0ac7 \u0ab9\u0ac1\u0a82 \u0ab8\u0aa4\u0acd\u0aaf\u0aae\u0abe\u0a82 \u0aa4\u0aae\u0abe\u0ab0\u0abe \u0aaa\u0ab0 \u0aaa\u0acd\u0ab0\u0ac7\u0aae \u0ab0\u0abe\u0a96\u0ac1\u0a82 \u0a9b\u0ac1\u0a82 \u0a85\u0aa8\u0ac7 \u0a8f\u0a95\u0ab2\u0acb \u0ab9\u0ac1\u0a82 \u0aa8\u0ab9\u0abf, \u0aaa\u0aa3 \u0a9c\u0ac7 \u0ab8\u0a98\u0ab3\u0abe \u0ab8\u0aa4\u0acd\u0aaf\u0aa8\u0ac7 \u0a9c\u0abe\u0aa3\u0ac7 \u0a9b\u0ac7 \u0aa4\u0ac7\u0a93 \u0aaa\u0aa3 \u0ab0\u0abe\u0a96\u0ac7 \u0a9b\u0ac7."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0a88\u0ab6\u0acd\u0ab5\u0ab0\u0aaa\u0abf\u0aa4\u0abe\u0aa5\u0ac0 \u0aa4\u0aa5\u0abe \u0aa4\u0ac7\u0aae\u0aa8\u0abe \u0aaa\u0ac1\u0aa4\u0acd\u0ab0 \u0a88\u0ab8\u0ac1 \u0a96\u0acd\u0ab0\u0abf\u0ab8\u0acd\u0aa4\u0aa5\u0ac0 \u0a95\u0ac3\u0aaa\u0abe, \u0aa6\u0aaf\u0abe \u0aa4\u0aa5\u0abe \u0ab6\u0abe\u0a82\u0aa4\u0abf \u0a86\u0aaa\u0aa3\u0ac0 \u0ab8\u0abe\u0aa5\u0ac7 \u0ab8\u0aa4\u0acd\u0aaf \u0aa4\u0aa5\u0abe \u0aaa\u0acd\u0ab0\u0ac7\u0aae\u0aae\u0abe\u0a82 \u0ab0\u0ab9\u0ac7\u0ab6\u0ac7."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0a9c\u0ac7\u0aae \u0a86\u0aaa\u0aa3\u0ac7 \u0aaa\u0abf\u0aa4\u0abe\u0aa5\u0ac0 \u0a86\u0a9c\u0acd\u0a9e\u0abe \u0aaa\u0abe\u0aae\u0acd\u0aaf\u0abe, \u0aa4\u0ac7\u0aae \u0ab8\u0aa4\u0acd\u0aaf\u0aae\u0abe\u0a82 \u0a9a\u0abe\u0ab2\u0aa4\u0abe\u0a82 \u0aa4\u0abe\u0ab0\u0abe\u0a82 \u0a95\u0ac7\u0a9f\u0ab2\u0abe\u0a95 \u0aac\u0abe\u0ab3\u0a95\u0acb\u0aa8\u0ac7 \u0aae\u0ac7\u0a82 \u0a9c\u0acb\u0aaf\u0abe\u0a82 \u0a9b\u0ac7, \u0aae\u0abe\u0a9f\u0ac7 \u0ab9\u0ac1\u0a82 \u0a98\u0aa3\u0acb \u0a96\u0ac1\u0ab6 \u0aa5\u0abe\u0a89\u0a82 \u0a9b\u0ac1\u0a82."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0ab9\u0ab5\u0ac7, \u0aac\u0ab9\u0ac7\u0aa8, \u0ab9\u0ac1\u0a82 \u0aa8\u0ab5\u0ac0 \u0a86\u0a9c\u0acd\u0a9e\u0abe \u0ab2\u0a96\u0ac1\u0a82 \u0a9b\u0ac1\u0a82 \u0a8f\u0aae \u0aa8\u0ab9\u0abf, \u0aaa\u0aa3 \u0a86\u0ab0\u0a82\u0aad\u0aa5\u0ac0 \u0a9c\u0ac7 \u0a86\u0a9c\u0acd\u0a9e\u0abe \u0a86\u0aaa\u0aa3\u0aa8\u0ac7 \u0aae\u0ab3\u0ac7\u0ab2\u0ac0 \u0a9b\u0ac7 \u0aa4\u0ac7 \u0ab2\u0a96\u0aa4\u0abe\u0a82 \u0aa4\u0aa8\u0ac7 \u0a85\u0ab0\u0a9c \u0a95\u0ab0\u0ac1\u0a82 \u0a9b\u0ac1\u0a82 \u0a95\u0ac7 \u0a86\u0aaa\u0aa3\u0ac7 \u0aae\u0abe\u0a82\u0ab9\u0acb\u0aae\u0abe\u0a82\u0ab9\u0ac7 \u0aaa\u0acd\u0ab0\u0ac7\u0aae \u0ab0\u0abe\u0a96\u0ac0\u0a8f."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0a86\u0aaa\u0aa3\u0ac7 \u0aa4\u0ac7\u0aa8\u0ac0 \u0a86\u0a9c\u0acd\u0a9e\u0abe\u0a93 \u0aaa\u0acd\u0ab0\u0aae\u0abe\u0aa3\u0ac7 \u0a9a\u0abe\u0ab2\u0ac0\u0a8f \u0aa4\u0ac7 \u0a9c \u0aaa\u0acd\u0ab0\u0ac7\u0aae \u0a9b\u0ac7 \u0a85\u0aa8\u0ac7 \u0a9c\u0ac7\u0aae \u0aa4\u0aae\u0ac7 \u0a86\u0ab0\u0a82\u0aad\u0aa5\u0ac0 \u0ab8\u0abe\u0a82\u0aad\u0ab3\u0acd\u0aaf\u0ac1\u0a82 \u0a9b\u0ac7 \u0aa4\u0ac7\u0aae \u0a86\u0a9c\u0acd\u0a9e\u0abe \u0aa4\u0ac7 \u0a9c \u0a9b\u0ac7 \u0a95\u0ac7 \u0aa4\u0aae\u0ac7 \u0aaa\u0acd\u0ab0\u0ac7\u0aae\u0aae\u0abe\u0a82 \u0a9a\u0abe\u0ab2\u0acb."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0a95\u0abe\u0ab0\u0aa3 \u0a95\u0ac7 \u0aa6\u0ac1\u0aa8\u0abf\u0aaf\u0abe\u0aae\u0abe\u0a82 \u0a98\u0aa3\u0abe\u0a82 \u0a9b\u0ac7\u0aa4\u0ab0\u0aa8\u0abe\u0ab0\u0abe\u0a82 \u0a8a\u0aad\u0abe \u0aa5\u0aaf\u0abe \u0a9b\u0ac7; \u0a9c\u0ac7\u0a93 \u0a88\u0ab8\u0ac1 \u0a96\u0acd\u0ab0\u0abf\u0ab8\u0acd\u0aa4\u0aa8\u0ac1\u0a82 \u0aae\u0aa8\u0ac1\u0ab7\u0acd\u0aaf\u0aa6\u0ac7\u0ab9\u0aae\u0abe\u0a82 \u0a86\u0ab5\u0ab5\u0ac1\u0a82 \u0a95\u0aac\u0ac2\u0ab2 \u0a95\u0ab0\u0aa4\u0abe \u0aa8\u0aa5\u0ac0, \u0aa4\u0ac7 \u0a9c \u0a9b\u0ac7\u0aa4\u0ab0\u0aa8\u0abe\u0ab0 \u0aa4\u0aa5\u0abe \u0a96\u0acd\u0ab0\u0abf\u0ab8\u0acd\u0aa4-\u0ab5\u0abf\u0ab0\u0acb\u0aa7\u0ac0 \u0a9b\u0ac7."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0aa4\u0aae\u0ac7 \u0aaa\u0acb\u0aa4\u0abe\u0aa8\u0abe \u0ab5\u0abf\u0ab7\u0ac7 \u0ab8\u0abe\u0ab5\u0a9a\u0ac7\u0aa4 \u0ab0\u0ab9\u0acb, \u0a95\u0ac7 \u0a9c\u0ac7\u0aa5\u0ac0 \u0a85\u0aae\u0ac7 \u0a9c\u0ac7 \u0a95\u0abe\u0aae \u0a95\u0ab0\u0acd\u0aaf\u0ac1\u0a82 \u0a9b\u0ac7 \u0aa4\u0ac7\u0aa8\u0acb \u0aa4\u0aae\u0ac7 \u0aa8\u0abe\u0ab6 \u0aa8 \u0a95\u0ab0\u0acb, \u0aaa\u0aa3 \u0aa4\u0ac7\u0aa8\u0ac1\u0a82 \u0aaa\u0ac2\u0ab0\u0acd\u0aa3 \u0aaa\u0acd\u0ab0\u0aa4\u0abf\u0aab\u0ab3 \u0aaa\u0abe\u0aae\u0acb."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0a9c\u0ac7 \u0a95\u0acb\u0a88 \u0ab9\u0aa6 \u0aac\u0ab9\u0abe\u0ab0 \u0a9c\u0abe\u0aaf \u0a9b\u0ac7 \u0a85\u0aa8\u0ac7 \u0a96\u0acd\u0ab0\u0abf\u0ab8\u0acd\u0aa4\u0aa8\u0abe\u0a82 \u0ab6\u0abf\u0a95\u0acd\u0ab7\u0aa3\u0aae\u0abe\u0a82 \u0ab0\u0ab9\u0ac7\u0aa4\u0acb \u0aa8\u0aa5\u0ac0, \u0aa4\u0ac7\u0aa8\u0ac0 \u0aaa\u0abe\u0ab8\u0ac7 \u0a88\u0ab6\u0acd\u0ab5\u0ab0 \u0aa8\u0aa5\u0ac0; \u0ab6\u0abf\u0a95\u0acd\u0ab7\u0aa3\u0aae\u0abe\u0a82 \u0a9c\u0ac7 \u0ab0\u0ab9\u0ac7 \u0a9b\u0ac7, \u0aa4\u0ac7\u0aa8\u0ac7 \u0a9c \u0aaa\u0abf\u0aa4\u0abe \u0aa4\u0aa5\u0abe \u0aaa\u0ac1\u0aa4\u0acd\u0ab0 \u0aaa\u0aa3 \u0a9b\u0ac7."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0a9c\u0acb \u0a95\u0acb\u0a88 \u0aa4\u0aae\u0abe\u0ab0\u0ac0 \u0aaa\u0abe\u0ab8\u0ac7 \u0a86\u0ab5\u0ac7 \u0a85\u0aa8\u0ac7 \u0aa4\u0ac7 \u0a9c \u0ab6\u0abf\u0a95\u0acd\u0ab7\u0aa3 \u0aa8 \u0ab2\u0abe\u0ab5\u0ac7, \u0aa4\u0acb \u0aa4\u0ac7\u0aa8\u0ac7 \u0a98\u0ab0\u0aae\u0abe\u0a82 \u0aaa\u0ac7\u0ab8\u0ab5\u0abe \u0aa8 \u0aa6\u0acb \u0a85\u0aa8\u0ac7 \u0aa4\u0ac7\u0aa8\u0ac7 \u0a95\u0acd\u0ab7\u0ac7\u0aae\u0a95\u0ac1\u0ab6\u0ab3 \u0aa8 \u0a95\u0ab9\u0acb."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0a95\u0ac7\u0aae \u0a95\u0ac7 \u0a9c\u0ac7 \u0aa4\u0ac7\u0aa8\u0ac7 \u0a95\u0acd\u0ab7\u0ac7\u0aae\u0a95\u0ac1\u0ab6\u0ab3 \u0a95\u0ab9\u0ac7 \u0a9b\u0ac7 \u0aa4\u0ac7 \u0aa4\u0ac7\u0aa8\u0abe\u0a82 \u0aa6\u0ac1\u0ab7\u0acd\u0a9f\u0a95\u0ab0\u0acd\u0aae\u0acb\u0aa8\u0acb \u0aad\u0abe\u0a97\u0ac0\u0aa6\u0abe\u0ab0 \u0aa5\u0abe\u0aaf \u0a9b\u0ac7."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0aae\u0abe\u0ab0\u0ac7 \u0aa4\u0aae\u0aa8\u0ac7 \u0ab2\u0a96\u0ab5\u0abe\u0aa8\u0ac1\u0a82 \u0aa4\u0acb \u0a98\u0aa3\u0ac1\u0a82 \u0a9b\u0ac7, \u0aa4\u0acb\u0aaa\u0aa3 \u0a95\u0abe\u0a97\u0ab3 \u0aa4\u0aa5\u0abe \u0ab6\u0abe\u0ab9\u0ac0\u0aa5\u0ac0 \u0ab2\u0a96\u0ab5\u0ac1\u0a82 \u0a8f\u0ab5\u0ac0 \u0aae\u0abe\u0ab0\u0ac0 \u0a87\u0a9a\u0acd\u0a9b\u0abe \u0aa8\u0aa5\u0ac0, \u0aaa\u0aa3 \u0aa4\u0aae\u0abe\u0ab0\u0acb \u0a86\u0aa8\u0a82\u0aa6 \u0ab8\u0a82\u0aaa\u0ac2\u0ab0\u0acd\u0aa3 \u0aa5\u0abe\u0aaf \u0aae\u0abe\u0a9f\u0ac7 \u0aa4\u0aae\u0abe\u0ab0\u0ac0 \u0aae\u0ac1\u0ab2\u0abe\u0a95\u0abe\u0aa4 \u0ab2\u0a88\u0aa8\u0ac7 \u0ab0\u0ac2\u0aac\u0ab0\u0ac2 \u0ab5\u0abe\u0aa4 \u0a95\u0ab0\u0ab5\u0abe\u0aa8\u0ac0 \u0ab9\u0ac1\u0a82 \u0a86\u0ab6\u0abe \u0ab0\u0abe\u0a96\u0ac1\u0a82 \u0a9b\u0ac1\u0a82."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0aa4\u0abe\u0ab0\u0ac0 \u0aaa\u0ab8\u0a82\u0aa6 \u0a95\u0ab0\u0ac7\u0ab2\u0ac0 \u0aac\u0ab9\u0ac7\u0aa8\u0aa8\u0abe\u0a82 \u0aac\u0abe\u0ab3\u0a95\u0acb \u0aa4\u0aa8\u0ac7 \u0a95\u0acd\u0ab7\u0ac7\u0aae\u0a95\u0ac1\u0ab6\u0ab3 \u0a95\u0ab9\u0ac7 \u0a9b\u0ac7."
                }
            }
        }
    }
}