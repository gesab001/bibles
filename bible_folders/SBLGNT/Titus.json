{
    "version": "SBLGNT",
    "book_name": "Titus",
    "book_nr": "56",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u03a0\u03b1\u1fe6\u03bb\u03bf\u03c2 \u03b4\u03bf\u1fe6\u03bb\u03bf\u03c2 \u03b8\u03b5\u03bf\u1fe6, \u1f00\u03c0\u03cc\u03c3\u03c4\u03bf\u03bb\u03bf\u03c2 \u03b4\u1f72 \u1f38\u03b7\u03c3\u03bf\u1fe6 \u03a7\u03c1\u03b9\u03c3\u03c4\u03bf\u1fe6 \u03ba\u03b1\u03c4\u1f70 \u03c0\u03af\u03c3\u03c4\u03b9\u03bd \u1f10\u03ba\u03bb\u03b5\u03ba\u03c4\u1ff6\u03bd \u03b8\u03b5\u03bf\u1fe6 \u03ba\u03b1\u1f76 \u1f10\u03c0\u03af\u03b3\u03bd\u03c9\u03c3\u03b9\u03bd \u1f00\u03bb\u03b7\u03b8\u03b5\u03af\u03b1\u03c2 \u03c4\u1fc6\u03c2 \u03ba\u03b1\u03c4\u2019 \u03b5\u1f50\u03c3\u03ad\u03b2\u03b5\u03b9\u03b1\u03bd"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1f10\u03c0\u2019 \u1f10\u03bb\u03c0\u03af\u03b4\u03b9 \u03b6\u03c9\u1fc6\u03c2 \u03b1\u1f30\u03c9\u03bd\u03af\u03bf\u03c5, \u1f23\u03bd \u1f10\u03c0\u03b7\u03b3\u03b3\u03b5\u03af\u03bb\u03b1\u03c4\u03bf \u1f41 \u1f00\u03c8\u03b5\u03c5\u03b4\u1f74\u03c2 \u03b8\u03b5\u1f78\u03c2 \u03c0\u03c1\u1f78 \u03c7\u03c1\u03cc\u03bd\u03c9\u03bd \u03b1\u1f30\u03c9\u03bd\u03af\u03c9\u03bd"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1f10\u03c6\u03b1\u03bd\u03ad\u03c1\u03c9\u03c3\u03b5\u03bd \u03b4\u1f72 \u03ba\u03b1\u03b9\u03c1\u03bf\u1fd6\u03c2 \u1f30\u03b4\u03af\u03bf\u03b9\u03c2, \u03c4\u1f78\u03bd \u03bb\u03cc\u03b3\u03bf\u03bd \u03b1\u1f50\u03c4\u03bf\u1fe6 \u1f10\u03bd \u03ba\u03b7\u03c1\u03cd\u03b3\u03bc\u03b1\u03c4\u03b9 \u1f43 \u1f10\u03c0\u03b9\u03c3\u03c4\u03b5\u03cd\u03b8\u03b7\u03bd \u1f10\u03b3\u1f7c \u03ba\u03b1\u03c4\u2019 \u1f10\u03c0\u03b9\u03c4\u03b1\u03b3\u1f74\u03bd \u03c4\u03bf\u1fe6 \u03c3\u03c9\u03c4\u1fc6\u03c1\u03bf\u03c2 \u1f21\u03bc\u1ff6\u03bd \u03b8\u03b5\u03bf\u1fe6,"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u03a4\u03af\u03c4\u1ff3 \u03b3\u03bd\u03b7\u03c3\u03af\u1ff3 \u03c4\u03ad\u03ba\u03bd\u1ff3 \u03ba\u03b1\u03c4\u1f70 \u03ba\u03bf\u03b9\u03bd\u1f74\u03bd \u03c0\u03af\u03c3\u03c4\u03b9\u03bd\u00b7 \u03c7\u03ac\u03c1\u03b9\u03c2 \u03ba\u03b1\u1f76 \u03b5\u1f30\u03c1\u03ae\u03bd\u03b7 \u1f00\u03c0\u1f78 \u03b8\u03b5\u03bf\u1fe6 \u03c0\u03b1\u03c4\u03c1\u1f78\u03c2 \u03ba\u03b1\u1f76 \u03a7\u03c1\u03b9\u03c3\u03c4\u03bf\u1fe6 \u1f38\u03b7\u03c3\u03bf\u1fe6 \u03c4\u03bf\u1fe6 \u03c3\u03c9\u03c4\u1fc6\u03c1\u03bf\u03c2 \u1f21\u03bc\u1ff6\u03bd."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u03a4\u03bf\u03cd\u03c4\u03bf\u03c5 \u03c7\u03ac\u03c1\u03b9\u03bd \u1f00\u03c0\u03ad\u03bb\u03b9\u03c0\u03cc\u03bd \u03c3\u03b5 \u1f10\u03bd \u039a\u03c1\u03ae\u03c4\u1fc3 \u1f35\u03bd\u03b1 \u03c4\u1f70 \u03bb\u03b5\u03af\u03c0\u03bf\u03bd\u03c4\u03b1 \u1f10\u03c0\u03b9\u03b4\u03b9\u03bf\u03c1\u03b8\u03ce\u03c3\u1fc3, \u03ba\u03b1\u1f76 \u03ba\u03b1\u03c4\u03b1\u03c3\u03c4\u03ae\u03c3\u1fc3\u03c2 \u03ba\u03b1\u03c4\u1f70 \u03c0\u03cc\u03bb\u03b9\u03bd \u03c0\u03c1\u03b5\u03c3\u03b2\u03c5\u03c4\u03ad\u03c1\u03bf\u03c5\u03c2, \u1f61\u03c2 \u1f10\u03b3\u03ce \u03c3\u03bf\u03b9 \u03b4\u03b9\u03b5\u03c4\u03b1\u03be\u03ac\u03bc\u03b7\u03bd,"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u03b5\u1f34 \u03c4\u03af\u03c2 \u1f10\u03c3\u03c4\u03b9\u03bd \u1f00\u03bd\u03ad\u03b3\u03ba\u03bb\u03b7\u03c4\u03bf\u03c2, \u03bc\u03b9\u1fb6\u03c2 \u03b3\u03c5\u03bd\u03b1\u03b9\u03ba\u1f78\u03c2 \u1f00\u03bd\u03ae\u03c1, \u03c4\u03ad\u03ba\u03bd\u03b1 \u1f14\u03c7\u03c9\u03bd \u03c0\u03b9\u03c3\u03c4\u03ac, \u03bc\u1f74 \u1f10\u03bd \u03ba\u03b1\u03c4\u03b7\u03b3\u03bf\u03c1\u03af\u1fb3 \u1f00\u03c3\u03c9\u03c4\u03af\u03b1\u03c2 \u1f22 \u1f00\u03bd\u03c5\u03c0\u03cc\u03c4\u03b1\u03ba\u03c4\u03b1."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u03b4\u03b5\u1fd6 \u03b3\u1f70\u03c1 \u03c4\u1f78\u03bd \u1f10\u03c0\u03af\u03c3\u03ba\u03bf\u03c0\u03bf\u03bd \u1f00\u03bd\u03ad\u03b3\u03ba\u03bb\u03b7\u03c4\u03bf\u03bd \u03b5\u1f36\u03bd\u03b1\u03b9 \u1f61\u03c2 \u03b8\u03b5\u03bf\u1fe6 \u03bf\u1f30\u03ba\u03bf\u03bd\u03cc\u03bc\u03bf\u03bd, \u03bc\u1f74 \u03b1\u1f50\u03b8\u03ac\u03b4\u03b7, \u03bc\u1f74 \u1f40\u03c1\u03b3\u03af\u03bb\u03bf\u03bd, \u03bc\u1f74 \u03c0\u03ac\u03c1\u03bf\u03b9\u03bd\u03bf\u03bd, \u03bc\u1f74 \u03c0\u03bb\u03ae\u03ba\u03c4\u03b7\u03bd, \u03bc\u1f74 \u03b1\u1f30\u03c3\u03c7\u03c1\u03bf\u03ba\u03b5\u03c1\u03b4\u1fc6,"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1f00\u03bb\u03bb\u1f70 \u03c6\u03b9\u03bb\u03cc\u03be\u03b5\u03bd\u03bf\u03bd, \u03c6\u03b9\u03bb\u03ac\u03b3\u03b1\u03b8\u03bf\u03bd, \u03c3\u03ce\u03c6\u03c1\u03bf\u03bd\u03b1, \u03b4\u03af\u03ba\u03b1\u03b9\u03bf\u03bd, \u1f45\u03c3\u03b9\u03bf\u03bd, \u1f10\u03b3\u03ba\u03c1\u03b1\u03c4\u1fc6,"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1f00\u03bd\u03c4\u03b5\u03c7\u03cc\u03bc\u03b5\u03bd\u03bf\u03bd \u03c4\u03bf\u1fe6 \u03ba\u03b1\u03c4\u1f70 \u03c4\u1f74\u03bd \u03b4\u03b9\u03b4\u03b1\u03c7\u1f74\u03bd \u03c0\u03b9\u03c3\u03c4\u03bf\u1fe6 \u03bb\u03cc\u03b3\u03bf\u03c5, \u1f35\u03bd\u03b1 \u03b4\u03c5\u03bd\u03b1\u03c4\u1f78\u03c2 \u1f96 \u03ba\u03b1\u1f76 \u03c0\u03b1\u03c1\u03b1\u03ba\u03b1\u03bb\u03b5\u1fd6\u03bd \u1f10\u03bd \u03c4\u1fc7 \u03b4\u03b9\u03b4\u03b1\u03c3\u03ba\u03b1\u03bb\u03af\u1fb3 \u03c4\u1fc7 \u1f51\u03b3\u03b9\u03b1\u03b9\u03bd\u03bf\u03cd\u03c3\u1fc3 \u03ba\u03b1\u1f76 \u03c4\u03bf\u1f7a\u03c2 \u1f00\u03bd\u03c4\u03b9\u03bb\u03ad\u03b3\u03bf\u03bd\u03c4\u03b1\u03c2 \u1f10\u03bb\u03ad\u03b3\u03c7\u03b5\u03b9\u03bd."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0395\u1f30\u03c3\u1f76\u03bd \u03b3\u1f70\u03c1 \u03c0\u03bf\u03bb\u03bb\u03bf\u1f76 \u03ba\u03b1\u1f76 \u1f00\u03bd\u03c5\u03c0\u03cc\u03c4\u03b1\u03ba\u03c4\u03bf\u03b9, \u03bc\u03b1\u03c4\u03b1\u03b9\u03bf\u03bb\u03cc\u03b3\u03bf\u03b9 \u03ba\u03b1\u1f76 \u03c6\u03c1\u03b5\u03bd\u03b1\u03c0\u03ac\u03c4\u03b1\u03b9, \u03bc\u03ac\u03bb\u03b9\u03c3\u03c4\u03b1 \u03bf\u1f31 \u1f10\u03ba \u03c4\u1fc6\u03c2 \u03c0\u03b5\u03c1\u03b9\u03c4\u03bf\u03bc\u1fc6\u03c2,"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u03bf\u1f53\u03c2 \u03b4\u03b5\u1fd6 \u1f10\u03c0\u03b9\u03c3\u03c4\u03bf\u03bc\u03af\u03b6\u03b5\u03b9\u03bd, \u03bf\u1f35\u03c4\u03b9\u03bd\u03b5\u03c2 \u1f45\u03bb\u03bf\u03c5\u03c2 \u03bf\u1f34\u03ba\u03bf\u03c5\u03c2 \u1f00\u03bd\u03b1\u03c4\u03c1\u03ad\u03c0\u03bf\u03c5\u03c3\u03b9\u03bd \u03b4\u03b9\u03b4\u03ac\u03c3\u03ba\u03bf\u03bd\u03c4\u03b5\u03c2 \u1f03 \u03bc\u1f74 \u03b4\u03b5\u1fd6 \u03b1\u1f30\u03c3\u03c7\u03c1\u03bf\u1fe6 \u03ba\u03ad\u03c1\u03b4\u03bf\u03c5\u03c2 \u03c7\u03ac\u03c1\u03b9\u03bd."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u03b5\u1f36\u03c0\u03ad\u03bd \u03c4\u03b9\u03c2 \u1f10\u03be \u03b1\u1f50\u03c4\u1ff6\u03bd, \u1f34\u03b4\u03b9\u03bf\u03c2 \u03b1\u1f50\u03c4\u1ff6\u03bd \u03c0\u03c1\u03bf\u03c6\u03ae\u03c4\u03b7\u03c2, \u039a\u03c1\u1fc6\u03c4\u03b5\u03c2 \u1f00\u03b5\u1f76 \u03c8\u03b5\u1fe6\u03c3\u03c4\u03b1\u03b9, \u03ba\u03b1\u03ba\u1f70 \u03b8\u03b7\u03c1\u03af\u03b1, \u03b3\u03b1\u03c3\u03c4\u03ad\u03c1\u03b5\u03c2 \u1f00\u03c1\u03b3\u03b1\u03af\u00b7"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1f21 \u03bc\u03b1\u03c1\u03c4\u03c5\u03c1\u03af\u03b1 \u03b1\u1f55\u03c4\u03b7 \u1f10\u03c3\u03c4\u1f76\u03bd \u1f00\u03bb\u03b7\u03b8\u03ae\u03c2. \u03b4\u03b9\u2019 \u1f23\u03bd \u03b1\u1f30\u03c4\u03af\u03b1\u03bd \u1f14\u03bb\u03b5\u03b3\u03c7\u03b5 \u03b1\u1f50\u03c4\u03bf\u1f7a\u03c2 \u1f00\u03c0\u03bf\u03c4\u03cc\u03bc\u03c9\u03c2, \u1f35\u03bd\u03b1 \u1f51\u03b3\u03b9\u03b1\u03af\u03bd\u03c9\u03c3\u03b9\u03bd \u1f10\u03bd \u03c4\u1fc7 \u03c0\u03af\u03c3\u03c4\u03b5\u03b9,"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u03bc\u1f74 \u03c0\u03c1\u03bf\u03c3\u03ad\u03c7\u03bf\u03bd\u03c4\u03b5\u03c2 \u1f38\u03bf\u03c5\u03b4\u03b1\u03ca\u03ba\u03bf\u1fd6\u03c2 \u03bc\u03cd\u03b8\u03bf\u03b9\u03c2 \u03ba\u03b1\u1f76 \u1f10\u03bd\u03c4\u03bf\u03bb\u03b1\u1fd6\u03c2 \u1f00\u03bd\u03b8\u03c1\u03ce\u03c0\u03c9\u03bd \u1f00\u03c0\u03bf\u03c3\u03c4\u03c1\u03b5\u03c6\u03bf\u03bc\u03ad\u03bd\u03c9\u03bd \u03c4\u1f74\u03bd \u1f00\u03bb\u03ae\u03b8\u03b5\u03b9\u03b1\u03bd."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u03c0\u03ac\u03bd\u03c4\u03b1 \u03ba\u03b1\u03b8\u03b1\u03c1\u1f70 \u03c4\u03bf\u1fd6\u03c2 \u03ba\u03b1\u03b8\u03b1\u03c1\u03bf\u1fd6\u03c2\u00b7 \u03c4\u03bf\u1fd6\u03c2 \u03b4\u1f72 \u03bc\u03b5\u03bc\u03b9\u03b1\u03bc\u03bc\u03ad\u03bd\u03bf\u03b9\u03c2 \u03ba\u03b1\u1f76 \u1f00\u03c0\u03af\u03c3\u03c4\u03bf\u03b9\u03c2 \u03bf\u1f50\u03b4\u1f72\u03bd \u03ba\u03b1\u03b8\u03b1\u03c1\u03cc\u03bd, \u1f00\u03bb\u03bb\u1f70 \u03bc\u03b5\u03bc\u03af\u03b1\u03bd\u03c4\u03b1\u03b9 \u03b1\u1f50\u03c4\u1ff6\u03bd \u03ba\u03b1\u1f76 \u1f41 \u03bd\u03bf\u1fe6\u03c2 \u03ba\u03b1\u1f76 \u1f21 \u03c3\u03c5\u03bd\u03b5\u03af\u03b4\u03b7\u03c3\u03b9\u03c2."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u03b8\u03b5\u1f78\u03bd \u1f41\u03bc\u03bf\u03bb\u03bf\u03b3\u03bf\u1fe6\u03c3\u03b9\u03bd \u03b5\u1f30\u03b4\u03ad\u03bd\u03b1\u03b9, \u03c4\u03bf\u1fd6\u03c2 \u03b4\u1f72 \u1f14\u03c1\u03b3\u03bf\u03b9\u03c2 \u1f00\u03c1\u03bd\u03bf\u1fe6\u03bd\u03c4\u03b1\u03b9, \u03b2\u03b4\u03b5\u03bb\u03c5\u03ba\u03c4\u03bf\u1f76 \u1f44\u03bd\u03c4\u03b5\u03c2 \u03ba\u03b1\u1f76 \u1f00\u03c0\u03b5\u03b9\u03b8\u03b5\u1fd6\u03c2 \u03ba\u03b1\u1f76 \u03c0\u03c1\u1f78\u03c2 \u03c0\u1fb6\u03bd \u1f14\u03c1\u03b3\u03bf\u03bd \u1f00\u03b3\u03b1\u03b8\u1f78\u03bd \u1f00\u03b4\u03cc\u03ba\u03b9\u03bc\u03bf\u03b9."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u03a3\u1f7a \u03b4\u1f72 \u03bb\u03ac\u03bb\u03b5\u03b9 \u1f03 \u03c0\u03c1\u03ad\u03c0\u03b5\u03b9 \u03c4\u1fc7 \u1f51\u03b3\u03b9\u03b1\u03b9\u03bd\u03bf\u03cd\u03c3\u1fc3 \u03b4\u03b9\u03b4\u03b1\u03c3\u03ba\u03b1\u03bb\u03af\u1fb3."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u03c0\u03c1\u03b5\u03c3\u03b2\u03cd\u03c4\u03b1\u03c2 \u03bd\u03b7\u03c6\u03b1\u03bb\u03af\u03bf\u03c5\u03c2 \u03b5\u1f36\u03bd\u03b1\u03b9, \u03c3\u03b5\u03bc\u03bd\u03bf\u03cd\u03c2, \u03c3\u03ce\u03c6\u03c1\u03bf\u03bd\u03b1\u03c2, \u1f51\u03b3\u03b9\u03b1\u03af\u03bd\u03bf\u03bd\u03c4\u03b1\u03c2 \u03c4\u1fc7 \u03c0\u03af\u03c3\u03c4\u03b5\u03b9, \u03c4\u1fc7 \u1f00\u03b3\u03ac\u03c0\u1fc3, \u03c4\u1fc7 \u1f51\u03c0\u03bf\u03bc\u03bf\u03bd\u1fc7."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u03a0\u03c1\u03b5\u03c3\u03b2\u03cd\u03c4\u03b9\u03b4\u03b1\u03c2 \u1f61\u03c3\u03b1\u03cd\u03c4\u03c9\u03c2 \u1f10\u03bd \u03ba\u03b1\u03c4\u03b1\u03c3\u03c4\u03ae\u03bc\u03b1\u03c4\u03b9 \u1f31\u03b5\u03c1\u03bf\u03c0\u03c1\u03b5\u03c0\u03b5\u1fd6\u03c2, \u03bc\u1f74 \u03b4\u03b9\u03b1\u03b2\u03cc\u03bb\u03bf\u03c5\u03c2 \u03bc\u03b7\u03b4\u1f72 \u03bf\u1f34\u03bd\u1ff3 \u03c0\u03bf\u03bb\u03bb\u1ff7 \u03b4\u03b5\u03b4\u03bf\u03c5\u03bb\u03c9\u03bc\u03ad\u03bd\u03b1\u03c2, \u03ba\u03b1\u03bb\u03bf\u03b4\u03b9\u03b4\u03b1\u03c3\u03ba\u03ac\u03bb\u03bf\u03c5\u03c2,"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1f35\u03bd\u03b1 \u03c3\u03c9\u03c6\u03c1\u03bf\u03bd\u03af\u03b6\u03c9\u03c3\u03b9 \u03c4\u1f70\u03c2 \u03bd\u03ad\u03b1\u03c2 \u03c6\u03b9\u03bb\u03ac\u03bd\u03b4\u03c1\u03bf\u03c5\u03c2 \u03b5\u1f36\u03bd\u03b1\u03b9, \u03c6\u03b9\u03bb\u03bf\u03c4\u03ad\u03ba\u03bd\u03bf\u03c5\u03c2,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u03c3\u03ce\u03c6\u03c1\u03bf\u03bd\u03b1\u03c2, \u1f01\u03b3\u03bd\u03ac\u03c2, \u03bf\u1f30\u03ba\u03bf\u03c5\u03c1\u03b3\u03bf\u03cd\u03c2, \u1f00\u03b3\u03b1\u03b8\u03ac\u03c2, \u1f51\u03c0\u03bf\u03c4\u03b1\u03c3\u03c3\u03bf\u03bc\u03ad\u03bd\u03b1\u03c2 \u03c4\u03bf\u1fd6\u03c2 \u1f30\u03b4\u03af\u03bf\u03b9\u03c2 \u1f00\u03bd\u03b4\u03c1\u03ac\u03c3\u03b9\u03bd, \u1f35\u03bd\u03b1 \u03bc\u1f74 \u1f41 \u03bb\u03cc\u03b3\u03bf\u03c2 \u03c4\u03bf\u1fe6 \u03b8\u03b5\u03bf\u1fe6 \u03b2\u03bb\u03b1\u03c3\u03c6\u03b7\u03bc\u1fc6\u03c4\u03b1\u03b9."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u03a4\u03bf\u1f7a\u03c2 \u03bd\u03b5\u03c9\u03c4\u03ad\u03c1\u03bf\u03c5\u03c2 \u1f61\u03c3\u03b1\u03cd\u03c4\u03c9\u03c2 \u03c0\u03b1\u03c1\u03b1\u03ba\u03ac\u03bb\u03b5\u03b9 \u03c3\u03c9\u03c6\u03c1\u03bf\u03bd\u03b5\u1fd6\u03bd\u00b7"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u03c0\u03b5\u03c1\u1f76 \u03c0\u03ac\u03bd\u03c4\u03b1 \u03c3\u03b5\u03b1\u03c5\u03c4\u1f78\u03bd \u03c0\u03b1\u03c1\u03b5\u03c7\u03cc\u03bc\u03b5\u03bd\u03bf\u03c2 \u03c4\u03cd\u03c0\u03bf\u03bd \u03ba\u03b1\u03bb\u1ff6\u03bd \u1f14\u03c1\u03b3\u03c9\u03bd, \u1f10\u03bd \u03c4\u1fc7 \u03b4\u03b9\u03b4\u03b1\u03c3\u03ba\u03b1\u03bb\u03af\u1fb3 \u1f00\u03c6\u03b8\u03bf\u03c1\u03af\u03b1\u03bd, \u03c3\u03b5\u03bc\u03bd\u03cc\u03c4\u03b7\u03c4\u03b1,"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u03bb\u03cc\u03b3\u03bf\u03bd \u1f51\u03b3\u03b9\u1fc6 \u1f00\u03ba\u03b1\u03c4\u03ac\u03b3\u03bd\u03c9\u03c3\u03c4\u03bf\u03bd, \u1f35\u03bd\u03b1 \u1f41 \u1f10\u03be \u1f10\u03bd\u03b1\u03bd\u03c4\u03af\u03b1\u03c2 \u1f10\u03bd\u03c4\u03c1\u03b1\u03c0\u1fc7 \u03bc\u03b7\u03b4\u1f72\u03bd \u1f14\u03c7\u03c9\u03bd \u03bb\u03ad\u03b3\u03b5\u03b9\u03bd \u03c0\u03b5\u03c1\u1f76 \u1f21\u03bc\u1ff6\u03bd \u03c6\u03b1\u1fe6\u03bb\u03bf\u03bd."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0394\u03bf\u03cd\u03bb\u03bf\u03c5\u03c2 \u1f30\u03b4\u03af\u03bf\u03b9\u03c2 \u03b4\u03b5\u03c3\u03c0\u03cc\u03c4\u03b1\u03b9\u03c2 \u1f51\u03c0\u03bf\u03c4\u03ac\u03c3\u03c3\u03b5\u03c3\u03b8\u03b1\u03b9 \u1f10\u03bd \u03c0\u1fb6\u03c3\u03b9\u03bd, \u03b5\u1f50\u03b1\u03c1\u03ad\u03c3\u03c4\u03bf\u03c5\u03c2 \u03b5\u1f36\u03bd\u03b1\u03b9, \u03bc\u1f74 \u1f00\u03bd\u03c4\u03b9\u03bb\u03ad\u03b3\u03bf\u03bd\u03c4\u03b1\u03c2,"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u03bc\u1f74 \u03bd\u03bf\u03c3\u03c6\u03b9\u03b6\u03bf\u03bc\u03ad\u03bd\u03bf\u03c5\u03c2, \u1f00\u03bb\u03bb\u1f70 \u03c0\u1fb6\u03c3\u03b1\u03bd \u03c0\u03af\u03c3\u03c4\u03b9\u03bd \u1f10\u03bd\u03b4\u03b5\u03b9\u03ba\u03bd\u03c5\u03bc\u03ad\u03bd\u03bf\u03c5\u03c2 \u1f00\u03b3\u03b1\u03b8\u03ae\u03bd, \u1f35\u03bd\u03b1 \u03c4\u1f74\u03bd \u03b4\u03b9\u03b4\u03b1\u03c3\u03ba\u03b1\u03bb\u03af\u03b1\u03bd \u03c4\u1f74\u03bd \u03c4\u03bf\u1fe6 \u03c3\u03c9\u03c4\u1fc6\u03c1\u03bf\u03c2 \u1f21\u03bc\u1ff6\u03bd \u03b8\u03b5\u03bf\u1fe6 \u03ba\u03bf\u03c3\u03bc\u1ff6\u03c3\u03b9\u03bd \u1f10\u03bd \u03c0\u1fb6\u03c3\u03b9\u03bd."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1f18\u03c0\u03b5\u03c6\u03ac\u03bd\u03b7 \u03b3\u1f70\u03c1 \u1f21 \u03c7\u03ac\u03c1\u03b9\u03c2 \u03c4\u03bf\u1fe6 \u03b8\u03b5\u03bf\u1fe6 \u03c3\u03c9\u03c4\u03ae\u03c1\u03b9\u03bf\u03c2 \u03c0\u1fb6\u03c3\u03b9\u03bd \u1f00\u03bd\u03b8\u03c1\u03ce\u03c0\u03bf\u03b9\u03c2"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u03c0\u03b1\u03b9\u03b4\u03b5\u03cd\u03bf\u03c5\u03c3\u03b1 \u1f21\u03bc\u1fb6\u03c2, \u1f35\u03bd\u03b1 \u1f00\u03c1\u03bd\u03b7\u03c3\u03ac\u03bc\u03b5\u03bd\u03bf\u03b9 \u03c4\u1f74\u03bd \u1f00\u03c3\u03ad\u03b2\u03b5\u03b9\u03b1\u03bd \u03ba\u03b1\u1f76 \u03c4\u1f70\u03c2 \u03ba\u03bf\u03c3\u03bc\u03b9\u03ba\u1f70\u03c2 \u1f10\u03c0\u03b9\u03b8\u03c5\u03bc\u03af\u03b1\u03c2 \u03c3\u03c9\u03c6\u03c1\u03cc\u03bd\u03c9\u03c2 \u03ba\u03b1\u1f76 \u03b4\u03b9\u03ba\u03b1\u03af\u03c9\u03c2 \u03ba\u03b1\u1f76 \u03b5\u1f50\u03c3\u03b5\u03b2\u1ff6\u03c2 \u03b6\u03ae\u03c3\u03c9\u03bc\u03b5\u03bd \u1f10\u03bd \u03c4\u1ff7 \u03bd\u1fe6\u03bd \u03b1\u1f30\u1ff6\u03bd\u03b9,"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u03c0\u03c1\u03bf\u03c3\u03b4\u03b5\u03c7\u03cc\u03bc\u03b5\u03bd\u03bf\u03b9 \u03c4\u1f74\u03bd \u03bc\u03b1\u03ba\u03b1\u03c1\u03af\u03b1\u03bd \u1f10\u03bb\u03c0\u03af\u03b4\u03b1 \u03ba\u03b1\u1f76 \u1f10\u03c0\u03b9\u03c6\u03ac\u03bd\u03b5\u03b9\u03b1\u03bd \u03c4\u1fc6\u03c2 \u03b4\u03cc\u03be\u03b7\u03c2 \u03c4\u03bf\u1fe6 \u03bc\u03b5\u03b3\u03ac\u03bb\u03bf\u03c5 \u03b8\u03b5\u03bf\u1fe6 \u03ba\u03b1\u1f76 \u03c3\u03c9\u03c4\u1fc6\u03c1\u03bf\u03c2 \u1f21\u03bc\u1ff6\u03bd \u1f38\u03b7\u03c3\u03bf\u1fe6 \u03a7\u03c1\u03b9\u03c3\u03c4\u03bf\u1fe6,"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u1f43\u03c2 \u1f14\u03b4\u03c9\u03ba\u03b5\u03bd \u1f11\u03b1\u03c5\u03c4\u1f78\u03bd \u1f51\u03c0\u1f72\u03c1 \u1f21\u03bc\u1ff6\u03bd \u1f35\u03bd\u03b1 \u03bb\u03c5\u03c4\u03c1\u03ce\u03c3\u03b7\u03c4\u03b1\u03b9 \u1f21\u03bc\u1fb6\u03c2 \u1f00\u03c0\u1f78 \u03c0\u03ac\u03c3\u03b7\u03c2 \u1f00\u03bd\u03bf\u03bc\u03af\u03b1\u03c2 \u03ba\u03b1\u1f76 \u03ba\u03b1\u03b8\u03b1\u03c1\u03af\u03c3\u1fc3 \u1f11\u03b1\u03c5\u03c4\u1ff7 \u03bb\u03b1\u1f78\u03bd \u03c0\u03b5\u03c1\u03b9\u03bf\u03cd\u03c3\u03b9\u03bf\u03bd, \u03b6\u03b7\u03bb\u03c9\u03c4\u1f74\u03bd \u03ba\u03b1\u03bb\u1ff6\u03bd \u1f14\u03c1\u03b3\u03c9\u03bd."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u03a4\u03b1\u1fe6\u03c4\u03b1 \u03bb\u03ac\u03bb\u03b5\u03b9 \u03ba\u03b1\u1f76 \u03c0\u03b1\u03c1\u03b1\u03ba\u03ac\u03bb\u03b5\u03b9 \u03ba\u03b1\u1f76 \u1f14\u03bb\u03b5\u03b3\u03c7\u03b5 \u03bc\u03b5\u03c4\u1f70 \u03c0\u03ac\u03c3\u03b7\u03c2 \u1f10\u03c0\u03b9\u03c4\u03b1\u03b3\u1fc6\u03c2\u00b7 \u03bc\u03b7\u03b4\u03b5\u03af\u03c2 \u03c3\u03bf\u03c5 \u03c0\u03b5\u03c1\u03b9\u03c6\u03c1\u03bf\u03bd\u03b5\u03af\u03c4\u03c9."
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1f59\u03c0\u03bf\u03bc\u03af\u03bc\u03bd\u1fc3\u03c3\u03ba\u03b5 \u03b1\u1f50\u03c4\u03bf\u1f7a\u03c2 \u1f00\u03c1\u03c7\u03b1\u1fd6\u03c2 \u1f10\u03be\u03bf\u03c5\u03c3\u03af\u03b1\u03b9\u03c2 \u1f51\u03c0\u03bf\u03c4\u03ac\u03c3\u03c3\u03b5\u03c3\u03b8\u03b1\u03b9 \u03c0\u03b5\u03b9\u03b8\u03b1\u03c1\u03c7\u03b5\u1fd6\u03bd, \u03c0\u03c1\u1f78\u03c2 \u03c0\u1fb6\u03bd \u1f14\u03c1\u03b3\u03bf\u03bd \u1f00\u03b3\u03b1\u03b8\u1f78\u03bd \u1f11\u03c4\u03bf\u03af\u03bc\u03bf\u03c5\u03c2 \u03b5\u1f36\u03bd\u03b1\u03b9,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u03bc\u03b7\u03b4\u03ad\u03bd\u03b1 \u03b2\u03bb\u03b1\u03c3\u03c6\u03b7\u03bc\u03b5\u1fd6\u03bd, \u1f00\u03bc\u03ac\u03c7\u03bf\u03c5\u03c2 \u03b5\u1f36\u03bd\u03b1\u03b9, \u1f10\u03c0\u03b9\u03b5\u03b9\u03ba\u03b5\u1fd6\u03c2, \u03c0\u1fb6\u03c3\u03b1\u03bd \u1f10\u03bd\u03b4\u03b5\u03b9\u03ba\u03bd\u03c5\u03bc\u03ad\u03bd\u03bf\u03c5\u03c2 \u03c0\u03c1\u03b1\u03b0\u03c4\u03b7\u03c4\u03b1 \u03c0\u03c1\u1f78\u03c2 \u03c0\u03ac\u03bd\u03c4\u03b1\u03c2 \u1f00\u03bd\u03b8\u03c1\u03ce\u03c0\u03bf\u03c5\u03c2."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1f26\u03bc\u03b5\u03bd \u03b3\u03ac\u03c1 \u03c0\u03bf\u03c4\u03b5 \u03ba\u03b1\u1f76 \u1f21\u03bc\u03b5\u1fd6\u03c2 \u1f00\u03bd\u03cc\u03b7\u03c4\u03bf\u03b9, \u1f00\u03c0\u03b5\u03b9\u03b8\u03b5\u1fd6\u03c2, \u03c0\u03bb\u03b1\u03bd\u03ce\u03bc\u03b5\u03bd\u03bf\u03b9, \u03b4\u03bf\u03c5\u03bb\u03b5\u03cd\u03bf\u03bd\u03c4\u03b5\u03c2 \u1f10\u03c0\u03b9\u03b8\u03c5\u03bc\u03af\u03b1\u03b9\u03c2 \u03ba\u03b1\u1f76 \u1f21\u03b4\u03bf\u03bd\u03b1\u1fd6\u03c2 \u03c0\u03bf\u03b9\u03ba\u03af\u03bb\u03b1\u03b9\u03c2, \u1f10\u03bd \u03ba\u03b1\u03ba\u03af\u1fb3 \u03ba\u03b1\u1f76 \u03c6\u03b8\u03cc\u03bd\u1ff3 \u03b4\u03b9\u03ac\u03b3\u03bf\u03bd\u03c4\u03b5\u03c2, \u03c3\u03c4\u03c5\u03b3\u03b7\u03c4\u03bf\u03af, \u03bc\u03b9\u03c3\u03bf\u1fe6\u03bd\u03c4\u03b5\u03c2 \u1f00\u03bb\u03bb\u03ae\u03bb\u03bf\u03c5\u03c2."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1f45\u03c4\u03b5 \u03b4\u1f72 \u1f21 \u03c7\u03c1\u03b7\u03c3\u03c4\u03cc\u03c4\u03b7\u03c2 \u03ba\u03b1\u1f76 \u1f21 \u03c6\u03b9\u03bb\u03b1\u03bd\u03b8\u03c1\u03c9\u03c0\u03af\u03b1 \u1f10\u03c0\u03b5\u03c6\u03ac\u03bd\u03b7 \u03c4\u03bf\u1fe6 \u03c3\u03c9\u03c4\u1fc6\u03c1\u03bf\u03c2 \u1f21\u03bc\u1ff6\u03bd \u03b8\u03b5\u03bf\u1fe6,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u03bf\u1f50\u03ba \u1f10\u03be \u1f14\u03c1\u03b3\u03c9\u03bd \u03c4\u1ff6\u03bd \u1f10\u03bd \u03b4\u03b9\u03ba\u03b1\u03b9\u03bf\u03c3\u03cd\u03bd\u1fc3 \u1f03 \u1f10\u03c0\u03bf\u03b9\u03ae\u03c3\u03b1\u03bc\u03b5\u03bd \u1f21\u03bc\u03b5\u1fd6\u03c2 \u1f00\u03bb\u03bb\u1f70 \u03ba\u03b1\u03c4\u1f70 \u03c4\u1f78 \u03b1\u1f50\u03c4\u03bf\u1fe6 \u1f14\u03bb\u03b5\u03bf\u03c2 \u1f14\u03c3\u03c9\u03c3\u03b5\u03bd \u1f21\u03bc\u1fb6\u03c2 \u03b4\u03b9\u1f70 \u03bb\u03bf\u03c5\u03c4\u03c1\u03bf\u1fe6 \u03c0\u03b1\u03bb\u03b9\u03b3\u03b3\u03b5\u03bd\u03b5\u03c3\u03af\u03b1\u03c2 \u03ba\u03b1\u1f76 \u1f00\u03bd\u03b1\u03ba\u03b1\u03b9\u03bd\u03ce\u03c3\u03b5\u03c9\u03c2 \u03c0\u03bd\u03b5\u03cd\u03bc\u03b1\u03c4\u03bf\u03c2 \u1f01\u03b3\u03af\u03bf\u03c5,"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u03bf\u1f57 \u1f10\u03be\u03ad\u03c7\u03b5\u03b5\u03bd \u1f10\u03c6\u2019 \u1f21\u03bc\u1fb6\u03c2 \u03c0\u03bb\u03bf\u03c5\u03c3\u03af\u03c9\u03c2 \u03b4\u03b9\u1f70 \u1f38\u03b7\u03c3\u03bf\u1fe6 \u03a7\u03c1\u03b9\u03c3\u03c4\u03bf\u1fe6 \u03c4\u03bf\u1fe6 \u03c3\u03c9\u03c4\u1fc6\u03c1\u03bf\u03c2 \u1f21\u03bc\u1ff6\u03bd,"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u1f35\u03bd\u03b1 \u03b4\u03b9\u03ba\u03b1\u03b9\u03c9\u03b8\u03ad\u03bd\u03c4\u03b5\u03c2 \u03c4\u1fc7 \u1f10\u03ba\u03b5\u03af\u03bd\u03bf\u03c5 \u03c7\u03ac\u03c1\u03b9\u03c4\u03b9 \u03ba\u03bb\u03b7\u03c1\u03bf\u03bd\u03cc\u03bc\u03bf\u03b9 \u03b3\u03b5\u03bd\u03b7\u03b8\u1ff6\u03bc\u03b5\u03bd \u03ba\u03b1\u03c4\u2019 \u1f10\u03bb\u03c0\u03af\u03b4\u03b1 \u03b6\u03c9\u1fc6\u03c2 \u03b1\u1f30\u03c9\u03bd\u03af\u03bf\u03c5."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u03a0\u03b9\u03c3\u03c4\u1f78\u03c2 \u1f41 \u03bb\u03cc\u03b3\u03bf\u03c2, \u03ba\u03b1\u1f76 \u03c0\u03b5\u03c1\u1f76 \u03c4\u03bf\u03cd\u03c4\u03c9\u03bd \u03b2\u03bf\u03cd\u03bb\u03bf\u03bc\u03b1\u03af \u03c3\u03b5 \u03b4\u03b9\u03b1\u03b2\u03b5\u03b2\u03b1\u03b9\u03bf\u1fe6\u03c3\u03b8\u03b1\u03b9, \u1f35\u03bd\u03b1 \u03c6\u03c1\u03bf\u03bd\u03c4\u03af\u03b6\u03c9\u03c3\u03b9\u03bd \u03ba\u03b1\u03bb\u1ff6\u03bd \u1f14\u03c1\u03b3\u03c9\u03bd \u03c0\u03c1\u03bf\u0390\u03c3\u03c4\u03b1\u03c3\u03b8\u03b1\u03b9 \u03bf\u1f31 \u03c0\u03b5\u03c0\u03b9\u03c3\u03c4\u03b5\u03c5\u03ba\u03cc\u03c4\u03b5\u03c2 \u03b8\u03b5\u1ff7. \u03c4\u03b1\u1fe6\u03c4\u03ac \u1f10\u03c3\u03c4\u03b9\u03bd \u03ba\u03b1\u03bb\u1f70 \u03ba\u03b1\u1f76 \u1f60\u03c6\u03ad\u03bb\u03b9\u03bc\u03b1 \u03c4\u03bf\u1fd6\u03c2 \u1f00\u03bd\u03b8\u03c1\u03ce\u03c0\u03bf\u03b9\u03c2\u00b7"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u03bc\u03c9\u03c1\u1f70\u03c2 \u03b4\u1f72 \u03b6\u03b7\u03c4\u03ae\u03c3\u03b5\u03b9\u03c2 \u03ba\u03b1\u1f76 \u03b3\u03b5\u03bd\u03b5\u03b1\u03bb\u03bf\u03b3\u03af\u03b1\u03c2 \u03ba\u03b1\u1f76 \u1f14\u03c1\u03b5\u03b9\u03c2 \u03ba\u03b1\u1f76 \u03bc\u03ac\u03c7\u03b1\u03c2 \u03bd\u03bf\u03bc\u03b9\u03ba\u1f70\u03c2 \u03c0\u03b5\u03c1\u03b9\u0390\u03c3\u03c4\u03b1\u03c3\u03bf, \u03b5\u1f30\u03c3\u1f76\u03bd \u03b3\u1f70\u03c1 \u1f00\u03bd\u03c9\u03c6\u03b5\u03bb\u03b5\u1fd6\u03c2 \u03ba\u03b1\u1f76 \u03bc\u03ac\u03c4\u03b1\u03b9\u03bf\u03b9."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u03b1\u1f31\u03c1\u03b5\u03c4\u03b9\u03ba\u1f78\u03bd \u1f04\u03bd\u03b8\u03c1\u03c9\u03c0\u03bf\u03bd \u03bc\u03b5\u03c4\u1f70 \u03bc\u03af\u03b1\u03bd \u03ba\u03b1\u1f76 \u03b4\u03b5\u03c5\u03c4\u03ad\u03c1\u03b1\u03bd \u03bd\u03bf\u03c5\u03b8\u03b5\u03c3\u03af\u03b1\u03bd \u03c0\u03b1\u03c1\u03b1\u03b9\u03c4\u03bf\u1fe6,"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u03b5\u1f30\u03b4\u1f7c\u03c2 \u1f45\u03c4\u03b9 \u1f10\u03be\u03ad\u03c3\u03c4\u03c1\u03b1\u03c0\u03c4\u03b1\u03b9 \u1f41 \u03c4\u03bf\u03b9\u03bf\u1fe6\u03c4\u03bf\u03c2 \u03ba\u03b1\u1f76 \u1f01\u03bc\u03b1\u03c1\u03c4\u03ac\u03bd\u03b5\u03b9, \u1f62\u03bd \u03b1\u1f50\u03c4\u03bf\u03ba\u03b1\u03c4\u03ac\u03ba\u03c1\u03b9\u03c4\u03bf\u03c2."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u1f4d\u03c4\u03b1\u03bd \u03c0\u03ad\u03bc\u03c8\u03c9 \u1f08\u03c1\u03c4\u03b5\u03bc\u1fb6\u03bd \u03c0\u03c1\u1f78\u03c2 \u03c3\u1f72 \u1f22 \u03a4\u03c5\u03c7\u03b9\u03ba\u03cc\u03bd, \u03c3\u03c0\u03bf\u03cd\u03b4\u03b1\u03c3\u03bf\u03bd \u1f10\u03bb\u03b8\u03b5\u1fd6\u03bd \u03c0\u03c1\u03cc\u03c2 \u03bc\u03b5 \u03b5\u1f30\u03c2 \u039d\u03b9\u03ba\u03cc\u03c0\u03bf\u03bb\u03b9\u03bd, \u1f10\u03ba\u03b5\u1fd6 \u03b3\u1f70\u03c1 \u03ba\u03ad\u03ba\u03c1\u03b9\u03ba\u03b1 \u03c0\u03b1\u03c1\u03b1\u03c7\u03b5\u03b9\u03bc\u03ac\u03c3\u03b1\u03b9."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u03b6\u03b7\u03bd\u1fb6\u03bd \u03c4\u1f78\u03bd \u03bd\u03bf\u03bc\u03b9\u03ba\u1f78\u03bd \u03ba\u03b1\u1f76 \u1f08\u03c0\u03bf\u03bb\u03bb\u1ff6\u03bd \u03c3\u03c0\u03bf\u03c5\u03b4\u03b1\u03af\u03c9\u03c2 \u03c0\u03c1\u03cc\u03c0\u03b5\u03bc\u03c8\u03bf\u03bd, \u1f35\u03bd\u03b1 \u03bc\u03b7\u03b4\u1f72\u03bd \u03b1\u1f50\u03c4\u03bf\u1fd6\u03c2 \u03bb\u03b5\u03af\u03c0\u1fc3."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u03bc\u03b1\u03bd\u03b8\u03b1\u03bd\u03ad\u03c4\u03c9\u03c3\u03b1\u03bd \u03b4\u1f72 \u03ba\u03b1\u1f76 \u03bf\u1f31 \u1f21\u03bc\u03ad\u03c4\u03b5\u03c1\u03bf\u03b9 \u03ba\u03b1\u03bb\u1ff6\u03bd \u1f14\u03c1\u03b3\u03c9\u03bd \u03c0\u03c1\u03bf\u0390\u03c3\u03c4\u03b1\u03c3\u03b8\u03b1\u03b9 \u03b5\u1f30\u03c2 \u03c4\u1f70\u03c2 \u1f00\u03bd\u03b1\u03b3\u03ba\u03b1\u03af\u03b1\u03c2 \u03c7\u03c1\u03b5\u03af\u03b1\u03c2, \u1f35\u03bd\u03b1 \u03bc\u1f74 \u1f66\u03c3\u03b9\u03bd \u1f04\u03ba\u03b1\u03c1\u03c0\u03bf\u03b9."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u1f08\u03c3\u03c0\u03ac\u03b6\u03bf\u03bd\u03c4\u03b1\u03af \u03c3\u03b5 \u03bf\u1f31 \u03bc\u03b5\u03c4\u2019 \u1f10\u03bc\u03bf\u1fe6 \u03c0\u03ac\u03bd\u03c4\u03b5\u03c2. \u1f04\u03c3\u03c0\u03b1\u03c3\u03b1\u03b9 \u03c4\u03bf\u1f7a\u03c2 \u03c6\u03b9\u03bb\u03bf\u1fe6\u03bd\u03c4\u03b1\u03c2 \u1f21\u03bc\u1fb6\u03c2 \u1f10\u03bd \u03c0\u03af\u03c3\u03c4\u03b5\u03b9. \u1f21 \u03c7\u03ac\u03c1\u03b9\u03c2 \u03bc\u03b5\u03c4\u1f70 \u03c0\u03ac\u03bd\u03c4\u03c9\u03bd \u1f51\u03bc\u1ff6\u03bd."
                }
            }
        }
    }
}