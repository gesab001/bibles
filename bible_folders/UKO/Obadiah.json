    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0412\u0438\u0434\u0456\u043d\u043d\u044f \u041e\u0432\u0434\u0456\u044f. \u0422\u0430\u043a \u0441\u043a\u0430\u0437\u0430\u0432 \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u0411\u043e\u0433 \u043d\u0430 \u0415\u0434\u043e\u043c: \u041f\u043e\u0447\u0443\u043b\u0438 \u043c\u0438 \u0432\u0456\u0441\u0442\u043a\u0443 \u0432\u0456\u0434 \u0413\u043e\u0441\u043f\u043e\u0434\u0430, \u0456 \u043f\u043e\u0441\u043b\u0430\u043d\u0438\u0439 \u0431\u0443\u0432 \u043c\u0456\u0436 \u043d\u0430\u0440\u043e\u0434\u0456\u0432 \u043f\u043e\u0441\u043e\u043b, \u0449\u043e\u0431 \u0441\u043a\u0430\u0437\u0430\u0442\u0438: \u0423\u0441\u0442\u0430\u0432\u0430\u0439\u0442\u0435, \u0456 \u0441\u0442\u0430\u043d\u044c\u043c\u043e \u043d\u0430 \u043d\u044c\u043e\u0433\u043e \u0434\u043e \u0431\u043e\u044e!"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u041e\u0446\u0435 \u042f \u043c\u0430\u043b\u0438\u043c \u0442\u0435\u0431\u0435 \u0434\u0430\u0432 \u043c\u0456\u0436 \u043d\u0430\u0440\u043e\u0434\u0438, \u0442\u0438 \u0434\u0443\u0436\u0435 \u043f\u043e\u0433\u043e\u0440\u0434\u0436\u0435\u043d\u0438\u0439."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0413\u043e\u0440\u0434\u0456\u0441\u0442\u044c \u0441\u0435\u0440\u0446\u044f \u0442\u0432\u043e\u0433\u043e \u043e\u0431\u043c\u0430\u043d\u0438\u043b\u0430 \u0442\u0435\u0431\u0435, \u044f\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u0431\u0443\u0432\u0430\u0454\u0448 \u043f\u043e \u0449\u0456\u043b\u0438\u043d\u0430\u0445 \u0441\u043a\u0435\u043b\u044c\u043d\u0438\u0445, \u0443 \u0432\u0438\u0441\u043e\u043a\u0456\u043c \u0441\u0438\u0434\u0456\u043d\u043d\u0456 \u0441\u0432\u043e\u0457\u043c, \u0449\u043e \u0433\u043e\u0432\u043e\u0440\u0438\u0448 \u0443 \u0441\u0435\u0440\u0446\u0456 \u0441\u0432\u043e\u0454\u043c\u0443: \u0425\u0442\u043e \u0441\u043a\u0438\u043d\u0435 \u043d\u0430 \u0437\u0435\u043c\u043b\u044e \u043c\u0435\u043d\u0435?"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u042f\u043a\u0449\u043e \u0431 \u0442\u0438 \u043f\u0456\u0434\u043d\u0456\u0441\u0441\u044f, \u043d\u0435\u043c\u043e\u0432 \u0442\u043e\u0439 \u043e\u0440\u0435\u043b, \u0456 \u044f\u043a\u0449\u043e \u0431 \u0442\u0438 \u043a\u0443\u0431\u043b\u043e \u0441\u0432\u043e\u0454 \u0441\u043a\u043b\u0430\u0432 \u043f\u043e\u043c\u0456\u0436 \u0437\u043e\u0440\u044f\u043c\u0438, \u0442\u043e \u0439 \u0437\u0432\u0456\u0434\u0442\u0438 \u042f \u0441\u043a\u0438\u043d\u0443 \u0442\u0435\u0431\u0435, \u043f\u0440\u043e\u043c\u043e\u0432\u043b\u044f\u0454 \u0413\u043e\u0441\u043f\u043e\u0434\u044c!"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0427\u0438 \u0436 \u0434\u043e \u0442\u0435\u0431\u0435 \u0437\u043b\u043e\u0434\u0456\u0457 \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u043b\u0438, \u0447\u0438 \u043d\u0456\u0447\u043d\u0456 \u0442\u0456 \u0433\u0440\u0430\u0431\u0456\u0436\u043d\u0438\u043a\u0438, \u0442\u0430\u043a\u0438\u0439 \u0442\u0438 \u043f\u043e\u043d\u0438\u0449\u0435\u043d\u0438\u0439! \u0447\u0438 \u0436 \u0432\u043e\u043d\u0438 \u043d\u0435 \u043d\u0430\u043a\u0440\u0430\u043b\u0438 \u0431 \u0441\u043e\u0431\u0456 \u0441\u043a\u0456\u043b\u044c\u043a\u0438 \u0442\u0440\u0435\u0431\u0430? \u042f\u043a\u0449\u043e \u0431 \u0434\u043e \u0442\u0435\u0431\u0435 \u043f\u0440\u0438\u0439\u0448\u043b\u0438 \u0437\u0431\u0438\u0440\u0430\u0447\u0456 \u0432\u0438\u043d\u043e\u0433\u0440\u0430\u0434\u0443, \u0447\u0438 \u0436 \u0432\u043e\u043d\u0438 \u043d\u0435 \u043b\u0438\u0448\u0438\u043b\u0438 \u0431 \u0445\u043e\u0447 \u0432\u0438\u0431\u0456\u0440\u043a\u0456\u0432?"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u042f\u043a \u0406\u0441\u0430\u0432 \u043f\u0435\u0440\u0435\u0448\u0443\u043a\u0430\u043d\u0438\u0439, \u044f\u043a \u043a\u0440\u0438\u0457\u0432\u043a\u0438 \u0439\u043e\u0433\u043e \u043f\u0435\u0440\u0435\u0433\u043b\u044f\u043d\u0435\u043d\u0456!"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0410\u0436 \u0434\u043e \u0433\u0440\u0430\u043d\u0438\u0446\u0456 \u043f\u0440\u043e\u0433\u043d\u0430\u043b\u0438 \u0442\u0435\u0431\u0435, \u043e\u0431\u043c\u0430\u043d\u044f\u0442\u044c \u0442\u0435\u0431\u0435 \u0442\u0432\u043e\u0457 \u0432\u0441\u0456 \u0441\u043e\u044e\u0437\u043d\u0438\u043a\u0438, \u043f\u0435\u0440\u0435\u043c\u043e\u0436\u0443\u0442\u044c \u0442\u0435\u0431\u0435 \u0442\u0432\u043e\u0457 \u043f\u0440\u0438\u044f\u0442\u0435\u043b\u0456! \u0422\u0456, \u0449\u043e \u0445\u043b\u0456\u0431 \u0442\u0432\u0456\u0439 \u0457\u0434\u044f\u0442\u044c, \u043f\u0430\u0441\u0442\u043a\u0443 \u0440\u043e\u0437\u0441\u0442\u0430\u0432\u043b\u044f\u0442\u044c \u043d\u0430 \u0442\u0435\u0431\u0435, \u043d\u0435\u043c\u0430 \u0432 \u0442\u043e\u043c\u0443 \u0440\u043e\u0437\u0443\u043c\u0443!"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0427\u0438 \u0436 \u043d\u0435 \u0441\u0442\u0430\u043d\u0435\u0442\u044c\u0441\u044f \u0446\u0435 \u0442\u043e\u0433\u043e \u0434\u043d\u044f, \u043f\u0440\u043e\u043c\u043e\u0432\u043b\u044f\u0454 \u0413\u043e\u0441\u043f\u043e\u0434\u044c, \u0456 \u0432\u0438\u0433\u0443\u0431\u043b\u044e \u042f \u043c\u0443\u0434\u0440\u0435\u0446\u0456\u0432 \u0456\u0437 \u0415\u0434\u043e\u043c\u0443, \u0430 \u0440\u043e\u0437\u0443\u043c \u0437 \u0433\u043e\u0440\u0438 \u0406\u0441\u0430\u0432\u043e\u0432\u043e\u0457?"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0406 \u043d\u0430\u0441\u0442\u0440\u0430\u0448\u0435\u043d\u0435 \u0431\u0443\u0434\u0435 \u043b\u0438\u0446\u0430\u0440\u0441\u0442\u0432\u043e \u0442\u0432\u043e\u0454, \u043e \u0422\u0435\u043c\u0430\u043d\u0435, \u0449\u043e\u0431 \u0431\u0443\u0432 \u0432\u0438\u0442\u044f\u0442\u0438\u0439 \u043a\u043e\u0436\u0435\u043d \u0437 \u0406\u0441\u0430\u0432\u0430 \u0443\u0431\u0438\u0432\u0441\u0442\u0432\u043e\u043c."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0427\u0435\u0440\u0435\u0437 \u043d\u0430\u0441\u0438\u043b\u043b\u044f \u043d\u0430 \u042f\u043a\u043e\u0432\u0430 \u043d\u0430 \u0431\u0440\u0430\u0442\u0430 \u0442\u0432\u043e\u0433\u043e \u0441\u043e\u0440\u043e\u043c \u043f\u043e\u043a\u0440\u0438\u0454 \u0442\u0435\u0431\u0435, \u0456 \u0442\u0438 \u0432\u0438\u0442\u044f\u0442\u0438\u0439 \u0431\u0443\u0434\u0435\u0448 \u043d\u0430\u0432\u0456\u043a\u0438."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0422\u043e\u0433\u043e \u0434\u043d\u044f, \u043a\u043e\u043b\u0438 \u0441\u0442\u0430\u0432 \u0442\u0438 \u043d\u0430\u0432\u043f\u0440\u043e\u0442\u0438, \u0442\u043e\u0433\u043e \u0434\u043d\u044f, \u044f\u043a \u0447\u0443\u0436\u0456 \u043f\u043e\u043b\u043e\u043d\u0438\u043b\u0438 \u0431\u0443\u043b\u0438 \u0439\u043e\u0433\u043e \u0432\u0456\u0439\u0441\u044c\u043a\u043e, \u0456 \u043a\u043e\u043b\u0438 \u0447\u0443\u0436\u0438\u043d\u0446\u0456 \u0432 \u0439\u043e\u0433\u043e \u0431\u0440\u0430\u043c\u0438 \u0432\u0432\u0456\u0439\u0448\u043b\u0438 \u0456 \u0436\u0435\u0440\u0435\u0431\u0430 \u043a\u0438\u0434\u0430\u043b\u0438 \u043f\u0440\u043e \u0404\u0440\u0443\u0441\u0430\u043b\u0438\u043c, \u0442\u043e \u0439 \u0442\u0438 \u0431\u0443\u0432, \u044f\u043a \u043e\u0434\u0438\u043d \u0437 \u043d\u0438\u0445!"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0406 \u0442\u043e\u043c\u0443 \u043d\u0435 \u0434\u0438\u0432\u0438\u0441\u044f \u043d\u0430 \u0434\u0435\u043d\u044c \u0441\u0432\u043e\u0433\u043e \u0431\u0440\u0430\u0442\u0430, \u043d\u0430 \u0434\u0435\u043d\u044c \u043b\u0438\u0445\u0430 \u0439\u043e\u0433\u043e, \u0456 \u043d\u0435 \u0442\u0456\u0448\u0441\u044f \u0437 \u042e\u0434\u0438\u043d\u0438\u0445 \u0441\u0438\u043d\u0456\u0432 \u0443 \u0434\u0435\u043d\u044c \u0457\u0445\u043d\u044c\u043e\u0457 \u0437\u0433\u0443\u0431\u0438, \u0456 \u043d\u0435 \u0440\u043e\u0437\u043a\u0440\u0438\u0432\u0430\u0439 \u0441\u0432\u043e\u0457\u0445 \u0443\u0441\u0442 \u0443 \u0434\u0435\u043d\u044c \u0443\u0442\u0438\u0441\u043a\u0443."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0406 \u043d\u0435 \u0432\u0445\u043e\u0434\u044c \u0442\u0438 \u0434\u043e \u0431\u0440\u0430\u043c\u0438 \u043d\u0430\u0440\u043e\u0434\u0443 \u041c\u043e\u0433\u043e \u0443 \u0434\u0435\u043d\u044c \u043b\u0438\u0445\u0430 \u0439\u043e\u0433\u043e, \u0456 \u043d\u0435 \u043f\u0440\u0438\u0433\u043b\u044f\u0434\u0430\u0439\u0441\u044f \u0434\u043e \u0437\u043b\u0430 \u0439\u043e\u0433\u043e \u0439 \u0442\u0438 \u0432 \u0434\u0435\u043d\u044c \u043d\u0435\u0449\u0430\u0441\u0442\u044f \u0439\u043e\u0433\u043e, \u0456 \u043d\u0435 \u043f\u0440\u043e\u0441\u0442\u044f\u0433\u0430\u0439\u0442\u0435 \u0441\u0432\u043e\u0454\u0457 \u0440\u0443\u043a\u0438 \u0434\u043e \u0431\u0430\u0433\u0430\u0442\u0441\u0442\u0432\u0430 \u0439\u043e\u0433\u043e \u0432 \u0434\u0435\u043d\u044c \u043d\u0435\u0449\u0430\u0441\u0442\u044f \u0439\u043e\u0433\u043e!"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0406 \u043d\u0430 \u0440\u043e\u0437\u0434\u043e\u0440\u0456\u0436\u0436\u0456 \u043d\u0435 \u0441\u0442\u0456\u0439, \u0449\u043e\u0431 \u0432\u0438\u0442\u0438\u043d\u0430\u0442\u0438 \u0439\u043e\u0433\u043e \u0432\u0442\u0456\u043a\u0430\u0447\u0456\u0432, \u0456 \u0432 \u0434\u0435\u043d\u044c \u0443\u0442\u0438\u0441\u043a\u0443 \u043d\u0435 \u0432\u0438\u0434\u0430\u0432\u0430\u0439 \u0439\u043e\u0433\u043e \u0440\u0435\u0448\u0442\u043e\u043a!"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0411\u043e \u0431\u043b\u0438\u0437\u044c\u043a\u0438\u0439 \u0434\u0435\u043d\u044c \u0413\u043e\u0441\u043f\u043e\u0434\u043d\u0456\u0439 \u043d\u0430\u0434 \u0443\u0441\u0456\u043c\u0430 \u043d\u0430\u0440\u043e\u0434\u0430\u043c\u0438, \u044f\u043a \u0437\u0440\u043e\u0431\u0438\u0432 \u0442\u0438, \u0442\u043e \u0442\u0430\u043a \u0456 \u0442\u043e\u0431\u0456 \u0431\u0443\u0434\u0435 \u0437\u0440\u043e\u0431\u043b\u0435\u043d\u043e: \u0432\u0435\u0440\u043d\u0435\u0442\u044c\u0441\u044f \u043d\u0430 \u0442\u0432\u043e\u044e \u0433\u043e\u043b\u043e\u0432\u0443 \u0447\u0438\u043d \u0442\u0432\u0456\u0439!"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0411\u043e \u044f\u043a \u0432\u0438 \u043f\u0438\u043b\u0438 \u043d\u0430 \u0441\u0432\u044f\u0442\u0456\u0439 \u041c\u043e\u0457\u0439 \u0433\u043e\u0440\u0456, \u0442\u0430\u043a \u043d\u0430\u0440\u043e\u0434\u0438 \u0443\u0441\u0456 \u0437\u0430\u0432\u0436\u0434\u0438 \u043f\u0438\u0442\u0438\u043c\u0443\u0442\u044c! \u0406 \u0431\u0443\u0434\u0443\u0442\u044c \u043f\u0438\u0442\u0438 \u0432\u043e\u043d\u0438, \u0456 \u0431\u0443\u0434\u0443\u0442\u044c \u0445\u043b\u0435\u043f\u0442\u0430\u0442\u0438, \u0456 \u0441\u0442\u0430\u043d\u0443\u0442\u044c \u0432\u043e\u043d\u0438, \u043d\u0435\u043c\u043e\u0432 \u0457\u0445 \u043d\u0435 \u0431\u0443\u043b\u043e."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0410 \u043d\u0430 \u0421\u0456\u043e\u043d\u0441\u044c\u043a\u0456\u0439 \u0433\u043e\u0440\u0456 \u0431\u0443\u0434\u0435 \u0441\u043f\u0430\u0441\u0456\u043d\u043d\u044f, \u0456 \u0431\u0443\u0434\u0435 \u0441\u0432\u044f\u0442\u043e\u044e \u0432\u043e\u043d\u0430, \u0456 \u0441\u043f\u0430\u0434\u043a\u0438 \u0441\u0432\u043e\u0457 \u0432\u0436\u0435 \u043f\u043e\u0441\u044f\u0434\u0435 \u0434\u0456\u043c \u042f\u043a\u043e\u0432\u0456\u0432."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0406 \u0434\u0456\u043c \u042f\u043a\u043e\u0432\u0430 \u0441\u0442\u0430\u043d\u0435 \u043e\u0433\u043d\u0435\u043c, \u0456 \u0434\u0456\u043c \u0419\u043e\u0441\u0438\u043f\u0430 \u043f\u043e\u043b\u0443\u043c'\u044f\u043c, \u0430 \u0434\u0456\u043c \u0406\u0441\u0430\u0432\u0430 \u0441\u043e\u043b\u043e\u043c\u043e\u044e, \u0456 \u0431\u0443\u0434\u0443\u0442\u044c \u043f\u0430\u043b\u0430\u0442\u0438 \u0432\u043e\u043d\u0438 \u043f\u0440\u043e\u0442\u0438 \u043d\u0438\u0445, \u0456 \u0457\u0445 \u043f\u043e\u0436\u0435\u0440\u0443\u0442\u044c, \u0456 \u043e\u0441\u0442\u0430\u043d\u043a\u0443 \u043d\u0435 \u0431\u0443\u0434\u0435 \u0456\u0437 \u0434\u043e\u043c\u0443 \u0406\u0441\u0430\u0432\u0430, \u0431\u043e \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u0446\u0435 \u0441\u043a\u0430\u0437\u0430\u0432."
                },
                "19": {
                    "verse_nr": "19",
