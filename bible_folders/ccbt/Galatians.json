        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u4fdd\u7f85\uff0c\u6211\u4f5c\u4f7f\u5f92\u4e26\u975e\u53d7\u4eba\u59d4\u6d3e\uff0c\u4e5f\u4e0d\u662f\u501a\u9760\u4eba\uff0c\u800c\u662f\u7531\u8036\u7a4c\u57fa\u7763\u548c\u53eb\u7942\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u7684\u7236\u4e0a\u5e1d\u59d4\u6d3e\u7684\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u548c\u6240\u6709\u8ddf\u6211\u5728\u4e00\u8d77\u7684\u5f1f\u5144\u5beb\u4fe1\u7d66\u52a0\u62c9\u592a\u7684\u5404\u6559\u6703\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u9858\u6211\u5011\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u8cdc\u7d66\u4f60\u5011\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u57fa\u7763\u9075\u7167\u6211\u5011\u7236\u4e0a\u5e1d\u7684\u65e8\u610f\u70ba\u6211\u5011\u7684\u7f6a\u737b\u4e0a\u4e86\u81ea\u5df1\uff0c\u597d\u62ef\u6551\u6211\u5011\u812b\u96e2\u9019\u7f6a\u60e1\u7684\u4e16\u4ee3\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u9858\u69ae\u8000\u6b78\u7d66\u4e0a\u5e1d\uff0c\u76f4\u5230\u6c38\u6c38\u9060\u9060\u3002\u963f\u5011\uff01"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u52ff\u96a8\u5225\u7684\u798f\u97f3 - \u6211\u5f88\u9a5a\u8a1d\uff0c\u4f60\u5011\u7adf\u7136\u90a3\u9ebc\u5feb\u5c31\u80cc\u68c4\u4e86\u85c9\u8457\u57fa\u7763\u7684\u6069\u5178\u547c\u53ec\u4f60\u5011\u7684\u4e0a\u5e1d\uff0c\u53bb\u8ffd\u96a8\u5225\u7684\u798f\u97f3\uff01"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5176\u5be6\u90a3\u4e26\u4e0d\u662f\u798f\u97f3\uff0c\u53ea\u662f\u67d0\u4e9b\u4eba\u64fe\u4e82\u4f60\u5011\u7684\u4fe1\u4ef0\uff0c\u60f3\u7be1\u6539\u57fa\u7763\u7684\u798f\u97f3\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5373\u4fbf\u662f\u6211\u5011\u6216\u5929\u4f7f\uff0c\u82e5\u53e6\u50b3\u798f\u97f3\u7d66\u4f60\u5011\uff0c\u8207\u6211\u5011\u4ee5\u524d\u50b3\u7d66\u4f60\u5011\u7684\u76f8\u6096\uff0c\u4e5f\u8a72\u53d7\u5492\u8a5b\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6211\u5011\u5df2\u7d93\u8aaa\u904e\uff0c\u73fe\u5728\u6211\u518d\u8aaa\u4e00\u6b21\uff1a\u5982\u679c\u6709\u4eba\u5411\u4f60\u5011\u50b3\u5225\u7684\u798f\u97f3\uff0c\u8ddf\u4f60\u5011\u4ee5\u524d\u63a5\u53d7\u7684\u4e0d\u540c\uff0c\u90a3\u4eba\u8a72\u53d7\u5492\u8a5b\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u73fe\u5728\u662f\u8981\u8d0f\u5f97\u4eba\u7684\u8d0a\u540c\u55ce\uff1f\u9084\u662f\u8981\u8d0f\u5f97\u4e0a\u5e1d\u7684\u8d0a\u540c\uff1f\u96e3\u9053\u6211\u662f\u8981\u8a0e\u597d\u4eba\u55ce\uff1f\u5982\u679c\u6211\u4ecd\u820a\u8981\u8a0e\u597d\u4eba\uff0c\u6211\u5c31\u4e0d\u662f\u57fa\u7763\u7684\u5974\u50d5\u4e86\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4fdd\u7f85\u53d7\u4e0a\u5e1d\u59d4\u6d3e\u50b3\u798f\u97f3 - \u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u544a\u8a34\u4f60\u5011\uff0c\u6211\u50b3\u7684\u798f\u97f3\u4e0d\u662f\u51fa\u65bc\u4eba\u7684\u610f\u601d\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u56e0\u70ba\u9019\u798f\u97f3\u65e2\u4e0d\u662f\u6211\u5f9e\u4eba\u90a3\u88e1\u9818\u53d7\u7684\uff0c\u4e5f\u4e0d\u662f\u6211\u8ddf\u4eba\u5b78\u4f86\u7684\uff0c\u800c\u662f\u8036\u7a4c\u57fa\u7763\u89aa\u81ea\u555f\u793a\u6211\u7684\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u5011\u90fd\u807d\u8aaa\u904e\u6211\u4fe1\u5949\u7336\u592a\u6559\u6642\u7684\u6240\u4f5c\u6240\u70ba\uff0c\u6211\u600e\u6a23\u6b98\u9177\u5730\u8feb\u5bb3\u4e0a\u5e1d\u7684\u6559\u6703\uff0c\u8a66\u5716\u6467\u6bc0\u5b83\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6211\u5728\u7336\u592a\u6559\u88e1\u6bd4\u8a31\u591a\u540c\u8f29\u7684\u7336\u592a\u4eba\u66f4\u9032\u53d6\uff0c\u72c2\u71b1\u5730\u7dad\u8b77\u7956\u5148\u7684\u50b3\u7d71\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u7136\u800c\uff0c\u7576\u6211\u9084\u5728\u6bcd\u8179\u4e2d\u7684\u6642\u5019\uff0c\u4e0a\u5e1d\u5c31\u65bd\u6069\u5c07\u6211\u5206\u5225\u51fa\u4f86\u4e26\u547c\u53ec\u4e86\u6211\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4e0a\u5e1d\u65e2\u7136\u6a02\u610f\u628a\u7942\u7684\u5152\u5b50\u555f\u793a\u7d66\u6211\uff0c\u53eb\u6211\u5411\u5916\u65cf\u4eba\u50b3\u63da\u7942\u7684\u798f\u97f3\uff0c\u6211\u5c31\u6c92\u6709\u8ddf\u4efb\u4f55\u4eba\u5546\u91cf\uff0c"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4e5f\u6c92\u6709\u4e0a\u8036\u8def\u6492\u51b7\u53bb\u898b\u90a3\u4e9b\u6bd4\u6211\u5148\u4f5c\u4f7f\u5f92\u7684\u4eba\uff0c\u800c\u662f\u7acb\u523b\u53bb\u4e86\u963f\u62c9\u4f2f\uff0c\u7136\u5f8c\u56de\u5230\u5927\u99ac\u58eb\u9769\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u904e\u4e86\u4e09\u5e74\uff0c\u6211\u624d\u5230\u8036\u8def\u6492\u51b7\u53bb\u62dc\u6703\u5f7c\u5f97\uff0c\u548c\u4ed6\u4e00\u8d77\u4f4f\u4e86\u5341\u4e94\u5929\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u6211\u4e5f\u898b\u4e86\u4e3b\u8036\u7a4c\u7684\u5f1f\u5f1f\u96c5\u5404\uff0c\u6b64\u5916\u6c92\u6709\u898b\u904e\u5176\u4ed6\u4f7f\u5f92\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u6211\u5728\u4e0a\u5e1d\u9762\u524d\u4fdd\u8b49\uff0c\u6211\u5beb\u7d66\u4f60\u5011\u7684\u7d55\u7121\u8b0a\u8a00\u3002"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u5f8c\u4f86\u6211\u53c8\u5230\u4e86\u6558\u5229\u4e9e\u548c\u57fa\u5229\u8fe6\u5730\u5340\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u90a3\u6642\uff0c\u7336\u592a\u5883\u5167\u57fa\u7763\u7684\u773e\u6559\u6703\u90fd\u9084\u6c92\u6709\u898b\u904e\u6211\u7684\u9762\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u4ed6\u5011\u53ea\u662f\u807d\u4eba\u8aaa\uff1a\u300c\u90a3\u500b\u5f9e\u524d\u8feb\u5bb3\u6211\u5011\u7684\u4eba\u5982\u4eca\u5728\u50b3\u63da\u4ed6\u66fe\u8a66\u5716\u6467\u6bc0\u7684\u4fe1\u4ef0\u3002\u300d"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u65bc\u662f\u4ed6\u5011\u56e0\u70ba\u6211\u7684\u7de3\u6545\u5c07\u69ae\u8000\u6b78\u7d66\u4e0a\u5e1d\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u773e\u4f7f\u5f92\u63a5\u7d0d\u4fdd\u7f85 - \u5341\u56db\u5e74\u5f8c\uff0c\u6211\u548c\u5df4\u62ff\u5df4\u53c8\u53bb\u8036\u8def\u6492\u51b7\uff0c\u4e26\u5e36\u4e86\u63d0\u591a\u540c\u53bb\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u662f\u9075\u7167\u4e0a\u5e1d\u7684\u555f\u793a\u53bb\u7684\u3002\u6211\u79c1\u4e0b\u62dc\u6703\u4e86\u90a3\u4e9b\u6709\u540d\u671b\u7684\u6559\u6703\u9818\u8896\uff0c\u9673\u660e\u6211\u5728\u5916\u65cf\u4eba\u4e2d\u9593\u6240\u50b3\u7684\u798f\u97f3\uff0c\u514d\u5f97\u6211\u904e\u53bb\u6216\u662f\u73fe\u5728\u7684\u52aa\u529b\u90fd\u767d\u8cbb\u4e86\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8ddf\u6211\u540c\u53bb\u7684\u63d0\u591a\u96d6\u7136\u662f\u5e0c\u81d8\u4eba\uff0c\u4f46\u6c92\u6709\u4eba\u52c9\u5f37\u4ed6\u63a5\u53d7\u5272\u79ae\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4e8b\u60c5\u7684\u7de3\u7531\u662f\u56e0\u70ba\u6709\u4e00\u4e9b\u5047\u4fe1\u5f92\u5077\u5077\u6df7\u4e86\u9032\u4f86\uff0c\u8981\u7aba\u63a2\u6211\u5011\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u4eab\u6709\u7684\u81ea\u7531\uff0c\u60f3\u53eb\u6211\u5011\u505a\u5f8b\u6cd5\u7684\u5974\u96b8\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f46\u70ba\u4e86\u53eb\u798f\u97f3\u7684\u771f\u7406\u5e38\u5728\u4f60\u5011\u7576\u4e2d\uff0c\u6211\u5011\u7d72\u6beb\u6c92\u6709\u5411\u4ed6\u5011\u59a5\u5354\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u81f3\u65bc\u90a3\u4e9b\u5fb7\u9ad8\u671b\u91cd\u7684\u6559\u6703\u9818\u8896\uff0c\u4ed6\u5011\u7684\u5730\u4f4d\u5c0d\u6211\u4f86\u8aaa\u7121\u95dc\u7dca\u8981\uff0c\u56e0\u70ba\u4e0a\u5e1d\u4e0d\u4ee5\u8c8c\u53d6\u4eba\u3002\u4ed6\u5011\u5c0d\u6211\u6240\u50b3\u7684\u4e26\u6c92\u6709\u589e\u52a0\u4ec0\u9ebc\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u76f8\u53cd\uff0c\u4ed6\u5011\u90fd\u5df2\u770b\u5230\uff0c\u4e0a\u5e1d\u5dee\u9063\u4e86\u6211\u5411\u5916\u65cf\u4eba\u50b3\u798f\u97f3\uff0c\u5c31\u50cf\u7942\u5dee\u9063\u4e86\u5f7c\u5f97\u5411\u7336\u592a\u4eba\u50b3\u798f\u97f3\u4e00\u6a23\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e0a\u5e1d\u611f\u52d5\u4e86\u5f7c\u5f97\uff0c\u547c\u53ec\u4ed6\u505a\u7336\u592a\u4eba\u7684\u4f7f\u5f92\uff0c\u7942\u4e5f\u540c\u6a23\u611f\u52d5\u4e86\u6211\uff0c\u547c\u53ec\u6211\u505a\u5916\u65cf\u4eba\u7684\u4f7f\u5f92\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7576\u6642\u88ab\u8b7d\u70ba\u6559\u6703\u67f1\u77f3\u7684\u96c5\u5404\u3001\u5f7c\u5f97\u548c\u7d04\u7ff0\u660e\u767d\u4e86\u4e0a\u5e1d\u8cdc\u7d66\u6211\u7684\u6069\u5178\u4e4b\u5f8c\uff0c\u5c31\u8207\u6211\u548c\u5df4\u62ff\u5df4\u7528\u53f3\u624b\u884c\u76f8\u4ea4\u4e4b\u79ae\uff0c\u8b93\u6211\u5011\u5411\u5916\u65cf\u4eba\u50b3\u798f\u97f3\uff0c\u4ed6\u5011\u5411\u53d7\u5272\u79ae\u7684\u4eba\u50b3\u798f\u97f3\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4ed6\u5011\u53ea\u8981\u6c42\u6211\u5011\u7167\u9867\u90a3\u4e9b\u8ca7\u56f0\u7684\u4eba\uff0c\u9019\u6b63\u662f\u6211\u4e00\u5411\u71b1\u8877\u7684\u4e8b\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4fdd\u7f85\u9762\u8cac\u5f7c\u5f97 - \u5f8c\u4f86\uff0c\u5f7c\u5f97\u5230\u4e86\u5b89\u63d0\u963f\uff0c\u56e0\u4ed6\u505a\u932f\u4e86\u4e8b\uff0c\u6211\u5c31\u7576\u9762\u8cac\u5099\u4ed6\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u5f9e\u96c5\u5404\u90a3\u88e1\u4f86\u7684\u4eba\u9084\u6c92\u6709\u62b5\u9054\u4e4b\u524d\uff0c\u5f7c\u5f97\u548c\u5916\u65cf\u7684\u4fe1\u5f92\u4e00\u8d77\u5403\u98ef\u3002\u4f46\u90a3\u4e9b\u4eba\u62b5\u9054\u4ee5\u5f8c\uff0c\u5f7c\u5f97\u56e0\u70ba\u6015\u90a3\u4e9b\u5805\u6301\u884c\u5272\u79ae\u7684\u7336\u592a\u4eba\u6279\u8a55\uff0c\u5c31\u8207\u5916\u65cf\u7684\u4fe1\u5f92\u5206\u958b\u4e86\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u5176\u4ed6\u7684\u7336\u592a\u57fa\u7763\u5f92\u4e5f\u8ddf\u8457\u5f7c\u5f97\u88dd\u5047\uff0c\u751a\u81f3\u9023\u5df4\u62ff\u5df4\u4e5f\u96a8\u5f9e\u4e86\u4ed6\u5011\u7684\u865b\u507d\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6211\u770b\u898b\u4ed6\u5011\u4e0d\u7167\u798f\u97f3\u7684\u771f\u7406\u884c\uff0c\u5c31\u7576\u773e\u5c0d\u5f7c\u5f97\u8aaa\uff1a\u300c\u4f60\u8eab\u70ba\u7336\u592a\u4eba\uff0c\u5982\u679c\u884c\u4e8b\u70ba\u4eba\u50cf\u5916\u65cf\u4eba\uff0c\u4e0d\u50cf\u7336\u592a\u4eba\uff0c\u53c8\u600e\u80fd\u5f37\u8feb\u5916\u65cf\u4eba\u6309\u7336\u592a\u4eba\u7684\u898f\u77e9\u751f\u6d3b\u5462\uff1f\u300d"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6211\u5011\u751f\u4f86\u662f\u7336\u592a\u4eba\uff0c\u4e0d\u662f\u5916\u65cf\u7f6a\u4eba\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4f46\u6211\u5011\u77e5\u9053\u4eba\u88ab\u7a31\u70ba\u7fa9\u4eba\u4e0d\u662f\u9760\u9075\u884c\u5f8b\u6cd5\uff0c\u800c\u662f\u9760\u4fe1\u8036\u7a4c\u57fa\u7763\u3002\u6240\u4ee5\u6211\u5011\u4fe1\u4e86\u57fa\u7763\u8036\u7a4c\uff0c\u4ee5\u4fbf\u56e0\u4fe1\u57fa\u7763\u800c\u88ab\u7a31\u70ba\u7fa9\u4eba\uff0c\u800c\u4e0d\u662f\u9760\u9075\u884c\u5f8b\u6cd5\uff0c\u56e0\u70ba\u7121\u4eba\u80fd\u5920\u9760\u9075\u884c\u5f8b\u6cd5\u800c\u88ab\u7a31\u70ba\u7fa9\u4eba\u3002"
                },
                "17": {
