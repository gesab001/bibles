        },
        "5": {
            "chapter_nr": "5",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8b66\u544a\u70ba\u5bcc\u4e0d\u4ec1\u8005 - \u4f60\u5011\u9019\u4e9b\u5bcc\u6709\u7684\u4eba\u554a\uff01\u70ba\u90a3\u5c07\u8981\u81e8\u5230\u4f60\u5011\u8eab\u4e0a\u7684\u707d\u96e3\u75db\u54ed\u54c0\u865f\u5427\uff01"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4f60\u5011\u7684\u8ca1\u7269\u673d\u58de\u4e86\uff0c\u8863\u670d\u4e5f\u88ab\u87f2\u86c0\u4e86\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4f60\u5011\u7684\u91d1\u9280\u751f\u4e86\u93fd\uff0c\u90a3\u93fd\u5fc5\u8981\u6307\u63a7\u4f60\u5011\uff0c\u50cf\u706b\u4e00\u6a23\u541e\u566c\u4f60\u5011\u7684\u8eab\u9ad4\u3002\u5728\u672b\u4e16\u7684\u65e5\u5b50\u88e1\uff0c\u4f60\u5011\u53ea\u77e5\u9053\u7a4d\u6522\u8ca1\u5bf6\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5de5\u4eba\u66ff\u4f60\u5011\u6536\u5272\u838a\u7a3c\uff0c\u4f60\u5011\u537b\u524b\u6263\u4ed6\u5011\u7684\u5de5\u9322\u3002\u90a3\u5de5\u9322\u767c\u51fa\u4e0d\u5e73\u7684\u547c\u558a\uff0c\u5de5\u4eba\u7684\u51a4\u8072\u5df2\u7d93\u50b3\u5230\u842c\u8ecd\u4e4b\u4e3b\u7684\u8033\u4e2d\u4e86\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u5011\u5728\u4e16\u4e0a\u5962\u4f88\u4eab\u6a02\uff0c\u5728\u88ab\u5bb0\u6bba\u7684\u65e5\u5b50\u88e1\uff0c\u628a\u81ea\u5df1\u990a\u5f97\u80a5\u80a5\u80d6\u80d6\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4f60\u5011\u51a4\u6789\u597d\u4eba\u3001\u6b98\u5bb3\u7121\u8f9c\uff0c\u4ed6\u5011\u4e5f\u6c92\u6709\u53cd\u6297\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8981\u5fcd\u8010\u5230\u5e95 - \u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u5011\u8981\u4e00\u76f4\u5fcd\u8010\u5230\u4e3b\u4f86\u3002\u4f60\u5011\u770b\u8fb2\u592b\u600e\u6a23\u8010\u5fc3\u7b49\u5019\u5730\u88e1\u5bf6\u8cb4\u7684\u51fa\u7522\uff0c\u7b49\u5019\u79cb\u96e8\u548c\u6625\u96e8\u7684\u964d\u81e8\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u540c\u6a23\uff0c\u4f60\u5011\u4e5f\u8981\u5fcd\u8010\uff0c\u5fc3\u5fd7\u5805\u5b9a\uff0c\u56e0\u70ba\u4e3b\u5feb\u4f86\u4e86\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5f1f\u5144\u59ca\u59b9\uff0c\u4e0d\u8981\u4e92\u76f8\u57cb\u6028\uff0c\u514d\u5f97\u53d7\u5be9\u5224\u3002\u4f60\u5011\u770b\uff0c\u5be9\u5224\u7684\u4e3b\u5df2\u7d93\u7ad9\u5728\u9580\u5916\u4e86\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u5011\u8981\u6548\u6cd5\u5f9e\u524d\u5949\u4e3b\u7684\u540d\u8aaa\u8a71\u7684\u5148\u77e5\uff0c\u4ee5\u4ed6\u5011\u7684\u53d7\u82e6\u548c\u5fcd\u8010\u70ba\u699c\u6a23\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u5011\u8a8d\u70ba\u90a3\u4e9b\u5fcd\u8010\u5230\u5e95\u7684\u4eba\u662f\u6709\u798f\u7684\u3002\u4f60\u5011\u90fd\u77e5\u9053\u7d04\u4f2f\u7684\u5fcd\u8010\uff0c\u4e5f\u77e5\u9053\u4e3b\u6700\u7d42\u600e\u6a23\u5f85\u4ed6\uff0c\u56e0\u70ba\u4e3b\u5145\u6eff\u6190\u61ab\u548c\u6148\u60b2\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u6700\u91cd\u8981\u7684\u662f\u4e0d\u53ef\u8d77\u8a93\uff0c\u4e0d\u53ef\u6307\u8457\u5929\u8d77\u8a93\uff0c\u4e5f\u4e0d\u53ef\u6307\u8457\u5730\u8d77\u8a93\uff0c\u4e0d\u53ef\u6307\u8457\u4efb\u4f55\u6771\u897f\u8d77\u8a93\u3002\u4f60\u5011\u8aaa\u8a71\uff0c\u662f\u5c31\u8aaa\u662f\uff0c\u4e0d\u662f\u5c31\u8aaa\u4e0d\u662f\uff0c\u514d\u5f97\u4f60\u5011\u53d7\u5be9\u5224\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u79b1\u544a\u7684\u529b\u91cf - \u4f60\u5011\u4e2d\u9593\u6b63\u5728\u53d7\u82e6\u7684\u4eba\u61c9\u8a72\u79b1\u544a\uff0c\u559c\u6a02\u7684\u4eba\u61c9\u8a72\u5531\u6b4c\u8b9a\u7f8e\uff0c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u751f\u75c5\u7684\u4eba\u61c9\u8a72\u8acb\u6559\u6703\u7684\u9577\u8001\u4f86\uff0c\u5949\u4e3b\u7684\u540d\u7528\u6cb9\u62b9\u4ed6\u7684\u8eab\u9ad4\u4e26\u70ba\u4ed6\u79b1\u544a\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u51fa\u65bc\u4fe1\u5fc3\u7684\u79b1\u544a\u5fc5\u80fd\u4f7f\u75c5\u4eba\u75ca\u7652\uff0c\u4e3b\u5fc5\u4f7f\u4ed6\u5eb7\u5fa9\u3002\u5018\u82e5\u4ed6\u72af\u4e86\u7f6a\uff0c\u4e5f\u5fc5\u5f97\u5230\u8d66\u514d\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u6240\u4ee5\u4f60\u5011\u8981\u5f7c\u6b64\u8a8d\u7f6a\uff0c\u4e92\u76f8\u4ee3\u79b1\uff0c\u597d\u5f97\u5230\u91ab\u6cbb\u3002\u7fa9\u4eba\u7684\u79b1\u544a\u6709\u6975\u5927\u7684\u529b\u91cf\u548c\u529f\u6548\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4ee5\u5229\u4e9e\u5148\u77e5\u548c\u6211\u5011\u4e00\u6a23\u540c\u662f\u8840\u8089\u4e4b\u8ec0\uff0c\u4ed6\u61c7\u5207\u5730\u7948\u6c42\u4e0d\u8981\u4e0b\u96e8\uff0c\u7d50\u679c\u4e09\u5e74\u534a\u6c92\u6709\u4e0b\u96e8\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u5f8c\u4f86\u4ed6\u518d\u79b1\u544a\u6c42\u96e8\uff0c\u5929\u5c31\u964d\u96e8\uff0c\u5730\u4e5f\u9577\u51fa\u4e86\u838a\u7a3c\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u6211\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u5982\u679c\u6709\u4eba\u9818\u4e00\u500b\u504f\u96e2\u771f\u9053\u7684\u4eba\u6b78\u56de\u6b63\u8def\uff0c"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4ed6\u8a72\u77e5\u9053\uff1a\u4f7f\u4e00\u500b\u8ff7\u9014\u4e2d\u7684\u7f6a\u4eba\u6b78\u56de\u6b63\u8def\u5c31\u662f\u6551\u4e00\u500b\u9748\u9b42\u812b\u96e2\u6b7b\u4ea1\uff0c\u4e26\u4e14\u6703\u906e\u84cb\u8a31\u591a\u7f6a\u3002"
                }
            }
        }
    }
