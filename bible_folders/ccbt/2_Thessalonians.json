            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u4fdd\u7f85\u540c\u897f\u62c9\u548c\u63d0\u6469\u592a\u5beb\u4fe1\u7d66\u5e16\u6492\u7f85\u5c3c\u8fe6\u5c6c\u65bc\u6211\u5011\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u6559\u6703\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u9858\u6211\u5011\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u8cdc\u7d66\u4f60\u5011\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u611f\u6069\u8207\u52f8\u52c9 - \u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u5011\u61c9\u7576\u70ba\u4f60\u5011\u5e38\u5e38\u611f\u8b1d\u4e0a\u5e1d\uff0c\u9019\u662f\u5408\u5b9c\u7684\uff0c\u56e0\u70ba\u4f60\u5011\u7684\u4fe1\u5fc3\u4e0d\u65b7\u589e\u9577\uff0c\u5f7c\u6b64\u76f8\u611b\u7684\u5fc3\u4e5f\u4e0d\u65b7\u589e\u52a0\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u56e0\u6b64\uff0c\u6211\u5011\u5728\u4e0a\u5e1d\u7684\u773e\u6559\u6703\u4e2d\u8a87\u734e\u4f60\u5011\u5728\u5404\u7a2e\u8feb\u5bb3\u548c\u60a3\u96e3\u4e2d\u7684\u5805\u5fcd\u548c\u4fe1\u5fc3\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u5011\u9019\u7a2e\u8868\u73fe\u6b63\u662f\u4e0a\u5e1d\u516c\u7fa9\u5be9\u5224\u7684\u660e\u8b49\uff0c\u4f7f\u4f60\u5011\u914d\u9032\u4e0a\u5e1d\u7684\u570b\uff0c\u4f60\u5011\u6b63\u5728\u70ba\u9019\u570b\u53d7\u82e6\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e0a\u5e1d\u662f\u516c\u7fa9\u7684\uff0c\u7942\u5fc5\u4ee5\u60a3\u96e3\u4f86\u5831\u61c9\u90a3\u4e9b\u8feb\u5bb3\u4f60\u5011\u7684\u4eba\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u7576\u4e3b\u8036\u7a4c\u548c\u7942\u5927\u80fd\u7684\u5929\u4f7f\u5728\u70c8\u7130\u4e2d\u5f9e\u5929\u4e0a\u986f\u73fe\u6642\uff0c\u7942\u5fc5\u4f7f\u4f60\u5011\u9019\u4e9b\u53d7\u82e6\u7684\u4eba\u548c\u6211\u5011\u540c\u5f97\u5b89\u6170\uff0c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u61f2\u7f70\u90a3\u4e9b\u4e0d\u8a8d\u8b58\u4e0a\u5e1d\u3001\u4e0d\u807d\u5f9e\u6709\u95dc\u6211\u5011\u4e3b\u8036\u7a4c\u4e4b\u798f\u97f3\u7684\u4eba\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u90a3\u4e9b\u4eba\u8981\u53d7\u7684\u5211\u7f70\u5c31\u662f\u96e2\u958b\u4e3b\u7684\u9762\u548c\u7942\u69ae\u8000\u7684\u6b0a\u80fd\uff0c\u6c38\u9060\u6ec5\u4ea1\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4e3b\u964d\u81e8\u7684\u90a3\u65e5\uff0c\u7942\u8981\u5728\u7942\u7684\u773e\u8056\u5f92\u4e2d\u5f97\u5230\u69ae\u8000\uff0c\u4f7f\u6240\u6709\u7684\u4fe1\u5f92\u9a5a\u6b4e\u4e0d\u5df2\u3002\u4f60\u5011\u4e5f\u6703\u5728\u7576\u4e2d\uff0c\u56e0\u70ba\u4f60\u5011\u76f8\u4fe1\u4e86\u6211\u5011\u505a\u7684\u898b\u8b49\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u6b64\uff0c\u6211\u5011\u5e38\u5e38\u70ba\u4f60\u5011\u79b1\u544a\uff0c\u9858\u6211\u5011\u7684\u4e0a\u5e1d\u770b\u4f60\u5011\u914d\u5f97\u7942\u7684\u547c\u53ec\uff0c\u7528\u5927\u80fd\u6210\u5168\u4f60\u5011\u4e00\u5207\u7f8e\u597d\u7684\u5fc3\u9858\u548c\u6191\u4fe1\u5fc3\u6240\u505a\u7684\u5de5\u4f5c\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u9019\u6a23\uff0c\u6309\u7167\u6211\u5011\u7684\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u6240\u8cdc\u7684\u6069\u5178\uff0c\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u540d\u4fbf\u5728\u4f60\u5011\u8eab\u4e0a\u5f97\u5230\u69ae\u8000\uff0c\u4f60\u5011\u4e5f\u5728\u7942\u8eab\u4e0a\u5f97\u5230\u69ae\u8000\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4e3b\u518d\u4f86\u7684\u5148\u5146 - \u5f1f\u5144\u59ca\u59b9\uff0c\u95dc\u65bc\u4e3b\u8036\u7a4c\u57fa\u7763\u518d\u4f86\u548c\u6211\u5011\u5230\u7942\u90a3\u88e1\u76f8\u805a\u7684\u4e8b\u60c5\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u5011\u73fe\u5728\u5949\u52f8\u4f60\u5011\uff1a\u7121\u8ad6\u662f\u4ec0\u9ebc\u9748\uff0c\u6216\u662f\u50b3\u805e\uff0c\u6216\u662f\u5192\u5145\u6211\u5011\u5beb\u7684\u4fe1\uff0c\u8aaa\u4e3b\u7684\u65e5\u5b50\u5df2\u7d93\u5230\u4e86\uff0c\u4f60\u5011\u90fd\u4e0d\u8981\u8f15\u6613\u52d5\u6416\uff0c\u4e5f\u4e0d\u8981\u9a5a\u614c\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4e0d\u7ba1\u5225\u4eba\u7528\u4ec0\u9ebc\u8a6d\u8a08\uff0c\u4f60\u5011\u90fd\u4e0d\u8981\u4e0a\u7576\u3002\u56e0\u70ba\u90a3\u65e5\u5b50\u4f86\u81e8\u4e4b\u524d\uff0c\u5fc5\u6709\u96e2\u7d93\u53db\u9053\u7684\u4e8b\u767c\u751f\uff0c\u800c\u90a3\u4e0d\u6cd5\u4e4b\u5f92\uff0c\u5c31\u662f\u90a3\u8a3b\u5b9a\u6ec5\u4ea1\u7684\u4eba\u4e5f\u8981\u51fa\u73fe\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4ed6\u6703\u62b5\u64cb\u4e3b\uff0c\u9ad8\u62ac\u81ea\u5df1\u8d85\u904e\u4e00\u5207\u6240\u8b02\u7684\u795e\u660e\u548c\u4eba\u5011\u5d07\u62dc\u7684\u5c0d\u8c61\uff0c\u751a\u81f3\u5750\u5728\u4e0a\u5e1d\u7684\u6bbf\u4e2d\u4ee5\u4e0a\u5e1d\u81ea\u5c45\uff01"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u6211\u9084\u5728\u4f60\u5011\u90a3\u88e1\u7684\u6642\u5019\uff0c\u66fe\u544a\u8a34\u904e\u4f60\u5011\u9019\u4e9b\u4e8b\uff0c\u4f60\u5011\u5fd8\u8a18\u4e86\u55ce\uff1f"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u73fe\u5728\uff0c\u4f60\u5011\u77e5\u9053\u662f\u4ec0\u9ebc\u6514\u963b\u4ed6\uff0c\u4f7f\u4ed6\u7b49\u5230\u7279\u5b9a\u7684\u6642\u9593\u624d\u51fa\u73fe\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5176\u5be6\u90a3\u4e0d\u6cd5\u4e4b\u5f92\u7684\u9670\u8b00\u5df2\u7d93\u5728\u919e\u91c0\u4e2d\uff0c\u7136\u800c\u73fe\u5728\u6709\u4e00\u4f4d\u5728\u6514\u963b\u4ed6\u3002\u7b49\u5230\u6514\u963b\u4ed6\u7684\u90a3\u4f4d\u4e00\u96e2\u958b\uff0c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ed6\u5c31\u6703\u51fa\u73fe\uff0c\u4f46\u4e3b\u8036\u7a4c\u6703\u7528\u81ea\u5df1\u53e3\u4e2d\u7684\u6c23\u6bc0\u6ec5\u4ed6\uff0c\u7528\u5f9e\u5929\u964d\u81e8\u7684\u69ae\u5149\u5ee2\u6389\u4ed6\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4ed6\u4f86\u8981\u6309\u7167\u6492\u65e6\u7684\u4f0e\u5006\u884c\u5404\u6a23\u865b\u5047\u7684\u7570\u80fd\u3001\u795e\u8e5f\u548c\u5947\u4e8b\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u7528\u76e1\u5404\u6a23\u8a6d\u8a08\u6b3a\u9a19\u90a3\u4e9b\u5c07\u8981\u6ec5\u4ea1\u7684\u4eba\uff0c\u56e0\u70ba\u4ed6\u5011\u4e0d\u559c\u6b61\u63a5\u53d7\u90a3\u80fd\u62ef\u6551\u4ed6\u5011\u7684\u771f\u7406\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4e0a\u5e1d\u5c31\u8b93\u4ed6\u5011\u662f\u975e\u4e0d\u8fa8\uff0c\u53bb\u76f8\u4fe1\u90a3\u4e9b\u865b\u5047\u7684\u8b0a\u8a00\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f7f\u4e00\u5207\u4e0d\u76f8\u4fe1\u771f\u7406\u3001\u53cd\u559c\u611b\u4e0d\u7fa9\u7684\u4eba\u90fd\u88ab\u5b9a\u7f6a\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8981\u5805\u5b9a\u4e0d\u79fb - \u4e3b\u6240\u611b\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u5011\u61c9\u8a72\u6642\u5e38\u70ba\u4f60\u5011\u611f\u8b1d\u4e0a\u5e1d\uff0c\u56e0\u70ba\u4e0a\u5e1d\u4e00\u958b\u59cb\u5c31\u63c0\u9078\u4e86\u4f60\u5011\uff0c\u70ba\u4e86\u4f7f\u4f60\u5011\u85c9\u8457\u8056\u9748\u5f97\u4ee5\u8056\u6f54\uff0c\u4e26\u4e14\u76f8\u4fe1\u771f\u7406\uff0c\u5f9e\u800c\u5f97\u6551\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e0a\u5e1d\u85c9\u8457\u6211\u5011\u6240\u50b3\u7684\u798f\u97f3\u547c\u53ec\u4e86\u4f60\u5011\uff0c\u4f7f\u4f60\u5011\u53ef\u4ee5\u5f97\u5230\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u69ae\u8000\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6240\u4ee5\uff0c\u5404\u4f4d\u5f1f\u5144\u59ca\u59b9\uff0c\u52d9\u8981\u5805\u5b9a\u4e0d\u79fb\uff0c\u7121\u8ad6\u662f\u6211\u5011\u4fe1\u4e0a\u7684\u6559\u5c0e\u9084\u662f\u53e3\u982d\u7684\u6559\u5c0e\uff0c\u4f60\u5011\u90fd\u8981\u5805\u5b88\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u9858\u4e3b\u8036\u7a4c\u57fa\u7763\u548c\u611b\u6211\u5011\u3001\u958b\u6069\u5c07\u6c38\u9060\u7684\u5b89\u6170\u548c\u7f8e\u597d\u7684\u76fc\u671b\u8cdc\u7d66\u6211\u5011\u7684\u7236\u4e0a\u5e1d\uff0c"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u5b89\u6170\u4f60\u5011\u7684\u5fc3\uff0c\u4f7f\u4f60\u5011\u5728\u4e00\u5207\u5584\u884c\u5584\u8a00\u4e0a\u525b\u5f37\u3002"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8acb\u6c42\u4ee3\u79b1 - \u5f1f\u5144\u59ca\u59b9\uff0c\u6700\u5f8c\u6211\u9084\u8981\u8acb\u4f60\u5011\u70ba\u6211\u5011\u79b1\u544a\uff0c\u597d\u4f7f\u4e3b\u7684\u9053\u50cf\u5728\u4f60\u5011\u90a3\u88e1\u4e00\u6a23\u8fc5\u901f\u50b3\u958b\uff0c\u53d7\u5230\u5c0a\u5d07\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4e26\u4f7f\u6211\u5011\u80fd\u907f\u958b\u90a3\u4e9b\u90aa\u60e1\u7684\u4eba\uff0c\u56e0\u70ba\u4e0d\u662f\u4eba\u4eba\u90fd\u6709\u4fe1\u5fc3\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4e3b\u662f\u4fe1\u5be6\u7684\uff0c\u7942\u5fc5\u4f7f\u4f60\u5011\u525b\u5f37\uff0c\u4fdd\u8b77\u4f60\u5011\u812b\u96e2\u90a3\u60e1\u8005\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6211\u5011\u9760\u8457\u4e3b\u6df1\u4fe1\u4f60\u5011\u73fe\u5728\u9075\u884c\u4e86\u6211\u5011\u7684\u5429\u5490\uff0c\u800c\u4e14\u4ee5\u5f8c\u9084\u6703\u7e7c\u7e8c\u9075\u884c\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u9858\u4e3b\u5f15\u5c0e\u4f60\u5011\u7684\u5fc3\u9818\u609f\u4e0a\u5e1d\u7684\u611b\u548c\u57fa\u7763\u7684\u5805\u5fcd\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e0d\u53ef\u904a\u624b\u597d\u9592 - \u5f1f\u5144\u59ca\u59b9\uff0c\u73fe\u5728\u6211\u5011\u5949\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u540d\u5429\u5490\u4f60\u5011\uff0c\u8981\u9060\u96e2\u90a3\u4e9b\u904a\u624b\u597d\u9592\u3001\u4e0d\u9075\u5b88\u6211\u5011\u6559\u5c0e\u7684\u4fe1\u5f92\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4f60\u5011\u81ea\u5df1\u77e5\u9053\u61c9\u8a72\u600e\u6a23\u6548\u6cd5\u6211\u5011\uff0c\u56e0\u70ba\u6211\u5011\u5728\u4f60\u5011\u90a3\u88e1\u7684\u6642\u5019\uff0c\u4e26\u6c92\u6709\u61f6\u61f6\u6563\u6563\uff0c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e5f\u6c92\u6709\u767d\u5403\u8ab0\u7684\u98ef\uff0c\u800c\u662f\u665d\u591c\u8f9b\u52e4\u5de5\u4f5c\uff0c\u514d\u5f97\u6210\u70ba\u4f60\u5011\u4efb\u4f55\u4eba\u7684\u8ca0\u64d4\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6211\u5011\u4e26\u4e0d\u662f\u6c92\u6709\u6b0a\u5229\u63a5\u53d7\u4f9b\u61c9\uff0c\u800c\u662f\u8981\u7d66\u4f60\u5011\u6a39\u7acb\u5b78\u7fd2\u7684\u699c\u6a23\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u5011\u5728\u4f60\u5011\u90a3\u88e1\u7684\u6642\u5019\uff0c\u66fe\u5429\u5490\u904e\u4f60\u5011\uff1a\u4e0d\u80af\u5de5\u4f5c\u7684\u4eba\u6c92\u6709\u8cc7\u683c\u5403\u98ef\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u5011\u807d\u8aaa\u5728\u4f60\u5011\u7576\u4e2d\u6709\u4e9b\u4eba\u904a\u624b\u597d\u9592\uff0c\u7121\u6240\u4e8b\u4e8b\uff0c\u537b\u5c08\u7ba1\u9592\u4e8b\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u5011\u5949\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u540d\u5429\u5490\u4e26\u52f8\u8aa1\u9019\u4e9b\u4eba\u8981\u5b89\u5206\u5b88\u5df1\uff0c\u81ea\u98df\u5176\u529b\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u5011\u8981\u5805\u6301\u884c\u5584\uff0c\u4e0d\u8981\u7070\u5fc3\u3002"
                },
