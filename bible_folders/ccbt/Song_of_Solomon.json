                    "verse": "\u4e0d\u77e5\u4e0d\u89ba\uff0c \u6211\u7684\u5fc3\u628a\u6211\u5e36\u5230\u6211\u5c0a\u9577\u7684\u8eca\u4e0a\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c11\u5973\uff1a \u56de\u4f86\u5427\uff0c\u56de\u4f86\u5427\uff0c \u66f8\u62c9\u5bc6\u7684\u5c11\u5973\uff01 \u56de\u4f86\u5427\uff0c\u56de\u4f86\u5427\uff0c \u597d\u8b93\u6211\u5011\u518d\u770b\u770b\u4f60\uff01 \u7537\u5b50\uff1a\u4f60\u5011\u70ba\u4f55\u76ee\u4e0d\u8f49\u775b\u5730\u770b\u8457\u66f8\u62c9\u5bc6\u7684\u5c11\u5973\uff0c \u597d\u50cf\u89c0\u770b\u746a\u54c8\u5ff5\u7684\u821e\u8e48\u5462\uff1f"
                }
            }
        },
        "7": {
            "chapter_nr": "7",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u7684\u516c\u4e3b\u554a\uff0c\u4f60\u7684\u96d9\u8173\u914d\u4e0a\u978b\u5b50\u591a\u9ebc\u7f8e\u554a\uff01 \u4f60\u7f8e\u7389\u822c\u5713\u6f64\u7684\u817f\u662f\u5de7\u5320\u7684\u5091\u4f5c\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4f60\u7684\u809a\u81cd\u597d\u50cf\u5713\u676f\uff0c \u4e0d\u7f3a\u8abf\u548c\u7684\u9152\u3002 \u4f60\u7684\u8170\u597d\u6bd4\u4e00\u5806\u9ea5\u5b50\uff0c \u56db\u5468\u570d\u7e5e\u8457\u767e\u5408\u82b1\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4f60\u7684\u80f8\u812f\u597d\u50cf\u4e00\u5c0d\u5b7f\u751f\u7684\u5c0f\u9e7f\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4f60\u7684\u8116\u5b50\u5982\u8c61\u7259\u5854\uff0c \u773c\u775b\u597d\u50cf\u5e0c\u5be6\u672c\u5883\u5167\u5df4\u7279\u00b7\u62c9\u4e26\u9580\u65c1\u7684\u6c34\u6c60\u3002 \u4f60\u7684\u9f3b\u5b50\u597d\u50cf\u773a\u671b\u5927\u99ac\u58eb\u9769\u7684\u9ece\u5df4\u5ae9\u5854\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u7684\u982d\u5982\u540c\u8fe6\u5bc6\u5c71\uff0c \u4f60\u7684\u79c0\u9aee\u597d\u50cf\u7d2b\u9ed1\u8272\u7684\u7dde\u5b50\uff0c \u541b\u738b\u7684\u5fc3\u88ab\u4f60\u7684\u7e37\u7e37\u9752\u7d72\u7e6b\u4f4f\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u7684\u611b\u4eba\uff0c\u4f60\u662f\u591a\u9ebc\u7f8e\u9e97\uff0c \u591a\u9ebc\u8a0e\u4eba\u559c\u611b\uff0c\u4ee4\u4eba\u9676\u9189\u554a\uff01"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4f60\u4ead\u4ead\u7389\u7acb\u5982\u540c\u68d5\u6ada\u6a39\uff0c \u80f8\u812f\u5c31\u50cf\u6a39\u4e0a\u7684\u679c\u4e32\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u8981\u6500\u4e0a\u9019\u68f5\u6a39\uff0c \u63e1\u4f4f\u6a39\u4e0a\u7684\u679c\u5b50\u3002 \u9858\u4f60\u7684\u80f8\u812f\u50cf\u8461\u8404\u4e32\uff0c \u4f60\u7684\u6c23\u606f\u5982\u860b\u679c\u822c\u6e05\u9999\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4f60\u7684\u89aa\u543b\u50cf\u4e0a\u597d\u7684\u7f8e\u9152\u3002 \u5973\u5b50\uff1a\u9858\u7f8e\u9152\u6d41\u5165\u6211\u826f\u4eba\u7684\u53e3\u4e2d\uff0c \u7de9\u7de9\u6ce8\u5165\u4ed6\u9676\u9189\u7684\u5507\u9f52\u9593\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u5c6c\u65bc\u6211\u7684\u826f\u4eba\uff0c \u662f\u4ed6\u6240\u6200\u6155\u7684\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u7684\u826f\u4eba\uff0c\u4f86\u5427\uff01 \u8b93\u6211\u5011\u5230\u7530\u9593\u53bb\uff0c \u5728\u6751\u838a\u88e1\u4f4f\u5bbf\u5427\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u5011\u6e05\u6668\u53bb\u8461\u8404\u5712\uff0c \u770b\u770b\u8461\u8404\u6a39\u662f\u5426\u5df2\u7d93\u767c\u82bd\u958b\u82b1\uff0c \u77f3\u69b4\u6a39\u662f\u5426\u6b63\u5728\u958b\u82b1\u3002 \u6211\u8981\u5728\u90a3\u88e1\u628a\u6211\u7684\u611b\u60c5\u737b\u7d66\u4f60\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u90a3\u88e1\u98a8\u8304\u98c4\u9999\uff0c \u9580\u53e3\u6709\u5404\u6a23\u4e0a\u597d\u7684\u65b0\u9673\u7f8e\u679c\u3002 \u6211\u7684\u826f\u4eba\uff0c\u9019\u90fd\u662f\u6211\u70ba\u4f60\u5b58\u7559\u7684\u3002"
                }
            }
        },
        "8": {
            "chapter_nr": "8",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5982\u679c\u4f60\u662f\u6211\u7684\u540c\u80de\u5144\u5f1f\u5c31\u597d\u4e86\u3002 \u9019\u6a23\uff0c\u5728\u5916\u982d\u9047\u898b\u4f60\uff0c \u6211\u4e5f\u53ef\u4ee5\u89aa\u543b\u4f60\uff0c \u6c92\u6709\u4eba\u6703\u53d6\u7b11\u6211\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u53ef\u4ee5\u628a\u4f60\u5e36\u9032\u6211\u6bcd\u89aa\u7684\u5bb6\u2014\u2014\u5979\u6559\u990a\u4e86\u6211\u3002 \u6211\u8981\u7d66\u4f60\u559d\u77f3\u69b4\u6c41\u91c0\u7684\u9187\u9152\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4f60\u7684\u5de6\u624b\u6276\u8457\u6211\u7684\u982d\uff0c \u53f3\u624b\u7dca\u62b1\u8457\u6211\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c11\u5973\u554a\uff0c \u6211\u61c7\u6c42\u4f60\u5011\uff0c \u4e0d\u8981\u53eb\u9192\u6216\u9a5a\u52d5\u611b\u60c5\uff0c \u7b49\u5b83\u81ea\u767c\u5427\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c11\u5973\uff1a \u90a3\u5f9e\u66e0\u91ce\u4e0a\u4f86\u3001\u9760\u5728\u826f\u4eba\u8eab\u65c1\u7684\u662f\u8ab0\u5462\uff1f \u5973\u5b50\uff1a\u6211\u5728\u860b\u679c\u6a39\u4e0b\u559a\u9192\u4f60\uff0c \u4f60\u6bcd\u89aa\u5728\u90a3\u88e1\u61f7\u4f60\u990a\u4f60\uff0c \u70ba\u4f60\u53d7\u751f\u7522\u4e4b\u82e6\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u8acb\u628a\u6211\u5370\u5728\u4f60\u7684\u5fc3\u4e0a\uff0c \u523b\u5728\u4f60\u7684\u624b\u81c2\u4e0a\uff0c \u56e0\u70ba\u611b\u60c5\u5982\u6b7b\u4ea1\u4e00\u822c\u5805\u56fa\uff0c \u5ac9\u5992\u50cf\u9670\u9593\u4e00\u6a23\u6b98\u9177\uff0c \u5b83\u71c3\u8d77\u7684\u706b\u7130\u662f\u71d2\u6ec5\u4e00\u5207\u7684\u70c8\u706b\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u611b\u60c5\uff0c\u5927\u6c34\u4e0d\u80fd\u7184\u6ec5\uff0c \u6c5f\u6cb3\u4e0d\u80fd\u6df9\u6c92\u3002 \u82e5\u6709\u4eba\u60f3\u50be\u5176\u5bb6\u8ca1\u8cb7\u611b\u60c5\uff0c \u5fc5\u906d\u553e\u68c4\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5973\u5b50\u7684\u5144\u9577\uff1a \u6211\u5011\u6709\u4e00\u500b\u5c0f\u59b9\uff0c \u5979\u7684\u80f8\u812f\u9084\u6c92\u6709\u767c\u80b2\u3002 \u82e5\u6709\u4eba\u4f86\u63d0\u89aa\uff0c \u6211\u5011\u8a72\u600e\u9ebc\u8fa6\uff1f"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5979\u82e5\u662f\u7246\uff0c \u6211\u5011\u8981\u5728\u4e0a\u9762\u5efa\u9020\u9280\u5854\uff1b \u5979\u82e5\u662f\u9580\uff0c \u6211\u5011\u8981\u7528\u9999\u67cf\u6728\u677f\u570d\u8b77\u5979\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5973\u5b50\uff1a \u6211\u662f\u4e00\u9762\u7246\uff0c \u96d9\u4e73\u50cf\u57ce\u6a13\uff0c \u6211\u5728\u4ed6\u773c\u4e2d\u50cf\u627e\u5230\u5e73\u5b89\u7684\u4eba\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6240\u7f85\u9580\u5728\u5df4\u529b\u00b7\u54c8\u5011\u6709\u4e00\u500b\u8461\u8404\u5712\uff0c \u4ed6\u628a\u8461\u8404\u5712\u79df\u7d66\u8fb2\u592b\uff0c \u79df\u91d1\u662f\u6bcf\u4eba\u4e00\u5343\u584a\u9280\u5b50\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u7684\u8461\u8404\u5712\uff0c\u6211\u81ea\u5df1\u4f5c\u4e3b\u3002 \u6240\u7f85\u9580\u554a\uff0c\u4f60\u7684\u4e00\u5343\u584a\u9280\u5b50\u81ea\u5df1\u7559\u8457\uff0c \u4e8c\u767e\u584a\u9280\u5b50\u7d66\u7ba1\u679c\u5712\u7684\u4eba\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u7537\u5b50\uff1a \u4f60\u9019\u4f4f\u5728\u5712\u4e2d\u7684\u4eba\u554a\uff0c \u540c\u4f34\u5011\u90fd\u5728\u8046\u807d\u4f60\u7684\u8072\u97f3\uff0c \u6c42\u4f60\u8b93\u6211\u4e5f\u807d\u898b\u5427\uff01"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5973\u5b50\uff1a \u6211\u7684\u826f\u4eba\u554a\uff0c \u9858\u4f60\u50cf\u9999\u8349\u5c71\u4e0a\u7684\u7f9a\u7f8a\u548c\u5c0f\u9e7f\uff0c \u5feb\u5feb\u4f86\u5230\u6211\u8eab\u908a\uff01"
                }
            }
        }
    }
