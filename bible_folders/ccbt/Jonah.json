                "2": {
                    "verse_nr": "2",
                    "verse": "\u300c\u4f60\u8d77\u8eab\u524d\u5f80\u5c3c\u5c3c\u5fae\u5927\u57ce\uff0c\u5411\u90a3\u88e1\u7684\u5c45\u6c11\u5ba3\u544a\u6211\u5148\u524d\u5429\u5490\u4f60\u7684\u8a71\u3002\u300d"
                },
                "3": {
