        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u7d04\u62ff\u7684\u79b1\u544a - \u7d04\u62ff\u5728\u9b5a\u8179\u4e2d\u5411\u4ed6\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\u79b1\u544a\u8aaa\uff1a"
