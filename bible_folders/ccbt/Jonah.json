                "2": {
                    "verse_nr": "2",
                    "verse": "\u300c\u4f60\u8d77\u4f86\u524d\u5f80\u5c3c\u5c3c\u5fae\u5927\u57ce\uff0c\u5927\u8072\u8b66\u544a\u90a3\u88e1\u7684\u5c45\u6c11\uff0c\u56e0\u70ba\u4ed6\u5011\u7684\u7f6a\u60e1\u6ed4\u5929\uff0c\u5df2\u7d93\u9054\u5230\u6211\u9762\u524d\u3002\u300d"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u53ef\u662f\uff0c\u7d04\u62ff\u537b\u8d77\u8eab\u9003\u5f80\u4ed6\u65bd\uff0c\u60f3\u8eb2\u907f\u8036\u548c\u83ef\u3002\u4ed6\u5230\u4e86\u7d04\u5e15\u6e2f\uff0c\u770b\u898b\u4e00\u8258\u8981\u958b\u5f80\u4ed6\u65bd\u7684\u8239\uff0c\u5c31\u4ed8\u4e86\u8239\u8cbb\uff0c\u4e0a\u4e86\u8239\uff0c\u60f3\u548c\u773e\u4eba\u4e00\u8d77\u53bb\u4ed6\u65bd\uff0c\u597d\u8eb2\u907f\u8036\u548c\u83ef\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7136\u800c\uff0c\u8036\u548c\u83ef\u4f7f\u6d77\u9762\u8d77\u4e86\u72c2\u98a8\uff0c\u6ed4\u5929\u5de8\u6d6a\u5e7e\u4e4e\u628a\u8239\u6253\u6210\u788e\u7247\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u6c34\u624b\u5011\u975e\u5e38\u5bb3\u6015\uff0c\u7d1b\u7d1b\u547c\u6c42\u81ea\u5df1\u7684\u795e\uff0c\u53c8\u628a\u8239\u4e0a\u7684\u8ca8\u7269\u5168\u90fd\u62cb\u9032\u6d77\u88e1\uff0c\u4ee5\u6e1b\u8f15\u91cd\u91cf\u3002\u9019\u6642\u5019\uff0c\u7d04\u62ff\u5df2\u4e0b\u5230\u5e95\u8259\uff0c\u8eba\u5728\u90a3\u88e1\u547c\u547c\u5927\u7761\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u8239\u9577\u4f86\u5230\u7d04\u62ff\u8eab\u908a\u5c0d\u4ed6\u8aaa\uff1a\u300c\u4f60\u9019\u500b\u4eba\uff0c\u600e\u9ebc\u9019\u6642\u5019\u9084\u5728\u7761\u89ba\uff01\u5feb\u8d77\u4f86\u7948\u6c42\u4f60\u7684\u795e\u5427\uff0c\u4e5f\u8a31\u4f60\u7684\u795e\u6703\u6190\u61ab\u6211\u5011\uff0c\u9952\u6211\u5011\u4e00\u547d\u3002\u300d"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5f8c\u4f86\uff0c\u8239\u4e0a\u7684\u4eba\u5f7c\u6b64\u5546\u91cf\u8aaa\uff1a\u300c\u4f86\uff0c\u6211\u5011\u62bd\u7c64\uff0c\u770b\u770b\u9019\u5834\u707d\u96e3\u662f\u8ab0\u60f9\u4f86\u7684\u3002\u300d\u65bc\u662f\u4ed6\u5011\u5c31\u62bd\u7c64\uff0c\u7d50\u679c\u62bd\u4e2d\u4e86\u7d04\u62ff\u3002"
