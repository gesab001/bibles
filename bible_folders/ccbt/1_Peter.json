    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u8036\u7a4c\u57fa\u7763\u7684\u4f7f\u5f92\u5f7c\u5f97\uff0c\u5beb\u4fe1\u7d66\u8499\u4e0a\u5e1d\u63c0\u9078\uff0c\u5bc4\u5c45\u5728\u672c\u90fd\u3001\u52a0\u62c9\u592a\u3001\u52a0\u5e15\u591a\u8fe6\u3001\u4e9e\u7d30\u4e9e\u3001\u5e87\u63a8\u5c3c\u5404\u5730\u7684\u4eba\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4f60\u5011\u8499\u63c0\u9078\u662f\u6309\u7167\u7236\u4e0a\u5e1d\u9810\u5148\u5b9a\u597d\u7684\u65e8\u610f\uff0c\u85c9\u8457\u8056\u9748\u5f97\u4ee5\u8056\u6f54\uff0c\u597d\u9806\u670d\u8036\u7a4c\u57fa\u7763\uff0c\u4e26\u9760\u7942\u7684\u8840\u5f97\u5230\u6f54\u6de8\u3002 \u9858\u4e0a\u5e1d\u8cdc\u7d66\u4f60\u5011\u8c50\u8c50\u5bcc\u5bcc\u7684\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u6d3b\u6f51\u7684\u76fc\u671b - \u9858\u980c\u8b9a\u6b78\u65bc\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u7236\u4e0a\u5e1d\uff01\u7942\u6709\u7121\u7aae\u7684\u6190\u61ab\uff0c\u85c9\u8457\u8036\u7a4c\u57fa\u7763\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u4f7f\u6211\u5011\u7372\u5f97\u91cd\u751f\uff0c\u6709\u6d3b\u6f51\u7684\u76fc\u671b\uff0c"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u53ef\u4ee5\u627f\u53d7\u90a3\u4e0d\u6703\u673d\u58de\u3001\u6c92\u6709\u6c61\u9ede\u3001\u4e0d\u6703\u8870\u6b98\u3001\u70ba\u4f60\u5011\u5b58\u7559\u5728\u5929\u4e0a\u7684\u7522\u696d\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u5011\u9019\u4e9b\u56e0\u4fe1\u800c\u8499\u4e0a\u5e1d\u7528\u5927\u80fd\u4fdd\u5b88\u7684\u4eba\uff0c\u5fc5\u80fd\u5f97\u5230\u90a3\u5df2\u7d93\u9810\u5099\u597d\u3001\u5728\u672b\u4e16\u8981\u986f\u660e\u7684\u6551\u6069\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u70ba\u6b64\uff0c\u5373\u4f7f\u73fe\u4eca\u4f60\u5011\u5fc5\u9808\u66ab\u6642\u5728\u767e\u822c\u8a66\u7149\u4e2d\u5fcd\u53d7\u75db\u82e6\uff0c\u4e5f\u8981\u6eff\u61f7\u559c\u6a02\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u9019\u6a23\uff0c\u4f60\u5011\u7684\u4fe1\u5fc3\u7d93\u904e\u8a66\u9a57\u4fbf\u986f\u660e\u662f\u771f\u5be6\u7684\uff0c\u6bd4\u90a3\u7d93\u904e\u706b\u7149\u4ecd\u6703\u58de\u6389\u7684\u91d1\u5b50\u66f4\u52a0\u5bf6\u8cb4\uff0c\u4f7f\u4f60\u5011\u53ef\u4ee5\u5728\u8036\u7a4c\u57fa\u7763\u986f\u73fe\u6642\u5f97\u5230\u7a31\u8b9a\u3001\u69ae\u8000\u548c\u5c0a\u8cb4\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u5011\u96d6\u7136\u6c92\u6709\u898b\u904e\u57fa\u7763\uff0c\u537b\u611b\u7942\uff1b\u96d6\u7136\u73fe\u5728\u770b\u4e0d\u898b\u7942\uff0c\u537b\u4fe1\u7942\uff0c\u4e26\u4e14\u6709\u7121\u6cd5\u5f62\u5bb9\u3001\u5145\u6eff\u69ae\u8000\u7684\u559c\u6a02\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u56e0\u70ba\u4f60\u5011\u5f97\u5230\u4e86\u4fe1\u5fc3\u5e36\u4f86\u7684\u679c\u6548\uff0c\u5c31\u662f\u4f60\u5011\u7684\u9748\u9b42\u5f97\u6551\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6709\u95dc\u9019\u6551\u6069\uff0c\u90a3\u4e9b\u9810\u8a00\u6069\u5178\u8981\u81e8\u5230\u4f60\u5011\u7684\u773e\u5148\u77e5\u65e9\u5df2\u8a73\u7d30\u5c0b\u6c42\u67e5\u8003\u4e86\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u57fa\u7763\u7684\u9748\u66fe\u5728\u4ed6\u5011\u5fc3\u4e2d\u6307\u793a\u4ed6\u5011\uff0c\u9810\u8a00\u57fa\u7763\u8981\u53d7\u82e6\uff0c\u7136\u5f8c\u5f97\u69ae\u8000\u3002\u4ed6\u5011\u4fbf\u8a73\u7d30\u67e5\u8003\u9019\u4e9b\u4e8b\u6703\u5728\u4ec0\u9ebc\u6642\u5019\u3001\u4ec0\u9ebc\u60c5\u6cc1\u4e0b\u767c\u751f\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4ed6\u5011\u5f97\u5230\u555f\u793a\uff0c\u77e5\u9053\u4ed6\u5011\u50b3\u8b1b\u9019\u4e9b\u4e8b\u4e0d\u662f\u70ba\u4e86\u81ea\u5df1\uff0c\u800c\u662f\u70ba\u4e86\u4f60\u5011\u3002\u73fe\u5728\uff0c\u90a3\u4e9b\u9760\u8457\u5f9e\u5929\u4e0a\u5dee\u4f86\u7684\u8056\u9748\u5411\u4f60\u5011\u50b3\u798f\u97f3\u7684\u4eba\uff0c\u5df2\u7d93\u5c07\u9019\u4e9b\u4e8b\u50b3\u7d66\u4f60\u5011\u4e86\u3002\u9023\u5929\u4f7f\u4e5f\u60f3\u628a\u9019\u4e9b\u4e8b\u770b\u500b\u660e\u767d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8981\u8056\u6f54\u50cf\u4e3b - \u6240\u4ee5\uff0c\u8981\u9810\u5099\u597d\u4f60\u5011\u7684\u5fc3\uff0c\u8b39\u614e\u81ea\u5f8b\uff0c\u5c08\u5fc3\u76fc\u671b\u8036\u7a4c\u57fa\u7763\u986f\u73fe\u6642\u8981\u5e36\u7d66\u4f60\u5011\u7684\u6069\u5178\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4f60\u5011\u65e2\u662f\u9806\u670d\u7684\u5152\u5973\uff0c\u5c31\u4e0d\u8981\u518d\u50cf\u5f9e\u524d\u7121\u77e5\u7684\u6642\u5019\u90a3\u6a23\u653e\u7e31\u79c1\u617e\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u90a3\u547c\u53ec\u4f60\u5011\u7684\u662f\u8056\u6f54\u7684\uff0c\u4f60\u5011\u5728\u4e00\u5207\u4e8b\u4e0a\u4e5f\u8981\u8056\u6f54\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u56e0\u70ba\u8056\u7d93\u4e0a\u8aaa\uff1a\u300c\u4f60\u5011\u8981\u8056\u6f54\uff0c\u56e0\u70ba\u6211\u662f\u8056\u6f54\u7684\u3002\u300d"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u65e2\u7136\u4f60\u5011\u7a31\u547c\u90a3\u4f4d\u6309\u5404\u4eba\u7684\u884c\u70ba\u516c\u6b63\u7121\u79c1\u5730\u5be9\u5224\u4eba\u7684\u4e0a\u5e1d\u70ba\u7236\uff0c\u5c31\u61c9\u8a72\u5b58\u8457\u656c\u754f\u7684\u5fc3\u904e\u4f60\u5011\u5728\u4e16\u4e0a\u5bc4\u5c45\u7684\u65e5\u5b50\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u8981\u77e5\u9053\uff0c\u4f60\u5011\u5f9e\u50b3\u7d71\u7684\u3001\u6c92\u6709\u610f\u7fa9\u7684\u751f\u6d3b\u4e2d\u88ab\u6551\u8d16\u51fa\u4f86\uff0c\u4e0d\u662f\u9760\u91d1\u9280\u7b49\u6703\u673d\u58de\u7684\u6771\u897f\uff0c"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u800c\u662f\u9760\u7121\u7455\u7121\u75b5\u7684\u4ee3\u7f6a\u7f94\u7f8a\u2014\u2014\u57fa\u7763\u7684\u5bf6\u8840\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u57fa\u7763\u662f\u4e0a\u5e1d\u5728\u5275\u4e16\u4ee5\u524d\u9810\u5148\u63c0\u9078\u7684\uff0c\u537b\u5728\u9019\u672b\u4e16\u70ba\u4f60\u5011\u986f\u73fe\u3002"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u4f60\u5011\u85c9\u8457\u57fa\u7763\u4fe1\u4e86\u4f7f\u7942\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u3001\u8cdc\u7942\u69ae\u8000\u7684\u4e0a\u5e1d\uff0c\u6240\u4ee5\uff0c\u4f60\u5011\u7684\u4fe1\u5fc3\u548c\u76fc\u671b\u90fd\u5728\u65bc\u4e0a\u5e1d\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u4f60\u5011\u65e2\u56e0\u9806\u670d\u771f\u7406\uff0c\u6f54\u6de8\u4e86\u81ea\u5df1\u7684\u5fc3\uff0c\u80fd\u5920\u771f\u8aa0\u5730\u611b\u5f1f\u5144\u59ca\u59b9\uff0c\u5c31\u7576\u4ee5\u6e05\u6f54\u7684\u5fc3\u5f7c\u6b64\u5207\u5be6\u76f8\u611b\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u4f60\u5011\u7372\u5f97\u91cd\u751f\uff0c\u4e0d\u662f\u85c9\u8457\u6703\u8150\u721b\u7684\u7a2e\u5b50\uff0c\u800c\u662f\u85c9\u8457\u4e0d\u6703\u8150\u721b\u7684\u7a2e\u5b50\u2014\u2014\u4e0a\u5e1d\u6d3b\u6f51\u6c38\u5b58\u7684\u9053\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u56e0\u70ba \u300c\u82b8\u82b8\u773e\u751f\u76e1\u5982\u8349\uff0c \u69ae\u83ef\u4e0d\u904e\u8349\u4e0a\u82b1\uff1b \u8349\u5fc5\u67af\u4e7e\u82b1\u5fc5\u6b98\uff0c"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u552f\u6709\u4e3b\u9053\u6c38\u9577\u5b58\u3002\u300d \u9019\u9053\u5c31\u662f\u50b3\u7d66\u4f60\u5011\u7684\u798f\u97f3\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6e34\u6155\u9748\u5976 - \u56e0\u6b64\uff0c\u4f60\u5011\u65e2\u7136\u5df2\u7d93\u9664\u53bb\u4e00\u5207\u60e1\u6bd2\u3001\u6b3a\u8a50\u3001\u865b\u507d\u3001\u5ac9\u5992\u548c\u6bc0\u8b17\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u5c31\u8981\u50cf\u521d\u751f\u7684\u5b30\u5152\u4e00\u6a23\u6e34\u6155\u7d14\u6de8\u7684\u9748\u5976\uff0c\u597d\u4e0d\u65b7\u6210\u9577\uff0c\u76f4\u5230\u5b8c\u5168\u5f97\u6551\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u56e0\u70ba\u4f60\u5011\u5690\u904e\u4e3b\u6069\u7684\u6ecb\u5473\u662f\u4f55\u7b49\u7518\u7f8e\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u9748\u5bae\u6d3b\u77f3 - \u4e3b\u662f\u6d3b\u77f3\uff0c\u96d6\u7136\u88ab\u4eba\u68c4\u7d55\uff0c\u537b\u88ab\u4e0a\u5e1d\u63c0\u9078\u3001\u8996\u70ba\u5bf6\u8cb4\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4f60\u5011\u4e5f\u5982\u540c\u6d3b\u77f3\uff0c\u88ab\u7528\u4f86\u5efa\u9020\u5c6c\u9748\u7684\u5bae\u6bbf\uff0c\u597d\u505a\u8056\u6f54\u7684\u796d\u53f8\uff0c\u85c9\u8457\u8036\u7a4c\u57fa\u7763\u737b\u4e0a\u8499\u4e0a\u5e1d\u6085\u7d0d\u7684\u9748\u796d\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u56e0\u70ba\u8056\u7d93\u4e0a\u8aaa\uff1a \u300c\u770b\u554a\uff0c\u6211\u5728\u932b\u5b89\u653e\u4e86\u4e00\u584a\u8499\u63c0\u9078\u7684\u5bf6\u8cb4\u623f\u89d2\u77f3\uff0c \u4fe1\u9760\u7942\u7684\u4eba\u5fc5\u4e0d\u6703\u8499\u7f9e\u3002\u300d"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6240\u4ee5\uff0c\u5c0d\u4f60\u5011\u4fe1\u7684\u4eba\u800c\u8a00\uff0c\u9019\u77f3\u982d\u662f\u5bf6\u8cb4\u7684\uff1b\u4f46\u5c0d\u90a3\u4e9b\u4e0d\u4fe1\u7684\u4eba\u4f86\u8aaa\uff0c\u537b\u662f\uff1a \u300c\u5de5\u5320\u4e1f\u68c4\u7684\u77f3\u982d\uff0c \u5df2\u6210\u4e86\u623f\u89d2\u77f3\u3002\u300d"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e5f\u662f\uff1a \u300c\u7d46\u8173\u77f3\u548c\u4f7f\u4eba\u8dcc\u5012\u7684\u78d0\u77f3\u3002\u300d \u4ed6\u5011\u8dcc\u5012\u662f\u56e0\u70ba\u4ed6\u5011\u4e0d\u9806\u670d\u771f\u9053\uff0c\u9019\u7a2e\u4e0b\u5834\u65e9\u5df2\u8a3b\u5b9a\u4e86\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4f46\u4f60\u5011\u662f\u8499\u63c0\u9078\u7684\u65cf\u7fa4\uff0c\u662f\u541b\u5c0a\u7684\u796d\u53f8\uff0c\u662f\u8056\u6f54\u7684\u570b\u5ea6\uff0c\u662f\u4e0a\u5e1d\u7684\u5b50\u6c11\uff0c\u56e0\u6b64\u4f60\u5011\u53ef\u4ee5\u5ba3\u63da\u4e0a\u5e1d\u7684\u7f8e\u5fb7\u3002\u7942\u66fe\u547c\u53ec\u4f60\u5011\u96e2\u958b\u9ed1\u6697\uff0c\u9032\u5165\u7942\u5947\u5999\u7684\u5149\u660e\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5f9e\u524d\u4f60\u5011\u4e0d\u662f\u4e0a\u5e1d\u7684\u5b50\u6c11\uff0c\u73fe\u5728\u537b\u4f5c\u4e86\u7942\u7684\u5b50\u6c11\uff1b\u5f9e\u524d\u4f60\u5011\u672a\u8499\u6190\u61ab\uff0c\u73fe\u5728\u537b\u8499\u4e86\u6190\u61ab\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4e0a\u5e1d\u7684\u50d5\u4eba - \u89aa\u611b\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u5011\u662f\u5ba2\u65c5\uff0c\u662f\u5bc4\u5c45\u7684\uff0c\u6211\u52f8\u4f60\u5011\u8981\u7981\u6212\u8207\u9748\u9b42\u70ba\u6575\u7684\u90aa\u60c5\u79c1\u617e\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f60\u5011\u5728\u7570\u6559\u5f92\u7576\u4e2d\u8981\u54c1\u884c\u7aef\u6b63\u3002\u9019\u6a23\uff0c\u5118\u7ba1\u4ed6\u5011\u6bc0\u8b17\u4f60\u5011\u662f\u4f5c\u60e1\u7684\u4eba\uff0c\u4f46\u770b\u898b\u4f60\u5011\u7684\u597d\u884c\u70ba\uff0c\u4e5f\u6703\u5728\u4e3b\u4f86\u7684\u65e5\u5b50\u628a\u69ae\u8000\u6b78\u7d66\u4e0a\u5e1d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u70ba\u4e3b\u7684\u7de3\u6545\uff0c\u4f60\u5011\u8981\u670d\u5f9e\u4eba\u9593\u7684\u4e00\u5207\u6b0a\u67c4\uff0c\u4e0d\u7ba1\u662f\u5c45\u9996\u4f4d\u7684\u541b\u738b\uff0c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u9084\u662f\u88ab\u541b\u738b\u6d3e\u4f86\u8cde\u5584\u7f70\u60e1\u7684\u5b98\u54e1\u3002"
                },
                "15": {
