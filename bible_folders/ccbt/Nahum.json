    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4ee5\u4e0b\u662f\u95dc\u65bc\u5c3c\u5c3c\u5fae\u7684\u9810\u8a00\uff0c\u662f\u4f0a\u52d2\u6b4c\u65af\u4eba\u90a3\u9d3b\u7684\u555f\u793a\u66f8\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u8036\u548c\u83ef\u5411\u5c3c\u5c3c\u5fae\u767c\u6012 - \u8036\u548c\u83ef\u662f\u75db\u6068\u4e0d\u8c9e\u3001\u65bd\u884c\u5831\u61c9\u7684\u4e0a\u5e1d\u3002 \u8036\u548c\u83ef\u65bd\u884c\u5831\u61c9\uff0c\u5145\u6eff\u70c8\u6012\u3002 \u8036\u548c\u83ef\u5831\u61c9\u7942\u7684\u4ec7\u6575\uff0c\u5411\u4ed6\u5011\u767c\u70c8\u6012\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8036\u548c\u83ef\u4e0d\u8f15\u6613\u767c\u6012\uff0c\u6709\u5049\u5927\u7684\u80fd\u529b\u3002 \u8036\u548c\u83ef\u65b7\u4e0d\u4ee5\u6709\u7f6a\u7684\u70ba\u7121\u7f6a\u3002 \u7942\u884c\u8d70\u5728\u65cb\u98a8\u548c\u66b4\u98a8\u4e2d\uff0c \u96f2\u5f69\u662f\u7942\u8173\u4e0b\u7684\u5875\u571f\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7942\u65a5\u8cac\u5927\u6d77\uff0c\u4f7f\u5927\u6d77\u4e7e\u6db8\uff1b \u7942\u4f7f\u4e00\u5207\u6cb3\u6d41\u67af\u7aed\u3002 \u5df4\u73ca\u548c\u8fe6\u5bc6\u7684\u8349\u6728\u51cb\u96f6\uff0c \u9ece\u5df4\u5ae9\u7684\u9bae\u82b1\u8870\u6b98\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5728\u7942\u9762\u524d\uff0c\u7fa4\u5c71\u9707\u52d5\uff0c\u4e18\u9675\u6d88\u878d\u3002 \u5728\u7942\u9762\u524d\uff0c\u5927\u5730\u9686\u8d77\uff0c \u4e16\u754c\u548c\u4e16\u4e0a\u7684\u4e00\u5207\u90fd\u6230\u6144\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u7942\u767c\u6012\uff0c\u8ab0\u80fd\u7ad9\u7acb\uff1f \u7942\u767c\u70c8\u6012\uff0c\u8ab0\u80fd\u627f\u53d7\uff1f \u7942\u7684\u61a4\u6012\u5982\u706b\u7130\u5674\u6e67\u800c\u51fa\uff0c \u78d0\u77f3\u5728\u7942\u9762\u524d\u5d29\u88c2\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8036\u548c\u83ef\u662f\u826f\u5584\u7684\uff0c \u662f\u4eba\u60a3\u96e3\u6642\u7684\u907f\u96e3\u6240\uff1b \u7942\u770b\u9867\u90a3\u4e9b\u4fe1\u9760\u7942\u7684\u4eba\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u7942\u5fc5\u7528\u6ed4\u6ed4\u6d2a\u6c34\u6ec5\u7d55\u7942\u7684\u6575\u4eba\uff0c \u628a\u4ed6\u5011\u9a45\u9010\u5230\u9ed1\u6697\u4e2d\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u5c3c\u5c3c\u5fae\u4eba\u554a\uff0c \u4f60\u5011\u70ba\u4f55\u5716\u8b00\u62b5\u64cb\u8036\u548c\u83ef\uff1f \u7942\u8981\u5fb9\u5e95\u6bc0\u6ec5\u4f60\u5011\uff0c \u7121\u9700\u64ca\u6253\u5169\u6b21\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4f60\u5011\u50cf\u7cfe\u7d50\u7684\u834a\u68d8\uff0c \u53c8\u50cf\u9169\u914a\u5927\u9189\u7684\u4eba\uff0c \u8981\u5982\u4e7e\u7a2d\u88ab\u706b\u71d2\u76e1\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4f60\u5011\u7576\u4e2d\u6709\u4e00\u500b\u4eba\uff0c \u4e00\u500b\u90aa\u60e1\u7684\u9670\u8b00\u8005\uff0c \u5716\u8b00\u62b5\u64cb\u8036\u548c\u83ef\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u5118\u7ba1\u5c3c\u5c3c\u5fae\u52e2\u529b\u5f37\u5927\u3001\u4eba\u53e3\u773e\u591a\uff0c \u4f46\u5fc5\u88ab\u5277\u9664\uff0c\u5316\u70ba\u70cf\u6709\u3002 \u6211\u7684\u5b50\u6c11\u554a\uff0c\u6211\u4f7f\u4f60\u5011\u53d7\u4e86\u82e6\uff0c \u4f46\u5fc5\u4e0d\u518d\u4f7f\u4f60\u5011\u53d7\u82e6\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u73fe\u5728\u6211\u8981\u6253\u788e\u4ed6\u5011\u5957\u5728\u4f60\u5011\u9838\u9805\u4e0a\u7684\u8edb\uff0c \u9b06\u958b\u4f60\u5011\u8eab\u4e0a\u7684\u9396\u93c8\u3002\u300d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u5c3c\u5c3c\u5fae\u554a\uff0c \u8036\u548c\u83ef\u5df2\u767c\u51fa\u6709\u95dc\u4f60\u7684\u547d\u4ee4\uff1a \u300c\u4f60\u7684\u540d\u4e0d\u6703\u50b3\u65bc\u5f8c\u4e16\u3002 \u6211\u8981\u6467\u6bc0\u4f60\u795e\u5edf\u4e2d\u96d5\u523b\u548c\u9444\u9020\u7684\u5076\u50cf\u3002 \u6211\u8981\u70ba\u4f60\u6398\u597d\u58b3\u5893\uff0c \u56e0\u70ba\u4f60\u6beb\u7121\u7528\u8655\u3002\u300d"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u770b\u554a\uff0c \u50b3\u559c\u8a0a\u3001\u5831\u5e73\u5b89\u7684\u4eba\u7a7f\u5c71\u8d8a\u5dba\u800c\u4f86\u3002 \u7336\u5927\u554a\uff0c \u6176\u795d\u4f60\u7684\u7bc0\u671f\uff0c \u9084\u4f60\u8a31\u7684\u9858\u5427\uff01 \u90aa\u60e1\u4e4b\u4eba\u5c07\u4e0d\u518d\u4fb5\u64fe\u4f60\uff0c \u4ed6\u5011\u90fd\u5c07\u88ab\u5fb9\u5e95\u6bc0\u6ec5\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5c3c\u5c3c\u5fae\u7684\u8986\u6ec5 - \u5c3c\u5c3c\u5fae\u554a\uff0c \u653b\u64ca\u8005\u5df2\u7d93\u5411\u4f60\u64b2\u4f86\u3002 \u4f60\u8981\u9032\u99d0\u5821\u58d8\uff0c \u628a\u5b88\u9053\u8def\uff0c \u96c6\u4e2d\u5168\u529b\uff0c \u6e96\u5099\u4e0a\u9663\uff01"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u96d6\u7136\u64c4\u63a0\u8005\u66fe\u64c4\u63a0\u96c5\u5404\uff0c \u64c4\u63a0\u4ee5\u8272\u5217\uff0c \u6bc0\u58de\u4ed6\u5011\u7684\u8461\u8404\u6a39\u679d\uff0c \u4f46\u8036\u548c\u83ef\u5fc5\u6062\u5fa9\u96c5\u5404\u7684\u69ae\u8000\uff0c \u6062\u5fa9\u4ee5\u8272\u5217\u7684\u69ae\u8000\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4ed6\u7684\u52c7\u58eb\u624b\u6301\u7d05\u8272\u76fe\u724c\uff0c \u4ed6\u7684\u58eb\u5175\u8eab\u7a7f\u6731\u7d05\u6230\u8863\u3002 \u6574\u88dd\u5f85\u767c\u4e4b\u65e5\uff0c \u6230\u8eca\u9435\u5149\u9583\u9583\uff0c \u677e\u6728\u77db\u69cd\u5728\u7a7a\u4e2d\u63ee\u821e\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6230\u8eca\u5728\u8857\u9053\u4e0a\u72c2\u5954\uff0c \u5728\u5ee3\u5834\u4e0a\u6a6b\u885d\u76f4\u649e\uff0c \u4eae\u5982\u706b\u70ac\uff0c\u75be\u4f3c\u9583\u96fb\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4e9e\u8ff0\u738b\u53ec\u96c6\u5c07\u9818\uff0c \u4ed6\u5011\u4e00\u8def\u8dcc\u8dcc\u649e\u649e\uff0c \u5954\u4e0a\u57ce\u7246\uff0c\u67b6\u8d77\u9632\u79a6\u76fe\u724c\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6cb3\u9598\u5927\u958b\uff0c\u738b\u5bae\u574d\u584c\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u738b\u540e\u8d64\u8eab\u88ab\u64c4\uff1b \u5bae\u5973\u6376\u80f8\u60b2\u6ce3\uff0c \u50cf\u9d3f\u5b50\u4e00\u6a23\u54c0\u9cf4\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5c3c\u5c3c\u5fae\u57ce\u81ea\u53e4\u4ee5\u4f86\u5c31\u50cf\u805a\u6c34\u7684\u6c60\u5b50\uff0c \u5982\u4eca\u5c45\u6c11\u537b\u50cf\u6d29\u6f0f\u7684\u6c60\u6c34\u56db\u6563\u5954\u9003\u3002 \u96d6\u6709\u4eba\u558a\uff1a\u300c\u7ad9\u4f4f\uff01\u7ad9\u4f4f\uff01\u300d \u537b\u7121\u4eba\u56de\u982d\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6436\u9280\u5b50\u5427\uff01\u6436\u91d1\u5b50\u5427\uff01 \u57ce\u88e1\u6709\u7121\u76e1\u7684\u8ca1\u5bcc\u548c\u6578\u4e0d\u6e05\u7684\u5bf6\u7269\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5c3c\u5c3c\u5fae\u88ab\u6d17\u52ab\u4e00\u7a7a\uff01 \u88ab\u6d17\u52ab\u4e00\u7a7a\uff01 \u4e00\u7247\u8352\u6dbc\uff01 \u4eba\u5011\u81bd\u6230\u5fc3\u9a5a\uff0c\u96d9\u819d\u54c6\u55e6\uff0c \u6e3e\u8eab\u767c\u6296\uff0c\u9762\u8272\u84bc\u767d\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5982\u4eca\u90a3\u7345\u5b50\u7684\u6d1e\u7a74\uff0c \u90a3\u9935\u990a\u58ef\u7345\u7684\u5730\u65b9\u5728\u54ea\u88e1\u5462\uff1f \u5f9e\u524d\uff0c\u96c4\u7345\u3001\u6bcd\u7345\u548c\u5e7c\u7345\u66fe\u5728\u90a3\u88e1\u51fa\u5165\uff0c \u7121\u4eba\u9a5a\u64fe\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u96c4\u7345\u70ba\u5e7c\u7345\u6495\u788e\u5145\u8db3\u7684\u98df\u7269\uff0c \u70ba\u6bcd\u7345\u6390\u6b7b\u7375\u7269\uff0c \u6d1e\u7a74\u88e1\u5806\u6eff\u7375\u7269\uff0c \u5806\u6eff\u6495\u788e\u7684\u8089\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u6211\u8981\u8207\u4f60\u70ba\u6575\uff0c \u71d2\u6bc0\u4f60\u7684\u6230\u8eca\uff0c \u4f7f\u5200\u528d\u541e\u566c\u4f60\u7684\u58ef\u7345\uff1b \u6211\u8981\u4f7f\u4f60\u7121\u6cd5\u518d\u64c4\u63a0\u4ed6\u570b\uff0c \u4f60\u4f7f\u8005\u7684\u8072\u97f3\u5c07\u5f9e\u6b64\u6d88\u901d\u3002\u300d"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5c3c\u5c3c\u5fae\u7684\u707d\u798d - \u5145\u6eff\u8840\u8165\u7684\u5c3c\u5c3c\u5fae\u6709\u798d\u4e86\uff01 \u57ce\u4e2d\u5145\u6eff\u8a6d\u8a50\u548c\u66b4\u529b\uff0c \u5bb3\u4eba\u7684\u4e8b\u5f9e\u672a\u6b62\u606f\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u97ad\u8072\u556a\u556a\uff0c\u8eca\u8f2a\u9686\u9686\uff0c \u6230\u99ac\u5954\u9a30\uff0c\u6230\u8eca\u75be\u99b3\uff0c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u9a0e\u58eb\u885d\u92d2\uff0c\u5200\u5149\u9583\u9583\uff0c\u77db\u69cd\u751f\u8f1d\uff1b \u88ab\u6bba\u8005\u773e\u591a\uff0c\u5c4d\u9aa8\u6210\u5806\uff0c \u4eba\u5011\u88ab\u904d\u5730\u7684\u5c4d\u9ad4\u7d46\u5012\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u9019\u5168\u662f\u56e0\u70ba\u5c3c\u5c3c\u5fae\u50cf\u5996\u5a9a\u7684\u5993\u5973\u5927\u8086\u6deb\u4e82\uff0c \u6163\u884c\u90aa\u8853\uff0c \u4ee5\u6deb\u884c\u8a98\u60d1\u5217\u570b\uff0c \u9760\u90aa\u8853\u6b3a\u9a19\u5404\u65cf\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u6211\u8981\u8207\u4f60\u70ba\u6575\u3002 \u6211\u8981\u628a\u4f60\u7684\u8863\u88d9\u6380\u5230\u4f60\u81c9\u4e0a\uff0c \u4f7f\u5217\u570b\u770b\u898b\u4f60\u7684\u88f8\u9ad4\uff0c \u4f7f\u5217\u90a6\u770b\u898b\u4f60\u7684\u7f9e\u6065\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u8981\u628a\u53ef\u618e\u4e4b\u7269\u62cb\u5728\u4f60\u8eab\u4e0a\uff0c \u4fae\u8fb1\u4f60\uff0c \u8b93\u773e\u4eba\u89c0\u770b\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u51e1\u770b\u898b\u4f60\u7684\u90fd\u8981\u9003\u907f\uff0c\u8aaa\uff0c \u300e\u5c3c\u5c3c\u5fae\u6bc0\u6ec5\u4e86\uff01 \u8ab0\u6703\u70ba\u5979\u60b2\u50b7\u5462\uff1f\u300f \u6211\u5230\u54ea\u88e1\u53bb\u5c0b\u627e\u5b89\u6170\u4f60\u7684\u4eba\u5462\uff1f\u300d"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u80fd\u5f37\u904e\u5e95\u6bd4\u65af\u55ce\uff1f \u5e95\u6bd4\u65af\u4f4d\u65bc\u5c3c\u7f85\u6cb3\u4e0a\uff0c \u56db\u9762\u74b0\u6c34\uff0c \u6c34\u662f\u5979\u7684\u5c4f\u969c\uff0c \u53c8\u662f\u5979\u7684\u57ce\u7246\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u53e4\u5be6\u548c\u57c3\u53ca\u662f\u5979\u7121\u7aae\u7684\u529b\u91cf\uff0c \u5f17\u4eba\u548c\u5229\u6bd4\u4e9e\u4eba\u662f\u5979\u7684\u5e6b\u624b\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u53ef\u662f\uff0c\u5979\u537b\u88ab\u64c4\u5230\u9060\u65b9\uff0c \u5979\u7684\u5b30\u5b69\u88ab\u6454\u6b7b\u5728\u5404\u500b\u8857\u982d\u3002 \u5979\u7684\u5b98\u9577\u88ab\u62bd\u7c64\u74dc\u5206\uff0c \u5979\u6240\u6709\u7684\u9996\u9818\u90fd\u88ab\u9396\u93c8\u9396\u8457\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5c3c\u5c3c\u5fae\u554a\uff0c \u4f60\u4e5f\u5fc5\u50cf\u9189\u6f22\u4e00\u6a23\uff0c \u4f60\u5fc5\u5c0b\u627e\u85cf\u8eab\u4e4b\u8655\u8eb2\u907f\u6575\u4eba\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f60\u6240\u6709\u7684\u5821\u58d8\u90fd\u50cf\u7121\u82b1\u679c\u6a39\u4e0a\u7684\u521d\u719f\u679c\u5b50\uff0c \u4e00\u6416\u6a39\uff0c\u679c\u5b50\u5c31\u6389\u9032\u5403\u7684\u4eba\u53e3\u4e2d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u770b\u554a\uff0c \u4f60\u7684\u8ecd\u968a\u5c31\u50cf\u67d4\u5f31\u7684\u5973\u4eba\uff0c \u4f60\u5883\u5167\u7684\u95dc\u53e3\u5411\u6575\u4eba\u655e\u958b\uff0c \u4f60\u7684\u9580\u9582\u88ab\u706b\u71d2\u6bc0\u3002"
                },
                "14": {
                    "verse_nr": "14",
