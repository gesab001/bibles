                "15": {
                    "verse_nr": "15",
                    "verse": "\u300c\u7528\u9152\u704c\u9189\u9130\u5c45\u4ee5\u89c0\u770b\u4ed6\u5011\u797c\u9ad4\u7684\u4eba\u554a\uff0c \u4f60\u5011\u6709\u798d\u4e86\uff01"
