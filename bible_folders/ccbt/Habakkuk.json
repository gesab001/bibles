                "12": {
                    "verse_nr": "12",
                    "verse": "\u300c\u4ee5\u4eba\u8840\u5efa\u57ce\u9091\uff0c \u4ee5\u7f6a\u60e1\u9020\u57ce\u6c60\u7684\u4eba\u554a\uff0c \u4f60\u5011\u6709\u798d\u4e86\uff01"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u5217\u90a6\u7684\u8ca1\u5bcc\u90fd\u5316\u70ba\u7070\u71fc\uff0c \u5217\u570b\u52de\u788c\u537b\u662f\u4e00\u5834\u7a7a\u3002 \u9019\u4e0d\u90fd\u662f\u51fa\u65bc\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u55ce\uff1f"
