                "10": {
                    "verse_nr": "10",
                    "verse": "\u4f60\u5011\u9670\u8b00\u6bc0\u6ec5\u5404\u65cf\uff0c \u4ee5\u81f4\u81ea\u8fb1\u5df1\u5bb6\uff0c\u81ea\u5bb3\u5df1\u547d\u3002"
