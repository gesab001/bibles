        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u70ba\u4e86\u50b3\u63da\u57fa\u7763\u8036\u7a4c\u800c\u88ab\u56da\u7981\u7684\u4fdd\u7f85\uff0c\u8207\u63d0\u6469\u592a\u5f1f\u5144\u5beb\u4fe1\u7d66\u6211\u5011\u89aa\u611b\u7684\u540c\u5de5\u8153\u5229\u9580\u5f1f\u5144\u3001"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4e9e\u8153\u4e9e\u59ca\u59b9\u548c\u6211\u5011\u7684\u6230\u53cb\u4e9e\u57fa\u5e03\uff0c\u4ee5\u53ca\u5728\u8153\u5229\u9580\u5bb6\u88e1\u805a\u6703\u7684\u5f1f\u5144\u59ca\u59b9\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u9858\u6211\u5011\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u8cdc\u7d66\u4f60\u5011\u6069\u5178\u548c\u5e73\u5b89\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7a31\u8b9a\u8153\u5229\u9580 - \u8153\u5229\u9580\u5f1f\u5144\uff0c\u6211\u5e38\u5e38\u5728\u79b1\u544a\u4e2d\u70ba\u4f60\u611f\u8b1d\u4e0a\u5e1d\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u56e0\u70ba\u6211\u807d\u8aaa\u4e86\u4f60\u5c0d\u4e3b\u8036\u7a4c\u7684\u4fe1\u5fc3\u548c\u5c0d\u773e\u8056\u5f92\u7684\u611b\u5fc3\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u6c42\u4e0a\u5e1d\u4f7f\u4f60\u80fd\u6709\u6548\u5730\u8207\u4eba\u5206\u4eab\u4fe1\u4ef0\uff0c\u660e\u767d\u6211\u5011\u6240\u505a\u7684\u4e00\u5207\u7f8e\u4e8b\u90fd\u662f\u70ba\u4e86\u57fa\u7763\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u5f1f\u5144\u554a\uff0c\u4f60\u7684\u611b\u5fc3\u7d66\u6211\u5e36\u4f86\u6975\u5927\u7684\u559c\u6a02\u548c\u5b89\u6170\uff0c\u56e0\u70ba\u4f60\u4f7f\u773e\u8056\u5f92\u611f\u5230\u6b23\u6170\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6191\u611b\u5fc3\u6c42\u60c5 - \u56e0\u6b64\uff0c\u6211\u5118\u7ba1\u53ef\u4ee5\u5949\u57fa\u7763\u7684\u540d\u5429\u5490\u4f60\u53bb\u505a\u7576\u505a\u7684\u4e8b\uff0c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7136\u800c\u6211\u9019\u4e0a\u4e86\u5e74\u7d00\u53c8\u70ba\u57fa\u7763\u8036\u7a4c\u7684\u7de3\u6545\u88ab\u56da\u7981\u7684\u4fdd\u7f85\u5be7\u9858\u6191\u611b\u5fc3\u6c42\u4f60\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5c31\u662f\u66ff\u963f\u5c3c\u897f\u8b00\u6c42\u4f60\u3002\u4ed6\u662f\u6211\u5728\u7344\u4e2d\u5e36\u9818\u6b78\u4e3b\u7684\u5c6c\u9748\u5152\u5b50\uff0c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4ed6\u904e\u53bb\u5c0d\u4f60\u6c92\u6709\u4ec0\u9ebc\u76ca\u8655\uff0c\u4f46\u73fe\u5728\u5c0d\u4f60\u5c0d\u6211\u90fd\u6709\u76ca\u8655\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u73fe\u5728\u6d3e\u6211\u6df1\u611b\u7684\u963f\u5c3c\u897f\u8b00\u56de\u4f60\u90a3\u88e1\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u6211\u672c\u60f3\u628a\u4ed6\u7559\u5728\u8eab\u908a\uff0c\u8b93\u4ed6\u5728\u6211\u70ba\u50b3\u798f\u97f3\u800c\u5750\u7262\u671f\u9593\u4ee3\u66ff\u4f60\u670d\u4f8d\u6211\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e0d\u904e\uff0c\u672a\u7d93\u4f60\u540c\u610f\uff0c\u6211\u4e0d\u9858\u610f\u9019\u6a23\u505a\uff0c\u56e0\u70ba\u6211\u76fc\u671b\u4f60\u7684\u5584\u884c\u662f\u51fa\u65bc\u7518\u5fc3\uff0c\u800c\u975e\u52c9\u5f37\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6216\u8a31\u4ed6\u66ab\u6642\u96e2\u958b\u4f60\u662f\u70ba\u4e86\u8b93\u4f60\u4ee5\u5f8c\u6c38\u9060\u5f97\u5230\u4ed6\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4f60\u5f97\u5230\u7684\u4e0d\u518d\u662f\u4e00\u500b\u5974\u96b8\uff0c\u800c\u662f\u4e00\u4f4d\u9060\u8d85\u904e\u5974\u96b8\u7684\u89aa\u611b\u5f1f\u5144\u3002\u5c0d\u6211\u800c\u8a00\uff0c\u7684\u78ba\u5982\u6b64\uff0c\u66f4\u4f55\u6cc1\u5c0d\u4f60\u5462\uff01\u5c31\u8089\u8eab\u8aaa\u4f60\u5011\u662f\u4e3b\u50d5\uff0c\u4f46\u5728\u4e3b\u88e1\u4f60\u5011\u662f\u5f1f\u5144\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u5982\u679c\u4f60\u7576\u6211\u662f\u540c\u4f34\uff0c\u5c31\u8acb\u4f60\u50cf\u63a5\u7d0d\u6211\u4e00\u6a23\u63a5\u7d0d\u4ed6\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u5982\u679c\u4ed6\u5f97\u7f6a\u4f60\uff0c\u6216\u8667\u6b20\u4f60\u4ec0\u9ebc\uff0c\u90fd\u8a18\u5728\u6211\u7684\u8cec\u4e0a\u5427\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u6211\u4fdd\u7f85\u5728\u6b64\u89aa\u7b46\u4fdd\u8b49\uff0c\u6211\u5fc5\u511f\u9084\u3002\u5176\u5be6\u6211\u4e0d\u8aaa\u4f60\u4e5f\u77e5\u9053\uff0c\u9023\u4f60\u81ea\u5df1\u4e5f\u6b20\u6211\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u5f1f\u5144\u554a\uff0c\u770b\u5728\u4e3b\u7684\u4efd\u4e0a\uff0c\u8acb\u4f60\u7b54\u61c9\u6211\u7684\u8acb\u6c42\uff0c\u5728\u57fa\u7763\u88e1\u8b93\u6211\u7684\u5fc3\u5f97\u5230\u5b89\u6170\u5427\uff01"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u6211\u5beb\u4fe1\u7d66\u4f60\uff0c\u6df1\u4fe1\u4f60\u4e00\u5b9a\u6703\u7167\u8fa6\uff0c\u751a\u81f3\u8d85\u904e\u6211\u7684\u8981\u6c42\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u540c\u6642\uff0c\u4e5f\u8acb\u4f60\u70ba\u6211\u9810\u5099\u4f4f\u8655\uff0c\u56e0\u70ba\u6211\u76fc\u671b\u85c9\u8457\u4f60\u5011\u7684\u79b1\u544a\uff0c\u6211\u53ef\u4ee5\u8499\u6069\u5230\u4f60\u5011\u90a3\u88e1\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u554f\u5019 - \u70ba\u4e86\u57fa\u7763\u8036\u7a4c\u7684\u7de3\u6545\u548c\u6211\u4e00\u540c\u5750\u7262\u7684\u4ee5\u5df4\u5f17\u554f\u5019\u4f60\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u6b64\u5916\uff0c\u6211\u7684\u540c\u5de5\u99ac\u53ef\u3001\u4e9e\u91cc\u9054\u53e4\u3001\u5e95\u99ac\u548c\u8def\u52a0\u90fd\u554f\u5019\u4f60\u3002"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u9858\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u6069\u5178\u8207\u4f60\u5011\u540c\u5728\u3002\u963f\u5011\uff01"
                }
            }
        }
    }
}