        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u5949\u4e0a\u5e1d\u65e8\u610f\u4f5c\u57fa\u7763\u8036\u7a4c\u4f7f\u5f92\u7684\u4fdd\u7f85\uff0c\u5beb\u4fe1\u7d66\u4ee5\u5f17\u6240\u5fe0\u65bc\u57fa\u7763\u8036\u7a4c\u7684\u773e\u8056\u5f92\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u9858\u6211\u5011\u7684\u7236\u4e0a\u5e1d\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u8cdc\u7d66\u4f60\u5011\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u57fa\u7763\u5f92\u7684\u5c6c\u9748\u6069\u798f - \u8b9a\u7f8e\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u7236\u4e0a\u5e1d\uff01\u7942\u5728\u57fa\u7763\u88e1\u8cdc\u7d66\u4e86\u6211\u5011\u5929\u4e0a\u5404\u6a23\u5c6c\u9748\u7684\u6069\u798f\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u65e9\u5728\u5275\u9020\u4e16\u754c\u4ee5\u524d\uff0c\u7942\u5df2\u7d93\u5728\u57fa\u7763\u88e1\u63c0\u9078\u4e86\u6211\u5011\uff0c\u4f7f\u6211\u5011\u5728\u7942\u773c\u4e2d\u6210\u70ba\u8056\u6f54\u7121\u7455\u7684\u4eba\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4e0a\u5e1d\u56e0\u70ba\u611b\u6211\u5011\uff0c\u5c31\u6309\u7167\u7942\u81ea\u5df1\u7f8e\u597d\u7684\u65e8\u610f\uff0c\u9810\u5b9a\u6211\u5011\u85c9\u8457\u8036\u7a4c\u57fa\u7763\u5f97\u5230\u4f5c\u7942\u5152\u5973\u7684\u540d\u5206\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u5011\u8b9a\u7f8e\u7942\u5947\u5999\u7121\u6bd4\u7684\u6069\u5178\uff0c\u9019\u6069\u5178\u662f\u4e0a\u5e1d\u85c9\u8457\u7942\u7684\u611b\u5b50\u8cdc\u7d66\u6211\u5011\u7684\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6211\u5011\u85c9\u8457\u7942\u611b\u5b50\u7684\u8840\u8499\u6551\u8d16\uff0c\u904e\u72af\u5f97\u5230\u8d66\u514d\uff0c\u9019\u90fd\u662f\u51fa\u65bc\u7942\u7684\u6d2a\u6069\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e0a\u5e1d\u7528\u5404\u7a2e\u667a\u6167\u548c\u8070\u660e\u628a\u9019\u6069\u5178\u8c50\u8c50\u5bcc\u5bcc\u5730\u8cdc\u7d66\u6211\u5011\uff0c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7167\u8457\u7942\u5728\u57fa\u7763\u88e1\u6240\u5b9a\u7684\u7f8e\u597d\u8a08\u5283\u53eb\u6211\u5011\u77e5\u9053\u7942\u65e8\u610f\u7684\u5967\u7955\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u7b49\u6240\u5b9a\u7684\u6642\u5019\u4e00\u5230\uff0c\u53eb\u5929\u5730\u842c\u7269\u4e00\u540c\u6b78\u5728\u57fa\u7763\u7684\u540d\u4e0b\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u5011\u4e5f\u5728\u57fa\u7763\u88e1\u88ab\u63c0\u9078\u6210\u70ba\u7942\u7684\u5b50\u6c11\u3002\u9019\u90fd\u662f\u6309\u81ea\u5df1\u65e8\u610f\u884c\u842c\u4e8b\u7684\u4e0a\u5e1d\u9810\u5148\u5b9a\u4e0b\u7684\u8a08\u5283\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u597d\u8b93\u6211\u5011\u9019\u4e9b\u9996\u5148\u5728\u57fa\u7763\u88e1\u5f97\u5230\u76fc\u671b\u7684\u4eba\u90fd\u4f86\u980c\u8b9a\u7942\u7684\u69ae\u8000\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u5011\u807d\u904e\u771f\u7406\u4e4b\u9053\uff0c\u5c31\u662f\u90a3\u4f7f\u4f60\u5011\u5f97\u6551\u7684\u798f\u97f3\uff0c\u800c\u4e14\u4e5f\u4fe1\u4e86\u57fa\u7763\u3002\u4f60\u5011\u65e2\u7136\u4fe1\u7942\uff0c\u5c31\u9818\u53d7\u4e86\u4e0a\u5e1d\u61c9\u8a31\u8cdc\u4e0b\u7684\u8056\u9748\u70ba\u5370\u8a18\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8056\u9748\u662f\u6211\u5011\u9818\u53d7\u7522\u696d\u7684\u64d4\u4fdd\uff0c\u76f4\u5230\u4e0a\u5e1d\u7684\u5b50\u6c11\u5f97\u5230\u6551\u8d16\uff0c\u4f7f\u7942\u7684\u69ae\u8000\u53d7\u5230\u980c\u8b9a\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u4fdd\u7f85\u7684\u79b1\u544a - \u6211\u807d\u5230\u4f60\u5011\u5c0d\u4e3b\u8036\u7a4c\u7684\u4fe1\u5fc3\u548c\u5c0d\u773e\u8056\u5f92\u7684\u611b\u5fc3\u5f8c\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4e0d\u65b7\u5730\u70ba\u4f60\u5011\u611f\u8b1d\u4e0a\u5e1d\uff0c\u79b1\u544a\u7684\u6642\u5019\u5e38\u5e38\u63d0\u5230\u4f60\u5011\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u6211\u6c42\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u4e0a\u5e1d\u2014\u2014\u69ae\u8000\u7684\u7236\u628a\u8cdc\u667a\u6167\u548c\u555f\u793a\u7684\u9748\u7d66\u4f60\u5011\uff0c\u597d\u4f7f\u4f60\u5011\u66f4\u6df1\u5730\u8a8d\u8b58\u7942\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u6211\u4e5f\u6c42\u4e0a\u5e1d\u7167\u4eae\u4f60\u5011\u5fc3\u4e2d\u7684\u773c\u775b\uff0c\u4f7f\u4f60\u5011\u77e5\u9053\u7942\u7684\u547c\u53ec\u7d66\u4f60\u5011\u5e36\u4f86\u4e86\u4f55\u7b49\u7684\u76fc\u671b\uff0c\u7942\u61c9\u8a31\u8cdc\u7d66\u773e\u8056\u5f92\u7684\u7522\u696d\u6709\u4f55\u7b49\u8c50\u5bcc\u7684\u69ae\u8000\uff0c"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u4e26\u4e14\u7942\u5728\u6211\u5011\u9019\u4e9b\u4fe1\u7684\u4eba\u8eab\u4e0a\u6240\u904b\u884c\u7684\u80fd\u529b\u662f\u4f55\u7b49\u6d69\u5927\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4e0a\u5e1d\u66fe\u7528\u9019\u5927\u80fd\u4f7f\u57fa\u7763\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\uff0c\u4f7f\u57fa\u7763\u5728\u5929\u4e0a\u5750\u5728\u81ea\u5df1\u53f3\u908a\uff0c"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u9060\u9060\u8d85\u8d8a\u4eca\u751f\u6c38\u4e16\u6240\u6709\u57f7\u653f\u7684\u3001\u638c\u6b0a\u7684\u3001\u6709\u80fd\u529b\u7684\u3001\u4f5c\u4e3b\u5bb0\u7684\u548c\u4e00\u5207\u7684\u6b0a\u52e2\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u7942\u53c8\u4f7f\u842c\u7269\u964d\u670d\u5728\u57fa\u7763\u8173\u4e0b\uff0c\u4f7f\u57fa\u7763\u70ba\u6559\u6703\u4f5c\u842c\u7269\u7684\u5143\u9996\u3002"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u6559\u6703\u662f\u57fa\u7763\u7684\u8eab\u9ad4\uff0c\u860a\u6db5\u8457\u90a3\u7121\u6240\u4e0d\u5728\u3001\u5145\u6eff\u842c\u7269\u8005\u7684\u8c50\u76db\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u9760\u6069\u5178\u3001\u6191\u4fe1\u5fc3 - \u4f60\u5011\u5f9e\u524d\u6b7b\u5728\u904e\u72af\u548c\u7f6a\u60e1\u4e4b\u4e2d\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u90a3\u6642\uff0c\u4f60\u5011\u96a8\u5f9e\u4eca\u4e16\u7684\u98a8\u4fd7\uff0c\u9806\u670d\u5728\u7a7a\u4e2d\u638c\u6b0a\u7684\u9b54\u9b3c\uff0c\u5c31\u662f\u5982\u4eca\u5728\u4e00\u5207\u53db\u9006\u4e0a\u5e1d\u4e4b\u4eba\u5fc3\u4e2d\u904b\u884c\u7684\u90aa\u9748\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u6211\u5011\u4ee5\u524d\u4e5f\u5728\u4ed6\u5011\u7576\u4e2d\u96a8\u5f9e\u7f6a\u60e1\u7684\u672c\u6027\uff0c\u653e\u7e31\u8089\u9ad4\u548c\u5fc3\u4e2d\u7684\u79c1\u617e\uff0c\u8207\u5176\u4ed6\u4eba\u4e00\u6a23\u672c\u662f\u60f9\u4e0a\u5e1d\u767c\u6012\u7684\u4eba\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7136\u800c\u4e0a\u5e1d\u6709\u8c50\u5bcc\u7684\u6190\u61ab\uff0c\u7942\u6df1\u611b\u6211\u5011\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5118\u7ba1\u6211\u5011\u6b7b\u5728\u904e\u72af\u4e4b\u4e2d\uff0c\u7942\u4ecd\u7136\u4f7f\u6211\u5011\u548c\u57fa\u7763\u4e00\u540c\u6d3b\u4e86\u904e\u4f86\u3002\u4f60\u5011\u5f97\u6551\u662f\u56e0\u70ba\u4e0a\u5e1d\u7684\u6069\u5178\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u5728\u57fa\u7763\u8036\u7a4c\u88e1\uff0c\u4e0a\u5e1d\u4f7f\u6211\u5011\u8207\u57fa\u7763\u4e00\u540c\u5fa9\u6d3b\uff0c\u4e00\u540c\u5750\u5728\u5929\u4e0a\uff0c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u597d\u85c9\u8457\u7942\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u5411\u6211\u5011\u6240\u65bd\u7684\u6069\u6148\uff0c\u5411\u5f8c\u4e16\u986f\u660e\u7942\u7121\u53ef\u6bd4\u64ec\u7684\u8c50\u5bcc\u6069\u5178\u3002"
