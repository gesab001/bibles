                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u5011\u5f97\u6551\u662f\u9760\u6069\u5178\uff0c\u6191\u4fe1\u5fc3\u3002\u9019\u4e0d\u662f\u51fa\u65bc\u4f60\u5011\u81ea\u5df1\uff0c\u800c\u662f\u4e0a\u5e1d\u7684\u79ae\u7269\uff1b"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4e5f\u4e0d\u662f\u7531\u65bc\u4f60\u5011\u7684\u884c\u70ba\uff0c\u4ee5\u514d\u6709\u4eba\u81ea\u8a87\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u5011\u662f\u4e0a\u5e1d\u7684\u5091\u4f5c\uff0c\u662f\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u5275\u9020\u7684\uff0c\u70ba\u8981\u53eb\u6211\u5011\u505a\u7942\u9810\u5148\u5b89\u6392\u7d66\u6211\u5011\u7684\u7f8e\u5584\u4e4b\u4e8b\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u5728\u57fa\u7763\u88e1\u5408\u800c\u70ba\u4e00 - \u56e0\u6b64\uff0c\u4f60\u5011\u8981\u8a18\u4f4f\uff0c\u5f9e\u524d\u6309\u7167\u8089\u9ad4\u4f86\u8aaa\u4f60\u5011\u662f\u5916\u65cf\u4eba\uff0c\u90a3\u4e9b\u8089\u9ad4\u4e0a\u53d7\u904e\u4eba\u624b\u6240\u884c\u5272\u79ae\u7684\u4ee5\u8272\u5217\u4eba\u7a31\u4f60\u5011\u70ba\u300c\u6c92\u6709\u53d7\u904e\u5272\u79ae\u7684\u300d\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u90a3\u6642\uff0c\u4f60\u5011\u8207\u57fa\u7763\u7121\u95dc\uff0c\u53c8\u4e0d\u662f\u4ee5\u8272\u5217\u4eba\uff0c\u5728\u4e0a\u5e1d\u61c9\u8a31\u7684\u8af8\u7d04\u4e0a\u662f\u5c40\u5916\u4eba\uff0c\u4f60\u5011\u6d3b\u5728\u4e16\u4e0a\u6c92\u6709\u76fc\u671b\uff0c\u6c92\u6709\u4e0a\u5e1d\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f46\u4f60\u5011\u9019\u4e9b\u5f9e\u524d\u9060\u96e2\u4e0a\u5e1d\u7684\u4eba\uff0c\u5982\u4eca\u5728\u57fa\u7763\u8036\u7a4c\u88e1\uff0c\u9760\u8457\u7942\u6240\u6d41\u7684\u8840\u5df2\u7d93\u88ab\u5e36\u5230\u4e0a\u5e1d\u9762\u524d\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u56e0\u70ba\u57fa\u7763\u8036\u7a4c\u89aa\u81ea\u7d66\u6211\u5011\u5e36\u4f86\u4e86\u548c\u5e73\uff0c\u4ee5\u81ea\u5df1\u7684\u8eab\u9ad4\u62c6\u6bc0\u4e86\u4ee5\u8272\u5217\u4eba\u548c\u5916\u65cf\u4eba\u4e4b\u9593\u5145\u6eff\u6575\u610f\u7684\u9694\u7246\uff0c\u4f7f\u96d9\u65b9\u5408\u800c\u70ba\u4e00\uff0c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u4e26\u5ee2\u9664\u4e86\u4ee5\u8aa1\u547d\u548c\u898f\u689d\u70ba\u5167\u5bb9\u7684\u5f8b\u6cd5\u5236\u5ea6\u3002\u7942\u9019\u6a23\u505a\u662f\u70ba\u4e86\u5728\u81ea\u5df1\u88e1\u9762\u628a\u96d9\u65b9\u9020\u6210\u4e00\u500b\u65b0\u4eba\uff0c\u6210\u5c31\u548c\u5e73\uff0c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4e26\u85c9\u8457\u5341\u5b57\u67b6\u6d88\u6ec5\u5f7c\u6b64\u9593\u7684\u6575\u610f\uff0c\u4f7f\u96d9\u65b9\u85c9\u8457\u4e00\u500b\u8eab\u9ad4\u8207\u4e0a\u5e1d\u548c\u597d\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u7942\u4f86\u50b3\u63da\u9019\u5e73\u5b89\u7684\u798f\u97f3\u7d66\u4f60\u5011\u9019\u4e9b\u96e2\u4e0a\u5e1d\u5f88\u9060\u7684\u5916\u65cf\u4eba\uff0c\u4e5f\u7d66\u6211\u5011\u9019\u4e9b\u96e2\u4e0a\u5e1d\u5f88\u8fd1\u7684\u4ee5\u8272\u5217\u4eba\u3002"
                },
                "18": {
