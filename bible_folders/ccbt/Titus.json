        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u554f\u5019 - \u6211\u4fdd\u7f85\u4f5c\u4e0a\u5e1d\u7684\u5974\u50d5\u548c\u8036\u7a4c\u57fa\u7763\u7684\u4f7f\u5f92\uff0c\u662f\u70ba\u4e86\u4f7f\u4e0a\u5e1d\u63c0\u9078\u7684\u4eba\u6709\u4fe1\u5fc3\u3001\u660e\u767d\u771f\u7406\uff0c\u5f9e\u800c\u904e\u656c\u8654\u7684\u751f\u6d3b\uff0c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6709\u6c38\u751f\u7684\u76fc\u671b\u3002\u9019\u6c38\u751f\u662f\u5f9e\u4e0d\u8aaa\u8b0a\u7684\u4e0a\u5e1d\u5728\u4e99\u53e4\u4ee5\u524d\u61c9\u8a31\u7684\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u7942\u5728\u6240\u5b9a\u7684\u6642\u5019\uff0c\u85c9\u8457\u4eba\u7684\u50b3\u63da\u5c07\u81ea\u5df1\u7684\u9053\u986f\u660e\u51fa\u4f86\u3002\u6211\u5949\u6211\u5011\u6551\u4e3b\u4e0a\u5e1d\u7684\u547d\u4ee4\u50b3\u63da\u9019\u9053\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u63d0\u591a\u554a\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\uff0c\u5c31\u6211\u5011\u5171\u540c\u7684\u4fe1\u4ef0\u4f86\u8aaa\uff0c\u4f60\u662f\u6211\u771f\u6b63\u7684\u5152\u5b50\u3002 \u9858\u7236\u4e0a\u5e1d\u548c\u6211\u5011\u7684\u6551\u4e3b\u57fa\u7763\u8036\u7a4c\u8cdc\u7d66\u4f60\u6069\u5178\u548c\u5e73\u5b89\uff01"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u9577\u8001\u7684\u8cc7\u683c - \u7576\u521d\uff0c\u6211\u628a\u4f60\u7559\u5728\u514b\u91cc\u7279\u5cf6\uff0c\u662f\u8981\u4f60\u5b8c\u6210\u672a\u8fa6\u5b8c\u7684\u4e8b\u5de5\uff0c\u4e26\u7167\u6211\u7684\u6307\u793a\u5728\u5404\u57ce\u9078\u7acb\u9577\u8001\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4f5c\u9577\u8001\u7684\u5fc5\u9808\u7121\u53ef\u6307\u8cac\uff0c\u53ea\u6709\u4e00\u4f4d\u59bb\u5b50\uff0c\u5152\u5973\u90fd\u4fe1\u4e3b\u3001\u6c92\u6709\u653e\u8569\u4e0d\u7f88\u7684\u884c\u70ba\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8eab\u70ba\u4e0a\u5e1d\u7684\u7ba1\u5bb6\uff0c\u4f5c\u76e3\u7763\u7684\u5fc5\u9808\u7121\u53ef\u6307\u8cac\uff0c\u4e0d\u50b2\u6162\uff0c\u4e0d\u66b4\u8e81\uff0c\u4e0d\u9157\u9152\uff0c\u4e0d\u597d\u9b25\uff0c\u4e0d\u8caa\u8ca1\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ed6\u5fc5\u9808\u597d\u5ba2\u3001\u6a02\u5584\u3001\u81ea\u5236\u3001\u6b63\u76f4\u3001\u8056\u6f54\u3001\u81ea\u5f8b\uff0c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6301\u5b88\u6240\u9818\u53d7\u7684\u771f\u9053\uff0c\u4ee5\u4fbf\u80fd\u5920\u4ee5\u7d14\u6b63\u7684\u6559\u5c0e\u52f8\u52c9\u4eba\uff0c\u99c1\u5012\u90a3\u4e9b\u53cd\u5c0d\u7684\u4eba\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u56e0\u70ba\u6709\u8a31\u591a\u6096\u9006\u4e4b\u4eba\u559c\u6b61\u7a7a\u8ac7\uff0c\u5584\u65bc\u6b3a\u9a19\uff0c\u5c24\u5176\u662f\u90a3\u4e9b\u5949\u884c\u5272\u79ae\u7684\u4eba\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4f60\u4e00\u5b9a\u8981\u5835\u4f4f\u4ed6\u5011\u7684\u53e3\uff0c\u56e0\u70ba\u4ed6\u5011\u70ba\u4e86\u4e0d\u7fa9\u4e4b\u8ca1\u6559\u4e0d\u8a72\u6559\u7684\uff0c\u8aa4\u5c0e\u4e86\u5225\u4eba\u5168\u5bb6\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u514b\u91cc\u7279\u4eba\u81ea\u5df1\u7684\u4e00\u4f4d\u5148\u77e5\u8aaa\uff1a\u300c\u514b\u91cc\u7279\u4eba\u8aaa\u8b0a\u6210\u6027\uff0c\u90aa\u60e1\u5982\u7378\uff0c\u597d\u5403\u61f6\u505a\u3002\u300d"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u9019\u8a71\u662f\u771f\u7684\u3002\u56e0\u6b64\u4f60\u8981\u56b4\u6b63\u5730\u65a5\u8cac\u4ed6\u5011\uff0c\u4f7f\u4ed6\u5011\u6709\u7d14\u6b63\u7684\u4fe1\u4ef0\uff0c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e0d\u7406\u6703\u7336\u592a\u4eba\u8352\u8a95\u7684\u50b3\u8aaa\u548c\u90a3\u4e9b\u9055\u80cc\u771f\u7406\u4e4b\u4eba\u7684\u8aa1\u5f8b\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u5c0d\u6f54\u6de8\u7684\u4eba\u800c\u8a00\uff0c\u4e00\u5207\u90fd\u662f\u6f54\u6de8\u7684\uff1b\u5c0d\u6c61\u7a62\u4e0d\u4fe1\u7684\u4eba\u800c\u8a00\uff0c\u4ec0\u9ebc\u90fd\u4e0d\u6f54\u6de8\uff0c\u9023\u4ed6\u5011\u7684\u601d\u60f3\u548c\u826f\u5fc3\u90fd\u662f\u6c61\u7a62\u7684\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u4ed6\u5011\u81ea\u7a31\u8a8d\u8b58\u4e0a\u5e1d\uff0c\u5728\u884c\u70ba\u4e0a\u537b\u5426\u8a8d\u7942\u3002\u4ed6\u5011\u4ee4\u4eba\u53ef\u618e\uff0c\u6096\u9006\u6210\u6027\uff0c\u4e00\u7121\u662f\u8655\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6559\u5c0e\u7d14\u6b63\u7684\u6559\u7fa9 - \u4f60\u7684\u6559\u5c0e\u8981\u7b26\u5408\u7d14\u6b63\u7684\u6559\u7fa9\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u52f8\u5e74\u9577\u7684\u7537\u5b50\u8981\u7bc0\u5236\u3001\u838a\u91cd\u3001\u81ea\u5f8b\uff0c\u6709\u5168\u5099\u7684\u4fe1\u5fc3\u3001\u611b\u5fc3\u548c\u8010\u5fc3\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u52f8\u5e74\u9577\u7684\u5a66\u5973\u8981\u904e\u656c\u8654\u7684\u751f\u6d3b\uff0c\u4e0d\u8aaa\u9577\u9053\u77ed\uff0c\u4e0d\u505a\u9152\u7684\u5974\u96b8\uff0c\u8981\u4ee5\u8eab\u4f5c\u5247\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u9019\u6a23\uff0c\u5979\u5011\u5c31\u80fd\u6559\u5c0e\u5e74\u8f15\u7684\u5a66\u5973\u611b\u4e08\u592b\u3001\u611b\u5152\u5973\u3001"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u81ea\u5236\u3001\u8c9e\u6f54\uff0c\u6301\u5bb6\u3001\u548c\u5584\u3001\u9806\u670d\u4e08\u592b\uff0c\u514d\u5f97\u4e0a\u5e1d\u7684\u9053\u88ab\u4eba\u6bc0\u8b17\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u52f8\u52c9\u5e74\u8f15\u7684\u7537\u5b50\u8981\u81ea\u5236\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u4f60\u81ea\u5df1\u8981\u5728\u5404\u6a23\u7684\u5584\u884c\u4e0a\u4f5c\u773e\u4eba\u7684\u699c\u6a23\uff0c\u5728\u6559\u5c0e\u4e0a\u8981\u8aa0\u61c7\u3001\u8a8d\u771f\u3001"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u8a00\u8a9e\u7d14\u5168\u3001\u7121\u53ef\u6307\u8cac\uff0c\u8b93\u90a3\u4e9b\u53cd\u5c0d\u7684\u4eba\u7121\u8a71\u53ef\u8aaa\u3001\u81ea\u89ba\u7f9e\u6127\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u52f8\u52c9\u4f5c\u5974\u50d5\u7684\u8981\u51e1\u4e8b\u9806\u670d\u4e3b\u4eba\uff0c\u8b93\u4e3b\u4eba\u6eff\u610f\uff0c\u4e0d\u9802\u649e\u4e3b\u4eba\uff0c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u4e0d\u5077\u62ff\u4e3b\u4eba\u7684\u6771\u897f\uff0c\u70ba\u4eba\u8981\u5fe0\u4fe1\u53ef\u9760\uff0c\u597d\u51e1\u4e8b\u5c0a\u5d07\u6211\u5011\u6551\u4e3b\u4e0a\u5e1d\u7684\u6559\u5c0e\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u70ba\u4e0a\u5e1d\u62ef\u6551\u4e16\u4eba\u7684\u6069\u5178\u5df2\u7d93\u986f\u660e\u4e86\uff0c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u9019\u6069\u5178\u6559\u5c0e\u6211\u5011\u9664\u6389\u4e0d\u656c\u8654\u7684\u5fc3\u548c\u4e16\u4fd7\u7684\u79c1\u617e\uff0c\u5728\u4eca\u4e16\u904e\u81ea\u5f8b\u3001\u516c\u7fa9\u3001\u656c\u754f\u4e0a\u5e1d\u7684\u751f\u6d3b\uff0c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u61f7\u8457\u7f8e\u597d\u7684\u76fc\u671b\u7b49\u5019\u8036\u7a4c\u57fa\u7763\u2014\u2014\u6211\u5011\u5049\u5927\u7684\u4e0a\u5e1d\u548c\u6551\u4e3b\u7684\u69ae\u8000\u986f\u73fe\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e3b\u8036\u7a4c\u70ba\u6211\u5011\u72a7\u7272\u81ea\u5df1\uff0c\u8981\u6551\u8d16\u6211\u5011\u812b\u96e2\u4e00\u5207\u7f6a\u60e1\uff0c\u4e26\u6f54\u6de8\u6211\u5011\uff0c\u4f7f\u6211\u5011\u4f5c\u7942\u7684\u5b50\u6c11\uff0c\u6210\u70ba\u71b1\u5fc3\u884c\u5584\u7684\u4eba\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u4f60\u8981\u8b1b\u660e\u9019\u4e9b\u4e8b\uff0c\u904b\u7528\u5404\u6a23\u6b0a\u67c4\u52f8\u52c9\u4eba\u3001\u8cac\u5099\u4eba\uff0c\u5225\u8b93\u4eba\u8f15\u770b\u4f60\u3002"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u5c08\u5fc3\u884c\u5584 - \u4f60\u8981\u63d0\u9192\u773e\u4eba\u9806\u670d\u57f7\u653f\u638c\u6b0a\u8005\uff0c\u9075\u5b88\u6cd5\u4ee4\uff0c\u96a8\u6642\u6e96\u5099\u505a\u5584\u4e8b\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4e0d\u8981\u6bc0\u8b17\uff0c\u4e0d\u8981\u722d\u5435\uff0c\u8981\u8b19\u548c\u3001\u606d\u656c\u5730\u5c0d\u5f85\u6bcf\u4e00\u500b\u4eba\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u5f9e\u524d\u6211\u5011\u4e5f\u611a\u8822\u3001\u6096\u9006\u3001\u6613\u53d7\u8ff7\u60d1\uff0c\u53d7\u5404\u7a2e\u79c1\u617e\u548c\u4eab\u6a02\u7684\u9a45\u4f7f\uff0c\u5fc3\u88e1\u5145\u6eff\u4e86\u5ac9\u5992\u548c\u60e1\u5ff5\uff0c\u4ee4\u4eba\u618e\u60e1\uff0c\u4e5f\u5f7c\u6b64\u618e\u6068\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4f46\u6211\u5011\u7684\u6551\u4e3b\u4e0a\u5e1d\u5411\u4eba\u5f70\u986f\u4e86\u7942\u7684\u6069\u6148\u548c\u4ec1\u611b\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u85c9\u8457\u91cd\u751f\u4e4b\u6d17\u548c\u8056\u9748\u7684\u66f4\u65b0\u62ef\u6551\u4e86\u6211\u5011\uff0c\u9019\u4e26\u975e\u56e0\u70ba\u6211\u5011\u6709\u7fa9\u884c\uff0c\u800c\u662f\u56e0\u70ba\u7942\u7684\u6190\u61ab\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4e0a\u5e1d\u85c9\u8457\u6211\u5011\u7684\u6551\u4e3b\u8036\u7a4c\u57fa\u7763\u628a\u8056\u9748\u8c50\u8c50\u5bcc\u5bcc\u5730\u6f86\u704c\u5728\u6211\u5011\u8eab\u4e0a\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u9019\u6a23\uff0c\u6211\u5011\u65e2\u7136\u9760\u8457\u7942\u7684\u6069\u5178\u800c\u88ab\u7a31\u70ba\u7fa9\u4eba\uff0c\u5c31\u53ef\u4ee5\u6210\u70ba\u5f8c\u55e3\uff0c\u6709\u6c38\u751f\u7684\u76fc\u671b\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4ee5\u4e0a\u9019\u4e9b\u8a71\u662f\u53ef\u4fe1\u7684\uff0c\u5e0c\u671b\u4f60\u8a8d\u771f\u6559\u5c0e\uff0c\u4f7f\u4fe1\u4e0a\u5e1d\u7684\u4eba\u5c08\u5fc3\u884c\u5584\u3002\u9019\u90fd\u662f\u9020\u798f\u773e\u4eba\u7684\u7f8e\u4e8b\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8981\u907f\u514d\u611a\u6627\u7684\u722d\u8ad6\u3001\u6709\u95dc\u5bb6\u8b5c\u7684\u8faf\u99c1\u548c\u5f8b\u6cd5\u4e0a\u7684\u722d\u57f7\uff0c\u56e0\u70ba\u9019\u4e9b\u6beb\u7121\u76ca\u8655\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5c0d\u65bc\u88fd\u9020\u5206\u88c2\u7684\u4eba\uff0c\u8b66\u544a\u904e\u4e00\u5169\u6b21\u5f8c\uff0c\u8981\u548c\u4ed6\u65b7\u7d55\u4f86\u5f80\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u70ba\u4f60\u77e5\u9053\u9019\u7a2e\u4eba\u5df2\u7d93\u80cc\u9053\u72af\u7f6a\uff0c\u81ea\u5b9a\u5df1\u7f6a\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4fe1\u672b\u7684\u56d1\u5490\u8207\u554f\u5019 - \u6211\u6703\u6d3e\u4e9e\u63d0\u99ac\u6216\u8005\u63a8\u57fa\u53e4\u53bb\u898b\u4f60\u3002\u5230\u6642\u5019\uff0c\u4f60\u8981\u7acb\u523b\u5230\u5c3c\u54e5\u6ce2\u7acb\u4f86\u898b\u6211\uff0c\u56e0\u70ba\u6211\u6c7a\u5b9a\u5728\u90a3\u88e1\u904e\u51ac\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u8981\u70ba\u897f\u7d0d\u5f8b\u5e2b\u548c\u4e9e\u6ce2\u7f85\u9001\u884c\uff0c\u76e1\u529b\u5e6b\u52a9\u4ed6\u5011\uff0c\u8b93\u4ed6\u5011\u8def\u4e0a\u4e00\u7121\u6240\u7f3a\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6211\u5011\u7684\u5f1f\u5144\u59ca\u59b9\u4e5f\u8981\u5b78\u7fd2\u884c\u5584\uff0c\u4f9b\u61c9\u6709\u6025\u9700\u7684\u4eba\uff0c\u514d\u5f97\u6beb\u7121\u8ca2\u737b\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u9019\u88e1\u7684\u5f1f\u5144\u59ca\u59b9\u90fd\u554f\u5019\u4f60\uff0c\u4e5f\u8acb\u4f60\u554f\u5019\u90a3\u4e9b\u56e0\u4fe1\u4ef0\u800c\u611b\u6211\u5011\u7684\u4eba\u3002 \u9858\u6069\u5178\u5e38\u8207\u4f60\u5011\u773e\u4eba\u540c\u5728\uff01"
