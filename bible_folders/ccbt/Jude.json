{
    "version": "ccbt",
    "book_name": "Jude",
    "book_nr": "65",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u662f\u8036\u7a4c\u57fa\u7763\u7684\u5974\u50d5\u3001\u96c5\u5404\u7684\u5144\u5f1f\u7336\u5927\uff0c\u73fe\u5728\u5beb\u4fe1\u7d66\u8499\u7236\u4e0a\u5e1d\u547c\u53ec\u3001\u7737\u611b\u3001\u88ab\u8036\u7a4c\u57fa\u7763\u770b\u9867\u7684\u4eba\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u9858\u4e0a\u5e1d\u8c50\u8c50\u5bcc\u5bcc\u5730\u8cdc\u7d66\u4f60\u5011\u6190\u61ab\u3001\u5e73\u5b89\u548c\u6148\u611b\uff01"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u634d\u885b\u771f\u9053 - \u89aa\u611b\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u6211\u4e00\u76f4\u8feb\u5207\u5730\u60f3\u5beb\u4fe1\u8ddf\u4f60\u5011\u8ac7\u8ac7\u6211\u5011\u6240\u5171\u4eab\u7684\u6551\u6069\uff0c\u4f46\u73fe\u5728\u6211\u89ba\u5f97\u6709\u5fc5\u8981\u5beb\u4fe1\u52f8\u52c9\u4f60\u5011\u7aed\u529b\u8b77\u885b\u4e00\u6b21\u5c31\u5b8c\u6574\u5730\u4ea4\u7d66\u8056\u5f92\u7684\u771f\u9053\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u56e0\u70ba\u6709\u4e9b\u4e0d\u656c\u8654\u7684\u4eba\u5077\u5077\u5730\u6df7\u9032\u4f60\u5011\u4e2d\u9593\uff0c\u4ee5\u4e0a\u5e1d\u7684\u6069\u5178\u4f5c\u85c9\u53e3\uff0c\u653e\u7e31\u60c5\u617e\uff0c\u5426\u8a8d\u7368\u4e00\u7684\u4e3b\u5bb0\u2014\u2014\u6211\u5011\u7684\u4e3b\u8036\u7a4c\u57fa\u7763\u3002\u8056\u7d93\u4e0a\u65e9\u5df2\u8a18\u8f09\uff0c\u9019\u6a23\u7684\u4eba\u5fc5\u53d7\u5230\u5be9\u5224\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u524d\u8eca\u4e4b\u9451 - \u4ee5\u4e0b\u7684\u4e8b\u60c5\uff0c\u4f60\u5011\u96d6\u7136\u90fd\u77e5\u9053\uff0c\u4f46\u6211\u9084\u8981\u518d\u63d0\u9192\u4f60\u5011\uff1a\u5f9e\u524d\u4e0a\u5e1d\u628a\u7942\u7684\u5b50\u6c11\u5f9e\u57c3\u53ca\u6551\u51fa\u4f86\uff0c\u5f8c\u4f86\u628a\u5176\u4e2d\u4e0d\u4fe1\u7684\u4eba\u6ec5\u7d55\u4e86\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u81f3\u65bc\u4e0d\u5b88\u672c\u5206\u3001\u64c5\u96e2\u5d17\u4f4d\u7684\u5929\u4f7f\uff0c\u4e0a\u5e1d\u4e5f\u7528\u9396\u93c8\u5c07\u4ed6\u5011\u6c38\u9060\u56da\u7981\u5728\u5e7d\u6697\u88e1\uff0c\u7b49\u5019\u6700\u5f8c\u5be9\u5224\u7684\u5927\u65e5\u5b50\u5230\u4f86\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6b64\u5916\uff0c\u6240\u591a\u746a\u3001\u86fe\u6469\u62c9\u53ca\u5176\u9644\u8fd1\u57ce\u93ae\u7684\u4eba\u540c\u6a23\u56e0\u70ba\u8352\u6deb\u7121\u5ea6\u3001\u6c89\u6eba\u65bc\u8b8a\u614b\u7684\u60c5\u617e\u800c\u906d\u5230\u6c38\u706b\u7684\u5211\u7f70\u3002\u9019\u4e9b\u4e8b\u90fd\u6210\u70ba\u6211\u5011\u7684\u8b66\u6212\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5047\u6559\u5e2b\u7684\u60e1\u884c - \u540c\u6a23\uff0c\u9019\u4e9b\u4f5c\u5922\u7684\u4eba\u73b7\u6c61\u81ea\u5df1\u7684\u8eab\u9ad4\uff0c\u4e0d\u670d\u6b0a\u67c4\uff0c\u893b\u7006\u6709\u5c0a\u69ae\u7684\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7576\u5929\u4f7f\u9577\u7c73\u8fe6\u52d2\u70ba\u6469\u897f\u7684\u5c4d\u9ad4\u8ddf\u9b54\u9b3c\u722d\u8faf\u7684\u6642\u5019\uff0c\u5c1a\u4e14\u4e0d\u6562\u7528\u6bc0\u8b17\u7684\u8a71\u8b74\u8cac\u7260\uff0c\u53ea\u8aaa\uff1a\u300c\u9858\u4e3b\u8cac\u7f70\u4f60\uff01\u300d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u9019\u4e9b\u4eba\u537b\u6bc0\u8b17\u81ea\u5df1\u4e0d\u660e\u767d\u7684\u4e8b\uff0c\u50cf\u6c92\u6709\u7406\u6027\u7684\u91ce\u7378\u4e00\u6a23\u6191\u672c\u80fd\u884c\u4e8b\uff0c\u7d50\u679c\u81ea\u53d6\u6ec5\u4ea1\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4ed6\u5011\u5927\u798d\u81e8\u982d\u4e86\uff01\u4ed6\u5011\u6b65\u4e86\u8a72\u96b1\u7684\u5f8c\u5875\uff0c\u70ba\u8b00\u5229\u800c\u91cd\u8e48\u5df4\u862d\u7684\u8b2c\u8aa4\uff0c\u53c8\u50cf\u53ef\u62c9\u4e00\u6a23\u56e0\u53db\u9006\u800c\u6ec5\u4ea1\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u9019\u4e9b\u4eba\u5728\u4f60\u5011\u7684\u611b\u5bb4\u4e2d\u662f\u6557\u985e\u3002\u4ed6\u5011\u8086\u7121\u5fcc\u619a\u5730\u5403\u559d\uff0c\u662f\u53ea\u9867\u9935\u990a\u81ea\u5df1\u7684\u7267\u4eba\uff1b\u662f\u6c92\u6709\u96e8\u7684\u96f2\uff0c\u96a8\u98a8\u98c4\u8569\uff1b\u662f\u6df1\u79cb\u4e0d\u7d50\u679c\u5b50\u7684\u6a39\uff0c\u88ab\u9023\u6839\u62d4\u8d77\uff0c\u5fb9\u5e95\u67af\u6b7b\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4ed6\u5011\u662f\u6d77\u4e2d\u7684\u72c2\u6fe4\uff0c\u7ffb\u52d5\u8457\u81ea\u5df1\u53ef\u6065\u7684\u6ce1\u6cab\uff0c\u662f\u6d41\u8569\u7684\u661f\u661f\uff0c\u6709\u5e7d\u5e7d\u9ed1\u6697\u6c38\u9060\u7559\u7d66\u4ed6\u5011\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4e9e\u7576\u7684\u7b2c\u4e03\u4ee3\u5b50\u5b6b\u4ee5\u8afe\u66fe\u7d93\u91dd\u5c0d\u9019\u4e9b\u4eba\u8aaa\u9810\u8a00\uff1a\u300c\u770b\u554a\uff01\u4e3b\u7387\u9818\u7942\u5343\u842c\u7684\u8056\u8005\u4e00\u540c\u964d\u81e8\uff0c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8981\u5be9\u5224\u6240\u6709\u7684\u4eba\uff0c\u6309\u4e0d\u8654\u4e0d\u656c\u7684\u7f6a\u4eba\u6240\u884c\u7684\u60e1\u4e8b\u548c\u4ed6\u5011\u893b\u7006\u4e0a\u5e1d\u7684\u8a71\u5b9a\u4ed6\u5011\u7684\u7f6a\u3002\u300d"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u9019\u4e9b\u4eba\u6eff\u8179\u7262\u9a37\uff0c\u6028\u5929\u5c24\u4eba\uff0c\u653e\u7e31\u81ea\u5df1\u7684\u90aa\u60c5\u79c1\u617e\u3002\u4ed6\u5011\u53e3\u51fa\u72c2\u8a00\uff0c\u70ba\u4e86\u8b00\u5229\u800c\u963f\u8adb\u5949\u627f\u4ed6\u4eba\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u7262\u8a18\u8b66\u544a - \u89aa\u611b\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u8981\u8b39\u8a18\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u4f7f\u5f92\u5f9e\u524d\u7d66\u4f60\u5011\u7684\u8b66\u544a\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u4ed6\u5011\u66fe\u5c0d\u4f60\u5011\u8aaa\uff1a\u300c\u672b\u4e16\u7684\u6642\u5019\uff0c\u5fc5\u5b9a\u6709\u4e0d\u656c\u8654\u3001\u597d\u8b4f\u7b11\u7684\u4eba\u653e\u7e31\u81ea\u5df1\u7684\u90aa\u60c5\u79c1\u617e\u3002\u300d"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u9019\u4e9b\u4eba\u88fd\u9020\u5206\u88c2\uff0c\u8840\u6c23\u7528\u4e8b\uff0c\u5fc3\u4e2d\u6c92\u6709\u8056\u9748\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u89aa\u611b\u7684\u5f1f\u5144\u59ca\u59b9\uff0c\u4f60\u5011\u8981\u5728\u81f3\u8056\u7684\u771f\u9053\u4e0a\u9020\u5c31\u81ea\u5df1\uff0c\u5728\u8056\u9748\u7684\u5f15\u5c0e\u4e0b\u79b1\u544a\uff0c"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u5e38\u5728\u4e0a\u5e1d\u7684\u611b\u4e2d\uff0c\u7b49\u5019\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u65bd\u6190\u61ab\u8cdc\u7d66\u4f60\u5011\u6c38\u751f\u3002"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u90a3\u4e9b\u5fc3\u5b58\u7591\u60d1\u7684\u4eba\uff0c\u4f60\u5011\u8981\u6190\u61ab\u4ed6\u5011\uff1b"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u6709\u4e9b\u4eba\uff0c\u4f60\u5011\u8981\u5c07\u4ed6\u5011\u5f9e\u706b\u4e2d\u6436\u6551\u51fa\u4f86\uff1b\u9084\u6709\u4e9b\u4eba\uff0c\u4f60\u5011\u8981\u61f7\u8457\u754f\u61fc\u7684\u5fc3\u6190\u61ab\u4ed6\u5011\uff0c\u751a\u81f3\u8981\u53ad\u60e1\u88ab\u4ed6\u5011\u7684\u90aa\u60c5\u79c1\u617e\u73b7\u6c61\u7684\u8863\u670d\u3002"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u795d\u980c - \u9858\u69ae\u8000\u6b78\u7d66\u6211\u5011\u7684\u6551\u4e3b\u2014\u2014\u7368\u4e00\u7684\u4e0a\u5e1d\uff01\u7942\u80fd\u4fdd\u5b88\u4f60\u5011\u4e0d\u5931\u8db3\u72af\u7f6a\uff0c\u4f7f\u4f60\u5011\u7121\u7455\u7121\u75b5\u3001\u6b61\u6b61\u559c\u559c\u5730\u7ad9\u5728\u7942\u7684\u69ae\u8000\u9762\u524d\u3002"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u9858\u69ae\u8000\u3001\u5a01\u56b4\u3001\u80fd\u529b\u548c\u6b0a\u67c4\u85c9\u8457\u6211\u5011\u7684\u4e3b\u8036\u7a4c\u57fa\u7763\u90fd\u6b78\u7d66\u7942\uff0c\u5f9e\u842c\u4e16\u4ee5\u524d\u76f4\u5230\u73fe\u4eca\uff0c\u4e00\u76f4\u5230\u6c38\u6c38\u9060\u9060\u3002\u963f\u5011\uff01"
                }
            }
        }
    }
}