                "10": {
                    "verse_nr": "10",
                    "verse": "\u4ee5\u8840\u8165\u5efa\u7acb\u932b\u5b89\uff0c \u4ee5\u7f6a\u60e1\u5efa\u9020\u8036\u8def\u6492\u51b7\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u57ce\u88e1\u7684\u9996\u9818\u70ba\u8caa\u8d13\u800c\u6789\u6cd5\uff0c \u796d\u53f8\u70ba\u9280\u5b50\u800c\u65bd\u6559\uff0c \u5148\u77e5\u70ba\u9322\u8ca1\u800c\u5360\u535c\u3002 \u4ed6\u5011\u537b\u5ba3\u7a31\u501a\u9760\u8036\u548c\u83ef\uff0c\u8aaa\uff1a \u300c\u8036\u548c\u83ef\u4e0d\u662f\u5728\u6211\u5011\u7576\u4e2d\u55ce\uff1f \u707d\u798d\u4e0d\u6703\u81e8\u5230\u6211\u5011\u3002\u300d"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6240\u4ee5\uff0c\u56e0\u4f60\u5011\u7684\u7de3\u6545\uff0c \u932b\u5b89\u5fc5\u50cf\u7530\u5730\u88ab\u8015\u7281\uff0c \u8036\u8def\u6492\u51b7\u5fc5\u6dea\u70ba\u5ee2\u589f\uff0c \u8056\u6bbf\u5c71\u5fc5\u6210\u70ba\u8352\u6797\u4e4b\u5ca1\u3002"
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8036\u8def\u6492\u51b7\u7684\u5c07\u4f86 - \u5728\u672b\u5f8c\u7684\u65e5\u5b50\uff0c \u8036\u548c\u83ef\u6bbf\u6240\u5728\u7684\u5c71\u5fc5\u8d85\u4e4e\u7fa4\u5c71\uff0c \u9ad8\u904e\u773e\u5dba\uff0c \u842c\u6c11\u90fd\u8981\u6b78\u5411\u5b83\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u591a\u570b\u7684\u6c11\u90fd\u8981\u524d\u4f86\uff0c\u8aaa\uff1a \u300c\u4f86\u5427\uff0c\u8b93\u6211\u5011\u767b\u8036\u548c\u83ef\u7684\u5c71\uff0c \u53bb\u96c5\u5404\u4e4b\u4e0a\u5e1d\u7684\u6bbf\u3002 \u7942\u5fc5\u6307\u793a\u6211\u5011\u7576\u884c\u7684\u8def\uff0c \u6211\u5011\u4e5f\u8981\u9075\u884c\u7942\u7684\u9053\u3002\u300d \u56e0\u70ba\u8a13\u8aa8\u5fc5\u51fa\u65bc\u932b\u5b89\uff0c \u8036\u548c\u83ef\u7684\u8a71\u8a9e\u5fc5\u4f86\u81ea\u8036\u8def\u6492\u51b7\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u7942\u8981\u5728\u5404\u6c11\u65cf\u4e2d\u65bd\u884c\u5be9\u5224\uff0c \u70ba\u9060\u65b9\u7684\u5f37\u570b\u65b7\u5b9a\u662f\u975e\u3002 \u4ed6\u5011\u8981\u628a\u5200\u528d\u9444\u6210\u7281\u982d\uff0c \u628a\u77db\u69cd\u6253\u6210\u942e\u5200\u3002 \u570b\u8207\u570b\u4e4b\u9593\u4e0d\u518d\u52d5\u5200\u69cd\uff0c \u4e5f\u4e0d\u518d\u5b78\u7fd2\u6230\u722d\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4eba\u4eba\u90fd\u5b89\u7136\u5750\u5728\u81ea\u5df1\u7684\u8461\u8404\u6a39\u548c\u7121\u82b1\u679c\u6a39\u4e0b\uff0c \u4e0d\u518d\u64d4\u9a5a\u53d7\u6015\u3002 \u9019\u662f\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u89aa\u53e3\u8aaa\u7684\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u842c\u6c11\u90fd\u9075\u5f9e\u81ea\u5df1\u7684\u795e\u660e\uff0c \u4f46\u6211\u5011\u8981\u6c38\u9060\u9075\u5f9e\u6211\u5011\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u5230\u90a3\u65e5\uff0c\u6211\u8981\u805a\u96c6\u7638\u817f\u7684\uff0c \u62db\u805a\u88ab\u64c4\u7684\u548c\u88ab\u6211\u61f2\u7f70\u7684\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u6211\u8981\u4f7f\u7638\u817f\u7684\u6210\u70ba\u9918\u6c11\uff0c \u8b93\u88ab\u64c4\u7684\u6210\u70ba\u5f37\u570b\u3002 \u8036\u548c\u83ef\u8981\u5728\u932b\u5b89\u5c71\u505a\u738b\u6cbb\u7406\u4ed6\u5011\uff0c \u5f9e\u90a3\u6642\u76f4\u5230\u6c38\u9060\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u300c\u4f60\u9019\u7f8a\u7fa4\u7684\u77ad\u671b\u5854\uff0c \u932b\u5b89\u57ce\u7684\u5805\u58d8\u554a\uff01 \u5f9e\u524d\u7684\u570b\u6b0a\u2014\u2014 \u8036\u8def\u6492\u51b7\u57ce\u7684\u738b\u4f4d\u5fc5\u91cd\u65b0\u6b78\u4f60\u3002\u300d"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u73fe\u5728\u4f60\u70ba\u4f55\u5927\u8072\u54ed\u558a\uff1f \u662f\u56e0\u70ba\u4f60\u6c92\u6709\u738b\u55ce\uff1f \u96e3\u9053\u4f60\u7684\u8b00\u58eb\u90fd\u6b7b\u4e86\u55ce\uff0c \u4ee5\u81f4\u4f60\u50cf\u5206\u5a29\u7684\u5a66\u4eba\u4e00\u6a23\u75db\u82e6\u4e0d\u582a\uff1f"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u932b\u5b89\u7684\u767e\u59d3\u554a\uff0c \u4f60\u5011\u8981\u50cf\u5206\u5a29\u7684\u5a66\u4eba\u4e00\u6a23\u8f3e\u8f49\u547b\u541f\uff0c \u56e0\u70ba\u4f60\u5011\u73fe\u5728\u8981\u96e2\u958b\u9019\u57ce\uff0c \u4f4f\u5728\u8352\u91ce\uff0c\u88ab\u64c4\u5230\u5df4\u6bd4\u502b\u3002 \u5728\u90a3\u88e1\uff0c\u4f60\u5011\u5c07\u5f97\u5230\u89e3\u6551\uff1b \u5728\u90a3\u88e1\uff0c\u8036\u548c\u83ef\u5fc5\u5f9e\u6575\u4eba\u624b\u4e2d\u6551\u8d16\u4f60\u5011\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u73fe\u5728\u8a31\u591a\u570b\u5bb6\u4e00\u8d77\u4f86\u653b\u64ca\u4f60\u5011\uff0c\u8aaa\uff1a \u300c\u9858\u932b\u5b89\u906d\u8e42\u8eaa\uff0c \u6211\u5011\u8981\u770b\u8457\u932b\u5b89\u88ab\u6bc0\u6ec5\u3002\u300d"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4ed6\u5011\u537b\u4e0d\u77e5\u9053\u8036\u548c\u83ef\u7684\u5fc3\u610f\uff0c \u4e5f\u4e0d\u660e\u767d\u7942\u7684\u8a08\u5283\u3002 \u7942\u805a\u96c6\u4ed6\u5011\u662f\u8981\u61f2\u7f70\u4ed6\u5011\uff0c \u50cf\u628a\u79be\u6346\u5806\u7a4d\u5728\u9ea5\u5834\u4e00\u6a23\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u932b\u5b89\u57ce\u554a\uff0c \u8d77\u4f86\u6253\u7a40\u5427\uff01 \u6211\u8981\u7d66\u4f60\u9435\u89d2\uff0c \u6211\u8981\u7d66\u4f60\u9285\u8e44\u3002 \u4f60\u8981\u6253\u788e\u5217\u570b\uff0c \u5c07\u4ed6\u5011\u7684\u4e0d\u7fa9\u4e4b\u8ca1\u737b\u7d66\u8036\u548c\u83ef\uff0c \u628a\u4ed6\u5011\u7684\u8ca1\u5bcc\u737b\u7d66\u666e\u5929\u4e0b\u7684\u4e3b\u3002\u300d"
                }
            }
        },
        "5": {
            "chapter_nr": "5",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8036\u548c\u83ef\u7684\u61c9\u8a31 - \u8036\u8def\u6492\u51b7\u7684\u5c45\u6c11\u554a\uff0c \u73fe\u5728\u4f60\u5011\u8981\u8abf\u96c6\u8ecd\u968a\uff01 \u6575\u4eba\u6b63\u56db\u9762\u570d\u653b\u6211\u5011\uff0c \u8981\u7528\u6756\u64ca\u6253\u4ee5\u8272\u5217\u9996\u9818\u7684\u81c9\u9830\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u4ee5\u6cd5\u4ed6\u7684\u4f2f\u5229\u6046\u554a\uff0c \u4f60\u5728\u7336\u5927\u5404\u57ce\u4e2d\u6beb\u4e0d\u8d77\u773c\uff0c \u4f46\u5c07\u6709\u4e00\u4f4d\u5f9e\u4f60\u90a3\u88e1\u51fa\u4f86\uff0c \u70ba\u6211\u7d71\u6cbb\u4ee5\u8272\u5217\uff1b \u7942\u7684\u6839\u6e90\u81ea\u4e99\u53e4\uff0c\u4f86\u81ea\u592a\u521d\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8036\u548c\u83ef\u8981\u5c07\u4ee5\u8272\u5217\u4eba\u4ea4\u7d66\u4ed6\u5011\u7684\u6575\u4eba\uff0c \u76f4\u5230\u90a3\u81e8\u76c6\u7684\u5a66\u4eba\u751f\u4e0b\u5152\u5b50\u3002 \u90a3\u6642\uff0c\u7942\u6d41\u4ea1\u7684\u5f1f\u5144\u5c07\u91cd\u8fd4\u4ed6\u5011\u7684\u4ee5\u8272\u5217\u540c\u80de\u90a3\u88e1\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7942\u8981\u633a\u8eab\u800c\u8d77\uff0c \u501a\u9760\u8036\u548c\u83ef\u7684\u80fd\u529b\uff0c \u5949\u7942\u4e0a\u5e1d\u8036\u548c\u83ef\u7684\u5a01\u540d\u7267\u990a\u7942\u7684\u7fa4\u7f8a\u3002 \u4ed6\u5011\u5c07\u5b89\u7136\u5c45\u4f4f\uff0c \u56e0\u70ba\u90a3\u6642\u7942\u5fc5\u53d7\u5c0a\u5d07\uff0c\u76f4\u9054\u5730\u6975\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u7942\u5fc5\u7d66\u4ed6\u5011\u5e36\u4f86\u5e73\u5b89\uff01 \u4e9e\u8ff0\u4eba\u4fb5\u7565\u6211\u5011\u7684\u570b\u571f\u3001\u8e10\u8e0f\u6211\u5011\u7684\u5bae\u6bbf\u6642\uff0c \u6211\u5011\u5c07\u9078\u7acb\u4e03\u4f4d\u7267\u8005\u548c\u516b\u4f4d\u9996\u9818\u6297\u64ca\u4ed6\u5011\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4ed6\u5011\u8981\u7528\u5200\u528d\u7d71\u6cbb\u4e9e\u8ff0\uff0c\u7d71\u6cbb\u5be7\u9304\u5730\u5340\u3002 \u4e9e\u8ff0\u4eba\u4fb5\u5165\u6211\u5011\u570b\u5883\u3001\u8e10\u8e0f\u6211\u5011\u7586\u571f\u6642\uff0c \u7942\u5fc5\u62ef\u6551\u6211\u5011\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u96c5\u5404\u9918\u5269\u7684\u5b50\u5b6b\u5c07\u5728\u842c\u6c11\u4e2d\u50cf\u5f9e\u8036\u548c\u83ef\u90a3\u88e1\u964d\u4e0b\u7684\u96e8\u9732\uff0c \u53c8\u50cf\u7051\u5728\u8349\u4e0a\u7684\u7518\u9716\u3002 \u4ed6\u5011\u4e0d\u4f9d\u9760\u4eba\uff0c \u4e0d\u5180\u671b\u65bc\u4e16\u4eba\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u5728\u5404\u570b\u5404\u6c11\u4e2d\uff0c \u96c5\u5404\u9918\u5269\u7684\u5b50\u5b6b\u7336\u5982\u6797\u9593\u767e\u7378\u4e2d\u7684\u7345\u5b50\uff0c \u53c8\u50cf\u95d6\u5165\u7f8a\u7fa4\u7684\u731b\u7345\uff0c \u5c07\u7375\u7269\u64b2\u5012\u6495\u788e\uff0c \u7121\u4eba\u80fd\u642d\u6551\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u9858\u4f60\u5011\u4f38\u624b\u6230\u52dd\u4ec7\u6575\uff01 \u9858\u4f60\u5011\u7684\u4ec7\u6575\u90fd\u88ab\u5277\u9664\uff01"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u5230\u90a3\u65e5\uff0c \u6211\u8981\u6d88\u6ec5\u4f60\u5011\u7684\u6230\u99ac\uff0c \u6bc0\u58de\u4f60\u5011\u7684\u6230\u8eca\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u8981\u6467\u6bc0\u4f60\u5011\u5883\u5167\u7684\u57ce\u9091\uff0c \u62c6\u9664\u4f60\u5011\u6240\u6709\u7684\u5821\u58d8\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u6211\u8981\u9664\u6389\u4f60\u5011\u624b\u4e2d\u7684\u5deb\u8853\uff0c \u4f7f\u4f60\u5011\u90a3\u88e1\u4e0d\u518d\u6709\u5360\u535c\u7684\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u6211\u8981\u5f9e\u4f60\u5011\u4e2d\u9593\u9664\u53bb\u96d5\u523b\u7684\u795e\u50cf\u548c\u795e\u67f1\uff0c \u4f7f\u4f60\u5011\u4e0d\u518d\u8dea\u62dc\u81ea\u5df1\u6240\u9020\u7684\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u6211\u8981\u5f9e\u4f60\u5011\u4e2d\u9593\u5277\u9664\u4e9e\u820d\u62c9\u795e\u50cf\uff0c \u6bc0\u6ec5\u4f60\u5011\u7684\u5076\u50cf\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u6211\u8981\u5728\u70c8\u6012\u4e2d\u5831\u61c9\u90a3\u4e9b\u4e0d\u807d\u5f9e\u6211\u7684\u570b\u5bb6\u3002\u300d"
                }
            }
        },
        "6": {
            "chapter_nr": "6",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8036\u548c\u83ef\u8b74\u8cac\u4ee5\u8272\u5217 - \u4ee5\u8272\u5217\u4eba\u554a\uff0c \u4f60\u5011\u8981\u807d\u8036\u548c\u83ef\u7684\u8a71\uff1a \u300c\u8d77\u4f86\uff0c\u4f60\u5011\u8981\u5728\u9ad8\u5c71\u9762\u524d\u7533\u8faf\uff0c \u8b93\u4e18\u9675\u807d\u4f60\u5011\u8a34\u8aaa\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u7fa4\u5c71\u554a\uff0c\u5927\u5730\u6c38\u56fa\u7684\u6839\u57fa\u554a\uff0c \u4f60\u5011\u8981\u807d\u8036\u548c\u83ef\u7684\u63a7\u544a\uff01 \u56e0\u70ba\u8036\u548c\u83ef\u8981\u63a7\u544a\u7942\u7684\u5b50\u6c11\uff0c \u8cac\u5099\u4ee5\u8272\u5217\u4eba\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u300c\u6211\u7684\u5b50\u6c11\u554a\uff0c \u6211\u505a\u4e86\u4ec0\u9ebc\u5c0d\u4e0d\u8d77\u4f60\u5011\u7684\u4e8b\uff1f \u6211\u54ea\u88e1\u4ee4\u4f60\u5011\u53ad\u7169\uff1f \u56de\u7b54\u6211\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6211\u66fe\u5e36\u9818\u4f60\u5011\u96e2\u958b\u57c3\u53ca\uff0c \u628a\u4f60\u5011\u5f9e\u53d7\u5974\u5f79\u4e4b\u5730\u6551\u8d16\u51fa\u4f86\uff0c \u53c8\u5dee\u9063\u6469\u897f\u3001\u4e9e\u502b\u548c\u7c73\u5229\u6697\u5f15\u9818\u4f60\u5011\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u6211\u7684\u5b50\u6c11\u554a\uff0c \u4f60\u5011\u8981\u56de\u60f3\u6469\u62bc\u738b\u5df4\u52d2\u7684\u9670\u8b00\u4ee5\u53ca\u6bd4\u73e5\u4e4b\u5b50\u5df4\u862d\u7d66\u4ed6\u7684\u56de\u7b54\uff0c \u8981\u56de\u60f3\u4f60\u5011\u5f9e\u4ec0\u4ead\u5230\u5409\u7532\u9014\u4e2d\u7684\u906d\u9047\uff0c \u4ee5\u4fbf\u8a8d\u8b58\u8036\u548c\u83ef\u516c\u7fa9\u7684\u4f5c\u70ba\u3002\u300d"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6211\u8981\u62ff\u4ec0\u9ebc\u671d\u898b\u8036\u548c\u83ef\uff0c \u5728\u5929\u4e0a\u7684\u4e0a\u5e1d\u9762\u524d\u53e9\u62dc\u5462\uff1f \u8981\u737b\u4e0a\u4e00\u6b72\u7684\u725b\u72a2\u4f5c\u71d4\u796d\u55ce\uff1f"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u8036\u548c\u83ef\u6703\u559c\u611b\u6210\u5343\u7684\u516c\u7f8a\u548c\u842c\u689d\u6cb9\u6cb3\u55ce\uff1f \u6211\u8981\u56e0\u904e\u72af\u800c\u737b\u4e0a\u81ea\u5df1\u7684\u9577\u5b50\uff0c \u56e0\u5fc3\u4e2d\u7684\u7f6a\u60e1\u800c\u737b\u4e0a\u89aa\u751f\u7684\u9aa8\u8089\u55ce\uff1f"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4e16\u4eba\u554a\uff0c \u8036\u548c\u83ef\u5df2\u7d93\u6307\u793a\u4f60\u5011\u4f55\u70ba\u826f\u5584\u3002 \u7942\u5411\u4f60\u5011\u6240\u8981\u7684\u662f\u4ec0\u9ebc\u5462\uff1f \u662f\u8981\u4f60\u5011\u884c\u516c\u7fa9\uff0c\u597d\u6190\u61ab\uff0c \u8b19\u5351\u5730\u8207\u4f60\u5011\u7684\u4e0a\u5e1d\u540c\u884c\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8036\u548c\u83ef\u5728\u5411\u8036\u8def\u6492\u51b7\u547c\u558a\uff0c \u667a\u8005\u5fc5\u656c\u754f\u7942\u7684\u540d\u3002\u7942\u8aaa\uff1a \u300c\u805a\u5728\u57ce\u88e1\u7684\u773e\u4eba\u554a\uff0c\u4f60\u5011\u8981\u807d\uff01"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6211\u8c48\u6703\u5fd8\u8a18\u60e1\u4eba\u5bb6\u4e2d\u7684\u4e0d\u7fa9\u4e4b\u8ca1\u548c\u53ef\u60e1\u7684\u5c0f\u5347\u6597\uff1f"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6211\u8c48\u6703\u653e\u904e\u4f7f\u7528\u4e0d\u516c\u9053\u7684\u79e4\u548c\u6cd5\u78bc\u7684\u4eba\uff1f"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u57ce\u4e2d\u7684\u5bcc\u4eba\u6b98\u66b4\u6210\u6027\uff0c \u5c45\u6c11\u8b0a\u8a71\u9023\u7bc7\uff0c \u6eff\u53e3\u8a6d\u8a50\u4e4b\u8a00\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u6240\u4ee5\uff0c\u6211\u8981\u91cd\u91cd\u5730\u64ca\u6253\u4f60\uff0c \u56e0\u4f60\u7684\u7f6a\u60e1\u800c\u6bc0\u6ec5\u4f60\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4f60\u5403\uff0c\u537b\u5403\u4e0d\u98fd\uff0c\u4ecd\u7136\u8179\u4e2d\u7a7a\u7a7a\u3002 \u4f60\u805a\u6582\u9322\u8ca1\uff0c\u537b\u4fdd\u4e0d\u4f4f\u3002 \u4f60\u4fdd\u4f4f\u7684\uff0c\u6211\u8981\u4ea4\u7d66\u62ff\u5200\u7684\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u4f60\u6492\u7a2e\uff0c\u537b\u4e0d\u5f97\u6536\u5272\uff1b \u4f60\u69a8\u6a44\u6b16\uff0c\u537b\u7121\u6cb9\u62b9\u8eab\uff1b \u4f60\u8e29\u8461\u8404\uff0c\u537b\u7121\u9152\u53ef\u559d\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u56e0\u70ba\u4f60\u9075\u5b88\u6697\u5229\u7684\u60e1\u898f\uff0c \u6548\u6cd5\u4e9e\u54c8\u5bb6\u7684\u60e1\u884c\uff0c \u96a8\u5f9e\u4ed6\u5011\u7684\u8a08\u8b00\uff0c \u6240\u4ee5\u6211\u8981\u4f7f\u4f60\u8352\u6dbc\uff0c \u8b93\u4f60\u7684\u5c45\u6c11\u6210\u70ba\u7b11\u67c4\u3002 \u4f60\u5011\u8981\u8499\u53d7\u5217\u570b\u7684\u7f9e\u8fb1\u3002\u300d"
                }
            }
        },
        "7": {
            "chapter_nr": "7",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4ee5\u8272\u5217\u7684\u6557\u58de - \u6211\u771f\u60b2\u6158\uff01 \u5c31\u50cf\u590f\u5929\u7684\u679c\u5b50\u5df2\u88ab\u6536\u76e1\uff0c \u8461\u8404\u5df2\u88ab\u6458\u6de8\uff0c \u627e\u4e0d\u5230\u4e00\u4e32\u53ef\u5403\u7684\uff0c \u5c0b\u4e0d\u898b\u6211\u5fc3\u4e2d\u6e34\u671b\u7684\u521d\u719f\u7121\u82b1\u679c\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u8654\u656c\u4eba\u5df2\u5f9e\u5927\u5730\u4e0a\u6ec5\u7d55\uff0c \u4e16\u9593\u518d\u6c92\u6709\u6b63\u76f4\u4eba\u3002 \u4eba\u4eba\u4f3a\u6a5f\u6bba\u4eba\u6d41\u8840\uff0c \u7528\u7db2\u7f85\u7375\u53d6\u81ea\u5df1\u7684\u5f1f\u5144\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u4ed6\u5011\u5584\u65bc\u4f5c\u60e1\u3002 \u9818\u8896\u548c\u5be9\u5224\u5b98\u7d22\u8981\u8cc4\u8cc2\uff0c \u986f\u8cb4\u8892\u9732\u5fc3\u4e2d\u7684\u60e1\u617e\uff0c\u670b\u6bd4\u70ba\u5978\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u4ed6\u5011\u7576\u4e2d\u6700\u597d\u7684\u4e0d\u904e\u662f\u84ba\u85dc\uff0c \u6700\u6b63\u76f4\u7684\u4e5f\u4e0d\u904e\u662f\u834a\u68d8\u7c6c\u7b06\u3002 \u4f60\u5011\u7684\u5b88\u671b\u8005\u6240\u8aaa\u7684\u65e5\u5b50\uff0c \u5c31\u662f\u4f60\u5011\u53d7\u61f2\u7f70\u7684\u65e5\u5b50\u5df2\u7d93\u4f86\u5230\u3002 \u4f60\u5011\u5fc5\u9a5a\u614c\u5931\u63aa\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u4e0d\u8981\u76f8\u4fe1\u9130\u5c45\uff0c \u4e0d\u8981\u4fe1\u8cf4\u670b\u53cb\uff0c \u5373\u4f7f\u5c0d\u4f60\u61f7\u4e2d\u7684\u59bb\u5b50\u4e5f\u8981\u5b88\u53e3\u5982\u74f6\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u56e0\u70ba\u5152\u5b50\u85d0\u8996\u7236\u89aa\uff0c \u5973\u5152\u8207\u6bcd\u89aa\u4f5c\u5c0d\uff0c \u5ab3\u5a66\u8207\u5a46\u5a46\u4f5c\u5c0d\uff1b \u5bb6\u4eba\u4e4b\u9593\u53cd\u76ee\u6210\u4ec7\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u81f3\u65bc\u6211\uff0c \u6211\u8981\u4ef0\u671b\u8036\u548c\u83ef\uff0c \u7b49\u5019\u62ef\u6551\u6211\u7684\u4e0a\u5e1d\uff1b \u6211\u7684\u4e0a\u5e1d\u5fc5\u5782\u807d\u6211\u7684\u7948\u6c42\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u7684\u4ec7\u6575\u554a\uff0c \u4f60\u5011\u4e0d\u8981\u5c0d\u6211\u5e78\u707d\u6a02\u798d\uff01 \u6211\u96d6\u8dcc\u5012\uff0c\u4f46\u5fc5\u518d\u722c\u8d77\u4f86\uff1b \u6211\u96d6\u5750\u5728\u9ed1\u6697\u88e1\uff0c \u4f46\u8036\u548c\u83ef\u5fc5\u505a\u6211\u7684\u5149\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u6211\u5f97\u7f6a\u4e86\u8036\u548c\u83ef\uff0c \u6240\u4ee5\u8981\u5fcd\u53d7\u7942\u7684\u61f2\u7f70\uff0c \u76f4\u5230\u7942\u70ba\u6211\u8faf\u8b77\uff0c\u66ff\u6211\u4f38\u51a4\u3002 \u7942\u5fc5\u9818\u6211\u9032\u5165\u5149\u660e\uff0c \u6211\u5fc5\u89aa\u7779\u7942\u7684\u516c\u7fa9\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u5f9e\u524d\u5632\u7b11\u6211\u8aaa\u300c\u4f60\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\u5728\u54ea\u88e1\u300d\u7684\u4ec7\u6575\u770b\u898b\u9019\u4e8b\uff0c \u90fd\u5fc5\u6eff\u9762\u7f9e\u6127\u3002 \u6211\u5fc5\u89aa\u773c\u770b\u898b\u4ed6\u5011\u906d\u5230\u5831\u61c9\uff0c \u4ed6\u5011\u8981\u50cf\u8857\u4e0a\u7684\u6ce5\u571f\u4e00\u6a23\u88ab\u4eba\u8e10\u8e0f\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u4ee5\u8272\u5217\u554a\uff0c \u7d42\u6709\u4e00\u65e5\u4f60\u6703\u91cd\u5efa\u81ea\u5df1\u7684\u57ce\u7246\u3002 \u5230\u90a3\u65e5\uff0c\u4f60\u5fc5\u64f4\u5f35\u81ea\u5df1\u7684\u7586\u57df\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u90a3\u65e5\uff0c\u5f9e\u4e9e\u8ff0\u5230\u57c3\u53ca\u7684\u5404\u57ce\uff0c \u5f9e\u57c3\u53ca\u5230\u5e7c\u767c\u62c9\u5e95\u6cb3\uff0c \u5f9e\u5929\u6daf\u5230\u6d77\u89d2\uff0c \u4eba\u5011\u90fd\u8981\u5230\u4f60\u9019\u88e1\u4f86\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u7136\u800c\uff0c\u5927\u5730\u8981\u56e0\u5730\u4e0a\u5c45\u6c11\u7684\u7f6a\u60e1\u800c\u8352\u6dbc\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8036\u548c\u83ef\u554a\uff0c \u6c42\u4f60\u7528\u4f60\u7684\u6756\u7267\u653e\u5c6c\u65bc\u4f60\u7684\u7f8a\u7fa4\uff0c \u5c31\u662f\u4f60\u7684\u5b50\u6c11\u3002 \u4ed6\u5011\u7368\u81ea\u5c45\u4f4f\u5728\u53e2\u6797\u4e2d\uff0c \u4f4f\u5728\u80a5\u6c83\u7684\u7530\u5712\u9593\u3002 \u9858\u4ed6\u5011\u50cf\u5f9e\u524d\u4e00\u6a23\u5728\u5df4\u73ca\u548c\u57fa\u5217\u5403\u8349\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a \u300c\u6211\u8981\u5411\u4ed6\u5011\u986f\u51fa\u795e\u8e5f\u5947\u4e8b\uff0c \u5c31\u50cf\u4ed6\u5011\u96e2\u958b\u57c3\u53ca\u6642\u4e00\u6a23\u3002\u300d"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u5217\u570b\u770b\u898b\u9019\u4e00\u5207\uff0c \u90fd\u5fc5\u56e0\u81ea\u5df1\u52e2\u529b\u8870\u5fae\u800c\u7f9e\u6127\uff0c \u90fd\u5fc5\u7528\u624b\u6342\u53e3\uff0c\u8033\u6735\u8b8a\u807e\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u4ed6\u5011\u8981\u50cf\u86c7\u4e00\u6a23\u8214\u571f\uff0c \u53c8\u5982\u5730\u4e0a\u7684\u722c\u87f2\uff0c \u6230\u6230\u5162\u5162\u5730\u8d70\u51fa\u71df\u5be8\uff1b \u4ed6\u5011\u5fc5\u754f\u61fc\u6211\u5011\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\uff0c \u4ed6\u5011\u5fc5\u5c0d\u4f60\u5145\u6eff\u6050\u61fc\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u6709\u4f55\u795e\u660e\u50cf\u4f60\u4e00\u6a23\u8d66\u514d\u7f6a\u60e1\uff0c \u9952\u6055\u4f60\u5016\u5b58\u5b50\u6c11\u7684\u904e\u72af\uff1f \u4f60\u4e0d\u6703\u6c38\u9060\u767c\u6012\uff0c \u56e0\u70ba\u4f60\u6a02\u65bc\u65bd\u6069\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u4f60\u5fc5\u518d\u6190\u61ab\u6211\u5011\uff0c \u5c07\u6211\u5011\u7684\u7f6a\u60e1\u8e10\u8e0f\u5728\u8173\u4e0b\uff0c \u628a\u6211\u5011\u7684\u4e00\u5207\u904e\u72af\u62cb\u9032\u6df1\u6d77\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u4f60\u5fc5\u6309\u53e4\u6642\u7d66\u6211\u5011\u5217\u7956\u7684\u8a93\u8a00\uff0c \u4ee5\u4fe1\u5be6\u5f85\u96c5\u5404\uff0c \u4ee5\u6148\u611b\u5f85\u4e9e\u4f2f\u62c9\u7f55\u3002"
                }
            }
        }
    }
}