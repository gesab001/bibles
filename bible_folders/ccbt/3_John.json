    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u9019\u4f5c\u9577\u8001\u7684\u5beb\u4fe1\u7d66\u89aa\u611b\u7684\u8a72\u7336\uff0c\u5c31\u662f\u6211\u672c\u8457\u771f\u7406\u6240\u611b\u7684\u4eba\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u89aa\u611b\u7684\u5f1f\u5144\uff0c\u8877\u5fc3\u5730\u795d\u4f60\u51e1\u4e8b\u9806\u5229\uff0c\u8eab\u9ad4\u548c\u9748\u9b42\u4e00\u6a23\u5065\u5eb7\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8a72\u7336\u7684\u597d\u54c1\u884c - \u6709\u5f1f\u5144\u5011\u4f86\u70ba\u4f60\u7684\u884c\u70ba\u505a\u898b\u8b49\uff0c\u8aaa\u4f60\u5fe0\u5fc3\u5730\u9075\u884c\u771f\u7406\uff0c\u9019\u4f7f\u6211\u975e\u5e38\u6b23\u6170\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u6c92\u6709\u6bd4\u807d\u898b\u81ea\u5df1\u7684\u5b69\u5b50\u9075\u884c\u771f\u7406\u66f4\u4f7f\u6211\u6b61\u559c\u4e86\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u89aa\u611b\u7684\u5f1f\u5144\uff0c\u4f60\u5e38\u5e38\u5fe0\u5fc3\u5730\u7167\u9867\u7d20\u4e0d\u76f8\u8b58\u7684\u5f1f\u5144\uff0c"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u4ed6\u5011\u628a\u4f60\u7684\u611b\u5fc3\u544a\u8a34\u4e86\u6559\u6703\u3002\u9858\u4f60\u7167\u8457\u4e0a\u5e1d\u6240\u559c\u6085\u7684\u65b9\u5f0f\u7e7c\u7e8c\u5e6b\u52a9\u4ed6\u5011\uff0c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u56e0\u70ba\u4ed6\u5011\u70ba\u4e86\u4e3b\u8036\u7a4c\u7684\u540d\u56db\u8655\u50b3\u9053\uff0c\u4e0d\u6536\u53d6\u975e\u4fe1\u5f92\u7684\u4efb\u4f55\u6771\u897f\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u5011\u61c9\u8a72\u63a5\u5f85\u9019\u6a23\u7684\u4eba\uff0c\u597d\u8207\u4ed6\u5011\u4e00\u540c\u50b3\u63da\u771f\u7406\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u4e1f\u7279\u8153\u7684\u60e1\u884c - \u6211\u66fe\u5beb\u4fe1\u7d66\u4f60\u90a3\u88e1\u7684\u6559\u6703\uff0c\u4f46\u559c\u6b61\u505a\u9818\u8896\u7684\u4e1f\u7279\u8153\u4e0d\u7406\u6703\u6211\u5011\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u56e0\u6b64\uff0c\u6211\u82e5\u53bb\u4f60\u5011\u90a3\u88e1\uff0c\u6703\u63d0\u53ca\u4ed6\u7684\u6240\u4f5c\u6240\u70ba\uff0c\u5c31\u662f\u4ed6\u600e\u6a23\u60e1\u8a00\u4e2d\u50b7\u6211\u5011\u3002\u4e0d\u50c5\u5982\u6b64\uff0c\u4ed6\u9084\u62d2\u7d55\u63a5\u5f85\u5f1f\u5144\uff0c\u4e26\u4e14\u7981\u6b62\u5225\u4eba\u63a5\u5f85\uff0c\u751a\u81f3\u5c07\u63a5\u5f85\u7684\u4eba\u8d95\u51fa\u6559\u6703\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u6291\u60e1\u63da\u5584 - \u89aa\u611b\u7684\u5f1f\u5144\uff0c\u4e0d\u8981\u6548\u6cd5\u7f6a\u60e1\uff0c\u8981\u6548\u6cd5\u7f8e\u5584\u3002\u884c\u5584\u7684\u4eba\u5c6c\u65bc\u4e0a\u5e1d\uff0c\u884c\u60e1\u7684\u4eba\u4e0d\u8a8d\u8b58\u4e0a\u5e1d\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u4f4e\u7c73\u4e1f\u5f97\u5230\u4e86\u773e\u4eba\u7684\u597d\u8a55\uff0c\u771f\u7406\u672c\u8eab\u4e5f\u70ba\u4ed6\u505a\u898b\u8b49\uff0c\u6211\u5011\u4e5f\u70ba\u4ed6\u505a\u898b\u8b49\u3002\u4f60\u5011\u77e5\u9053\u6211\u5011\u7684\u898b\u8b49\u662f\u771f\u5be6\u7684\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u7d50\u8a9e - \u6211\u672c\u4f86\u6709\u5f88\u591a\u4e8b\u60f3\u8ddf\u4f60\u8ac7\uff0c\u4f46\u4e0d\u60f3\u7528\u7b46\u58a8\u5beb\u7d66\u4f60\uff0c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u53ea\u5e0c\u671b\u53ef\u4ee5\u5feb\u5feb\u5730\u898b\u5230\u4f60\uff0c\u8ddf\u4f60\u7576\u9762\u66a2\u8ac7\u3002"
                }
            }
        }
    }
}