    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u6211\u9019\u4f5c\u9577\u8001\u7684\u5beb\u4fe1\u7d66\u8499\u63c0\u9078\u7684\u5973\u58eb\u548c\u4f60\u7684\u5152\u5973\uff0c\u5c31\u662f\u6211\u672c\u8457\u771f\u7406\u6240\u611b\u7684\u3002\u4f60\u5011\u4e0d\u55ae\u662f\u6211\u6240\u611b\u7684\uff0c\u4e5f\u662f\u4e00\u5207\u8a8d\u8b58\u771f\u7406\u7684\u4eba\u6240\u611b\u7684\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u6211\u5011\u611b\u4f60\u5011\uff0c\u56e0\u70ba\u771f\u7406\u5728\u6211\u5011\u88e1\u9762\uff0c\u4e26\u4e14\u6c38\u9060\u8207\u6211\u5011\u540c\u5728\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u5f9e\u7236\u4e0a\u5e1d\u548c\u7942\u5152\u5b50\u8036\u7a4c\u57fa\u7763\u800c\u4f86\u7684\u6069\u5178\u3001\u6190\u61ab\u548c\u5e73\u5b89\u4e5f\u5fc5\u5728\u771f\u7406\u548c\u611b\u4e2d\u8207\u6211\u5011\u540c\u5728\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u5f7c\u6b64\u76f8\u611b - \u4f60\u7684\u5152\u5973\u7576\u4e2d\u6709\u4eba\u6309\u7167\u6211\u5011\u5f9e\u7236\u9818\u53d7\u7684\u547d\u4ee4\u9075\u884c\u771f\u7406\uff0c\u6211\u77e5\u9053\u5f8c\u975e\u5e38\u6b23\u6170\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5973\u58eb\u554a\uff0c\u6211\u73fe\u5728\u52f8\u4f60\uff0c\u6211\u5011\u5927\u5bb6\u8981\u5f7c\u6b64\u76f8\u611b\u3002\u9019\u4e0d\u662f\u65b0\u547d\u4ee4\uff0c\u800c\u662f\u6211\u5011\u5f9e\u8d77\u521d\u5c31\u5df2\u7d93\u9818\u53d7\u7684\u547d\u4ee4\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u611b\u5c31\u662f\u9075\u884c\u4e0a\u5e1d\u7684\u547d\u4ee4\uff0c\u9019\u662f\u4f60\u5011\u5f9e\u8d77\u521d\u5c31\u807d\u904e\u53c8\u7576\u9075\u884c\u7684\u547d\u4ee4\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u63d0\u9632\u9a19\u5b50 - \u56e0\u70ba\u73fe\u5728\u4e16\u4e0a\u6709\u8a31\u591a\u9a19\u5b50\u51fa\u4f86\u8ff7\u60d1\u4eba\uff0c\u4ed6\u5011\u5426\u8a8d\u8036\u7a4c\u57fa\u7763\u66fe\u964d\u4e16\u70ba\u4eba\u3002\u9019\u6a23\u7684\u4eba\u662f\u9a19\u5b50\uff0c\u662f\u6575\u57fa\u7763\u8005\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u4f60\u5011\u8981\u8b39\u614e\uff0c\u597d\u5f97\u5230\u5b8c\u6eff\u7684\u8cde\u8cdc\uff0c\u514d\u5f97\u6211\u5011\u524d\u529f\u76e1\u68c4\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8ab0\u504f\u96e2\u57fa\u7763\u7684\u6559\u5c0e\uff0c\u8ab0\u5fc3\u4e2d\u5c31\u6c92\u6709\u4e0a\u5e1d\uff1b\u8ab0\u9075\u5b88\u57fa\u7763\u7684\u6559\u5c0e\uff0c\u8ab0\u5c31\u6709\u7236\u548c\u5b50\u8207\u4ed6\u540c\u5728\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u82e5\u6709\u4eba\u5230\u4f60\u5011\u90a3\u88e1\u4e0d\u50b3\u57fa\u7763\u7684\u6559\u5c0e\uff0c\u4e0d\u8981\u8acb\u4ed6\u5230\u5bb6\u88e1\uff0c\u4e5f\u4e0d\u8981\u554f\u5019\u4ed6\uff0c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u56e0\u70ba\u554f\u5019\u4ed6\u5c31\u662f\u8207\u4ed6\u540c\u6d41\u5408\u6c61\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u7d50\u8a9e - \u6211\u9084\u6709\u8a31\u591a\u4e8b\u8981\u544a\u8a34\u4f60\u5011\uff0c\u4f46\u4e0d\u60f3\u85c9\u52a9\u7d19\u58a8\uff0c\u800c\u662f\u5e0c\u671b\u6709\u6a5f\u6703\u5230\u4f60\u5011\u90a3\u88e1\u8207\u4f60\u5011\u7576\u9762\u66a2\u8ac7\uff0c\u4f7f\u6211\u5011\u5145\u6eff\u559c\u6a02\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u90a3\u8499\u63c0\u9078\u4e4b\u59ca\u59b9\u7684\u5152\u5973\u554f\u5019\u4f60\u3002"
                }
            }
        }
    }
}