    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u4ee5\u4e0b\u662f\u8036\u548c\u83ef\u5c0d\u6bd7\u571f\u73e5\u7684\u5152\u5b50\u7d04\u73e5\u6240\u8aaa\u7684\u8a71\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u56b4\u91cd\u7684\u8757\u707d - \u9577\u8001\u5011\u554a\uff0c\u8acb\u807d\u6211\u8aaa\uff01 \u5730\u4e0a\u7684\u6240\u6709\u5c45\u6c11\u554a\uff0c\u8981\u7559\u5fc3\u807d\uff01 \u5728\u4f60\u5011\u6216\u4f60\u5011\u7956\u5148\u7684\u6642\u4ee3\u53ef\u66fe\u767c\u751f\u904e\u9019\u6a23\u7684\u4e8b\uff1f"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u8981\u628a\u9019\u4e8b\u544a\u8a34\u4f60\u5011\u7684\u5b50\u5b50\u5b6b\u5b6b\uff0c \u50b3\u5230\u4e16\u4e16\u4ee3\u4ee3\uff01"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u8757\u87f2\u5403\u5269\u7684\uff0c\u877b\u5b50\u4f86\u5403\uff1b \u877b\u5b50\u5403\u5269\u7684\uff0c\u86b1\u8722\u4f86\u5403\uff1b \u86b1\u8722\u5403\u5269\u7684\uff0c\u879e\u86b1\u4f86\u5403\u3002"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u9189\u6f22\u554a\uff0c\u9192\u904e\u4f86\u54ed\u6ce3\u5427\uff01 \u8caa\u676f\u7684\u4eba\u554a\uff0c\u54c0\u865f\u5427\uff01 \u4f60\u5011\u518d\u7121\u7f8e\u9152\u53ef\u98f2\u4e86\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u6709\u4e00\u7fa4\u5147\u731b\u7684\u8757\u87f2\u4fb5\u72af\u6211\u7684\u570b\u571f\uff0c \u7260\u5011\u4e0d\u8a08\u5176\u6578\uff0c \u6709\u96c4\u7345\u7684\u7360\u7259\u548c\u6bcd\u7345\u7684\u5229\u9f52\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u7260\u5011\u6bc0\u58de\u6211\u7684\u8461\u8404\u6a39\u548c\u7121\u82b1\u679c\u6a39\uff0c \u525d\u5149\u6a39\u76ae\uff0c \u53ea\u7559\u4e0b\u5149\u79bf\u7684\u6a39\u679d\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u6211\u7684\u5b50\u6c11\u554a\uff0c\u4f60\u5011\u8981\u50cf\u6b7b\u4e86\u672a\u5a5a\u592b\u7684\u5c11\u5973\uff0c \u7a7f\u4e0a\u55aa\u670d\u75db\u54ed\u54c0\u865f\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u8036\u548c\u83ef\u6bbf\u4e2d\u7684\u7d20\u796d\u548c\u5960\u796d\u90fd\u505c\u6b62\u4e86\uff0c \u4e8b\u5949\u8036\u548c\u83ef\u7684\u796d\u53f8\u90fd\u54c0\u50b7\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u7530\u5712\u8352\u5ee2\uff0c\u904d\u5730\u6dd2\u6dbc\uff0c \u4e94\u7a40\u76e1\u6bc0\uff0c \u518d\u4e5f\u6c92\u6709\u65b0\u9152\u548c\u6cb9\u4e86\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u8fb2\u592b\u554a\uff0c\u4f60\u5011\u8981\u6182\u50b7\uff01 \u7167\u6599\u8461\u8404\u5712\u7684\u4eba\u554a\uff0c \u4f60\u5011\u8981\u54c0\u865f\uff01 \u56e0\u70ba\u7530\u9593\u7684\u5927\u9ea5\u548c\u5c0f\u9ea5\u5168\u6bc0\u4e86\u3002"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u8461\u8404\u6a39\u67af\u6b7b\uff0c \u7121\u82b1\u679c\u6a39\u51cb\u6b98\uff0c \u77f3\u69b4\u6a39\u3001\u68d5\u6a39\u3001\u860b\u679c\u6a39\u7b49\u7530\u91ce\u7684\u4e00\u5207\u6a39\u6728\u67af\u4e7e\u3002 \u4eba\u9593\u7684\u6b61\u6a02\u6d88\u901d\u4e86\u3002"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u796d\u53f8\u554a\uff0c\u7a7f\u8457\u55aa\u670d\u75db\u54ed\u5427\uff1b \u5728\u796d\u58c7\u524d\u4e8b\u5949\u7684\u4eba\u554a\uff0c\u8981\u54c0\u865f\uff1b \u6211\u4e0a\u5e1d\u7684\u50d5\u4eba\u554a\uff0c\u8981\u62ab\u4e0a\u9ebb\u8863\u904e\u591c\uff1b \u56e0\u70ba\u518d\u7121\u7d20\u796d\u548c\u5960\u796d\u737b\u5230\u4f60\u5011\u4e0a\u5e1d\u7684\u6bbf\u4e2d\u4e86\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u4f60\u5011\u8981\u5b9a\u4e00\u500b\u7981\u98df\u7684\u65e5\u5b50\uff0c \u8209\u884c\u838a\u56b4\u7684\u805a\u6703\uff0c \u53ec\u96c6\u5168\u570b\u7684\u9577\u8001\u548c\u4eba\u6c11\uff0c \u5230\u4f60\u5011\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\u7684\u6bbf\u4e2d\uff0c \u5411\u8036\u548c\u83ef\u547c\u6c42\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u592a\u53ef\u6015\u4e86\uff01\u8036\u548c\u83ef\u7684\u65e5\u5b50\u8fd1\u4e86\uff0c \u90a3\u662f\u5168\u80fd\u8005\u65bd\u884c\u6bc0\u6ec5\u7684\u65e5\u5b50\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u6211\u5011\u7684\u7ce7\u98df\u65b7\u7d55\u4e86\uff0c \u6211\u5011\u4e0a\u5e1d\u6bbf\u4e2d\u7684\u6b61\u6a02\u6d88\u5931\u4e86\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u7a2e\u5b50\u5728\u571f\u88e1\u8150\u721b\uff0c \u4e94\u7a40\u4e7e\u6b7b\uff0c\u5009\u5eab\u8352\u5ee2\uff0c \u7a40\u5009\u88ab\u62c6\u6bc0\u3002"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u7272\u755c\u54c0\u9cf4\uff0c \u725b\u7fa4\u56e0\u627e\u4e0d\u5230\u8349\u5834\u800c\u56db\u8655\u4e82\u7ac4\uff0c \u7f8a\u7fa4\u4e5f\u53d7\u82e6\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u8036\u548c\u83ef\u554a\uff0c\u6211\u5411\u4f60\u547c\u6c42\uff0c \u56e0\u70ba\u706b\u7130\u541e\u6ec5\u4e86\u66e0\u91ce\u7684\u8349\u5834\uff0c \u70c8\u706b\u71d2\u5149\u4e86\u7530\u91ce\u7684\u6a39\u6728\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u7530\u91ce\u7684\u8d70\u7378\u5411\u4f60\u547c\u6c42\uff0c \u56e0\u70ba\u6eaa\u6c34\u5df2\u7d93\u67af\u4e7e\uff0c \u706b\u4e5f\u71d2\u76e1\u4e86\u66e0\u91ce\u7684\u8349\u5834\u3002"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u8036\u548c\u83ef\u5be9\u5224\u7684\u65e5\u5b50 - \u4f60\u5011\u8981\u5728\u932b\u5b89\u5439\u97ff\u865f\u89d2\uff0c \u5728\u6211\u7684\u8056\u5c71\u4e0a\u767c\u51fa\u8b66\u8a0a\uff0c \u5730\u4e0a\u7684\u5c45\u6c11\u90fd\u8981\u6230\u6296\uff0c \u56e0\u70ba\u8036\u548c\u83ef\u7684\u65e5\u5b50\u5c07\u8981\u4f86\u81e8\uff0c \u5df2\u7d93\u8fd1\u4e86\u3002"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u90a3\u5c07\u662f\u70cf\u96f2\u5bc6\u4f48\u3001 \u5929\u660f\u5730\u6697\u7684\u65e5\u5b50\u3002 \u4f86\u4e86\u4e00\u652f\u7a7a\u524d\u7d55\u5f8c\u3001\u5f37\u76db\u7121\u6bd4\u7684\u8757\u87f2\u5927\u8ecd\uff0c \u7260\u5011\u50cf\u66d9\u5149\u4e00\u6a23\u4f48\u6eff\u5c71\u982d\u3002"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u7260\u5011\u7684\u524d\u968a\u5982\u706b\u71ce\u539f\uff0c \u5f8c\u968a\u5982\u718a\u718a\u70c8\u7130\u3002 \u7260\u5011\u672a\u5230\u4ee5\u524d\uff0c \u5927\u5730\u7f8e\u5f97\u50cf\u4f0a\u7538\u5712\uff1b \u7260\u5011\u904e\u53bb\u4e4b\u5f8c\uff0c \u5927\u5730\u8b8a\u6210\u4e00\u7247\u8352\u5834\uff0c \u5730\u4e0a\u7684\u4e00\u5207\u90fd\u9003\u4e0d\u904e\u9019\u4e00\u5834\u6d69\u52ab\u3002"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u7260\u5011\u7684\u5f62\u72c0\u50cf\u99ac\uff0c \u5feb\u901f\u5954\u99b3\u5982\u6230\u99ac\uff0c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u5728\u5c71\u5dba\u4e0a\u8df3\u8e8d\u5954\u9a30\uff0c \u5982\u9686\u9686\u6230\u8eca\uff0c \u597d\u50cf\u70c8\u706b\u541e\u566c\u4e7e\u8349\u7684\u8072\u97f3\uff0c \u53c8\u5982\u5927\u8ecd\u64fa\u958b\u9663\u5f0f\u6e96\u5099\u6253\u4ed7\u3002"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u7260\u5011\u4e00\u4f86\uff0c\u767e\u59d3\u5c31\u9a5a\u614c\u5931\u63aa\uff0c \u9762\u5982\u6b7b\u7070\u3002"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u7260\u5011\u5feb\u901f\u5954\u8dd1\u50cf\u52c7\u58eb\uff0c \u6500\u767b\u57ce\u7246\u5982\u6230\u58eb\uff0c \u6709\u689d\u4e0d\u7d0a\u5730\u524d\u9032\u3002"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u968a\u4f0d\u6574\u9f4a\uff0c\u5404\u884c\u5176\u9053\uff0c \u52e2\u5982\u7834\u7af9\uff0c\u7121\u5805\u4e0d\u6467\u3002"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u7260\u5011\u885d\u5411\u57ce\u9091\uff0c\u8ea5\u4e0a\u57ce\u7246\uff0c \u722c\u9032\u623f\u5c4b\uff0c \u5982\u76dc\u8cca\u822c\u7834\u7a97\u800c\u5165\u3002"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u6240\u5230\u4e4b\u8655\uff0c \u5929\u6416\u5730\u52d5\uff0c \u65e5\u6708\u660f\u6697\uff0c \u661f\u8fb0\u7121\u5149\u3002"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u8036\u548c\u83ef\u5411\u7942\u7684\u8ecd\u968a\u767c\u865f\u65bd\u4ee4\uff0c \u7942\u7684\u968a\u4f0d\u4e0d\u8a08\u5176\u6578\uff0c \u57f7\u884c\u7942\u547d\u4ee4\u7684\u662f\u4e00\u652f\u52c1\u65c5\u3002 \u8036\u548c\u83ef\u7684\u65e5\u5b50\u5049\u5927\u800c\u53ef\u754f\uff0c \u8ab0\u80fd\u627f\u53d7\u5f97\u4f4f\u5462\uff1f"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u547c\u7c72\u4eba\u6c11\u6094\u6539 - \u8036\u548c\u83ef\u8aaa\uff1a \u300c\u73fe\u5728\uff0c\u4f60\u5011\u8981\u7981\u98df\u3001\u54ed\u6ce3\u3001\u54c0\u865f\uff0c \u5168\u5fc3\u56de\u8f49\u6b78\u5411\u6211\u3002\u300d"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u4f60\u5011\u8981\u6495\u5fc3\u822c\u5730\u6094\u6539\uff0c \u800c\u4e0d\u662f\u6495\u88c2\u8863\u670d\u3002 \u6b78\u5411\u4f60\u5011\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\u5427\uff0c \u56e0\u70ba\u7942\u6709\u6069\u5178\u548c\u6190\u61ab\uff0c \u4e0d\u8f15\u6613\u767c\u6012\uff0c \u6709\u7121\u9650\u7684\u6148\u611b\uff0c \u4e0d\u5fcd\u5fc3\u964d\u707d\u798d\u3002"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u8ab0\u77e5\u9053\u5462\uff1f \u6216\u8a31\u7942\u8f49\u5ff5\u5fc3\u751f\u6190\u61ab\uff0c \u7d66\u4f60\u5011\u7559\u4e0b\u795d\u798f\uff0c \u4f7f\u4f60\u5011\u53ef\u4ee5\u518d\u6b21\u5411\u4f60\u5011\u7684\u4e0a\u5e1d\u8036\u548c\u83ef\u737b\u4e0a\u7d20\u796d\u548c\u5960\u796d\u3002"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u8981\u5728\u932b\u5b89\u5c71\u5439\u97ff\u865f\u89d2\uff0c \u5ba3\u4f48\u7981\u98df\u7684\u65e5\u5b50\uff0c \u8209\u884c\u838a\u56b4\u7684\u805a\u6703\u3002"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u8981\u62db\u805a\u8001\u4eba\uff0c\u805a\u96c6\u5b69\u7ae5\uff0c \u5305\u62ec\u5403\u5976\u7684\u5b30\u5152\uff1b \u8981\u5429\u5490\u65b0\u90ce\u51fa\u6d1e\u623f\u3001 \u65b0\u5a18\u51fa\u5167\u5ba4\uff1b \u8981\u53ec\u96c6\u5168\u9ad4\u4eba\u6c11\uff0c \u8b93\u4ed6\u5011\u6f54\u6de8\u81ea\u5df1\u3002"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u8b93\u4e8b\u5949\u8036\u548c\u83ef\u7684\u796d\u53f8\u7ad9\u5728\u8056\u6bbf\u9580\u5eca\u548c\u796d\u58c7\u4e2d\u9593\uff0c \u54ed\u6ce3\u61c7\u6c42\u8aaa\uff1a \u300c\u8036\u548c\u83ef\u554a\uff0c \u6c42\u4f60\u9867\u60dc\u4f60\u7684\u5b50\u6c11\uff0c \u4e0d\u8981\u8b93\u5916\u65cf\u4eba\u4fae\u8fb1\u3001 \u8b4f\u7b11\u4f60\u7684\u7522\u696d\u8aaa\uff0c \u300e\u4f60\u5011\u7684\u4e0a\u5e1d\u5728\u54ea\u88e1\u5462\uff1f\u300f\u300d"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u518d\u8cdc\u795d\u798f - \u8036\u548c\u83ef\u70ba\u81ea\u5df1\u7684\u571f\u5730\u767c\u71b1\u5fc3\uff0c \u7942\u6190\u61ab\u81ea\u5df1\u7684\u5b50\u6c11\u3002"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u7942\u56de\u61c9\u4ed6\u5011\u8aaa\uff1a \u300c\u6211\u5fc5\u8cdc\u4e94\u7a40\u3001\u65b0\u9152\u548c\u6cb9\u7d66\u4f60\u5011\uff0c \u4f7f\u4f60\u5011\u98fd\u8db3\uff0c \u4e0d\u518d\u8b93\u4f60\u5011\u53d7\u5217\u570b\u7684\u7f9e\u8fb1\u3002"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u6211\u8981\u8d95\u51fa\u5f9e\u5317\u65b9\u4f86\u7684\u8ecd\u968a\uff0c \u628a\u4ed6\u5011\u9a45\u9010\u5230\u4e00\u500b\u4e7e\u65f1\u8352\u856a\u7684\u5730\u65b9\uff0c \u628a\u4ed6\u5011\u7684\u9818\u982d\u90e8\u968a\u8d95\u9032\u6b7b\u6d77\uff0c \u518d\u628a\u4ed6\u5011\u7684\u6bbf\u5f8c\u90e8\u968a\u8d95\u9032\u5730\u4e2d\u6d77\uff0c \u90a3\u6642\u4ed6\u5011\u5fc5\u81ed\u6c23\u6c96\u5929\u3001 \u8165\u5473\u9a30\u7a7a\uff0c \u56e0\u70ba\u8036\u548c\u83ef\u70ba\u4f60\u5011\u884c\u4e86\u5927\u4e8b\u3002\u300d"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u5927\u5730\u554a\uff0c\u4e0d\u7528\u61fc\u6015\uff0c \u5012\u8981\u6b61\u559c\u5feb\u6a02\uff1b \u56e0\u70ba\u8036\u548c\u83ef\u884c\u4e86\u5927\u4e8b\u3002"
                },
                "22": {
                    "verse_nr": "22",
