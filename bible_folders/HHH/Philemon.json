                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u05e9\u05dc\u05d5\u05dd \u05d5\u05d1\u05e8\u05db\u05d4 \u05de\u05d0\u05ea \u05d0\u05dc\u05d5\u05d4\u05d9\u05e0\u05d5 \u05d5\u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u05e4\u05d9\u05dc\u05d9\u05de\u05d5\u05df \u05d4\u05d9\u05e7\u05e8, \u05d0\u05e0\u05d9 \u05de\u05d5\u05d3\u05d4 \u05ea\u05de\u05d9\u05d3 \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05db\u05d0\u05e9\u05e8 \u05d0\u05e0\u05d9 \u05de\u05ea\u05e4\u05dc\u05dc \u05d1\u05e2\u05d3\u05da,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u05db\u05d9 \u05d0\u05e0\u05d9 \u05de\u05de\u05e9\u05d9\u05da \u05dc\u05e9\u05de\u05d5\u05e2 \u05e2\u05dc \u05d0\u05de\u05d5\u05e0\u05ea\u05da \u05d1\u05de\u05e9\u05d9\u05d7 \u05d9\u05e9\u05d5\u05e2 \u05d5\u05d0\u05d4\u05d1\u05ea\u05da \u05dc\u05db\u05dc \u05e2\u05dd \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u05d0\u05e0\u05d9 \u05de\u05ea\u05e4\u05dc\u05dc \u05e9\u05db\u05d0\u05e9\u05e8 \u05ea\u05e1\u05e4\u05e8 \u05dc\u05d0\u05e0\u05e9\u05d9\u05dd \u05e9\u05d5\u05e0\u05d9\u05dd \u05e2\u05dc \u05d0\u05de\u05d5\u05e0\u05ea\u05da \u05d1\u05d0\u05d3\u05d5\u05df, \u05d4\u05dd \u05d9\u05d1\u05d7\u05d9\u05e0\u05d5 \u05d1\u05ea\u05db\u05d5\u05e0\u05d5\u05ea\u05d9\u05da \u05d4\u05d8\u05d5\u05d1\u05d5\u05ea \u05d5\u05d1\u05d1\u05e8\u05db\u05d5\u05ea \u05e9\u05d4\u05e9\u05e4\u05d9\u05e2 \u05e2\u05dc\u05d9\u05da \u05d4\u05d0\u05d3\u05d5\u05df, \u05d5\u05d2\u05dd \u05d4\u05dd \u05d9\u05d0\u05de\u05d9\u05e0\u05d5 \u05d1\u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u05d0\u05d7\u05d9 \u05d4\u05d9\u05e7\u05e8, \u05d0\u05d4\u05d1\u05ea\u05da \u05d4\u05d9\u05d9\u05ea\u05d4 \u05dc\u05d9 \u05dc\u05e9\u05de\u05d7\u05d4 \u05d5\u05e0\u05d7\u05de\u05d4, \u05e9\u05db\u05df \u05de\u05e2\u05e9\u05d9\u05da \u05d4\u05d8\u05d5\u05d1\u05d9\u05dd \u05e2\u05d5\u05d3\u05d3\u05d5 \u05de\u05d0\u05d5\u05d3 \u05d0\u05ea \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "verse doesn't exist in this bible version"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "verse doesn't exist in this bible version"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u05d1\u05e7\u05e9\u05ea\u05d9 \u05d4\u05d9\u05d0 \u05e9\u05ea\u05ea\u05e0\u05d4\u05d2 \u05d9\u05e4\u05d4 \u05e2\u05dd \u05d0\u05d5\u05e0\u05d9\u05e1\u05d9\u05de\u05d5\u05e1. \u05db\u05d0\u05df \u05d1\u05db\u05dc\u05d0 \u05d4\u05d3\u05e8\u05db\u05ea\u05d9 \u05d0\u05d5\u05ea\u05d5 \u05d0\u05dc \u05d4\u05d0\u05d3\u05d5\u05df \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7, \u05d5\u05e2\u05ea\u05d4 \u05d0\u05d5\u05e0\u05d9\u05e1\u05d9\u05de\u05d5\u05e1 \u05de\u05d0\u05de\u05d9\u05df \u05d1\u05d5."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u05d0\u05d5\u05e0\u05d9\u05e1\u05d9\u05de\u05d5\u05e1 (\u05e4\u05d9\u05e8\u05d5\u05e9 \u05d4\u05e9\u05dd: \u05e8\u05d1\u05be\u05ea\u05d5\u05e2\u05dc\u05ea) \u05dc\u05d0 \u05d4\u05d5\u05e2\u05d9\u05dc \u05dc\u05da \u05d1\u05e2\u05d1\u05e8, \u05d0\u05da \u05de\u05e2\u05ea\u05d4 \u05d5\u05d0\u05d9\u05dc\u05da \u05d9\u05d4\u05d9\u05d4 \u05dc\u05ea\u05d5\u05e2\u05dc\u05ea \u05e8\u05d1\u05d4 \u05dc\u05e9\u05e0\u05d9\u05e0\u05d5."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u05d1\u05e6\u05e2\u05e8 \u05e8\u05d1 \u05e0\u05d9\u05e4\u05e8\u05d3 \u05d0\u05e0\u05d9 \u05de\u05d1\u05e0\u05d9 \u05d1\u05d0\u05de\u05d5\u05e0\u05d4, \u05e9\u05d4\u05d5\u05d0 \u05d1\u05d1\u05ea \u05e2\u05d9\u05e0\u05d9, \u05d5\u05de\u05d7\u05d6\u05d9\u05e8\u05d5 \u05d0\u05dc\u05d9\u05da."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u05d7\u05e4\u05e6\u05ea\u05d9 \u05e9\u05d9\u05d9\u05e9\u05d0\u05e8 \u05dc\u05e2\u05d6\u05d5\u05e8 \u05dc\u05d9 \u05db\u05d0\u05df \u05d1\u05d1\u05d9\u05ea \u05d4\u05db\u05dc\u05d0,"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u05d0\u05da \u05dc\u05d0 \u05e8\u05e6\u05d9\u05ea\u05d9 \u05dc\u05db\u05e4\u05d5\u05ea \u05e2\u05dc\u05d9\u05da \u05d0\u05ea \u05e8\u05e6\u05d5\u05e0\u05d9."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u05d0\u05e0\u05d9 \u05de\u05e6\u05d9\u05e2 \u05e9\u05ea\u05e1\u05ea\u05db\u05dc \u05e2\u05dc \u05d4\u05de\u05e6\u05d1 \u05db\u05da: \u05d0\u05d5\u05e0\u05d9\u05e1\u05d9\u05de\u05d5\u05e1 \u05e2\u05d6\u05d1 \u05d0\u05d5\u05ea\u05da \u05dc\u05d6\u05de\u05df \u05e7\u05e6\u05e8 \u05db\u05d3\u05d9 \u05e9\u05e2\u05db\u05e9\u05d9\u05d5, \u05d1\u05e9\u05d5\u05d1\u05d5, \u05d9\u05d5\u05db\u05dc \u05dc\u05d4\u05d9\u05d5\u05ea \u05e9\u05dc\u05da \u05dc\u05ea\u05de\u05d9\u05d3."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u05d4\u05d5\u05d0 \u05dc\u05d0 \u05d9\u05d4\u05d9\u05d4 \u05d9\u05d5\u05ea\u05e8 \u05e1\u05ea\u05dd \u05e2\u05d1\u05d3, \u05d0\u05dc\u05d0 \u05de\u05d9\u05e9\u05d4\u05d5 \u05d7\u05e9\u05d5\u05d1 \u05d9\u05d5\u05ea\u05e8 \u2013 \u05d0\u05d7 \u05d0\u05d4\u05d5\u05d1, \u05d5\u05d1\u05de\u05d9\u05d5\u05d7\u05d3 \u05d0\u05d4\u05d5\u05d1 \u05e2\u05dc\u05d9. \u05de\u05e2\u05ea\u05d4 \u05d9\u05d2\u05d3\u05dc \u05e2\u05e8\u05db\u05d5 \u05d1\u05e2\u05d9\u05e0\u05d9\u05da, \u05e9\u05db\u05df \u05d0\u05d9\u05df \u05d4\u05d5\u05d0 \u05e1\u05ea\u05dd \u05e2\u05d1\u05d3, \u05d0\u05dc\u05d0 \u05d2\u05dd \u05d0\u05d7\u05d9\u05da \u05d1\u05de\u05e9\u05d9\u05d7."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u05d0\u05dd \u05d0\u05ea\u05d4 \u05d1\u05d0\u05de\u05ea \u05d7\u05d5\u05e9\u05d1 \u05d0\u05d5\u05ea\u05d9 \u05dc\u05d9\u05d3\u05d9\u05d3 \u05d8\u05d5\u05d1 \u05e9\u05dc\u05da, \u05d0\u05e0\u05d0, \u05e7\u05d1\u05dc\u05d4\u05d5 \u05d1\u05d7\u05de\u05d9\u05de\u05d5\u05ea \u05db\u05e9\u05dd \u05e9\u05d4\u05d9\u05d9\u05ea \u05de\u05e7\u05d1\u05dc \u05d0\u05d5\u05ea\u05d9 \u05dc\u05d5 \u05d0\u05e0\u05d9 \u05d4\u05d9\u05d9\u05ea\u05d9 \u05d1\u05d0."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u05d0\u05dd \u05d0\u05d5\u05e0\u05d9\u05e1\u05d9\u05de\u05d5\u05e1 \u05d2\u05e8\u05dd \u05dc\u05da \u05e0\u05d6\u05e7 \u05db\u05dc\u05e9\u05d4\u05d5 \u05d1\u05e2\u05d1\u05e8 \u05d0\u05d5 \u05d2\u05e0\u05d1 \u05de\u05de\u05da \u05de\u05e9\u05d4\u05d5, \u05d6\u05e7\u05d5\u05e3 \u05d6\u05d0\u05ea \u05dc\u05d7\u05d5\u05d1\u05ea\u05d9,"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u05d5\u05d0\u05e0\u05d9 \u05de\u05ea\u05d7\u05d9\u05d9\u05d1 \u05d1\u05d6\u05d0\u05ea \u05dc\u05e9\u05dc\u05dd \u05d0\u05ea \u05db\u05dc \u05de\u05d4 \u05e9\u05de\u05d2\u05d9\u05e2 \u05dc\u05da. (\u05d4\u05e8\u05d9\u05e0\u05d9 \u05de\u05d0\u05e9\u05e8 \u05d1\u05db\u05ea\u05d1 \u05d9\u05d3\u05d9 \u05e9\u05d0\u05e0\u05d9 \u05e2\u05e8\u05d1 \u05dc\u05db\u05dc \u05de\u05e2\u05e9\u05d9\u05d5 \u05e9\u05dc \u05d0\u05d5\u05e0\u05d9\u05e1\u05d9\u05de\u05d5\u05e1). \u05d0\u05d9\u05e0\u05d9 \u05e6\u05e8\u05d9\u05da \u05dc\u05d4\u05d6\u05db\u05d9\u05e8 \u05dc\u05da \u05db\u05de\u05d4 \u05d0\u05ea\u05d4 \u05d7\u05d9\u05d9\u05d1 \u05dc\u05d9; \u05d0\u05ea\u05d4 \u05d7\u05d9\u05d9\u05d1 \u05dc\u05d9 \u05db\u05dc\u05be\u05db\u05da \u05d4\u05e8\u05d1\u05d4 \u05d0\u05e4\u05d9\u05dc\u05d5 \u05d0\u05ea \u05d7\u05d9\u05d9\u05da!"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u05d0\u05d7\u05d9 \u05d4\u05d9\u05e7\u05e8, \u05d0\u05e0\u05d0 \u05e2\u05e9\u05d4 \u05d0\u05ea \u05d4\u05de\u05e2\u05e9\u05d4 \u05d4\u05e0\u05e2\u05dc\u05d4 \u05d4\u05d6\u05d4 \u05d0\u05e9\u05e8 \u05d9\u05d2\u05e8\u05d5\u05dd \u05dc\u05d9 \u05e9\u05de\u05d7\u05d4 \u05e8\u05d1\u05d4, \u05d5\u05d0\u05e0\u05d9 \u05d0\u05d5\u05d3\u05d4 \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05de\u05e2\u05d5\u05de\u05e7 \u05dc\u05d1\u05d9."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u05db\u05ea\u05d1\u05ea\u05d9 \u05dc\u05da \u05de\u05db\u05ea\u05d1 \u05d6\u05d4 \u05db\u05d9 \u05d0\u05e0\u05d9 \u05d1\u05d8\u05d5\u05d7 \u05e9\u05ea\u05de\u05dc\u05d0 \u05d0\u05ea \u05d1\u05e7\u05e9\u05ea\u05d9. (\u05d5\u05d0\u05e3 \u05dc\u05de\u05e2\u05dc\u05d4 \u05de\u05d6\u05d0\u05ea)."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u05d0\u05ea\u05d4 \u05db\u05d1\u05e8 \u05d9\u05db\u05d5\u05dc \u05dc\u05d4\u05db\u05d9\u05df \u05dc\u05de\u05e2\u05e0\u05d9 \u05d0\u05ea \u05d7\u05d3\u05e8 \u05d4\u05d0\u05d5\u05e8\u05d7\u05d9\u05dd, \u05db\u05d9 \u05d0\u05e0\u05d9 \u05de\u05e7\u05d5\u05d5\u05d4 \u05e9\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d9\u05e2\u05e0\u05d4 \u05dc\u05ea\u05e4\u05d9\u05dc\u05d5\u05ea\u05d9\u05db\u05dd \u05d5\u05d9\u05d0\u05e4\u05e9\u05e8 \u05dc\u05d9 \u05dc\u05d1\u05e7\u05e8 \u05d0\u05ea\u05db\u05dd \u05d1\u05e7\u05e8\u05d5\u05d1."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u05d3\u05e8\u05d9\u05e9\u05ea \u05e9\u05dc\u05d5\u05dd \u05dc\u05da \u05de\u05d0\u05e4\u05e4\u05e8\u05e1 \u05e9\u05e0\u05de\u05e6\u05d0 \u05d0\u05d9\u05ea\u05d9 \u05d1\u05db\u05dc\u05d0. \u05d2\u05dd \u05d4\u05d5\u05d0 \u05e0\u05d0\u05e1\u05e8 \u05e8\u05e7 \u05d1\u05d2\u05dc\u05dc \u05e9\u05e1\u05d9\u05e4\u05e8 \u05dc\u05d0\u05e0\u05e9\u05d9\u05dd \u05e2\u05dc \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "verse doesn't exist in this bible version"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u05d7\u05e1\u05d3 \u05d4\u05d0\u05d3\u05d5\u05df \u05d9\u05e9\u05d5\u05e2 \u05e2\u05dd \u05e8\u05d5\u05d7\u05db\u05dd."
                }
            }
        }
    }
}