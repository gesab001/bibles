{
    "version": "HHH",
    "book_name": "1 Thessalonians",
    "book_nr": "52",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u05d0\u05dc \u05e7\u05d4\u05d9\u05dc\u05ea \u05ea\u05e1\u05dc\u05d5\u05e0\u05d9\u05e7\u05d9, \u05e9\u05e9\u05d9\u05d9\u05db\u05ea \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05d1\u05d9\u05e0\u05d5 \u05d5\u05dc\u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5. \u05de\u05d0\u05ea \u05e4\u05d5\u05dc\u05d5\u05e1, \u05e1\u05d9\u05dc\u05d5\u05d5\u05e0\u05d5\u05e1 \u05d5\u05d8\u05d9\u05de\u05d5\u05ea\u05d9\u05d5\u05e1. \u05e9\u05dc\u05d5\u05dd \u05d5\u05d1\u05e8\u05db\u05d4 \u05de\u05d0\u05ea \u05d4\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05d1\u05d9\u05e0\u05d5 \u05d5\u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u05d1\u05db\u05dc \u05e4\u05e2\u05dd \u05e9\u05d0\u05e0\u05d5 \u05de\u05ea\u05e4\u05dc\u05dc\u05d9\u05dd \u05d1\u05e2\u05d3\u05db\u05dd \u05d0\u05e0\u05d5 \u05de\u05d5\u05d3\u05d9\u05dd \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e2\u05dc \u05db\u05d5\u05dc\u05db\u05dd,"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u05d5\u05de\u05d6\u05db\u05d9\u05e8\u05d9\u05dd \u05dc\u05e4\u05e0\u05d9\u05d5 \u05ea\u05de\u05d9\u05d3 \u05d0\u05ea \u05de\u05e2\u05e9\u05d9\u05db\u05dd \u05d4\u05d8\u05d5\u05d1\u05d9\u05dd, \u05d0\u05ea \u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd \u05d4\u05d7\u05d6\u05e7\u05d4, \u05d0\u05ea \u05d0\u05d4\u05d1\u05ea\u05db\u05dd \u05d5\u05d0\u05ea \u05e6\u05d9\u05e4\u05d9\u05d9\u05ea\u05db\u05dd \u05d4\u05e1\u05d1\u05dc\u05e0\u05d9\u05ea \u05dc\u05e9\u05d5\u05d1\u05d5 \u05e9\u05dc \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd \u05d5\u05d0\u05d4\u05d5\u05d1\u05d9 \u05d4\u05d0\u05d3\u05d5\u05df, \u05d0\u05e0\u05d7\u05e0\u05d5 \u05d9\u05d5\u05d3\u05e2\u05d9\u05dd \u05e9\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d1\u05d7\u05e8 \u05d1\u05db\u05dd,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u05e9\u05d4\u05e8\u05d9 \u05db\u05d0\u05e9\u05e8 \u05d1\u05d9\u05e9\u05e8\u05e0\u05d5 \u05dc\u05db\u05dd \u05d0\u05ea \u05d1\u05e9\u05d5\u05e8\u05ea \u05d4\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05dc\u05d0 \u05d4\u05de\u05d9\u05dc\u05d9\u05dd \u05e9\u05dc\u05e0\u05d5 \u05e9\u05db\u05e0\u05e2\u05d5 \u05d0\u05ea\u05db\u05dd, \u05d0\u05dc\u05d0 \u05e8\u05d5\u05d7 \u05d4\u05e7\u05d5\u05d3\u05e9 \u05d4\u05d5\u05d0 \u05e9\u05e9\u05db\u05e0\u05e2 \u05d0\u05ea\u05db\u05dd \u05d5\u05e2\u05d6\u05e8 \u05dc\u05db\u05dd \u05dc\u05d4\u05d0\u05de\u05d9\u05df \u05d1\u05d1\u05e9\u05d5\u05e8\u05d4 \u05d1\u05d4\u05ea\u05dc\u05d4\u05d1\u05d5\u05ea \u05d5\u05d1\u05d3\u05d1\u05e7\u05d5\u05ea. \u05d2\u05dd \u05d0\u05d5\u05e8\u05d7 \u05d7\u05d9\u05d9\u05e0\u05d5 \u05e9\u05d9\u05de\u05e9 \u05d4\u05d5\u05db\u05d7\u05d4 \u05d7\u05d5\u05ea\u05db\u05ea \u05dc\u05d0\u05de\u05d9\u05ea\u05d5\u05ea \u05d4\u05d1\u05e9\u05d5\u05e8\u05d4 \u05e9\u05d1\u05d9\u05e9\u05e8\u05e0\u05d5 \u05dc\u05db\u05dd."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u05d5\u05db\u05da \u05d4\u05dc\u05db\u05ea\u05dd \u05d1\u05e2\u05e7\u05d1\u05d5\u05ea\u05d9\u05e0\u05d5 \u05db\u05e9\u05dd \u05e9\u05d0\u05e0\u05d5 \u05d4\u05dc\u05db\u05e0\u05d5 \u05d1\u05e2\u05e7\u05d1\u05d5\u05ea \u05d4\u05d0\u05d3\u05d5\u05df. \u05dc\u05de\u05e8\u05d5\u05ea \u05e9\u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd \u05d1\u05d1\u05e9\u05d5\u05e8\u05d4 \u05d4\u05d1\u05d9\u05d0\u05d4 \u05e2\u05dc\u05d9\u05db\u05dd \u05e6\u05e8\u05d5\u05ea \u05d5\u05e1\u05d1\u05dc \u05de\u05d0\u05d5\u05d9\u05d1\u05d9\u05db\u05dd, \u05de\u05d9\u05dc\u05d0 \u05d0\u05ea\u05db\u05dd \u05e8\u05d5\u05d7 \u05d4\u05e7\u05d5\u05d3\u05e9 \u05e9\u05de\u05d7\u05d4,"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u05d5\u05ea\u05d5\u05da \u05d6\u05de\u05df \u05e7\u05e6\u05e8 \u05d4\u05e4\u05db\u05ea\u05dd \u05d1\u05e2\u05e6\u05de\u05db\u05dd \u05dc\u05d3\u05d5\u05d2\u05de\u05d4 \u05d5\u05e2\u05e9\u05d9\u05ea\u05dd \u05e9\u05dd \u05d1\u05d9\u05df \u05db\u05dc \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05d1\u05d9\u05d5\u05d5\u05df."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u05d1\u05d9\u05e0\u05ea\u05d9\u05d9\u05dd \u05e4\u05e8\u05e1\u05de\u05ea\u05dd \u05d0\u05ea \u05d3\u05d1\u05e8\u05be\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d1\u05db\u05dc \u05de\u05e7\u05d5\u05dd, \u05d5\u05d0\u05e0\u05d5 \u05e9\u05d5\u05de\u05e2\u05d9\u05dd \u05e2\u05dc \u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd \u05d4\u05e0\u05e4\u05dc\u05d0\u05d4 \u05d1\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d1\u05db\u05dc \u05de\u05e7\u05d5\u05dd \u05e9\u05d0\u05e0\u05d5 \u05d4\u05d5\u05dc\u05db\u05d9\u05dd. \u05dc\u05e0\u05d5 \u05dc\u05d0 \u05e0\u05d5\u05ea\u05e8 \u05de\u05d4 \u05dc\u05e1\u05e4\u05e8!"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u05d0\u05e0\u05e9\u05d9\u05dd \u05de\u05e1\u05e4\u05e8\u05d9\u05dd \u05dc\u05e0\u05d5 \u05e2\u05dc \u05e7\u05d1\u05dc\u05ea\u05be\u05d4\u05e4\u05e0\u05d9\u05dd \u05d4\u05d7\u05de\u05d4 \u05e9\u05e2\u05e8\u05db\u05ea\u05dd \u05dc\u05e0\u05d5, \u05d5\u05db\u05d9\u05e6\u05d3 \u05e2\u05d6\u05d1\u05ea\u05dd \u05d0\u05ea \u05d4\u05d0\u05dc\u05d9\u05dc\u05d9\u05dd \u05d5\u05d4\u05d0\u05de\u05e0\u05ea\u05dd \u05d1\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d4\u05d7\u05d9 \u05d5\u05d4\u05d0\u05de\u05d9\u05ea\u05d9."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u05d4\u05dd \u05d2\u05dd \u05e1\u05d9\u05e4\u05e8\u05d5 \u05dc\u05e0\u05d5 \u05e2\u05dc \u05e6\u05d9\u05e4\u05d9\u05d9\u05ea\u05db\u05dd \u05dc\u05e9\u05d5\u05d1\u05d5 \u05e9\u05dc \u05d1\u05df\u05be\u05d4\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05de\u05d4\u05e9\u05de\u05d9\u05dd, \u05d4\u05dc\u05d0 \u05d4\u05d5\u05d0 \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05e9\u05e7\u05dd \u05dc\u05ea\u05d7\u05d9\u05d9\u05d4 \u05d5\u05d4\u05d5\u05e9\u05d9\u05e2\u05e0\u05d5 \u05de\u05d6\u05e2\u05dd \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e2\u05dc \u05d4\u05d7\u05d8\u05d0."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d4\u05dc\u05d0 \u05d9\u05d5\u05d3\u05e2\u05d9\u05dd \u05d0\u05ea\u05dd \u05e9\u05d1\u05d9\u05e7\u05d5\u05e8\u05e0\u05d5 \u05d0\u05e6\u05dc\u05db\u05dd \u05d4\u05d9\u05d4 \u05de\u05d0\u05d5\u05d3 \u05e4\u05d5\u05e8\u05d4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u05dc\u05de\u05e8\u05d5\u05ea \u05d4\u05e2\u05d9\u05e0\u05d5\u05d9\u05d9\u05dd \u05d5\u05d4\u05d7\u05e8\u05e4\u05d4 \u05e9\u05e1\u05d1\u05dc\u05e0\u05d5 \u05d1\u05e4\u05d9\u05dc\u05d9\u05e4\u05d9, \u05e0\u05ea\u05df \u05dc\u05e0\u05d5 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05d5\u05de\u05e5 \u05dc\u05d1\u05d5\u05d0 \u05d5\u05dc\u05d1\u05e9\u05e8 \u05d2\u05dd \u05dc\u05db\u05dd \u05d0\u05ea \u05d4\u05d1\u05e9\u05d5\u05e8\u05d4 \u05e2\u05dc\u05be\u05d0\u05e3 \u05d0\u05d5\u05d9\u05d1\u05d9\u05e0\u05d5 \u05d4\u05e8\u05d1\u05d9\u05dd."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u05de\u05db\u05d0\u05df \u05d9\u05db\u05d5\u05dc\u05d9\u05dd \u05d0\u05ea\u05dd \u05dc\u05e8\u05d0\u05d5\u05ea \u05e9\u05d0\u05d9\u05e0\u05e0\u05d5 \u05de\u05d3\u05d1\u05e8\u05d9\u05dd \u05d0\u05dc\u05d9\u05db\u05dd \u05de\u05ea\u05d5\u05da \u05d8\u05e2\u05d5\u05ea, \u05d5\u05e9\u05de\u05e0\u05d9\u05e2\u05d9\u05e0\u05d5 \u05d8\u05d4\u05d5\u05e8\u05d9\u05dd \u05d5\u05d8\u05d5\u05d1\u05d9\u05dd."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u05d0\u05e0\u05d5 \u05e9\u05dc\u05d9\u05d7\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d5\u05d4\u05d5\u05d0 \u05e1\u05d5\u05de\u05da \u05e2\u05dc\u05d9\u05e0\u05d5 \u05e9\u05e0\u05d0\u05de\u05e8 \u05d0\u05ea \u05d4\u05d0\u05de\u05ea \u05d1\u05dc\u05d1\u05d3. \u05d0\u05d9\u05e0\u05e0\u05d5 \u05de\u05e9\u05ea\u05d3\u05dc\u05d9\u05dd \u05dc\u05de\u05e6\u05d5\u05d0\u05be\u05d7\u05df \u05d1\u05e2\u05d9\u05e0\u05d9 \u05d1\u05e0\u05d9\u05be\u05d0\u05d3\u05dd, \u05d0\u05dc\u05d0 \u05d1\u05e2\u05d9\u05e0\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e9\u05d7\u05d5\u05e7\u05e8 \u05d5\u05d1\u05d5\u05d7\u05df \u05d0\u05ea \u05dc\u05d1\u05e0\u05d5."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u05db\u05d9\u05d3\u05d5\u05e2 \u05dc\u05db\u05dd, \u05d0\u05d9\u05e9 \u05d0\u05d9\u05e0\u05d5 \u05d9\u05db\u05d5\u05dc \u05dc\u05d5\u05de\u05e8 \u05e9\u05e0\u05d9\u05e1\u05d9\u05e0\u05d5 \u05dc\u05e7\u05e0\u05d5\u05ea \u05d0\u05ea \u05dc\u05d1\u05db\u05dd \u05d1\u05d3\u05d1\u05e8\u05d9 \u05d7\u05dc\u05e7\u05d5\u05ea \u05d1\u05de\u05d8\u05e8\u05d4 \u05dc\u05e1\u05d7\u05d5\u05d8 \u05de\u05db\u05dd \u05db\u05e1\u05e3, \u05d5\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d9\u05db\u05d5\u05dc \u05dc\u05d4\u05e2\u05d9\u05d3 \u05e2\u05dc \u05db\u05da."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u05de\u05e2\u05d5\u05dc\u05dd \u05dc\u05d0 \u05d1\u05d9\u05e7\u05e9\u05e0\u05d5 \u05db\u05d1\u05d5\u05d3 \u05de\u05d1\u05e0\u05d9\u05be\u05d0\u05d3\u05dd \u2013 \u05dc\u05d0 \u05de\u05db\u05dd \u05d5\u05dc\u05d0 \u05de\u05de\u05d9\u05e9\u05d4\u05d5 \u05d0\u05d7\u05e8, \u05d0\u05e3 \u05db\u05d9 \u05d1\u05ea\u05d5\u05e7\u05e3 \u05e1\u05de\u05db\u05d5\u05ea\u05e0\u05d5 \u05db\u05e9\u05dc\u05d9\u05d7\u05d9 \u05d4\u05de\u05e9\u05d9\u05d7 \u05d6\u05db\u05d0\u05d9\u05dd \u05d0\u05e0\u05d5 \u05dc\u05de\u05e2\u05d8 \u05db\u05d1\u05d5\u05d3 \u05de\u05db\u05dd!"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u05d4\u05ea\u05e0\u05d4\u05d2\u05e0\u05d5 \u05d0\u05ea\u05db\u05dd \u05d1\u05e2\u05d3\u05d9\u05e0\u05d5\u05ea \u05d5\u05d1\u05e8\u05db\u05d5\u05ea, \u05db\u05de\u05d5 \u05d0\u05dd \u05e9\u05d3\u05d5\u05d0\u05d2\u05ea \u05dc\u05d9\u05dc\u05d3\u05d9\u05d4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u05d1\u05d0\u05d4\u05d1\u05ea\u05e0\u05d5 \u05d4\u05e8\u05d1\u05d4 \u05d0\u05dc\u05d9\u05db\u05dd \u05d4\u05d9\u05d9\u05e0\u05d5 \u05de\u05d5\u05db\u05e0\u05d9\u05dd \u05dc\u05ea\u05ea \u05dc\u05db\u05dd \u05dc\u05d0 \u05e8\u05e7 \u05d0\u05ea \u05d1\u05e9\u05d5\u05e8\u05ea \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd, \u05d0\u05dc\u05d0 \u05d0\u05e3 \u05d0\u05ea \u05d7\u05d9\u05d9\u05e0\u05d5."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d0\u05ea\u05dd \u05d5\u05d3\u05d0\u05d9 \u05d6\u05d5\u05db\u05e8\u05d9\u05dd \u05db\u05de\u05d4 \u05e7\u05e9\u05d4 \u05e2\u05d1\u05d3\u05e0\u05d5 \u05d1\u05e2\u05ea \u05e9\u05d1\u05d9\u05e9\u05e8\u05e0\u05d5 \u05dc\u05db\u05dd \u05d0\u05ea \u05d1\u05e9\u05d5\u05e8\u05ea \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd. \u05d9\u05d5\u05de\u05dd \u05d5\u05dc\u05d9\u05dc\u05d4 \u05e2\u05de\u05dc\u05e0\u05d5 \u05d5\u05d4\u05d6\u05e2\u05e0\u05d5 \u05db\u05d3\u05d9 \u05dc\u05d4\u05e8\u05d5\u05d5\u05d9\u05d7 \u05d0\u05ea \u05dc\u05d7\u05de\u05e0\u05d5, \u05db\u05d9 \u05dc\u05d0 \u05e8\u05e6\u05d9\u05e0\u05d5 \u05e9\u05de\u05d9\u05e9\u05d4\u05d5 \u05de\u05db\u05dd \u05d9\u05d9\u05d0\u05dc\u05e5 \u05dc\u05e9\u05d0\u05ea \u05d1\u05e2\u05d5\u05dc \u05e4\u05e8\u05e0\u05e1\u05ea\u05e0\u05d5."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u05d2\u05dd \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d5\u05d2\u05dd \u05d0\u05ea\u05dd \u05e2\u05d3\u05d9\u05dd \u05dc\u05db\u05da \u05e9\u05e0\u05d4\u05d2\u05e0\u05d5 \u05e2\u05dd \u05db\u05dc \u05d0\u05d7\u05d3 \u05de\u05db\u05dd \u05d1\u05d9\u05d5\u05e9\u05e8, \u05e7\u05d3\u05d5\u05e9\u05d4 \u05d5\u05d0\u05d4\u05d1\u05d4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u05d4\u05d6\u05d4\u05e8\u05e0\u05d5 \u05d0\u05ea\u05db\u05dd \u05db\u05d0\u05d1 \u05e9\u05de\u05d6\u05d4\u05d9\u05e8 \u05d0\u05ea \u05d9\u05dc\u05d3\u05d9\u05d5 \u2013 \u05d4\u05d6\u05d5\u05db\u05e8\u05d9\u05dd \u05d0\u05ea\u05dd? \u2013 \u05d4\u05ea\u05d7\u05e0\u05e0\u05d5 \u05dc\u05e4\u05e0\u05d9\u05db\u05dd, \u05e2\u05d5\u05d3\u05d3\u05e0\u05d5 \u05d0\u05ea\u05db\u05dd, \u05d5\u05d0\u05e3 \u05d3\u05e8\u05e9\u05e0\u05d5 \u05de\u05db\u05dd"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u05dc\u05d7\u05d9\u05d5\u05ea \u05d0\u05ea \u05d7\u05d9\u05d9\u05db\u05dd \u05d1\u05e6\u05d5\u05e8\u05d4 \u05e8\u05d0\u05d5\u05d9\u05d4 \u05d1\u05e2\u05d9\u05e0\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e9\u05de\u05d6\u05de\u05d9\u05df \u05d0\u05ea\u05db\u05dd \u05dc\u05d7\u05dc\u05e7 \u05e2\u05de\u05d5 \u05d0\u05ea \u05db\u05d1\u05d5\u05d3\u05d5 \u05d5\u05de\u05dc\u05db\u05d5\u05ea\u05d5."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u05d0\u05e0\u05d5 \u05de\u05de\u05e9\u05d9\u05db\u05d9\u05dd \u05dc\u05d4\u05d5\u05d3\u05d5\u05ea \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e2\u05dc \u05e9\u05dc\u05d0 \u05d4\u05ea\u05d9\u05d9\u05d7\u05e1\u05ea\u05dd \u05dc\u05d1\u05e9\u05d5\u05e8\u05d4 \u05e9\u05d4\u05d1\u05d0\u05e0\u05d5 \u05dc\u05db\u05dd \u05db\u05d0\u05d9\u05dc\u05d5 \u05d4\u05de\u05e6\u05d0\u05e0\u05d5 \u05d0\u05d5\u05ea\u05d4 \u05d1\u05e2\u05e6\u05de\u05e0\u05d5, \u05d0\u05dc\u05d0 \u05d4\u05d0\u05de\u05e0\u05ea\u05dd \u05e9\u05d0\u05dc\u05d4 \u05d4\u05dd \u05d1\u05d0\u05de\u05ea \u05d3\u05d1\u05e8\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d7\u05d9\u05d9\u05dd, \u05d5\u05e2\u05dc \u05db\u05df \u05d4\u05dd \u05e9\u05d9\u05e0\u05d5 \u05d0\u05ea \u05d7\u05d9\u05d9\u05db\u05dd."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u05d5\u05d0\u05d6, \u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d4\u05d7\u05dc\u05d5 \u05d1\u05e0\u05d9 \u05e2\u05de\u05db\u05dd \u05dc\u05d4\u05e6\u05d9\u05e7 \u05dc\u05db\u05dd \u05d5\u05dc\u05e8\u05d3\u05d5\u05e3 \u05d0\u05ea\u05db\u05dd, \u05db\u05e4\u05d9 \u05e9\u05e7\u05d4\u05d9\u05dc\u05d5\u05ea \u05d4\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d1\u05d9\u05d4\u05d5\u05d3\u05d4 \u05e0\u05e8\u05d3\u05e4\u05d5 \u05e2\u05dc\u05be\u05d9\u05d3\u05d9 \u05d1\u05e0\u05d9\u05be\u05e2\u05de\u05dd \u05d4\u05d9\u05d4\u05d5\u05d3\u05d9\u05dd."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u05dc\u05d0\u05d7\u05e8 \u05e9\u05d4\u05e8\u05d2\u05d5 \u05d0\u05ea \u05e0\u05d1\u05d9\u05d0\u05d9\u05d4\u05dd \u05d4\u05e8\u05d2\u05d5 \u05d2\u05dd \u05d0\u05ea \u05d4\u05d0\u05d3\u05d5\u05df \u05d9\u05e9\u05d5\u05e2, \u05d5\u05d0\u05e3 \u05d2\u05d9\u05e8\u05e9\u05d5 \u05d5\u05e8\u05d3\u05e4\u05d5 \u05d0\u05d5\u05ea\u05e0\u05d5. \u05d4\u05dd \u05e2\u05d5\u05e9\u05d9\u05dd \u05d0\u05ea \u05d4\u05e8\u05e2 \u05d1\u05e2\u05d9\u05e0\u05d9 \u05d4\u05d0\u05d3\u05dd \u05d5\u05d1\u05e2\u05d9\u05e0\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd,"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u05d5\u05d0\u05d9\u05e0\u05dd \u05de\u05e0\u05d9\u05d7\u05d9\u05dd \u05dc\u05e0\u05d5 \u05dc\u05d1\u05e9\u05e8 \u05dc\u05d2\u05d5\u05d9\u05d9\u05dd, \u05de\u05d7\u05e9\u05e9 \u05e9\u05d0\u05d7\u05d3\u05d9\u05dd \u05de\u05d4\u05dd \u05d9\u05d9\u05d5\u05d5\u05e9\u05e2\u05d5. \u05db\u05da \u05de\u05ea\u05e8\u05d1\u05d9\u05dd \u05d7\u05d8\u05d0\u05d9\u05d4\u05dd \u05e2\u05d3 \u05e9\u05dc\u05d1\u05e1\u05d5\u05e3 \u05d9\u05e9\u05e4\u05d5\u05da \u05e2\u05dc\u05d9\u05d4\u05dd \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05ea \u05d6\u05e2\u05de\u05d5."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d6\u05de\u05df \u05e7\u05e6\u05e8 \u05dc\u05d0\u05d7\u05e8 \u05e9\u05e0\u05e4\u05e8\u05d3\u05e0\u05d5 \u05de\u05db\u05dd (\u05d1\u05d2\u05d5\u05e4\u05e0\u05d5 \u05d0\u05da \u05dc\u05d0 \u05d1\u05e0\u05e4\u05e9\u05e0\u05d5!) \u05ea\u05e7\u05e4\u05d5 \u05d0\u05d5\u05ea\u05e0\u05d5 \u05d2\u05e2\u05d2\u05d5\u05e2\u05d9\u05dd \u05e2\u05d6\u05d9\u05dd \u05d0\u05dc\u05d9\u05db\u05dd,"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u05d5\u05e0\u05db\u05e1\u05e4\u05e0\u05d5 \u05dc\u05e9\u05d5\u05d1 \u05d5\u05dc\u05e8\u05d0\u05d5\u05ea\u05db\u05dd. \u05d0\u05e0\u05d9, \u05e4\u05d5\u05dc\u05d5\u05e1, \u05e0\u05d9\u05e1\u05d9\u05ea\u05d9 \u05dc\u05e9\u05d5\u05d1 \u05d0\u05dc\u05d9\u05db\u05dd \u05e4\u05e2\u05de\u05d9\u05dd \u05e8\u05d1\u05d5\u05ea, \u05d0\u05da \u05d4\u05e9\u05d8\u05df \u05de\u05e0\u05e2 \u05d6\u05d0\u05ea \u05de\u05d0\u05d9\u05ea\u05e0\u05d5."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u05db\u05db\u05dc\u05d5\u05ea \u05d4\u05db\u05d5\u05dc \u05d0\u05ea\u05dd \u05ea\u05e7\u05d5\u05d5\u05ea\u05e0\u05d5, \u05e9\u05de\u05d7\u05ea\u05e0\u05d5 \u05d5\u05d2\u05d0\u05d5\u05d5\u05ea\u05e0\u05d5 \u05dc\u05e0\u05d5\u05db\u05d7 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05d1\u05d1\u05d5\u05d0\u05d5."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u05e9\u05db\u05df \u05d4\u05d9\u05e0\u05db\u05dd \u05db\u05d1\u05d5\u05d3\u05e0\u05d5 \u05d5\u05e9\u05de\u05d7\u05ea\u05e0\u05d5."
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u05db\u05d0\u05e9\u05e8 \u05dc\u05d0 \u05d9\u05db\u05d5\u05dc\u05e0\u05d5 \u05d9\u05d5\u05ea\u05e8 \u05dc\u05d4\u05ea\u05d0\u05e4\u05e7 \u05d4\u05d7\u05dc\u05d8\u05e0\u05d5 \u05dc\u05d4\u05d9\u05e9\u05d0\u05e8 \u05db\u05d0\u05df \u05d1\u05d0\u05ea\u05d5\u05e0\u05d4,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "verse doesn't exist in this bible version"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "verse doesn't exist in this bible version"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u05db\u05e9\u05d4\u05d9\u05d9\u05e0\u05d5 \u05d0\u05e6\u05dc\u05db\u05dd \u05d4\u05d6\u05d4\u05e8\u05e0\u05d5 \u05d0\u05ea\u05db\u05dd \u05de\u05e8\u05d0\u05e9 \u05de\u05e4\u05e0\u05d9 \u05db\u05dc \u05d4\u05e6\u05e8\u05d5\u05ea \u05d4\u05d0\u05dc\u05d4, \u05d5\u05d0\u05db\u05df \u05d3\u05d1\u05e8\u05d9\u05e0\u05d5 \u05d4\u05ea\u05e7\u05d9\u05d9\u05de\u05d5."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u05de\u05e9\u05d5\u05dd \u05db\u05da \u05dc\u05d0 \u05d9\u05db\u05d5\u05dc\u05ea\u05d9 \u05dc\u05d4\u05ea\u05d0\u05e4\u05e7 \u05d9\u05d5\u05ea\u05e8 \u05d5\u05e9\u05dc\u05d7\u05ea\u05d9 \u05d0\u05dc\u05d9\u05db\u05dd \u05d0\u05ea \u05d8\u05d9\u05de\u05d5\u05ea\u05d9\u05d5\u05e1, \u05db\u05d3\u05d9 \u05dc\u05d1\u05e8\u05e8 \u05d0\u05dd \u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd \u05e2\u05d3\u05d9\u05d9\u05df \u05d0\u05d9\u05ea\u05e0\u05d4. \u05d7\u05e9\u05e9\u05ea\u05d9 \u05e9\u05de\u05d0 \u05d9\u05e0\u05e1\u05d4 \u05d4\u05e9\u05d8\u05df \u05dc\u05e4\u05ea\u05d5\u05ea\u05db\u05dd \u05d5\u05db\u05dc \u05e2\u05d1\u05d5\u05d3\u05ea\u05e0\u05d5 \u05ea\u05d4\u05d9\u05d4 \u05dc\u05e9\u05d5\u05d5\u05d0."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u05d0\u05da \u05e2\u05ea\u05d4 \u05d8\u05d9\u05de\u05d5\u05ea\u05d9\u05d5\u05e1 \u05d7\u05d6\u05e8 \u05d5\u05d4\u05d1\u05d9\u05d0 \u05e2\u05de\u05d5 \u05d0\u05ea \u05d4\u05d7\u05d3\u05e9\u05d5\u05ea \u05d4\u05e0\u05e4\u05dc\u05d0\u05d5\u05ea \u05e9\u05d0\u05ea\u05dd \u05d0\u05d9\u05ea\u05e0\u05d9\u05dd \u05d1\u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd \u05d5\u05d0\u05d4\u05d1\u05ea\u05db\u05dd, \u05e9\u05d0\u05ea\u05dd \u05d6\u05d5\u05db\u05e8\u05d9\u05dd \u05dc\u05d8\u05d5\u05d1\u05d4 \u05d0\u05ea \u05d1\u05d9\u05e7\u05d5\u05e8\u05e0\u05d5, \u05d5\u05d0\u05e3 \u05de\u05d9\u05d9\u05d7\u05dc\u05d9\u05dd \u05dc\u05e8\u05d0\u05d5\u05ea\u05e0\u05d5 \u05e9\u05e0\u05d9\u05ea \u05db\u05e4\u05d9 \u05e9\u05d0\u05e0\u05d5 \u05de\u05d9\u05d9\u05d7\u05dc\u05d9\u05dd \u05dc\u05e8\u05d0\u05d5\u05ea\u05db\u05dd."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd \u05d4\u05d7\u05d6\u05e7\u05d4 \u05e0\u05d9\u05d7\u05de\u05d4 \u05d0\u05d5\u05ea\u05e0\u05d5 \u05d1\u05db\u05dc \u05d4\u05e6\u05e8\u05d5\u05ea \u05d5\u05d4\u05e1\u05d1\u05dc \u05e9\u05e2\u05d5\u05d1\u05e8\u05d9\u05dd \u05e2\u05dc\u05d9\u05e0\u05d5 \u05db\u05d0\u05df;"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u05db\u05dc \u05e2\u05d5\u05d3 \u05d0\u05e0\u05d5 \u05d9\u05d5\u05d3\u05e2\u05d9\u05dd \u05e9\u05d0\u05ea\u05dd \u05e0\u05d0\u05de\u05e0\u05d9\u05dd \u05dc\u05d0\u05d3\u05d5\u05df, \u05e7\u05dc \u05dc\u05e0\u05d5 \u05d9\u05d5\u05ea\u05e8 \u05dc\u05e9\u05d0\u05ea \u05d0\u05ea \u05d4\u05e1\u05d1\u05dc."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u05d0\u05d9\u05e0\u05e0\u05d5 \u05d9\u05d5\u05d3\u05e2\u05d9\u05dd \u05db\u05d9\u05e6\u05d3 \u05dc\u05d4\u05d5\u05d3\u05d5\u05ea \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e2\u05dc \u05d4\u05e9\u05de\u05d7\u05d4 \u05d5\u05d4\u05d0\u05d5\u05e9\u05e8 \u05e9\u05d2\u05e8\u05de\u05ea\u05dd \u05dc\u05e0\u05d5."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u05d0\u05e0\u05d7\u05e0\u05d5 \u05de\u05ea\u05e4\u05dc\u05dc\u05d9\u05dd \u05d9\u05d5\u05de\u05dd \u05d5\u05dc\u05d9\u05dc\u05d4 \u05d5\u05de\u05ea\u05d7\u05e0\u05e0\u05d9\u05dd \u05dc\u05e4\u05e0\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e9\u05d9\u05e0\u05d9\u05d7 \u05dc\u05e0\u05d5 \u05dc\u05e8\u05d0\u05d5\u05ea\u05db\u05dd \u05e9\u05e0\u05d9\u05ea, \u05d5\u05e9\u05d9\u05d0\u05e4\u05e9\u05e8 \u05dc\u05e0\u05d5 \u05dc\u05d4\u05e9\u05dc\u05d9\u05dd \u05d0\u05ea \u05d4\u05d7\u05e1\u05e8 \u05d1\u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u05de\u05d9 \u05d9\u05d9\u05ea\u05df \u05e9\u05d0\u05dc\u05d5\u05d4\u05d9\u05e0\u05d5 \u05d5\u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05d9\u05d7\u05d6\u05d9\u05e8\u05e0\u05d5 \u05d0\u05dc\u05d9\u05db\u05dd,"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u05d5\u05e9\u05d9\u05d1\u05e8\u05da \u05d0\u05ea\u05db\u05dd \u05d5\u05d9\u05e8\u05d1\u05d4 \u05d0\u05ea \u05d0\u05d4\u05d1\u05ea\u05db\u05dd \u05d0\u05d9\u05e9 \u05dc\u05e8\u05e2\u05d4\u05d5 \u05d5\u05dc\u05db\u05dc \u05d0\u05d3\u05dd \u2013 \u05db\u05e9\u05dd \u05e9\u05d0\u05e0\u05d5 \u05d0\u05d5\u05d4\u05d1\u05d9\u05dd \u05d0\u05ea\u05db\u05dd."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u05db\u05da \u05d9\u05d7\u05d6\u05e7 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05ea \u05dc\u05d1\u05db\u05dd, \u05d9\u05d8\u05d4\u05e8\u05db\u05dd \u05d5\u05d9\u05e7\u05d3\u05e9\u05db\u05dd, \u05db\u05d3\u05d9 \u05e9\u05d1\u05e9\u05d5\u05d1 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05ea\u05e2\u05de\u05d3\u05d5 \u05dc\u05e4\u05e0\u05d9\u05d5 \u05e0\u05e7\u05d9\u05d9\u05dd \u05de\u05d7\u05d8\u05d0 \u05d9\u05d7\u05d3 \u05e2\u05dd \u05db\u05dc \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd."
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u05d4\u05e8\u05e9\u05d5 \u05dc\u05d9 \u05dc\u05d5\u05de\u05e8 \u05e2\u05d5\u05d3 \u05d3\u05d1\u05e8, \u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd: \u05d0\u05e0\u05d5 \u05de\u05ea\u05e4\u05dc\u05dc\u05d9\u05dd \u05d5\u05de\u05ea\u05d7\u05e0\u05e0\u05d9\u05dd, \u05d1\u05e9\u05dd \u05d4\u05de\u05e9\u05d9\u05d7 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5, \u05e9\u05ea\u05de\u05e9\u05d9\u05db\u05d5 \u05d5\u05d0\u05e3 \u05ea\u05e8\u05d1\u05d5 \u05dc\u05e2\u05e9\u05d5\u05ea \u05d0\u05ea \u05d4\u05d8\u05d5\u05d1 \u05d1\u05e2\u05d9\u05e0\u05d9 \u05d4\u05f3, \u05db\u05e4\u05d9 \u05e9\u05dc\u05d9\u05de\u05d3\u05e0\u05d5 \u05d0\u05ea\u05db\u05dd."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u05d4\u05dc\u05d0 \u05d6\u05d5\u05db\u05e8\u05d9\u05dd \u05d0\u05ea\u05dd \u05d0\u05ea \u05d4\u05de\u05e6\u05d5\u05d5\u05ea \u05d5\u05d4\u05d4\u05d5\u05e8\u05d0\u05d5\u05ea \u05e9\u05e0\u05ea\u05e0\u05d5 \u05dc\u05db\u05dd \u05d1\u05e9\u05dd \u05d9\u05e9\u05d5\u05e2 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "verse doesn't exist in this bible version"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "verse doesn't exist in this bible version"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u05d5\u05dc\u05d0 \u05de\u05ea\u05d5\u05da \u05ea\u05d0\u05d5\u05d5\u05ea \u05d4\u05d1\u05e9\u05e8 \u05db\u05de\u05d5 \u05d4\u05d2\u05d5\u05d9\u05d9\u05dd \u05e9\u05d0\u05d9\u05e0\u05dd \u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05d1\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d3\u05d5\u05e8\u05e9 \u05e9\u05dc\u05d0 \u05ea\u05e8\u05de\u05d5 \u05d1\u05e2\u05e0\u05d9\u05d9\u05df \u05d6\u05d4 \u05d5\u05e9\u05dc\u05d0 \u05ea\u05e0\u05e1\u05d5 \u05dc\u05e4\u05ea\u05d5\u05ea \u05e0\u05e9\u05d9\u05dd \u05e0\u05e9\u05d5\u05d0\u05d5\u05ea. \u05d0\u05dd \u05ea\u05de\u05e8\u05d5 \u05d0\u05ea \u05e4\u05d9\u05d5 \u05d4\u05d5\u05d0 \u05d9\u05e2\u05e0\u05d9\u05e9 \u05d0\u05ea\u05db\u05dd \u05d1\u05e2\u05d5\u05e0\u05e9 \u05d7\u05de\u05d5\u05e8 \u05de\u05d0\u05d5\u05d3, \u05db\u05e4\u05d9 \u05e9\u05db\u05d1\u05e8 \u05d0\u05de\u05e8\u05e0\u05d5 \u05dc\u05db\u05dd."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u05e9\u05d4\u05e8\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05dc\u05d0 \u05d1\u05d7\u05e8 \u05d1\u05e0\u05d5 \u05db\u05d3\u05d9 \u05e9\u05e0\u05d7\u05d9\u05d4 \u05d7\u05d9\u05d9 \u05d6\u05e0\u05d5\u05ea \u05d5\u05e0\u05d9\u05d0\u05d5\u05e3, \u05d0\u05dc\u05d0 \u05db\u05d3\u05d9 \u05e9\u05e0\u05d4\u05d9\u05d4 \u05e7\u05d3\u05d5\u05e9\u05d9\u05dd \u05d5\u05d8\u05d4\u05d5\u05e8\u05d9\u05dd."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u05dc\u05db\u05df \u05de\u05d9 \u05e9\u05de\u05e7\u05dc \u05d1\u05e2\u05e0\u05d9\u05d9\u05df \u05d6\u05d4 \u05d5\u05de\u05de\u05e8\u05d4 \u05d0\u05ea \u05e4\u05d9 \u05d4\u05f3, \u05d0\u05d9\u05e0\u05d5 \u05d7\u05d5\u05d8\u05d0 \u05e0\u05d2\u05d3 \u05d1\u05e0\u05d9\u05be\u05d0\u05d3\u05dd, \u05d0\u05dc\u05d0 \u05e0\u05d2\u05d3 \u05d4\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05e9\u05e8 \u05d4\u05e9\u05db\u05d9\u05df \u05d1\u05e7\u05e8\u05d1\u05e0\u05d5 \u05d0\u05ea \u05e8\u05d5\u05d7 \u05d4\u05e7\u05d5\u05d3\u05e9."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u05d0\u05d9\u05e0\u05e0\u05d5 \u05e6\u05e8\u05d9\u05db\u05d9\u05dd \u05dc\u05d4\u05d5\u05db\u05d9\u05d7 \u05d0\u05ea\u05db\u05dd \u05d1\u05db\u05dc \u05d4\u05e0\u05d5\u05d2\u05e2 \u05dc\u05d0\u05d4\u05d1\u05ea \u05d4\u05d0\u05d7\u05d9\u05dd \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd, \u05db\u05d9 \u05d0\u05e0\u05d9 \u05d9\u05d5\u05d3\u05e2 \u05d4\u05d9\u05d8\u05d1 \u05e9\u05d4\u05f3 \u05e2\u05e6\u05de\u05d5 \u05de\u05dc\u05de\u05d3 \u05d0\u05ea\u05db\u05dd \u05dc\u05d0\u05d4\u05d5\u05d1 \u05d0\u05d9\u05e9 \u05d0\u05ea \u05d0\u05d7\u05d9\u05d5,"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u05d5\u05d4\u05d5\u05db\u05d7\u05ea\u05dd \u05d6\u05d0\u05ea \u05d1\u05d0\u05d4\u05d1\u05ea\u05db\u05dd \u05d4\u05e8\u05d1\u05d4 \u05dc\u05db\u05dc \u05d4\u05d0\u05d7\u05d9\u05dd \u05d1\u05de\u05e7\u05d3\u05d5\u05e0\u05d9\u05d4. \u05d1\u05db\u05dc \u05d6\u05d0\u05ea \u05d0\u05e0\u05d5 \u05de\u05d1\u05e7\u05e9\u05d9\u05dd \u05de\u05db\u05dd \u05dc\u05d0\u05d4\u05d5\u05d1 \u05d9\u05d5\u05ea\u05e8,"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u05dc\u05e9\u05d0\u05d5\u05e3 \u05dc\u05d7\u05d9\u05d9\u05dd \u05e9\u05e7\u05d8\u05d9\u05dd, \u05dc\u05d0 \u05dc\u05d7\u05d8\u05d8 \u05d1\u05e2\u05e0\u05d9\u05d9\u05e0\u05d9\u05d4\u05dd \u05e9\u05dc \u05d0\u05d7\u05e8\u05d9\u05dd, \u05d5\u05dc\u05d4\u05e8\u05d5\u05d5\u05d9\u05d7 \u05d0\u05ea \u05e4\u05e8\u05e0\u05e1\u05ea\u05db\u05dd \u05d1\u05e2\u05d1\u05d5\u05d3\u05ea\u05be\u05db\u05e4\u05d9\u05d9\u05dd, \u05db\u05e4\u05d9 \u05e9\u05db\u05d1\u05e8 \u05d1\u05d9\u05e7\u05e9\u05e0\u05d5 \u05de\u05db\u05dd,"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u05db\u05d3\u05d9 \u05e9\u05ea\u05e8\u05db\u05e9\u05d5 \u05d0\u05ea \u05d0\u05de\u05d5\u05e0\u05dd \u05e9\u05dc \u05d4\u05dc\u05d0\u05be\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd, \u05d5\u05db\u05d3\u05d9 \u05e9\u05ea\u05d4\u05d9\u05d5 \u05e2\u05e6\u05de\u05d0\u05d9\u05dd \u05de\u05d1\u05d7\u05d9\u05e0\u05d4 \u05db\u05e1\u05e4\u05d9\u05ea \u05d5\u05dc\u05d0 \u05ea\u05dc\u05d5\u05d9\u05d9\u05dd \u05d1\u05d7\u05e1\u05d3\u05dd \u05e9\u05dc \u05d0\u05d7\u05e8\u05d9\u05dd."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d1\u05e8\u05e6\u05d5\u05e0\u05d9 \u05dc\u05d5\u05de\u05e8 \u05dc\u05db\u05dd \u05de\u05d4 \u05e7\u05d5\u05e8\u05d4 \u05dc\u05de\u05d0\u05de\u05d9\u05df \u05db\u05e9\u05d4\u05d5\u05d0 \u05e0\u05e4\u05d8\u05e8, \u05db\u05d3\u05d9 \u05e9\u05dc\u05d0 \u05ea\u05ea\u05d0\u05d1\u05dc\u05d5 \u05de\u05e8\u05d4 \u05e2\u05dc \u05d4\u05de\u05ea \u05db\u05de\u05d5 \u05d4\u05dc\u05d0\u05be\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05e9\u05d0\u05d9\u05df \u05dc\u05d4\u05dd \u05ea\u05e7\u05d5\u05d5\u05d4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u05db\u05e9\u05dd \u05e9\u05d0\u05e0\u05d5 \u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05db\u05d9 \u05d9\u05e9\u05d5\u05e2 \u05de\u05ea \u05d5\u05dc\u05d0\u05d7\u05e8 \u05de\u05db\u05df \u05e7\u05dd \u05dc\u05ea\u05d7\u05d9\u05d9\u05d4, \u05db\u05da \u05d0\u05e0\u05d5 \u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05e9\u05db\u05d0\u05e9\u05e8 \u05d9\u05e9\u05d5\u05e2 \u05d9\u05d7\u05d6\u05d5\u05e8, \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d9\u05e9\u05d9\u05d1 \u05d0\u05d9\u05ea\u05d5 \u05d0\u05ea \u05db\u05dc \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05d1\u05d5 \u05e9\u05de\u05ea\u05d5."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d2\u05d9\u05dc\u05d4 \u05dc\u05e0\u05d5 \u05d0\u05ea \u05d4\u05e2\u05d5\u05d1\u05d3\u05d5\u05ea \u05d4\u05d1\u05d0\u05d5\u05ea: \u05d0\u05dc\u05d4 \u05e9\u05e0\u05e9\u05d0\u05e8\u05d9\u05dd \u05d1\u05d7\u05d9\u05d9\u05dd \u05d1\u05d1\u05d5\u05d0 \u05d4\u05d0\u05d3\u05d5\u05df \u05dc\u05d0 \u05d9\u05e4\u05d2\u05e9\u05d5\u05d4\u05d5 \u05dc\u05e4\u05e0\u05d9 \u05d4\u05de\u05ea\u05d9\u05dd."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u05d4\u05d0\u05d3\u05d5\u05df \u05e2\u05e6\u05de\u05d5 \u05d9\u05d9\u05e8\u05d3 \u05d0\u05dc\u05d9\u05e0\u05d5 \u05de\u05d4\u05e9\u05de\u05d9\u05dd \u05d1\u05ea\u05e8\u05d5\u05e2\u05d4 \u05d2\u05d3\u05d5\u05dc\u05d4, \u05dc\u05e7\u05e8\u05d9\u05d0\u05ea \u05e9\u05e8\u05be\u05d4\u05de\u05dc\u05d0\u05db\u05d9\u05dd \u05d5\u05dc\u05e7\u05d5\u05dc \u05ea\u05e7\u05d9\u05e2\u05ea \u05e9\u05d5\u05e4\u05e8 \u05d4\u05f3, \u05d5\u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05e9\u05de\u05ea\u05d5 \u05d9\u05e7\u05d5\u05de\u05d5 \u05e8\u05d0\u05e9\u05d5\u05e0\u05d9\u05dd \u05dc\u05e4\u05d2\u05e9\u05d5."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u05dc\u05d0\u05d7\u05e8 \u05de\u05db\u05df \u05d4\u05e0\u05d5\u05ea\u05e8\u05d9\u05dd \u05d1\u05d7\u05d9\u05d9\u05dd \u05d9\u05d9\u05dc\u05e7\u05d7\u05d5 \u05d9\u05d7\u05d3 \u05d0\u05d9\u05ea\u05dd \u05d1\u05e2\u05e0\u05e0\u05d9\u05dd, \u05d9\u05e4\u05d2\u05e9\u05d5 \u05d0\u05ea \u05d4\u05d0\u05d3\u05d5\u05df \u05d1\u05d0\u05d5\u05d5\u05d9\u05e8 \u05d5\u05d9\u05d9\u05e9\u05d0\u05e8\u05d5 \u05d0\u05d9\u05ea\u05d5 \u05dc\u05e0\u05e6\u05d7."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u05e2\u05dc \u05db\u05df \u05e0\u05d7\u05de\u05d5 \u05d5\u05e2\u05d5\u05d3\u05d3\u05d5 \u05d0\u05d9\u05e9 \u05d0\u05ea \u05e8\u05e2\u05d4\u05d5 \u05d1\u05d3\u05d1\u05e8\u05d9\u05dd \u05d0\u05dc\u05d4."
                }
            }
        },
        "5": {
            "chapter_nr": "5",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d0\u05d9\u05e0\u05d9 \u05e6\u05e8\u05d9\u05da \u05dc\u05db\u05ea\u05d5\u05d1 \u05dc\u05db\u05dd \u05de\u05ea\u05d9 \u05db\u05dc \u05d6\u05d4 \u05d9\u05ea\u05e8\u05d7\u05e9,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u05db\u05d9 \u05d4\u05dc\u05d0 \u05d9\u05d5\u05d3\u05e2\u05d9\u05dd \u05d0\u05ea\u05dd \u05e9\u05d0\u05d9\u05e9 \u05d0\u05d9\u05e0\u05d5 \u05d9\u05d5\u05d3\u05e2 \u05d0\u05ea \u05d4\u05de\u05d5\u05e2\u05d3. \u05d9\u05d5\u05dd \u05d4\u05f3 \u05d9\u05d1\u05d5\u05d0 \u05d1\u05de\u05e4\u05ea\u05d9\u05e2 \u2013 \u05db\u05d2\u05e0\u05d1 \u05d1\u05dc\u05d9\u05dc\u05d4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u05db\u05d0\u05e9\u05e8 \u05d1\u05e0\u05d9\u05be\u05d4\u05d0\u05d3\u05dd \u05d9\u05d7\u05d9\u05d5 \u05d1\u05e9\u05dc\u05d5\u05d5\u05d4 \u05d5\u05d9\u05d7\u05e9\u05d1\u05d5 \u05e9\u05dc\u05d0 \u05d9\u05e7\u05e8\u05d4 \u05d3\u05d1\u05e8, \u05d9\u05d9\u05e8\u05d3 \u05e2\u05dc\u05d9\u05d4\u05dd \u05d4\u05d0\u05e1\u05d5\u05df \u05d1\u05de\u05e4\u05ea\u05d9\u05e2, \u05db\u05d9\u05d5\u05dc\u05d3\u05ea \u05e9\u05d0\u05d9\u05e0\u05d4 \u05d9\u05d5\u05d3\u05e2\u05ea \u05de\u05ea\u05d9 \u05d9\u05ea\u05e7\u05e4\u05d5\u05d4 \u05e6\u05d9\u05e8\u05d9 \u05d4\u05dc\u05d9\u05d3\u05d4, \u05d5\u05d0\u05d9\u05e9 \u05dc\u05d0 \u05d9\u05d5\u05db\u05dc \u05dc\u05d1\u05e8\u05d5\u05d7."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u05d0\u05d1\u05dc \u05d0\u05ea\u05dd, \u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d0\u05d9\u05e0\u05db\u05dd \u05e9\u05e8\u05d5\u05d9\u05d9\u05dd \u05d1\u05d7\u05e9\u05db\u05d4 \u05d1\u05db\u05dc \u05d4\u05e0\u05d5\u05d2\u05e2 \u05dc\u05d3\u05d1\u05e8\u05d9\u05dd \u05d0\u05dc\u05d4, \u05d5\u05dc\u05db\u05df \u05d9\u05d5\u05dd \u05d4\u05f3 \u05dc\u05d0 \u05d9\u05e4\u05ea\u05d9\u05e2 \u05d0\u05ea\u05db\u05dd \u05db\u05d2\u05e0\u05d1."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u05db\u05d5\u05dc\u05db\u05dd \u05d1\u05e0\u05d9\u05be\u05d4\u05d0\u05d5\u05e8 \u05d5\u05d1\u05e0\u05d9\u05be\u05d4\u05d9\u05d5\u05dd; \u05d0\u05d9\u05e0\u05db\u05dd \u05d1\u05e0\u05d9\u05be\u05d4\u05dc\u05d9\u05dc\u05d4 \u05d5\u05d0\u05d9\u05e0\u05db\u05dd \u05e9\u05d9\u05d9\u05db\u05d9\u05dd \u05dc\u05d7\u05d5\u05e9\u05da."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u05de\u05e9\u05d5\u05dd \u05db\u05da \u05e2\u05de\u05d3\u05d5 \u05e2\u05dc \u05d4\u05de\u05e9\u05de\u05e8! \u05d0\u05dc \u05ea\u05d9\u05e8\u05d3\u05de\u05d5 \u05db\u05de\u05d5 \u05d4\u05d0\u05d7\u05e8\u05d9\u05dd, \u05d0\u05dc\u05d0 \u05e6\u05e4\u05d5 \u05dc\u05d1\u05d5\u05d0\u05d5 \u05d1\u05e9\u05e7\u05d9\u05d3\u05d4 \u05d5\u05d1\u05e2\u05e8\u05e0\u05d5\u05ea,"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u05db\u05d9 \u05d0\u05dc\u05d4 \u05e9\u05e0\u05e8\u05d3\u05de\u05d9\u05dd \u05e0\u05e8\u05d3\u05de\u05d9\u05dd \u05d1\u05dc\u05d9\u05dc\u05d4 \u05d5\u05d4\u05e9\u05d9\u05db\u05d5\u05e8\u05d9\u05dd \u05de\u05e9\u05ea\u05db\u05e8\u05d9\u05dd \u05d1\u05dc\u05d9\u05dc\u05d4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u05d0\u05d5\u05dc\u05dd \u05d0\u05e0\u05d7\u05e0\u05d5 \u05d1\u05e0\u05d9 \u05d0\u05d5\u05e8\u05be\u05d4\u05d9\u05d5\u05dd, \u05d5\u05e2\u05dc \u05db\u05df \u05e0\u05d9\u05e9\u05d0\u05e8 \u05e2\u05e8\u05d9\u05dd \u05d5\u05de\u05e4\u05d5\u05db\u05d7\u05d9\u05dd, \u05de\u05d5\u05d2\u05e0\u05d9\u05dd \u05e2\u05dc\u05be\u05d9\u05d3\u05d9 \u05d0\u05de\u05d5\u05e0\u05ea\u05e0\u05d5 \u05d5\u05d0\u05d4\u05d1\u05ea\u05e0\u05d5 \u05d5\u05d7\u05d5\u05e1\u05d9\u05dd \u05d1\u05ea\u05e7\u05d5\u05d5\u05d4 \u05d4\u05e0\u05e4\u05dc\u05d0\u05d4 \u05e9\u05dc \u05d9\u05e9\u05d5\u05e2\u05ea\u05e0\u05d5."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05dc\u05d0 \u05d1\u05d7\u05e8 \u05d1\u05e0\u05d5 \u05db\u05d3\u05d9 \u05dc\u05e9\u05e4\u05d5\u05da \u05d0\u05ea \u05d6\u05e2\u05de\u05d5 \u05e2\u05dc\u05d9\u05e0\u05d5, \u05d0\u05dc\u05d0 \u05db\u05d3\u05d9 \u05dc\u05d4\u05d5\u05e9\u05d9\u05e2\u05e0\u05d5 \u05e2\u05dc\u05be\u05d9\u05d3\u05d9 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7,"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u05d0\u05e9\u05e8 \u05de\u05ea \u05d1\u05e2\u05d3\u05e0\u05d5 \u05dc\u05de\u05e2\u05df \u05e0\u05d7\u05d9\u05d4 \u05d0\u05d9\u05ea\u05d5 \u05dc\u05e0\u05e6\u05d7, \u05d1\u05d9\u05df \u05d0\u05dd \u05e0\u05d4\u05d9\u05d4 \u05d7\u05d9\u05d9\u05dd \u05d0\u05d5 \u05de\u05ea\u05d9\u05dd \u05d1\u05e9\u05d5\u05d1\u05d5."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u05dc\u05db\u05df \u05d4\u05de\u05e9\u05d9\u05db\u05d5 \u05dc\u05e2\u05d5\u05d3\u05d3 \u05d5\u05dc\u05d1\u05e0\u05d5\u05ea \u05d0\u05d9\u05e9 \u05d0\u05ea \u05d0\u05d7\u05d9\u05d5 \u05db\u05e4\u05d9 \u05e9\u05e2\u05e9\u05d9\u05ea\u05dd \u05e2\u05d3 \u05db\u05d4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d0\u05e0\u05d5 \u05de\u05d1\u05e7\u05e9\u05d9\u05dd \u05de\u05db\u05dd \u05dc\u05db\u05d1\u05d3 \u05d0\u05ea \u05d4\u05d0\u05d7\u05e8\u05d0\u05d9\u05dd \u05e2\u05dc\u05d9\u05db\u05dd \u05de\u05d8\u05e2\u05dd \u05d4\u05e7\u05d4\u05d9\u05dc\u05d4, \u05db\u05d9 \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05de\u05d9\u05e0\u05d4 \u05d0\u05d5\u05ea\u05dd \u05dc\u05d4\u05e9\u05d2\u05d9\u05d7 \u05e2\u05dc\u05d9\u05db\u05dd \u05d5\u05dc\u05d4\u05d5\u05db\u05d9\u05d7 \u05d0\u05ea\u05db\u05dd \u05d1\u05e2\u05ea \u05d4\u05e6\u05d5\u05e8\u05da."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u05e2\u05dc\u05d9\u05db\u05dd \u05dc\u05d4\u05e2\u05e8\u05d9\u05da \u05d0\u05ea \u05d4\u05e2\u05d1\u05d5\u05d3\u05d4 \u05e9\u05d4\u05dd \u05e2\u05d5\u05e9\u05d9\u05dd \u05dc\u05de\u05e2\u05e0\u05db\u05dd \u05d5\u05dc\u05d0\u05d4\u05d5\u05d1 \u05d0\u05d5\u05ea\u05dd, \u05e9\u05db\u05df \u05d4\u05dd \u05de\u05e9\u05ea\u05d3\u05dc\u05d9\u05dd \u05dc\u05e2\u05d6\u05d5\u05e8 \u05dc\u05db\u05dd. \u05d6\u05db\u05e8\u05d5, \u05d7\u05d9\u05d5 \u05d1\u05e9\u05dc\u05d5\u05dd \u05d6\u05d4 \u05e2\u05dd \u05d6\u05d4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d4\u05d6\u05d4\u05d9\u05e8\u05d5 \u05d0\u05ea \u05d4\u05e2\u05e6\u05dc\u05e0\u05d9\u05dd \u05e9\u05d1\u05d9\u05e0\u05d9\u05db\u05dd, \u05e2\u05d5\u05d3\u05d3\u05d5 \u05d0\u05ea \u05d4\u05e4\u05d5\u05d7\u05d3\u05d9\u05dd, \u05d7\u05d6\u05e7\u05d5 \u05d0\u05ea \u05d4\u05d7\u05dc\u05e9\u05d9\u05dd \u05d5\u05d4\u05d9\u05d5 \u05e1\u05d1\u05dc\u05e0\u05d9\u05dd \u05e2\u05dd \u05db\u05d5\u05dc\u05dd."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u05d0\u05dc \u05ea\u05e0\u05d9\u05d7\u05d5 \u05dc\u05d0\u05d9\u05e9 \u05dc\u05e9\u05dc\u05dd \u05e8\u05e2\u05d4 \u05ea\u05d7\u05ea \u05e8\u05e2\u05d4, \u05d0\u05dc\u05d0 \u05e2\u05d6\u05e8\u05d5 \u05ea\u05de\u05d9\u05d3 \u05d0\u05d9\u05e9 \u05dc\u05d0\u05d7\u05d9\u05d5 \u05d5\u05dc\u05db\u05dc \u05d0\u05d3\u05dd."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u05e9\u05de\u05d7\u05d5 \u05ea\u05de\u05d9\u05d3 \u05d1\u05d0\u05de\u05d5\u05e0\u05ea\u05db\u05dd."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u05d4\u05ea\u05de\u05d9\u05d3\u05d5 \u05d1\u05ea\u05e4\u05d9\u05dc\u05d4."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u05d4\u05d5\u05d3\u05d5 \u05dc\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05e2\u05dc \u05db\u05dc \u05d3\u05d1\u05e8, \u05db\u05d9 \u05d6\u05d4\u05d5 \u05e8\u05e6\u05d5\u05df \u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05de\u05db\u05dc \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd \u05d1\u05de\u05e9\u05d9\u05d7."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u05d0\u05dc \u05ea\u05d1\u05dc\u05de\u05d5 \u05d5\u05d0\u05dc \u05ea\u05db\u05dc\u05d0\u05d5 \u05d0\u05ea \u05e8\u05d5\u05d7 \u05d4\u05e7\u05d5\u05d3\u05e9."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u05dc\u05e2\u05d5\u05dc\u05dd \u05d0\u05dc \u05ea\u05d6\u05dc\u05d6\u05dc\u05d5 \u05d1\u05de\u05d4 \u05e9\u05e0\u05d0\u05de\u05e8 \u05d1\u05e9\u05dd \u05d4\u05f3."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u05d1\u05d7\u05e0\u05d5 \u05db\u05dc \u05d3\u05d1\u05e8 \u05d5\u05d4\u05d0\u05de\u05d9\u05e0\u05d5 \u05e8\u05e7 \u05d1\u05de\u05d4 \u05e9\u05d8\u05d5\u05d1, \u05d9\u05e9\u05e8 \u05d5\u05d0\u05de\u05d9\u05ea\u05d9."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u05d4\u05ea\u05e8\u05d7\u05e7\u05d5 \u05de\u05db\u05dc \u05de\u05e2\u05e9\u05d4 \u05e8\u05e2."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u05d5\u05d0\u05dc\u05d5\u05d4\u05d9 \u05d4\u05e9\u05dc\u05d5\u05dd \u05d9\u05d8\u05d4\u05e8 \u05d5\u05d9\u05e7\u05d3\u05e9 \u05d0\u05ea\u05db\u05dd \u05dc\u05e2\u05e6\u05de\u05d5, \u05d5\u05d9\u05e9\u05de\u05d5\u05e8 \u05d0\u05ea \u05e8\u05d5\u05d7\u05db\u05dd, \u05e0\u05e4\u05e9\u05db\u05dd \u05d5\u05d2\u05d5\u05e4\u05db\u05dd \u05d1\u05e8\u05d9\u05d0\u05d9\u05dd \u05d5\u05dc\u05dc\u05d0\u05be\u05d7\u05d8\u05d0, \u05e2\u05d3 \u05d9\u05d5\u05dd \u05e9\u05d5\u05d1\u05d5 \u05e9\u05dc \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u05d0\u05ea\u05dd \u05d9\u05db\u05d5\u05dc\u05d9\u05dd \u05dc\u05e1\u05de\u05d5\u05da \u05e2\u05dc \u05d4\u05d0\u05dc\u05d5\u05d4\u05d9\u05dd \u05d0\u05e9\u05e8 \u05d1\u05d7\u05e8 \u05d1\u05db\u05dd \u05e9\u05d9\u05de\u05dc\u05d0 \u05d0\u05ea \u05db\u05dc \u05d4\u05d1\u05d8\u05d7\u05d5\u05ea\u05d9\u05d5."
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u05d0\u05d7\u05d9\u05dd \u05d9\u05e7\u05e8\u05d9\u05dd, \u05d4\u05ea\u05e4\u05dc\u05dc\u05d5 \u05d1\u05e2\u05d3\u05e0\u05d5."
                },
                "26": {
                    "verse_nr": "26",
                    "verse": "\u05d3\u05e8\u05e9\u05d5 \u05d1\u05e9\u05dc\u05d5\u05dd \u05db\u05dc \u05d4\u05d0\u05d7\u05d9\u05dd \u05d1\u05e0\u05e9\u05d9\u05e7\u05d4 \u05d4\u05e7\u05d3\u05d5\u05e9\u05d4."
                },
                "27": {
                    "verse_nr": "27",
                    "verse": "\u05d1\u05e9\u05dd \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d0\u05e0\u05d9 \u05de\u05e9\u05d1\u05d9\u05e2 \u05d0\u05ea\u05db\u05dd \u05e9\u05ea\u05e7\u05e8\u05d0\u05d5 \u05de\u05db\u05ea\u05d1 \u05d6\u05d4 \u05d1\u05d0\u05d5\u05d6\u05e0\u05d9 \u05db\u05dc \u05d4\u05de\u05d0\u05de\u05d9\u05e0\u05d9\u05dd."
                },
                "28": {
                    "verse_nr": "28",
                    "verse": "\u05d7\u05e1\u05d3 \u05d0\u05d3\u05d5\u05e0\u05e0\u05d5 \u05d9\u05e9\u05d5\u05e2 \u05d4\u05de\u05e9\u05d9\u05d7 \u05e2\u05dd \u05db\u05dc \u05d0\u05d7\u05d3 \u05d5\u05d0\u05d7\u05d3 \u05de\u05db\u05dd. \u2013 \u05d0\u05de\u05df."
                }
            }
        }
    }
}