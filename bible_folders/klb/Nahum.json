{
    "version": "klb",
    "book_name": "Nahum",
    "book_nr": "34",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\ub2c8\ub290\uc6e8\uc5d0 \ub300\ud55c \uc5ec\ud638\uc640\uc758 \ubd84\ub178 - \uc774\uac83\uc740 \uc5d8\uace0\uc2a4 \uc0ac\ub78c \ub098\ud6d4\uc774 \ub2c8\ub290\uc6e8\uc5d0 \ub300\ud558\uc5ec \uacc4\uc2dc\ubc1b\uc740 \ub9d0\uc500\uc774\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uc5ec\ud638\uc640\ub294 \uc2dc\uae30\ud558\uc2dc\uba70 \ubcf4\ubcf5\ud558\uc2dc\ub294 \ud558\ub098\ub2d8\uc774\uc2dc\ub2e4. \uc5ec\ud638\uc640\ub294 \ubcf4\ubcf5\ud558\uc2dc\uba70 \ubd84\ub178\ud558\uc2dc\ub418 \uc790\uae30 \ub300\uc801\uc5d0\uac8c \ubcf4\ubcf5\ud558\uc2dc\uace0 \uc790\uae30 \uc6d0\uc218\uc5d0\uac8c \ubd84\ub178\ub97c \ud488\uc73c\uc2e0\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uc5ec\ud638\uc640\ub294 \uc880\ucc98\ub7fc \ud654\ub97c \ub0b4\uc9c0 \uc54a\uc73c\uc2dc\uace0 \uadf8 \ub2a5\ub825\uc774 \ud06c\uc2dc\uc9c0\ub9cc \uc8c4\uc778\uc744 \ubc8c\ud558\uc9c0 \uc54a\uc740 \ucc44 \uadf8\ub300\ub85c \ub0b4\ubc84\ub824 \ub450\ub294 \ubc95\uc774 \uc5c6\uc73c\uc2dc\ub2e4. \uc5ec\ud638\uc640\uc758 \uae38\uc740 \ud68c\uc624\ub9ac\ubc14\ub78c\uacfc \ud3ed\ud48d \uc18d\uc5d0 \uc788\uace0 \uad6c\ub984\uc740 \uadf8 \ubc1c\ubc11\uc758 \ud2f0\ub04c\uc774\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uadf8\uac00 \ubc14\ub2e4\ub97c \uafb8\uc9d6\uc5b4 \ub9d0\ub9ac\uc2dc\uace0 \ubaa8\ub4e0 \uac15\ub4e4\uc744 \ub9c8\ub974\uac8c \ud558\uc2dc\ub2c8 \ubc14\uc0b0 \ub4e4\uacfc \uac08\uba5c\uc0b0\uc774 \uba54\ub9c8\ub974\uace0 \ub808\ubc14\ub17c\uc758 \uaf43\uc774 \uc2dc\ub4e0\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc5ec\ud638\uc640 \uc55e\uc5d0\uc11c \ub192\uc740 \uc0b0\ub4e4\uc774 \uc9c4\ub3d9\ud558\uace0 \uc57c\uc0b0\uc774 \ub179\uc73c\uba70 \ub545\uc774 \ud754\ub4e4\ub9ac\uace0 \uc138\uacc4\uc640 \uadf8 \uac00\uc6b4\ub370 \uc0ac\ub294 \ubaa8\ub4e0 \uc790\ub4e4\uc774 \ub5a0\ub294\uad6c\ub098."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\ub204\uac00 \ubd84\ub178\ud558\uc2e0 \uc5ec\ud638\uc640 \uc55e\uc5d0 \uc124 \uc218 \uc788\uc73c\uba70 \ub204\uac00 \uadf8\uc758 \ubb34\uc11c\uc6b4 \uc9c4\ub178\uc5d0 \uc0b4\uc544 \ub0a8\uc744 \uc218 \uc788\ub294\uac00? \uadf8\uac00 \uc9c4\ub178\ub97c \ubd88\ucc98\ub7fc \uc3df\uc73c\uc2dc\ubbc0\ub85c \uadf8 \uc55e\uc5d0\uc11c \ubc14\uc704\ub4e4\uc774 \uae68\uc5b4\uc9c4\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uc5ec\ud638\uc640\ub294 \uc120\ud558\uc2dc\uace0 \ud658\ub09c \ub54c\uc5d0 \ud53c\ub09c\ucc98\uac00 \ub418\uc2dc\uba70 \uc790\uae30\ub97c \uc2e0\ub8b0\ud558\ub294 \uc790\ub4e4\uc744 \ubcf4\ud638\ud558\uc2e0\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uadf8\uac00 \ud070 \ud64d\uc218\ub85c \uc790\uae30 \ub300\uc801\ub4e4\uc744 \uc4f8\uc5b4 \ubc84\ub9ac\uace0 \uadf8\ub4e4\uc744 \ud751\uc554\uc73c\ub85c \ucad3\uc544\ub0b4\uc2e4 \uac83\uc774\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub108\ud76c\uac00 \uc5ec\ud638\uc640\uaed8 \ubb34\uc2a8 \uc74c\ubaa8\ub97c \uafb8\ubbf8\ub290\ub0d0? \uadf8\uac00 \ub108\ud76c\ub97c \uc644\uc804\ud788 \uba78\ub9dd\uc2dc\ud0ac \uac83\uc774\ub2c8 \ub2e4\uc2dc \uce60 \ud544\uc694\uac00 \uc5c6\uc744 \uac83\uc774\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub4a4\uc5c9\ud0a8 \uac00\uc2dc\ub364\ubd88 \uac19\uace0 \uc220 \ucde8\ud55c \uc0ac\ub78c \uac19\uc740 \uadf8\ub4e4\uc740 \ubc14\uc2f9 \ub9c8\ub978 \uc9c0\ud478\ub77c\uae30\ucc98\ub7fc \ubd88\uc5d0 \ud0c0\uc11c \uc5c6\uc5b4\uc9c8 \uac83\uc774\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\ub2c8\ub290\uc6e8\uc57c, \uc5ec\ud638\uc640\uaed8 \uc545\uc744 \uaf80\ud558\ub294 \ud55c \uc0ac\ub78c\uc774 \ub124 \uac00\uc6b4\ub370\uc11c \ub098\uc640 \uc545\uc744 \uad8c\uc720\ud558\uace0 \uc788\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uc5ec\ud638\uc640\uaed8\uc11c \ub9d0\uc500\ud558\uc2e0\ub2e4. \u201c\uc557\uc2dc\ub9ac\uc544 \uc0ac\ub78c\ub4e4\uc774 \uc544\ubb34\ub9ac \uac15\ud558\uace0 \uc218\uac00 \ub9ce\uc544\ub3c4 \uadf8\ub4e4\uc740 \uba78\ub9dd\ud558\uc5ec \uc0ac\ub77c\uc9c8 \uac83\uc774\ub2e4. \uc720\ub2e4\uc57c, \uc9c0\uae08\uae4c\uc9c0 \ub0b4\uac00 \ub108\ub97c \uad34\ub86d\ud614\uc73c\ub098 \ub354 \uc774\uc0c1 \ub108\ub97c \uad34\ub86d\ud788\uc9c0 \uc54a\uc744 \uac83\uc774\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uc774\uc81c \ub0b4\uac00 \uc557\uc2dc\ub9ac\uc544 \uc655\uc774 \uba54\uac8c \ud55c \ub108\uc758 \uba4d\uc5d0\ub97c \uae68\ub728\ub9ac\uace0 \ub108\ub97c \ubb36\uace0 \uc788\ub294 \uc0ac\uc2ac\uc744 \ub04a\uc5b4 \ubc84\ub9ac\uaca0\ub2e4.\u201d"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\ub2c8\ub290\uc6e8\uc57c, \uc5ec\ud638\uc640\uaed8\uc11c \ub108\uc5d0 \ub300\ud558\uc5ec \ub9d0\uc500\ud558\uc168\ub2e4. \u201c\ub124 \uc774\ub984\uc774 \ub354 \uc774\uc0c1 \uc874\uc7ac\ud558\uc9c0 \uc54a\uc744 \uac83\uc774\ub2e4. \ub0b4\uac00 \ub108\uc758 \uc2e0\uc804\uc5d0 \uc788\ub294 \uc6b0\uc0c1\ub4e4\uc744 \uae68\ub728\ub824 \ubc84\ub9ac\uace0 \ub108\uc758 \ubb34\ub364\uc744 \uc900\ube44\ud558\uaca0\ub2e4. \uc774\uac83\uc740 \ub124\uac00 \uc544\uc8fc \ub354\ub7ec\uc6cc\uc84c\uae30 \ub54c\ubb38\uc774\ub2e4.\u201d"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\ubcf4\ub77c, \uc88b\uc740 \uc18c\uc2dd\uc744 \uac00\uc9c0\uace0 \ud3c9\ud654\ub97c \uc804\ud558\ub294 \uc790\uc758 \ubc1c\uc774 \uc0b0 \uc704\uc5d0 \uc788\ub2e4! \uc720\ub2e4\uc57c, \ub124 \uba85\uc808\uc744 \uc9c0\ud0a4\uace0 \ub124\uac00 \uc11c\uc57d\ud55c \uac83\uc744 \uc774\ud589\ud558\ub77c. \uc545\uc778\ub4e4\uc774 \uc804\uba78\ub418\uc5c8\uc73c\ubbc0\ub85c \uadf8\ub4e4\uc774 \ub2e4\uc2dc\ub294 \ub124 \ub545\uc744 \uce68\ub7b5\ud558\uc9c0 \ubabb\ud560 \uac83\uc774\ub2e4."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\ub2c8\ub290\uc6e8\uc758 \uba78\ub9dd - \ub2c8\ub290\uc6e8\uc57c, \uce68\ub7b5\uc790\uac00 \ub108\ub97c \uce58\ub7ec \uc62c\ub77c\uc654\ub2e4. \ub108\ub294 \uc694\uc0c8\ub97c \ubc29\uc5b4\ud558\uace0 \uae38\uc744 \uc9c0\ud0a4\uba70 \uc804\ud22c \ud0dc\uc138\ub97c \uac16\ucd94\uace0 \ub124 \ud798\uc744 \ubaa8\uc544\ub77c."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uc6d0\uc218\ub4e4\uc774 \uc774\uc2a4\ub77c\uc5d8\uc744 \uc57d\ud0c8\ud558\uace0 \ud3ec\ub3c4\ub098\ubb34\ub97c \ubabb \uc4f0\uac8c \ub9cc\ub4e4\uc5c8\uc9c0\ub9cc \uc5ec\ud638\uc640\uaed8\uc11c\ub294 \uadf8 \uc601\uad11\uc744 \ud68c\ubcf5\uc2dc\ud0a4\uc2e4 \uac83\uc774\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uc801\uad70\uc774 \ubd89\uc740 \ubc29\ud328\ub97c \ub4e4\uace0 \ubd89\uc740 \uc81c\ubcf5\uc744 \uc785\uace0 \uacf5\uaca9 \ud0dc\uc138\ub97c \uac16\ucd94\uc5c8\uc73c\ubbc0\ub85c \uc804\ucc28\uac00 \ubc88\uca4d\uc774\uace0 \uc18c\ub098\ubb34 \ucc3d\uc774 \ucda4\uc744 \ucd94\uace0 \uc788\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ubbf8\uce5c \ub4ef\uc774 \uac70\ub9ac\ub97c \uce58\ub2ec\uc544 \uad11\uc7a5\uc5d0\uc11c \ube60\ub974\uac8c \uc654\ub2e4\uac14\ub2e4\ud558\ub294 \uc804\ucc28\ub4e4\uc758 \ubaa8\uc2b5\uc740 \ud683\ubd88 \uac19\uace0 \uadf8 \ube60\ub974\uae30\ub294 \ubc88\uac1c\uc640\ub3c4 \uac19\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc655\uc774 \uc815\uc608\ubcd1\uc744 \uc18c\uc9d1\uc2dc\ud0a4\uc790 \uadf8\ub4e4\uc774 \uc4f0\ub7ec\uc9c8 \ub4ef\uc774 \uae09\ud788 \uc131\uc73c\ub85c \ub2ec\ub824\uac00\uc11c \ubc29\uc5b4 \ud0dc\uc138\ub97c \ucde8\ud55c\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uadf8\ub7ec\ub098 \ub54c\uac00 \ub108\ubb34 \ub2a6\uc5c8\ub2e4! \uac15\ub4e4\uc758 \uc218\ubb38\uc774 \uc5f4\ub9ac\uace0 \uc655\uad81\uc740 \uacf5\ud3ec\uc5d0 \ud729\uc2f8\uc774\uace0 \ub9d0\uc558\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uc655\ud6c4\uac00 \ubc8c\uac70\ubc97\uc740 \ubab8\uc73c\ub85c \ub04c\ub824\uac00\ub2c8 \uc2dc\ub140\ub4e4\uc774 \uac00\uc2b4\uc744 \uce58\uba70 \ube44\ub458\uae30\ucc98\ub7fc \uc2ac\ud53c \uc6b0\ub294\uad6c\ub098."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub2c8\ub290\uc6e8\ub294 \ubb3c\uc774 \ubaa8\uc778 \uc5f0\ubabb\ucc98\ub7fc \ub9ce\uc740 \uc0ac\ub78c\ub4e4\ub85c \ubd90\ube44\ub354\ub2c8 \uc774\uc81c\ub294 \uadf8\ub4e4\uc774 \ub2ec\uc544\ub098\uace0 \uc788\ub2e4. \u201c\uba48\ucdb0\ub77c! \uba48\ucdb0\ub77c!\u201d \ud558\uace0 \uc678\uccd0\ub3c4 \ub3cc\uc544\ubcf4\ub294 \uc790\uac00 \uc5c6\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uc740\uc744 \uc57d\ud0c8\ud558\ub77c! \uae08\uc744 \uc57d\ud0c8\ud558\ub77c! \uc628\uac16 \ubcf4\ubb3c\uc774 \uac00\ub4dd\ud558\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub2c8\ub290\uc6e8\uac00 \ubaa8\uc870\ub9ac \uc57d\ud0c8\uc744 \ub2f9\ud558\uace0 \ud30c\uad34\ub418\uc5b4 \ud669\ud3d0\ud574\uc84c\uc73c\ubbc0\ub85c \ub450\ub824\uc6cc\uc11c \uc0ac\ub78c\ub4e4\uc758 \ub9c8\uc74c\uc774 \ub179\uace0 \ubb34\ub98e\uc774 \ub5a8\ub9ac\uba70 \uc628 \ubab8\uc5d0 \ub9e5\uc774 \ud480\ub9ac\uace0 \uc5bc\uad74\uc774 \ucc3d\ubc31\ud574\uc9c4\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc0ac\uc790\uac00 \uc790\uae30 \uc0c8\ub07c\ub4e4\uc744 \uba39\uc774\ub358 \uad74\uc774 \uc9c0\uae08 \uc5b4\ub514 \uc788\ub290\ub0d0? \uc804\uc5d0\ub294 \uc218\uc0ac\uc790\uc640 \uc554\uc0ac\uc790\uac00 \uc0c8\ub07c\uc640 \ud568\uaed8 \ub2e4\ub140\ub3c4 \uadf8\ub4e4\uc744 \ub450\ub835\uac8c \ud560 \uc790\uac00 \uc5c6\uc5c8\uc73c\uba70"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uc218\uc0ac\uc790\uac00 \uc554\uc0ac\uc790\uc640 \uadf8 \uc0c8\ub07c\ub97c \uc704\ud574 \uba39\uc774\ub97c \uc7a1\uc544 \uac08\uae30\uac08\uae30 \ucc22\uc5b4\uc11c \uadf8 \uad74\uc744 \uba39\uc774\ub85c \uac00\ub4dd \ucc44\uc6e0\uc5c8\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uc804\ub2a5\ud558\uc2e0 \uc5ec\ud638\uc640\uaed8\uc11c \ub9d0\uc500\ud558\uc2e0\ub2e4. \u201c\ub0b4\uac00 \ub108\ub97c \ub300\uc801\ud558\uc5ec \ub108\uc758 \uc804\ucc28\ub4e4\uc744 \ubd88\uc5d0 \ud0dc\uc6cc\uc11c \uc5f0\uae30\uac00 \ub418\uac8c \ud558\uace0 \ub108\uc758 \ubcd1\uc0ac\ub4e4\uc774 \uc804\uc7c1\uc73c\ub85c \uc8fd\uac8c \ud560 \uac83\uc774\uba70 \ub124\uac00 \uc57d\ud0c8\ud55c \uac83\uc744 \ub2e4 \uc5c6\uc560 \ubc84\ub9ac\uaca0\ub2e4. \ub124 \uc0ac\uc808\ub2e8\uc758 \ubaa9\uc18c\ub9ac\uac00 \ub2e4\uc2dc\ub294 \ub4e4\ub9ac\uc9c0 \uc54a\uc744 \uac83\uc774\ub2e4.\u201d"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\ub2c8\ub290\uc6e8\uc5d0 \ubbf8\uce60 \ud654 - \ud53c\uc758 \uc131\uc5d0 \ud654\uac00 \uc788\uc744 \uac83\uc774\ub2e4. \uc0ac\uae30\uc640 \uc57d\ud0c8\uc774 \ud310\uc744 \uce58\uace0 \uc788\uc73c\ubbc0\ub85c \ud76c\uc0dd\uc790\uac00 \ub5a0 \ub0a0 \ub0a0\uc774 \uc5c6\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ud719\ud719\ud558\ub294 \ucc44\ucc0d \uc18c\ub9ac, \ub35c\uac70\ub369\uac70\ub9ac\ub294 \uc804\ucc28 \ubc14\ud034 \uc18c\ub9ac, \ub6f0\ub294 \ub9d0\uacfc \ub2ec\ub9ac\ub294 \uc804\ucc28,"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uaca9\ub3cc\ud558\ub294 \uae30\ubcd1, \ubc88\uca4d\uc774\ub294 \uce7c, \ube5b\ub098\ub294 \ucc3d, \uc218\ub9ce\uc740 \uc0ac\uc0c1\uc790, \uc2dc\uccb4\ub354\ubbf8, \ud5e4\uc544\ub9b4 \uc218 \uc5c6\ub294 \uc8fc\uac80\ub4e4, \uc2dc\uccb4\uc5d0 \uac78\ub824 \ub118\uc5b4\uc9c0\ub294 \uc0ac\ub78c\ub4e4!"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uc774 \ubaa8\ub4e0 \uac83\uc740 \ub2c8\ub290\uc6e8\uac00 \uc694\uc5fc\ud55c \ucc3d\ub140\ucc98\ub7fc \uc74c\ud589\ud558\uc600\uae30 \ub54c\ubb38\uc774\ub2e4. \uc800\uac00 \uc74c\ud589\uc73c\ub85c \uc5ec\ub7ec \ub098\ub77c\ub97c \uc0ac\ub85c\uc7a1\uace0 \ub9c8\ub825\uc73c\ub85c \uadf8 \ubc31\uc131\uc744 \ubbf8\ud639\ud558\uc600\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc804\ub2a5\ud558\uc2e0 \uc5ec\ud638\uc640\uaed8\uc11c \ub9d0\uc500\ud558\uc2e0\ub2e4. \u201c\ub2c8\ub290\uc6e8\uc57c, \ub0b4\uac00 \ub108\ub97c \ub300\uc801\ud558\uc5ec \ub124 \uce58\ub9c8\ub97c \ub124 \uc5bc\uad74\uae4c\uc9c0 \uac77\uc5b4\uc62c\ub824 \ub108\uc758 \ubc8c\uac70\ubc97\uc740 \uc218\uce58\ub97c \uc628 \uc138\uacc4\uc5d0 \ubcf4\uc77c \uac83\uc774\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\ub0b4\uac00 \ub610 \ub354\ub7ec\uc6b4 \uac83\uc744 \ub108\uc5d0\uac8c \ub358\uc838 \ub108\ub97c \ubaa8\uc695\ud558\uace0 \ub108\ub97c \uad6c\uacbd\uac70\ub9ac\uac00 \ub418\uac8c \ud558\uaca0\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ub108\ub97c \ubcf4\ub294 \uc790\ub4e4\uc774 \ub2e4 \ub124\uac8c\uc11c \ub3c4\ub9dd\ud558\uba70 \u2018\ub2c8\ub290\uc6e8\uac00 \ud669\ud3d0\ud558\uc600\ub2e4. \ub204\uac00 \ub108\ub97c \uc704\ud574 \uc2ac\ud53c \uc6b8\uae4c?\u2019 \ud558\uace0 \ub9d0\ud560 \uac83\uc774\ub2e4. \ub108\ub97c \uc704\ub85c\ud560 \uc790\ub97c \ub0b4\uac00 \uc5b4\ub514\uc11c \uad6c\ud560 \uc218 \uc788\uaca0\ub294\uac00?\u201d"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub124\uac00 \ub370\ubca0\uc2a4\ubcf4\ub2e4 \ub098\uc740\uac00? \uadf8\ub294 \ub098\uc77c \uac15\uac00\uc5d0 \uc704\uce58\ud558\uc5ec \ubb3c\ub85c \ub458\ub7ec\uc2f8\uc600\uc73c\ubbc0\ub85c \uac15\uc774 \ubc29\uc5b4\uc120\uc774 \ub418\uace0 \ubb3c\uc774 \uc131\ubcbd\uc774 \ub418\uc5c8\uc73c\uba70"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uc5d0\ud2f0\uc624\ud53c\uc544\uc640 \uc774\uc9d1\ud2b8\uac00 \uadf8\uc5d0\uac8c \uac15\ub825\ud55c \ud798\uc774 \ub418\uc5c8\uace0 \ubd93\uacfc \ub9ac\ube44\uc544\ub3c4 \uadf8\uc758 \ub3d9\ub9f9\uad6d\uc774 \ub418\uc5c8\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uadf8\ub7f0\ub370\ub3c4 \uadf8\ub294 \ud3ec\ub85c\uac00 \ub418\uc5b4 \uc0ac\ub85c\uc7a1\ud600\uac14\uc73c\uba70 \uadf8 \uc5b4\ub9b0 \uc544\uc774\ub4e4\uc740 \uae38 \ubaa8\ud241\uc774\uc5d0 \ub0b4\ub3d9\ub315\uc774\uce68\uc744 \ub2f9\ud574 \ubc15\uc0b4\uc774 \ub0ac\uace0 \uadc0\uc871\ub4e4\uc740 \uc885\uc73c\ub85c \ud314\ub824\uac00\uae30 \uc704\ud574 \uc81c\ube44 \ubf51\ud614\uc73c\uba70 \ubaa8\ub4e0 \uc9c0\ub3c4\uae09 \uc778\uc0ac\ub4e4\uc740 \uc0ac\uc2ac\uc5d0 \ubb36\uc600\uc73c\ub2c8"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\ub108\ub3c4 \ucde8\ud558\uac8c \ub420 \uac83\uc774\uba70 \ub124 \ub300\uc801\uc744 \ud53c\ud558\uc5ec \uc228\uc744 \uacf3\uc744 \ucc3e\uac8c \ub420 \uac83\uc774\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ubb34\ud654\uacfc\ub098\ubb34\uc758 \ucc98\uc74c \uc775\uc740 \uc5f4\ub9e4\uac00 \ud754\ub4e4\uae30\ub9cc \ud558\uba74 \uba39\ub294 \uc790\uc758 \uc785\uc5d0 \ub5a8\uc5b4\uc9c0\ub4ef\uc774 \ub108\uc758 \ubaa8\ub4e0 \uc694\uc0c8\ub4e4\uc774 \uc27d\uac8c \ubb34\ub108\uc9c8 \uac83\uc774\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ub108\uc758 \ubcd1\uc0ac\ub4e4\uc744 \ubcf4\uc544\ub77c. \uadf8\ub4e4\uc740 \uc5ec\uc790\ucc98\ub7fc \ubb34\ub825\ud558\ub2e4! \ub124 \ub545\uc758 \uc131\ubb38\ub4e4\uc774 \ub124 \ub300\uc801 \uc55e\uc5d0 \ud65c\uc9dd \uc5f4\ub824 \uc788\uc73c\ub2c8 \uadf8 \ube57\uc7a5\ub4e4\uc774 \ubd88\uc5d0 \ud0c8 \uac83\uc774\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\ub108\ub294 \ud3ec\uc704\ub2f9\ud560 \uc77c\uc5d0 \ub300\ube44\ud558\uc5ec \ubb3c\uc744 \uae38\uc5b4 \ub450\uace0 \ubc29\uc5b4\uc9c4\uc744 \ud2bc\ud2bc\ud558\uac8c \ud558\uba70 \uc9c4\ud759\uc744 \ubc1f\uc544 \ubcbd\ub3cc \uac00\ub9c8\ub97c \uc218\ub9ac\ud558\ub77c."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\ub7ec\ub098 \ub124\uac00 \uc544\ubb34\ub9ac \ubc29\ube44\ud558\uc5ec\ub3c4 \ubd88\uc774 \ub108\ub97c \uc0bc\ud0a4\uba70 \uce7c\uc774 \ub108\ub97c \ubca0\uace0 \uba54\ub69c\uae30\ub5bc\uac00 \uace1\uc2dd\uc744 \uba39\uc5b4\uac00\ub4ef\uc774 \ub124 \ub300\uc801\uc774 \ub108\ub97c \uba39\uc744 \uac83\uc774\ub2e4. \uba54\ub69c\uae30\ub5bc\ucc98\ub7fc \ub124 \uc778\uad6c\uc218\ub97c \ub9ce\uac8c \ud574 \ubcf4\uc544\ub77c."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\ub124\uac00 \ub124 \uc0c1\uc778\ub4e4\uc758 \uc218\ub97c \ud558\ub298\uc758 \ubcc4\ucc98\ub7fc \ub9ce\uac8c \ud558\uc600\uc73c\ub098 \uba54\ub69c\uae30\ucc98\ub7fc \ub2e4 \ub0a0\uc544\uac00 \ubc84\ub9ac\uace0 \ub9d0\uc558\ub2e4."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\ub108\uc758 \ubcd1\uc0ac\ub4e4\uacfc \uc9c0\ud718\uad00\ub4e4\uc740 \ucd94\uc6b4 \ub0a0 \uc6b8\ud0c0\ub9ac\uc5d0 \uae43\ub4e4\uc600\ub2e4\uac00 \ud574\uac00 \ub728\uba74 \ub0a0\uc544\uac00 \ubc84\ub9ac\ub294 \uba54\ub69c\uae30\ub5bc\uc640 \uac19\uc544\uc11c \uc544\ubb34\ub3c4 \uadf8\ub4e4\uc774 \uac04 \uacf3\uc744 \uc54c\uc9c0 \ubabb\ud55c\ub2e4."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uc557\uc2dc\ub9ac\uc544 \uc655\uc544, \ub124 \ubaa9\uc790\ub4e4\uc740 \uc8fd\uc5c8\uace0 \ub124 \uadc0\uc871\ub4e4\ub3c4 \uc601\uc6d0\ud788 \uc7a0\ub4e4\uc5c8\uc73c\uba70 \ub124 \ubc31\uc131\ub4e4\uc740 \uc0b0\uc5d0 \ud769\uc5b4\uc84c\ub294\ub370\ub3c4 \uadf8\ub4e4\uc744 \ubaa8\uc744 \uc790\uac00 \uc5c6\ub2e4."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\ub124\uac00 \uc911\uc0c1\uc744 \uc785\uc5c8\uc73c\ubbc0\ub85c \uc544\ubb34\ub3c4 \ub124 \uc0c1\ucc98\ub97c \uace0\uce60 \uc218 \uc5c6\ub2e4. \ub124 \uc18c\uc2dd\uc744 \ub4e3\ub294 \uc790\ub4e4\uc774 \ub2e4 \uae30\ubed0\uc11c \uc190\ubf09\uc744 \uce58\uace0 \uc788\uc73c\ub2c8 \uc794\ud559\ud55c \ub108\uc5d0\uac8c \ud53c\ud574\ub97c \uc785\uc9c0 \uc54a\uc740 \uc790\uac00 \uc544\ubb34\ub3c4 \uc5c6\uae30 \ub54c\ubb38\uc774\ub2e4."
                }
            }
        }
    }
}