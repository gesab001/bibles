{
    "version": "klb",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc744 \uc804\ud30c\ud558\ub2e4\uac00 \uac07\ud78c \ubc14\uc6b8\uacfc \ub514\ubaa8\ub370\ub294 \uc0ac\ub791\ud558\ub294 \ub3d9\uc5ed\uc790 \ube4c\ub808\ubaac\uacfc"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uc790\ub9e4 \uc555\ube44\uc544\uc640 \ub610 \ud568\uaed8 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uad70\uc778\uc774 \ub41c \uc544\ud0b5\ubcf4\uc640 \uadf8\ub300\uc758 \uc9d1\uc5d0\uc11c \ubaa8\uc774\ub294 \uad50\ud68c\uc5d0"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\ud558\ub098\ub2d8 \uc6b0\ub9ac \uc544\ubc84\uc9c0\uc640 \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc740\ud61c\uc640 \ud3c9\uc548\uc774 \ud568\uaed8\ud558\uae30\ub97c \uae30\ub3c4\ud569\ub2c8\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ubc14\uc6b8\uc758 \ubd80\ud0c1 - \ub098\ub294 \uae30\ub3c4\ud560 \ub54c\ub9c8\ub2e4 \uadf8\ub300\ub97c \uc0dd\uac01\ud558\uba70 \uc5b8\uc81c\ub098 \ub0b4 \ud558\ub098\ub2d8\uaed8 \uac10\uc0ac\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uadf8\uac83\uc740 \uadf8\ub300\uac00 \uc8fc \uc608\uc218\ub2d8\uc744 \uad73\uac8c \ubbff\uace0 \ubaa8\ub4e0 \uc131\ub3c4\ub4e4\uc744 \uc0ac\ub791\ud55c\ub2e4\ub294 \ub9d0\uc744 \ub0b4\uac00 \ub4e3\uace0 \uc788\uae30 \ub54c\ubb38\uc785\ub2c8\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uadf8\ub300\ub294 \ubbff\uc74c\uc758 \uad50\uc81c\ub97c \ud568\uc73c\ub85c\uc368 \uc6b0\ub9ac\uac00 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \uac00\uc9c4 \ubaa8\ub4e0 \uc120\ud55c \uac83\uc744 \ucda9\ubd84\ud788 \uc774\ud574\ud558\uae30\ub97c \uae30\ub3c4\ud569\ub2c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ud615\uc81c\uc5ec, \ub098\ub294 \uadf8\ub300\uac00 \ubca0\ud47c \uc0ac\ub791\uc73c\ub85c \uc131\ub3c4\ub4e4\uc774 \uc6a9\uae30\ub97c \uc5bb\uac8c \ub418\uc5c8\ub2e4\ub294 \ub9d0\uc744 \ub4e3\uace0 \ud070 \uae30\uc068\uacfc \uc704\ub85c\ub97c \ubc1b\uc558\uc2b5\ub2c8\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \ub0b4\uac00 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \uadf8\ub300\uac00 \ud574\uc57c \ud560 \uc77c\uc744 \ub2f4\ub300\ud558\uac8c \uba85\ub839\ud560 \uc218\ub3c4 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uadf8\ub7ec\ub098 \uc0ac\ub791\ud558\ub294 \ub9c8\uc74c\uc73c\ub85c \uadf8\ub300\uc5d0\uac8c \ubd80\ud0c1\ud558\ub824\uace0 \ud569\ub2c8\ub2e4. \uc774\uc81c \ub098\uc774 \ub9ce\uc544 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc744 \uc704\ud574 \uc8c4\uc218\uac00 \ub41c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub098 \ubc14\uc6b8\uc740 \uac07\ud78c \uc911\uc5d0 \uc5bb\uc740 \ubbff\uc74c\uc758 \uc544\ub4e4 \uc624\ub124\uc2dc\ubaa8\ub97c \uc704\ud574 \uadf8\ub300\uc5d0\uac8c \ubd80\ud0c1\ud569\ub2c8\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uadf8\uac00 \uc804\uc5d0\ub294 \uadf8\ub300\uc5d0\uac8c \uc4f8\ubaa8\uc5c6\ub294 \uc0ac\ub78c\uc774\uc5c8\uc73c\ub098 \uc774\uc81c\ub294 \uadf8\ub300\uc640 \ub098\uc5d0\uac8c \ud544\uc694\ud55c \uc0ac\ub78c\uc774 \ub418\uc5c8\uc2b5\ub2c8\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uadf8\ub798\uc11c \ub0b4 \uc2ec\uc7a5\uacfc \uac19\uc740 \uadf8\ub97c \uadf8\ub300\uc5d0\uac8c \ub3cc\ub824\ubcf4\ub0c5\ub2c8\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ub098\ub294 \uae30\uc05c \uc18c\uc2dd\uc744 \uc704\ud574 \ub0b4\uac00 \uac07\ud600 \uc788\ub294 \ub3d9\uc548 \uadf8\ub97c \ub0b4 \uacc1\uc5d0 \ub450\uace0 \uadf8\ub300\ub97c \ub300\uc2e0\ud574\uc11c \ub098\ub97c \ub3d5\uac8c \ud558\uace0 \uc2f6\uc5c8\uc2b5\ub2c8\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uadf8\ub7ec\ub098 \ub098\ub294 \uadf8\ub300\uc758 \uc2b9\ub099 \uc5c6\uc774\ub294 \uc544\ubb34\uac83\ub3c4 \ud558\uace0 \uc2f6\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4. \uc774\uac83\uc740 \uadf8\ub300\uc758 \uc120\ud55c \uc77c\uc774 \uc5b5\uc9c0\uac00 \uc544\ub2c8\ub77c \uc790\ubc1c\uc801\uc778 \uac83\uc774 \ub418\ub3c4\ub85d \ud558\uae30 \uc704\ud55c \uac83\uc785\ub2c8\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\uac00 \uc7a0\uc2dc \uadf8\ub300\ub97c \ub5a0\ub098\uac8c \ub41c \uc774\uac83\uc73c\ub85c \uc778\ud574\uc11c \uadf8\uac00 \uc601\uc6d0\ud788 \uadf8\ub300\ub97c \uc12c\uae30\uac8c \ub420\uc9c0\ub3c4 \ubaa8\ub985\ub2c8\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uc774\uc81c\ubd80\ud130\ub294 \uadf8\ub97c \uc885\uc73c\ub85c\uc11c\uac00 \uc544\ub2c8\ub77c \uc885 \uc774\uc0c1\uc758 \uc0ac\ub791\ud558\ub294 \ud615\uc81c\ub85c \ub300\ud574 \uc8fc\uc2dc\uc624. \ub098\uc5d0\uac8c \uc774\ucc98\ub7fc \uc18c\uc911\ud55c \uadf8\ub294 \uc885\uc73c\ub85c\uc11c, \uc8fc\ub2d8\uc744 \ubbff\ub294 \ud615\uc81c\ub85c\uc11c \uadf8\ub300\uc5d0\uac8c \ub354\uc6b1\ub354 \uc18c\uc911\ud560 \uac83\uc785\ub2c8\ub2e4."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\ub9cc\uc77c \uadf8\ub300\uac00 \ub098\ub97c \uce5c\uad6c\ub85c \uc5ec\uae34\ub2e4\uba74 \ub098\ub97c \ub300\ud558\ub4ef \uadf8\ub97c \ubc18\uac11\uac8c \ub9de\uc544 \uc8fc\uc2dc\uc624."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uadf8\uac00 \uadf8\ub300\uc5d0\uac8c \uc190\ud574\ub97c \ub07c\ucce4\uac70\ub098 \ube5a\uc9c4 \uac83\uc774 \uc788\uc73c\uba74 \uadf8\uac83\uc744 \ub098\uc5d0\uac8c \uccad\uad6c\ud558\uc2dc\uc624. \ub0b4\uac00 \ub2e4 \uac1a\uaca0\uc2b5\ub2c8\ub2e4."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\ub0b4\uac00 \uc9c1\uc811 \uc774 \uae00\uc744 \uc368\uc11c \ub9d0\ud558\uc9c0\ub9cc \ub0b4\uac00 \uac1a\uaca0\uc2b5\ub2c8\ub2e4. \ub0b4\uac00 \uc774\ub7f0 \ub9d0\uc744 \ud558\uc9c0 \ub9d0\uc544\uc57c\uaca0\uc9c0\ub9cc \uadf8\ub300\ub3c4 \ub098\uc5d0\uac8c \ube5a\uc9c4 \uac83\uc774 \uc788\uc74c\uc744 \uae30\uc5b5\ud558\uc2dc\uc624."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\ud615\uc81c\uc5ec, \ub0b4\uac00 \uc8fc\ub2d8 \uc548\uc5d0\uc11c \uadf8\ub300\ub97c \ud1b5\ud574 \uae30\uc068\uc744 \uc5bb\uac8c \ud558\uace0 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \ub0b4 \ub9c8\uc74c\uc774 \ud3c9\uc548\ud558\uac8c \ud574 \uc8fc\uc2dc\uc624."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\ub098\ub294 \uadf8\ub300\uac00 \ub0b4 \ub9d0\uc5d0 \uc21c\uc885\ud558\ub9ac\ub77c\ub294 \uac83\uc744 \ud655\uc2e0\ud558\uace0 \ub610 \uae30\ub300 \uc774\uc0c1\uc73c\ub85c \ud574 \uc904 \uac83\uc744 \uc54c\uace0 \uc774 \uae00\uc744 \uc501\ub2c8\ub2e4."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\uadf8\ub9ac\uace0 \ub0b4\uac00 \uba38\ubb3c \ubc29\ub3c4 \ud558\ub098 \ub9c8\ub828\ud574 \uc8fc\uc2dc\uc624. \uc5ec\ub7ec\ubd84\uc758 \uae30\ub3c4\ub85c \ub0b4\uac00 \uadf8 \uacf3\uc5d0 \uac08 \uc218 \uc788\uac8c \ub418\uae30\ub97c \ubc14\ub78d\ub2c8\ub2e4."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc744 \uc804\ud30c\ud558\ub2e4\uac00 \ud568\uaed8 \uac07\ud78c \uc5d0\ubc14\ube0c\ub77c\uac00 \uadf8\ub300\uc5d0\uac8c \ubb38\uc548\ud569\ub2c8\ub2e4."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\ub610 \ud568\uaed8 \uc77c\ud558\ub294 \ub9c8\uac00\uc640 \uc544\ub9ac\uc2a4\ub2e4\uace0\uc640 \ub370\ub9c8\uc640 \ub204\uac00\ub3c4 \uadf8\ub300\uc5d0\uac8c \ubb38\uc548\ud569\ub2c8\ub2e4."
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc740\ud61c\uac00 \uc5ec\ub7ec\ubd84\uacfc \ud568\uaed8\ud558\uae30\ub97c \uae30\ub3c4\ud569\ub2c8\ub2e4."
                }
            }
        }
    }
}