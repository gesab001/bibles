    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uc131\uc804\uc744 \uc9c0\uc73c\ub77c\ub294 \uc5ec\ud638\uc640\uc758 \uba85\ub839 - \ud398\ub974\uc2dc\uc544\uc758 \ub2e4\ub9ac\uc6b0\uc2a4\ud669\uc81c 2\ub144 6\uc6d4 \uc77c\uc5d0 \uc5ec\ud638\uc640\uaed8\uc11c \uc608\uc5b8\uc790 \ud559\uac1c\ub97c \ud1b5\ud558\uc5ec \uc2a4 \uc54c\ub514\uc5d8\uc758 \uc544\ub4e4\uc778 \uc720\ub2e4 \ucd1d\ub3c5 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4\uc778 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc5d0\uac8c \ub9d0\uc500\ud558\uc168\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201c\uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud55c\ub2e4. \uc774 \ubc31\uc131\ub4e4\uc774 \uc131\uc804\uc744 \uac74\ucd95\ud560 \ub54c\uac00 \uc544\uc9c1 \ub418\uc9c0 \uc54a\uc558\ub2e4\uace0 \ub9d0\ud558\uace0 \uc788\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uc5ec\ud638\uc640\uaed8\uc11c \ub2e4\uc2dc \uc608\uc5b8\uc790 \ud559\uac1c\ub97c \ud1b5\ud558\uc5ec \ub9d0\uc500\ud558\uc168\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u201c\ub0b4 \uc131\uc804\uc740 \ud669\ud3d0\ud55c \ucc44\ub85c \uc788\ub294\ub370 \ub108\ud76c\ub294 \ud638\ud654 \uc8fc\ud0dd\uc5d0 \uc0b4\uace0 \uc788\ub290\ub0d0?"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud55c\ub2e4. \ub108\ud76c\ub294 \uc790\uae30 \uc18c\ud589\uc744 \uc798 \uc0b4\ud3b4\ubcf4\uc544\ub77c."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\ub108\ud76c\uac00 \ub9ce\uc774 \uc2ec\uc5c8\uc73c\ub098 \uc801\uac8c \uac70\ub450\uc5c8\uc73c\uba70 \ub108\ud76c\uac00 \uba39\uc5b4\ub3c4 \ubc30\ubd80\ub974\uc9c0 \uc54a\uace0 \ub9c8\uc154\ub3c4 \ud761\uc871\ud558\uc9c0 \uc54a\uc73c\uba70 \uc785\uc5b4\ub3c4 \ub530\ub73b\ud558\uc9c0 \uc54a\uace0 \ud488\uc0af\uc744 \ubc1b\uc544\ub3c4 \uad6c\uba4d \ub6ab\ub9b0 \uc9c0\uac11\uc5d0 \ub123\ub294 \uaca9\uc774 \ub418\uc5c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u201c\uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud55c\ub2e4. \ub108\ud76c\ub294 \uc790\uae30 \uc18c\ud589\uc744 \uc798 \uc0b4\ud3b4\ubcf4\uc544\ub77c."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub108\ud76c\ub294 \uc0b0\uc5d0 \uc62c\ub77c\uac00\uc11c \ubaa9\uc7ac\ub97c \uac00\uc838\ub2e4\uac00 \uc131\uc804\uc744 \uac74\ucd95\ud558\ub77c. \uadf8\ub7ec\uba74 \ub0b4\uac00 \uadf8\uac83\uc73c\ub85c \uae30\ubed0\ud558\uace0 \uc601\uad11\uc744 \uc5bb\uc744 \uac83\uc774\ub2e4. \uc774\uac83\uc740 \ub098 \uc5ec\ud638\uc640\uc758 \ub9d0\uc774\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub108\ud76c\uac00 \ub9ce\uc740 \uc218\ud655\uc744 \uae30\ub300\ud588\uc73c\ub098 \uc5bb\uc740 \uac83\uc740 \uc801\uc5c8\uc73c\uba70 \ub108\ud76c\uac00 \uc218\ud655\ud55c \uac83\uc744 \uc9d1\uc73c\ub85c \uac00\uc838\uac14\uc73c\ub098 \ub0b4\uac00 \uadf8\uac83\uc744 \ub0a0\ub824 \ubc84\ub838\ub2e4. \uadf8 \uc774\uc720\uac00 \ubb34\uc5c7\uc774\ub0d0? \uc774\uac83\uc740 \ub0b4 \uc9d1\uc774 \ud669\ud3d0\ud55c\ub370\ub3c4 \ub108\ud76c\uac00 \uc790\uae30 \uc9d1\uc548 \uc77c\uc5d0\ub9cc \uae09\uae09\ud558\uc600\uae30 \ub54c\ubb38\uc774\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uadf8\ub798\uc11c \ub108\ud76c \ub54c\ubb38\uc5d0 \ud558\ub298\uc774 \uc774\uc2ac\uc744 \ub0b4\ub9ac\uc9c0 \uc54a\uace0 \ub545\uc5d0\uc11c \ub18d\uc0b0\ubb3c\uc774 \ub098\uc9c0 \uc54a\uac8c \ub418\uc5c8\uc73c\uba70"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\ub610 \ub0b4\uac00 \uac00\ubb44\uc774 \ub4e4\uac8c \ud558\uc600\uc73c\ubbc0\ub85c \ub4e4\uacfc \uc0b0\uacfc \uace1\uc2dd\uacfc \uc0c8 \ud3ec\ub3c4\uc8fc\uc640 \uae30\ub984\uacfc \ub545\uc758 \ubaa8\ub4e0 \ub18d\uc0b0\ubb3c\uacfc \uc0ac\ub78c\uacfc \uc9d0\uc2b9\uacfc \uc560\uc368 \uc218\uace0\ud55c \ubaa8\ub4e0 \uac83\uc774 \ud53c\ud574\ub97c \uc785\uac8c \ub418\uc5c8\ub2e4.\u201d"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ubc31\uc131\ub4e4\uc758 \uc21c\uc885 - \uadf8\ub54c \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4\uc778 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc640 \ub0a8\uc740 \ubaa8\ub4e0 \ubc31\uc131\uc774 \uc608\uc5b8\uc790 \ud559\uac1c\uac00 \uc804\ud55c \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc5d0 \uc21c\uc885\ud558\uc600\ub2e4. \uc774\uac83\uc740 \uadf8\ub4e4\uc758 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\uaed8\uc11c \ud559\uac1c\ub97c \ubcf4\ub0b4\uc168\uae30 \ub54c\ubb38\uc774\uc5c8\ub2e4. \uadf8\ub798\uc11c \ubc31\uc131\ub4e4\uc740 \uc5ec\ud638\uc640\ub97c \ub450\ub824\uc6cc\ud558\uc600\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uadf8\ub54c \ud559\uac1c\ub294 \u201c\ub0b4\uac00 \ub108\ud76c\uc640 \ud568\uaed8\ud55c\ub2e4\u201d \ub294 \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc744 \ubc31\uc131\ub4e4\uc5d0\uac8c \uc804\ud558\uc600\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uc5ec\ud638\uc640\uaed8\uc11c \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4\uc778 \uc720\ub2e4 \ucd1d\ub3c5 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4\uc778 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc640 \uadf8\ub9ac\uace0 \ub0a8\uc544 \uc788\ub294 \ubaa8\ub4e0 \ubc31\uc131\uc758 \ub9c8\uc74c\uc744 \uac10\ub3d9\uc2dc\ud0a4\uc2dc\ubbc0\ub85c \uadf8\ub4e4\uc774 \uc790\uae30\ub4e4\uc758 \ud558\ub098\ub2d8, \uc804\ub2a5\ud558\uc2e0 \uc5ec\ud638\uc640\uc758 \uc131\uc804\uc744 \uc7ac\uac74\ud558\uae30 \uc2dc\uc791\ud588\ub294\ub370"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\ub54c\ub294 \ub2e4\ub9ac\uc6b0\uc2a4\ud669\uc81c 2\ub144 6\uc6d4 24\uc77c\uc774\uc5c8\ub2e4."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uc0c8 \uc131\uc804\uc774 \ub354 \ud654\ub824\ud560 \uac83\uc744 \uc57d\uc18d\ud568 - \uadf8 \ud574 7\uc6d4 21\uc77c\uc5d0 \uc5ec\ud638\uc640\uaed8\uc11c \uc608\uc5b8\uc790 \ud559\uac1c\uc5d0\uac8c \ub2e4\uc2dc \ub9d0\uc500\ud558\uc168\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201c\ub108\ub294 \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4\uc778 \uc720\ub2e4 \ucd1d\ub3c5 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4\uc778 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc640 \ub0a8\uc740 \ubc31\uc131\uc5d0\uac8c \ub0b4\uac00 \uc774\ub807\uac8c \ub9d0\ud55c\ub2e4\uace0 \uc77c\ub7ec \uc8fc\uc5b4\ub77c."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u2018\ub108\ud76c \uac00\uc6b4\ub370 \uc774\uc804\uc758 \uc131\uc804\uc774 \uc5bc\ub9c8\ub098 \ud654\ub824\ud588\ub294\uc9c0 \uae30\uc5b5\ud560 \uc218 \uc788\ub294 \uc790\uac00 \uc788\ub290\ub0d0? \uc9c0\uae08 \uc774 \uc131\uc804\uc740 \ub108\ud76c\uc5d0\uac8c \uc5b4\ub5bb\uac8c \ubcf4\uc774\ub290\ub0d0? \uc774\uac83\uc740 \ub108\ud76c\uc5d0\uac8c \ubcf4\uc798\uac83\uc5c6\uc5b4 \ubcf4\uc774\uc9c0 \uc54a\ub290\ub0d0?"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uadf8\ub7ec\ub098 \uc6a9\uae30\ub97c \uac00\uc838\ub77c. \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc218\uc544\uc640 \uc774 \ub545 \ubaa8\ub4e0 \ubc31\uc131\uc544, \ud798\uc744 \ub0b4\uc5b4 \uc77c\ud558\ub77c. \ub0b4\uac00 \ub108\ud76c\uc640 \ud568\uaed8\ud55c\ub2e4. \uc774\uac83\uc740 \uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uc758 \ub9d0\uc774\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\ub108\ud76c\uac00 \uc774\uc9d1\ud2b8\uc5d0\uc11c \ub098\uc62c \ub54c\uc5d0 \ub0b4\uac00 \ud56d\uc0c1 \ub108\ud76c\uc640 \ud568\uaed8\ud558\uaca0\ub2e4\uace0 \uc57d\uc18d\ud55c \ub300\ub85c \ub0b4\uac00 \uc9c0\uae08\ub3c4 \ub108\ud76c\uc640 \ud568\uaed8\ud55c\ub2e4. \uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\ub294 \ub450\ub824\uc6cc\ud558\uc9c0 \ub9d0\uc544\ub77c."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u201c \u2018\uc870\uae08 \ud6c4\uc5d0 \ub0b4\uac00 \ud55c \ubc88 \ub354 \ud558\ub298\uacfc \ub545\uacfc \ubc14\ub2e4\uc640 \uc721\uc9c0\ub97c \uc9c4\ub3d9\uc2dc\ud0a4\uace0"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ubaa8\ub4e0 \ub098\ub77c\ub97c \ub4a4\ud754\ub4e4\uc5b4 \ub193\uaca0\ub2e4. \uc628 \uc138\uc0c1\uc758 \ubcf4\ubb3c\uc774 \uc774\ub9ac\ub85c \uad74\ub7ec\ub4e4\uc5b4\uc62c \uac83\uc774\uba70 \ub0b4\uac00 \uc774 \uc131\uc804\uc744 \uc601\uad11\uc73c\ub85c \ucda9\ub9cc\ud558\uac8c \ud560 \uac83\uc774\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uc740\ub3c4 \ub0b4 \uac83\uc774\uc694 \uae08\ub3c4 \ub0b4 \uac83\uc774\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uc0c8 \uc131\uc804\uc774 \uc61b \uc131\uc804\ubcf4\ub2e4 \ub354\uc6b1 \ucc2c\ub780\ud560 \uac83\uc774\ub2e4. \uadf8\ub9ac\uace0 \ub0b4\uac00 \uc774 \uacf3\uc5d0 \ud3c9\uc548\uc744 \uc8fc\uaca0\ub2e4. \uc774\uac83\uc740 \uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uc758 \ub9d0\uc774\ub2e4.\u2019 \u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ubd80\uc815\ud55c \ubc31\uc131\uacfc \ucd95\ubcf5\uc744 \uc57d\uc18d\ud558\uc2dc\ub294 \ud558\ub098\ub2d8 - \ub2e4\ub9ac\uc6b0\uc2a4\ud669\uc81c 2\ub144 9\uc6d4 24\uc77c\uc5d0 \uc5ec\ud638\uc640\uaed8\uc11c \uc608\uc5b8\uc790 \ud559\uac1c\uc5d0\uac8c \ub9d0\uc500\ud558\uc168\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u201c\uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud55c\ub2e4. \ub108\ub294 \uc728\ubc95\uc5d0 \ub300\ud558\uc5ec \uc81c\uc0ac\uc7a5\uc5d0\uac8c \uc774\ub807\uac8c \ubb3c\uc5b4 \ubcf4\uc544\ub77c."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u2018\uc0ac\ub78c\uc774 \ub9cc\uc77c \uac70\ub8e9\ud55c \uc81c\ubb3c\uc758 \uace0\uae30\ub97c \uc637\uc790\ub77d\uc5d0 \uc2f8\uac00\uc9c0\uace0 \uac00\ub2e4\uac00 \uadf8 \uc637\uc790\ub77d\uc774 \ub5a1\uc774\ub098 \uad6d\uc774\ub098 \ud3ec\ub3c4\uc8fc\ub098 \uae30\ub984\uc774\ub098 \ub2e4\ub978 \uc74c\uc2dd\ubb3c\uc5d0 \ub2ff\uc73c\uba74 \uadf8\ub798\ub3c4 \uadf8\uac83\uc774 \uac70\ub8e9\ud558\uaca0\uc2b5\ub2c8\uae4c?\u2019 \u201d \uadf8\ub798\uc11c \ud559\uac1c\uac00 \uc81c\uc0ac\uc7a5\ub4e4\uc5d0\uac8c \ubb3b\uc790 \uadf8\ub4e4\uc740 \u201c\uc544\ub2d9\ub2c8\ub2e4\u201d \ud558\uace0 \ub300\ub2f5\ud558\uc600\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uadf8\ub54c \ud559\uac1c\uac00 \u201c\ub9cc\uc77c \uc2dc\uccb4\ub97c \ub9cc\uc838\uc11c \ubd80\uc815\ud558\uac8c \ub41c \uc790\uac00 \uc774 \uc74c\uc2dd\ubb3c \uc911 \ud558\ub098\ub97c \ub9cc\uc9c0\uba74 \uadf8\uac83\ub3c4 \uc5ed\uc2dc \ubd80\uc815\ud558\uaca0\uc2b5\ub2c8\uae4c?\u201d \ud558\uace0 \ub2e4\uc2dc \ubb3b\uc790 \uc81c\uc0ac\uc7a5\ub4e4\uc740 \u201c\ubd80\uc815\ud569\ub2c8\ub2e4\u201d \ud558\uace0 \ub300\ub2f5\ud558\uc600\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uadf8\ub7ec\uc790 \ud559\uac1c\uac00 \ub300\ub2f5\ud558\uc600\ub2e4. \u201c\uc5ec\ud638\uc640\uaed8\uc11c \uc774\ub807\uac8c \ub9d0\uc500\ud558\uc2ed\ub2c8\ub2e4. \u2018\ub0b4 \uc55e\uc5d0\uc11c\ub294 \uc774 \ubc31\uc131\uacfc \uc774 \ub098\ub77c\uac00 \ub9c8\ucc2c\uac00\uc9c0\uc774\ub2e4. \uadf8\ub4e4\uc774 \ud589\ud558\ub294 \uc77c\ub3c4 \ubd80\uc815\ud558\uace0 \uadf8\ub4e4\uc774 \ub4dc\ub9ac\ub294 \uac83\ub3c4 \ub2e4 \ubd80\uc815\ud558\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uc774\uc81c \ub108\ud76c\ub294 \uc131\uc804 \uc7ac\uac74 \uacf5\uc0ac\ub97c \uc2dc\uc791\ud558\uae30 \uc804\uc5d0 \uc788\uc5c8\ub358 \uc77c\uc744 \uc798 \uc0dd\uac01\ud574 \ubcf4\uc544\ub77c."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uadf8\ub54c \ub108\ud76c\ub294 \uace1\uc2dd 20\uc11d\uc744 \uae30\ub300\ud558\uace0 \uac14\uc73c\ub098 10\uc11d\ubfd0\uc774\uc5c8\uc73c\uba70 \ud3ec\ub3c4\uc8fc 50\ud1b5\uc744 \uae38\uc73c\ub824\uace0 \ud3ec\ub3c4\uc999\ud2c0\ub85c \uac14\uc73c\ub098 20\ud1b5\ubfd0\uc774\uc5c8\ub2e4."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\ub108\ud76c\uac00 \uae30\ub974\ub294 \ubaa8\ub4e0 \uac83\uc5d0 \ub0b4\uac00 \ub728\uac70\uc6b4 \ubc14\ub78c\uacfc \uacf0\ud321\uc774\uc640 \uc6b0\ubc15\uc73c\ub85c \ucce4\uc73c\ub098 \ub108\ud76c\ub294 \ub098\uc5d0\uac8c \ub3cc\uc544\uc624\uc9c0 \uc54a\uc558\ub2e4."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uc624\ub298\uc740 9\uc6d4 24\uc77c, \uace7 \uc131\uc804 \uae30\ucd08 \uacf5\uc0ac\ub97c \uc644\uc131\ud55c \ub0a0\uc774\ub2e4. \ub108\ud76c\ub294 \uc624\ub298 \uc774\ud6c4\uc758 \uc77c\uc744 \uc798 \uad00\ucc30\ud574 \ubcf4\uc544\ub77c."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\uc544\uc9c1\ub3c4 \ucc3d\uace0\uc5d0 \ub0a8\uc544 \uc788\ub294 \uc528\uc557\uc774 \uc788\ub290\ub0d0? \uc9c0\uae08\uae4c\uc9c0\ub294 \ud3ec\ub3c4\ub098\ubb34, \ubb34\ud654\uacfc\ub098\ubb34, \uc11d\ub958\ub098\ubb34, \uac10\ub78c\ub098\ubb34\uac00 \uc5f4\ub9e4\ub97c \ub9fa\uc9c0 \ubabb\ud558\uc600\uc73c\ub098 \uc624\ub298\ubd80\ud130\ub294 \ub0b4\uac00 \ub108\ud76c\uc5d0\uac8c \ubcf5\uc744 \ub0b4\ub824 \uc8fc\uaca0\ub2e4.\u2019 \u201d"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\uc2a4\ub8f9\ubc14\ubca8\uc5d0 \ub300\ud55c \uc5ec\ud638\uc640\uc758 \uc57d\uc18d - \uadf8 \ub0a0 \uc5ec\ud638\uc640\uaed8\uc11c \ub2e4\uc2dc \ud559\uac1c\uc5d0\uac8c \ub9d0\uc500\ud558\uc168\ub2e4."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u201c\ub108\ub294 \uc720\ub2e4 \ucd1d\ub3c5 \uc2a4\ub8f9\ubc14\ubca8\uc5d0\uac8c \uc774\ub807\uac8c \ub9d0\ud558\ub77c. \u2018\ub0b4\uac00 \ud558\ub298\uacfc \ub545\uc744 \uc9c4\ub3d9\uc2dc\ud0a4\uba70"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\uc5ec\ub7ec \ub098\ub77c\uc758 \uc655\uc704\ub97c \ub4a4\uc5ce\uace0 \uadf8 \uad8c\ub825\uc744 \ubb34\ub108\ub728\ub9ac\uba70 \uc804\ucc28\uc640 \uadf8 \uc804\ucc28\ubcd1\uc744 \uc5ce\uc5b4 \ubc84\ub9ac\uaca0\ub2e4. \ub9d0\uacfc \uae30\ubcd1\ub4e4\uc774 \uc11c\ub85c\uc11c\ub85c \uce5c \uce7c\ub0a0\uc5d0 \uc4f0\ub7ec\uc9c8 \uac83\uc774\ub2e4."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\uc804\ub2a5\ud55c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud55c\ub2e4. \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4 \ub0b4 \uc885 \uc2a4\ub8f9\ubc14\ubca8\uc544, \uadf8 \ub0a0\uc5d0 \ub0b4\uac00 \ub108\ub97c \uc138\uc6cc \ub0b4 \uc778\uc7a5 \ubc18\uc9c0\ucc98\ub7fc \uc0ac\uc6a9\ud558\uaca0\ub2e4. \uc774\uac83\uc740 \ub0b4\uac00 \ub108\ub97c \ud0dd\ud558\uc600\uae30 \ub54c\ubb38\uc774\ub2e4.\u2019 \u201d"
                }
            }
