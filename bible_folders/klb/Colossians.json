{
    "version": "klb",
    "book_name": "Colossians",
    "book_nr": "51",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\ud558\ub098\ub2d8\uc758 \ub73b\uc5d0 \ub530\ub77c \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc758 \uc0ac\ub3c4\uac00 \ub41c \ub098 \ubc14\uc6b8\uacfc \ud615\uc81c \ub514\ubaa8\ub370\ub294"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uace8\ub85c\uc0c8\uc5d0 \uc788\ub294 \uc131\ub3c4\ub4e4\uacfc \uadf8\ub9ac\uc2a4\ub3c4\ub97c \ubbff\ub294 \ud615\uc81c\ub4e4\uc5d0\uac8c \ud558\ub098\ub2d8 \uc6b0\ub9ac \uc544\ubc84\uc9c0\uc758 \uc740\ud61c\uc640 \ud3c9\uc548\uc774 \ud568\uaed8\ud558\uae30\ub97c \uae30\ub3c4\ud569\ub2c8\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uac10\uc0ac\uc640 \uae30\ub3c4 - \uc6b0\ub9ac\ub294 \uc5ec\ub7ec\ubd84\uc744 \uc704\ud574 \uae30\ub3c4\ud560 \ub54c\ub9c8\ub2e4 \uc6b0\ub9ac \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc544\ubc84\uc9c0 \ud558\ub098\ub2d8\uaed8 \ud56d\uc0c1 \uac10\uc0ac\ub97c \ub4dc\ub9bd\ub2c8\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uadf8\uac83\uc740 \uc5ec\ub7ec\ubd84\uc774 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc744 \uc798 \ubbff\uace0 \ubaa8\ub4e0 \uc131\ub3c4\ub97c \uc0ac\ub791\ud55c\ub2e4\ub294 \ub9d0\uc744 \uc6b0\ub9ac\uac00 \ub4e4\uc5c8\uae30 \ub54c\ubb38\uc785\ub2c8\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc774 \ubbff\uc74c\uacfc \uc0ac\ub791\uc740 \uc5ec\ub7ec\ubd84\uc774 \uc804\uc5d0 \uc9c4\ub9ac\uc758 \ub9d0\uc500\uc778 \uae30\uc05c \uc18c\uc2dd\uc744 \ub4e4\uc5b4\uc11c \ud655\uc2e0\ud558\uace0 \uc788\ub294 \ud558\ub298 \ub098\ub77c\uc758 \uc18c\ub9dd\uc5d0 \uadfc\uac70\ud558\uace0 \uc788\ub294 \uac83\uc785\ub2c8\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uc5ec\ub7ec\ubd84\uc774 \uae30\uc05c \uc18c\uc2dd\uc744 \ub4e3\uace0 \uc9c4\ub9ac \uac00\uc6b4\ub370 \ub098\ud0c0\ub09c \ud558\ub098\ub2d8\uc758 \uc740\ud61c\ub97c \uae68\ub2ec\uc740 \ub0a0\ubd80\ud130 \uc5f4\ub9e4\ub97c \ub9fa\uc740 \uac83\ucc98\ub7fc \uadf8 \ub9d0\uc500\uc774 \uc628 \uc138\uacc4\uc5d0\uc11c\ub3c4 \uadf8\ub807\uac8c \uc5f4\ub9e4\ub97c \ub9fa\uc73c\uba70 \uc804\ud30c\ub418\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc774 \uae30\uc05c \uc18c\uc2dd\uc744 \uc6b0\ub9ac\uc640 \ud568\uaed8 \ud558\ub098\ub2d8\uc758 \uc885\uc774 \ub41c \uc0ac\ub791\ud558\ub294 \uc5d0\ubc14\ube0c\ub77c\uc5d0\uac8c \ubc30\uc6e0\uc2b5\ub2c8\ub2e4. \uadf8\ub294 \uc5ec\ub7ec\ubd84\uc744 \uc704\ud574 \uc77c\ud558\ub294 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc2e0\uc2e4\ud55c \uc77c\uafbc\uc774\uba70"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uc131\ub839\ub2d8 \uc548\uc5d0\uc11c \uc5ec\ub7ec\ubd84\uc774 \uc0ac\ub791\uc73c\ub85c \uc0dd\ud65c\ud55c\ub2e4\ub294 \uc18c\uc2dd\uc744 \uc6b0\ub9ac\uc5d0\uac8c \uc804\ud574 \uc900 \uc0ac\ub78c\uc785\ub2c8\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uadf8 \uc18c\uc2dd\uc744 \ub4e4\uc740 \ud6c4\ubd80\ud130 \uc6b0\ub9ac\ub294 \uc26c\uc9c0 \uc54a\uace0 \uc5ec\ub7ec\ubd84\uc744 \uc704\ud574 \uae30\ub3c4\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc6b0\ub9ac\ub294 \ud558\ub098\ub2d8\uc774 \ubaa8\ub4e0 \uc601\uc801\uc778 \uc9c0\ud61c\uc640 \ucd1d\uba85\uc73c\ub85c \uc5ec\ub7ec\ubd84\uc5d0\uac8c \uadf8\uc758 \ub73b\uc744 \uc544\ub294 \uc9c0\uc2dd\uc73c\ub85c \ucc44\uc6cc \uc8fc\uc2dc\uae30\ub97c \uae30\ub3c4\ud569\ub2c8\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uadf8\ub798\uc11c \uc5ec\ub7ec\ubd84\uc774 \uc8fc\ub2d8\uc744 \ubbff\ub294 \uc131\ub3c4\ub2e4\uc6b4 \uc0dd\ud65c\uc744 \ud558\uc5ec \ubaa8\ub4e0 \uc77c\uc5d0 \uc8fc\ub2d8\uc744 \uae30\uc058\uc2dc\uac8c \ud558\uace0 \ubaa8\ub4e0 \uc120\ud55c \uc77c\uc5d0 \uc5f4\ub9e4\ub97c \ub9fa\uc73c\uba70 \ud558\ub098\ub2d8\uc744 \uc544\ub294 \uc9c0\uc2dd\uc774 \uc810\uc810 \uc790\ub77c\uae30\ub97c \ubc14\ub77c\ub294 \uac83\uc785\ub2c8\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uadf8\ub9ac\uace0 \ud558\ub098\ub2d8\uc758 \uc601\uad11\uc2a4\ub7ec\uc6b4 \ud798\uc744 \ud1b5\ud574 \uc624\ub294 \ubaa8\ub4e0 \ub2a5\ub825\uc73c\ub85c \uc5ec\ub7ec\ubd84\uc774 \uac15\ud574\uc838\uc11c \ubaa8\ub4e0 \uc77c\uc744 \uae30\uc068\uc73c\ub85c \ucc38\uace0 \uacac\ub514\uba70"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ube5b\uc758 \ub098\ub77c\uc5d0\uc11c \uc131\ub3c4\ub4e4\uc774 \uc5bb\uc744 \ucd95\ubcf5\uc5d0 \ucc38\uc5ec\ud560 \uc218 \uc788\ub294 \uc790\uaca9\uc744 \uc6b0\ub9ac\uc5d0\uac8c \uc8fc\uc2e0 \uc544\ubc84\uc9c0\uaed8 \uac10\uc0ac\ud558\uae30\ub97c \ubc14\ub78d\ub2c8\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ud558\ub098\ub2d8\uc740 \uc6b0\ub9ac\ub97c \uc5b4\ub450\uc6c0\uc758 \uad8c\uc138\uc5d0\uc11c \uad6c\ucd9c\ud558\uc5ec \uc790\uae30\uac00 \uc0ac\ub791\ud558\ub294 \uc544\ub4e4\uc758 \ub098\ub77c\ub85c \uc62e\uaca8 \uc8fc\uc168\uc2b5\ub2c8\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uc6b0\ub9ac\ub294 \uc774 \uc544\ub4e4\uc774 \ud53c \ud758\ub9b0 \ub300\uac00\ub85c \uc790\uc720\ub97c \uc5bb\uace0 \uc8c4\ub97c \uc6a9\uc11c\ubc1b\uc558\uc2b5\ub2c8\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \ud558\uc2e0 \uc601\uad11\uc2a4\ub7ec\uc6b4 \uc77c - \uadf8\ubd84\uc740 \ubcf4\uc774\uc9c0 \uc54a\ub294 \ud558\ub098\ub2d8\uc758 \ubaa8\uc2b5\uc774\uba70 \ud558\ub098\ub2d8\uaed8\uc11c \ubaa8\ub4e0 \uac83\uc744 \ucc3d\uc870\ud558\uc2dc\uae30 \uc804\ubd80\ud130 \uacc4\uc2dc\ub294 \ubd84\uc785\ub2c8\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uadf8\ubd84\uc5d0 \uc758\ud574\uc11c \ubaa8\ub4e0 \uac83\uc774 \ucc3d\uc870\ub418\uc5c8\uc2b5\ub2c8\ub2e4. \ud558\ub298\uacfc \ub545\uc5d0 \uc788\ub294 \uac83\ub4e4\uacfc \ubcf4\uc774\ub294 \uac83\uacfc \ubcf4\uc774\uc9c0 \uc54a\ub294 \uac83\ub4e4\uacfc \ucc9c\uc0ac\ub4e4\uacfc \uc601\uc801 \uc874\uc7ac\ub4e4\uacfc \ub9cc\ubb3c\uc774 \ub2e4 \uadf8\ubd84\uc5d0 \uc758\ud574\uc11c \ucc3d\uc870\ub418\uc5c8\uace0 \uadf8\ubd84\uc744 \uc704\ud574 \ucc3d\uc870\ub418\uc5c8\uc2b5\ub2c8\ub2e4."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uadf8\ub9ac\uace0 \uadf8\ubd84\uc740 \ubaa8\ub4e0 \uac83\uc774 \uc788\uae30 \uc804\uc5d0 \uacc4\uc2dc\uace0 \ubaa8\ub4e0 \uac83\uc740 \uadf8\ubd84\uc5d0 \uc758\ud574\uc11c \uc720\uc9c0\ub418\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c\ub294 \uc790\uc2e0\uc758 \ubab8\uc778 \uad50\ud68c\uc758 \uba38\ub9ac\uc640 \uc2dc\uc791\uc774 \ub418\uc2dc\uba70 \uc8fd\uc740 \uc0ac\ub78c\ub4e4 \uac00\uc6b4\ub370\uc11c \uba3c\uc800 \uc0b4\uc544\ub098\uc154\uc11c \ubaa8\ub4e0 \uac83\uc758 \uc73c\ub738\uc774 \ub418\uc168\uc2b5\ub2c8\ub2e4."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\uc774\uac83\uc740 \ud558\ub098\ub2d8\uaed8\uc11c \uae30\uaebc\uc774 \ubaa8\ub4e0 \uac83\uc744 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0 \ucda9\ub9cc\ud558\uac8c \ud558\uc168\uae30 \ub54c\ubb38\uc785\ub2c8\ub2e4."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\uadf8\ub9ac\uace0 \ud558\ub098\ub2d8\uc740 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \ud758\ub9ac\uc2e0 \uc2ed\uc790\uac00\uc758 \ud53c\ub85c \ud3c9\ud654\uc758 \uae38\uc744 \uc5f4\uc5b4 \ud558\ub298\uc5d0 \uc788\ub294 \uac83\uc774\ub4e0 \ub545\uc5d0 \uc788\ub294 \uac83\uc774\ub4e0 \ubaa8\ub4e0 \uac83\uc744 \uadf8\ubd84\uc744 \ud1b5\ud574 \uc790\uae30\uc640 \ud654\ud574\ud558\uac8c \ud558\uc168\uc2b5\ub2c8\ub2e4."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc804\uc5d0 \uc5ec\ub7ec\ubd84\uc758 \uc545\ud55c \ud589\uc2e4 \ub54c\ubb38\uc5d0 \ud558\ub098\ub2d8\uc744 \ub5a0\ub098 \ub9c8\uc74c\uc73c\ub85c \ud558\ub098\ub2d8\uacfc \uc6d0\uc218\uac00 \ub418\uc5c8\uc2b5\ub2c8\ub2e4."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\uadf8\ub7ec\ub098 \uc774\uc81c\ub294 \ud558\ub098\ub2d8\uc774 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc721\uccb4\uc801\uc778 \uc8fd\uc74c\uc744 \ud1b5\ud574 \uc5ec\ub7ec\ubd84\uacfc \ud654\ud574\ud558\uc168\uc2b5\ub2c8\ub2e4. \uc774\uac83\uc740 \uc5ec\ub7ec\ubd84\uc744 \uac70\ub8e9\ud558\uace0 \ud760\uc774 \uc5c6\uace0 \ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\ub294 \uc0ac\ub78c\uc73c\ub85c \uc790\uae30 \uc55e\uc5d0 \uc138\uc6b0\uae30 \uc704\ud55c \uac83\uc785\ub2c8\ub2e4."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\ub9cc\uc77c \uc5ec\ub7ec\ubd84\uc774 \ubbff\uc74c\uc5d0 \uad73\uac8c \uc11c\uc11c \uc5ec\ub7ec\ubd84\uc774 \ub4e4\uc740 \uae30\uc05c \uc18c\uc2dd\uc758 \ud76c\ub9dd\uc5d0\uc11c \ud754\ub4e4\ub9ac\uc9c0 \uc54a\ub294\ub2e4\uba74 \uadf8\ub807\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4. \uc774 \uae30\uc05c \uc18c\uc2dd\uc740 \ud558\ub298 \uc544\ub798 \ubaa8\ub4e0 \uc0ac\ub78c\uc5d0\uac8c \uc804\ud30c\ub418\uace0 \uc788\uc73c\uba70 \ub098 \ubc14\uc6b8\uc740 \uc774 \uae30\uc05c \uc18c\uc2dd\uc744 \uc804\ud558\ub294 \uc77c\uafbc\uc785\ub2c8\ub2e4."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\uad50\ud68c\ub97c \uc704\ud55c \ubc14\uc6b8\uc758 \ud5cc\uc2e0\uc801 \uc0ac\uc5ed - \uc9c0\uae08 \ub098\ub294 \uc5ec\ub7ec\ubd84\uc744 \uc704\ud574 \ubc1b\ub294 \uace0\ub09c\uc744 \uc624\ud788\ub824 \uae30\ubed0\ud558\uba70 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \uc790\uc2e0\uc758 \ubab8\uc778 \uad50\ud68c\ub97c \uc704\ud574 \ubc1b\uc73c\uc2e0 \uace0\ub09c\uc758 \ub0a8\uc740 \ubd80\ubd84\uc744 \ub0b4 \ubab8\uc73c\ub85c \uacaa\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\ub098\ub294 \ud558\ub098\ub2d8\uc758 \ub9d0\uc500\uc744 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \uc544\ub08c\uc5c6\uc774 \uc804\ud558\ub77c\ub294 \uc0ac\uba85\uc744 \ubc1b\uc740 \uad50\ud68c\uc758 \uc77c\uafbc\uc785\ub2c8\ub2e4."
                },
                "26": {
                    "verse_nr": "26",
                    "verse": "\uc774 \ub9d0\uc500\uc740 \uc624\ub79c \uc138\ub300 \ub3d9\uc548 \uc228\uaca8\uc84c\ub358 \ube44\ubc00\uc778\ub370 \uc9c0\uae08 \ud558\ub098\ub2d8\uc758 \uc131\ub3c4\ub4e4\uc5d0\uac8c \uc54c\ub824\uc84c\uc2b5\ub2c8\ub2e4."
                },
                "27": {
                    "verse_nr": "27",
                    "verse": "\ud558\ub098\ub2d8\uc740 \uc774\ubc29\uc778\ub4e4\uc5d0\uac8c \ub098\ud0c0\ub09c \uc774 \ube44\ubc00\uc758 \ud48d\uc131\ud55c \uc601\uad11\uc744 \uc790\uae30 \ubc31\uc131\uc5d0\uac8c \uc54c\ub9ac\uc2dc\uae30\ub97c \uc6d0\ud558\uc168\ub294\ub370 \uc774 \ube44\ubc00\uc740 \uc5ec\ub7ec\ubd84 \uc548\uc5d0 \uacc4\uc2e0 \uadf8\ub9ac\uc2a4\ub3c4 \uace7 \uc601\uad11\uc758 \ud76c\ub9dd\uc785\ub2c8\ub2e4."
                },
                "28": {
                    "verse_nr": "28",
                    "verse": "\uc6b0\ub9ac\uac00 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc804\ud30c\ud558\uba70 \uc628\uac16 \uc9c0\ud61c\ub85c \ubaa8\ub4e0 \uc0ac\ub78c\uc744 \uad8c\uba74\ud558\uace0 \uac00\ub974\uce58\ub294 \uac83\uc740 \uadf8\ub4e4\uc744 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \uc644\uc804\ud55c \uc0ac\ub78c\uc73c\ub85c \ud558\ub098\ub2d8\uaed8 \ubc14\uce58\uae30 \uc704\ud55c \uac83\uc785\ub2c8\ub2e4."
                },
                "29": {
                    "verse_nr": "29",
                    "verse": "\uc774 \uc77c\uc744 \uc704\ud574 \ub098\ub294 \ub0b4 \uc18d\uc5d0\uc11c \ud798 \uc788\uac8c \uc77c\ud558\uc2dc\ub294 \ubd84\uc758 \ub2a5\ub825\uc73c\ub85c \ucd5c\uc120\uc744 \ub2e4\ud574 \ud798\uc368 \uc77c\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uc5ec\ub7ec\ubd84\uacfc \ub77c\uc624\ub514\uac8c\uc544\uc758 \uc131\ub3c4\ub4e4\uacfc \ub610 \ub0b4 \uc5bc\uad74\uc744 \ubcf4\uc9c0 \ubabb\ud55c \uc0ac\ub78c\ub4e4\uc744 \uc704\ud574 \ub0b4 \uac00 \uc5bc\ub9c8\ub098 \uc560\uc4f0\uace0 \uc788\ub294\uac00\ub97c \uc5ec\ub7ec\ubd84\uc740 \uc54c\uae30 \ubc14\ub78d\ub2c8\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ub0b4\uac00 \uc774\ucc98\ub7fc \uc560\uc4f0\ub294 \uac83\uc740 \uadf8\ub4e4\uc774 \ub9c8\uc74c\uc5d0 \uc6a9\uae30 \ub97c \uc5bb\uace0 \uc0ac\ub791\uc73c\ub85c \ub2e8\ud569\ud558\uba70 \uc774\ud574\ud568\uc73c\ub85c \uc5bb\uac8c \ub418\ub294 \uc644\uc804\ud55c \ud655\uc2e0\uc758 \ubd80\uc694\ud568\uc5d0 \uc774\ub974\ub7ec \ud558\ub098\ub2d8\uc758 \ube44\ubc00\uc778 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc54c\ub3c4\ub85d \ud558\uae30 \uc704\ud55c \uac83\uc785\ub2c8\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\ub294 \uc9c0\ud61c\uc640 \uc9c0\uc2dd\uc758 \ubaa8\ub4e0 \ubcf4\ubb3c\uc774 \uac10\ucd94\uc5b4\uc838 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub098\ub294 \uc544\ubb34\ub3c4 \ub2ec\ucf64\ud55c \ub9d0\ub85c \uc5ec\ub7ec\ubd84\uc744 \uc18d\uc774\uc9c0 \ubabb\ud558\uac8c \ud558\ub824\uace0 \uc774 \ub9d0\uc744 \ud558\ub294 \uac83\uc785\ub2c8\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\ub0b4\uac00 \ubab8\uc740 \uc5ec\ub7ec\ubd84\uc744 \ub5a0\ub098 \uc788\uc73c\ub098 \ub9c8\uc74c\uc740 \uc5ec\ub7ec\ubd84\uacfc \ud568\uaed8 \uc788\uc5b4\uc11c \uc5ec\ub7ec\ubd84\uc774 \uc9c8\uc11c \uc788\ub294 \uc0dd\ud65c\uc744 \ud558\uace0 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \ubbff\ub294 \ubbff\uc74c\uc5d0 \uad73\uac8c \uc120 \uac83\uc744 \ubcf4\ub2c8 \uae30\uc069\ub2c8\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c\uc758 \uc0b6 - \uadf8\ub7ec\ubbc0\ub85c \uc5ec\ub7ec\ubd84\uc740 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc744 \uc8fc\ub2d8\uc73c\ub85c \ubc1b\uc544\ub4e4\uc600\uc73c\ub2c8 \uacc4\uc18d \uadf8\ubd84 \uc548\uc5d0\uc11c \uc0ac\uc2ed\uc2dc\uc624."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uadf8\ubd84 \uc548\uc5d0 \uae4a\uc774 \ubfcc\ub9ac\ub97c \ubc15\uace0 \uadf8\ubd84\uc744 \uae30\ucd08\ub85c \uc5ec\ub7ec\ubd84\uc758 \uc778\uc0dd\uc744 \uac74\uc124\ud558\uba70 \uac00\ub974\uce68\uc744 \ubc1b\uc740 \ub300\ub85c \ubbff\uc74c\uc5d0 \uad73\uac8c \uc11c\uc11c \uac10\uc0ac\uac00 \ub118\uce58\ub294 \uc0dd\ud65c\uc744 \ud558\uc2ed\uc2dc\uc624."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc2e4\uc18d \uc5c6\uace0 \uae30\ub9cc\uc801\uc778 \ucca0\ud559\uc5d0 \uc0ac\ub85c\uc7a1\ud788\uc9c0 \uc54a\ub3c4\ub85d \uc8fc\uc758\ud558\uc2ed\uc2dc\uc624. \uc774\uac83\uc740 \uc804\ud1b5\uc801\uc73c\ub85c \uc804\ud574 \ub0b4\ub824\uc624\ub294 \uc0ac\ub78c\uc758 \uac00\ub974\uce68\uc774\ub098 \uc774 \uc138\uc0c1\uc758 \ucd08\ubcf4\uc801\uc778 \uc6d0\ub9ac\uc5d0 \uadfc\uac70\ud55c \uac83\uc774\uc9c0 \uadf8\ub9ac\uc2a4\ub3c4\uc5d0\uac8c \uadfc\uac70\ud55c \uac83\uc774 \uc544\ub2d9\ub2c8\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\ub294 \uc628\uac16 \uc2e0\uc801 \uc131\ud488\uc774 \uc721\uccb4\uc801\uc778 \ud615\ud0dc\ub85c \ub098\ud0c0\ub098 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uadf8\ub9ac\uace0 \uc5ec\ub7ec\ubd84\ub3c4 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \uc644\uc804\ud574\uc84c\uc2b5\ub2c8\ub2e4. \uadf8\ub9ac\uc2a4\ub3c4\ub294 \ubaa8\ub4e0 \ucc9c\uc0ac\ub4e4\uc758 \uba38\ub9ac\uac00 \ub418\uc2dc\ub294 \ubd84\uc774\uc2ed\ub2c8\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc0ac\ub78c\uc758 \uc190\uc73c\ub85c \ubca0\ud47c \ud560\ub840\ub97c \ubc1b\uc740 \uac83\uc774 \uc544\ub2c8\ub77c \uc721\uc801\uc778 \uc8c4\uc758 \ubab8\uc744 \ubc97\uc5b4 \ubc84\ub9ac\ub294 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \ud560\ub840\ub97c \ubc1b\uc558\uc2b5\ub2c8\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc138\ub840\ub97c \ubc1b\uc74c\uc73c\ub85c \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \ub545\uc5d0 \ubb3b\ud614\uace0 \ub610\ud55c \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc8fd\uc740 \uc0ac\ub78c \uac00\uc6b4\ub370\uc11c \ub2e4\uc2dc \uc0b4\ub9ac\uc2e0 \ud558\ub098\ub2d8\uc758 \ub2a5\ub825\uc744 \ubbff\uc74c\uc73c\ub85c \uadf8\ubd84\uacfc \ud568\uaed8 \ub2e4\uc2dc \uc0b4\uc544\ub098\uac8c \ub41c \uac83\uc785\ub2c8\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uc5ec\ub7ec\ubd84\ub3c4 \uc804\uc5d0\ub294 \uc8c4\uc640 \ud560\ub840\ubc1b\uc9c0 \ubabb\ud55c \uc721\uc801\uc778 \uc695\ub9dd \ub54c\ubb38\uc5d0 \uc601\uc801\uc73c\ub85c \uc8fd\uc5b4 \uc788\uc5c8\uc73c\ub098 \ud558\ub098\ub2d8\uc774 \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \uc5ec\ub7ec\ubd84\uc744 \uc0b4\ub9ac\uc168\uc2b5\ub2c8\ub2e4. \ud558\ub098\ub2d8\uc740 \uc6b0\ub9ac\uc758 \ubaa8\ub4e0 \uc8c4\ub97c \uc6a9\uc11c\ud574 \uc8fc\uc2dc\uace0"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uc6b0\ub9ac\uc5d0\uac8c \ubd88\ub9ac\ud55c \uc728\ubc95\uc758 \ucc44\ubb34 \uc99d\uc11c\ub97c \uc2ed\uc790\uac00\uc5d0 \ubabb\ubc15\uc544 \uc5c6\uc560 \ubc84\ub9ac\uc168\uc2b5\ub2c8\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\ub9ac\uace0 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c\ub294 \uc0ac\ud0c4\uc758 \uad8c\uc138\ub97c \uc9d3\ubc1f\uc544 \uc2ed\uc790\uac00\ub85c \uc2b9\ub9ac\ud558\uc154\uc11c \uadf8\uac83\uc744 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \ubcf4\uc5ec \uc8fc\uc168\uc2b5\ub2c8\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \uc5ec\ub7ec\ubd84\uc740 \uba39\uace0 \ub9c8\uc2dc\ub294 \uac83\uc774\ub098 \uba85\uc808\uc774\ub098 \ub9e4\uc6d4 \ucd08\ud558\ub8e8\ub098 \uc548\uc2dd\uc77c\uc5d0 \uad00\ud574\uc11c \uc544\ubb34\ub3c4 \uc5ec\ub7ec\ubd84\uc744 \ube44\ud310\ud558\uc9c0 \ubabb\ud558\uac8c \ud558\uc2ed\uc2dc\uc624."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uc774\ub7f0 \uac83\ub4e4\uc740 \uc2e4\uccb4\uc774\uc2e0 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uac00\ub9ac\ud0a4\ub294 \uadf8\ub9bc\uc790\uc5d0 \ubd88\uacfc\ud569\ub2c8\ub2e4."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uc77c\ubd80\ub7ec \uacb8\uc190\ud55c \uccb4\ud558\uba70 \ucc9c\uc0ac\ub4e4\uc744 \uc22d\ubc30\ud558\ub294 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \uc18d\uc544\uc11c \uc5ec\ub7ec\ubd84\uc758 \uc0c1\uc744 \ube7c\uc557\uae30\ub294 \uc77c\uc774 \uc5c6\ub3c4\ub85d \ud558\uc2ed\uc2dc\uc624. \uadf8\ub4e4\uc740 \uc790\uae30\ub4e4\uc774 \ubcf8 \ud658\uc0c1\uc744 \uacfc\uc7a5\ud558\uba70 \ud5db\ub41c \uc0dd\uac01\uc73c\ub85c \ub4e4\ub5a0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\uadf8\ub4e4\uc740 \uba38\ub9ac \ub418\uc2dc\ub294 \uadf8\ub9ac\uc2a4\ub3c4\uc5d0\uac8c \ubd99\uc5b4 \uc788\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4. \uc628 \ubab8\uc740 \uc774 \uba38\ub9ac\ub97c \ud1b5\ud574 \uac01 \ub9c8\ub514\uc640 \ud798\uc904\ub85c \uc11c\ub85c \uc5f0\uacb0\ub418\uc5b4 \uc601\uc591 \uacf5\uae09\uc744 \ubc1b\uc544\uc11c \ud558\ub098\ub2d8\uc774 \uc6d0\ud558\uc2dc\ub294 \ub300\ub85c \uc790\ub77c\ub098\ub294 \uac83\uc785\ub2c8\ub2e4."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\uc5ec\ub7ec\ubd84\uc774 \uc774 \uc138\uc0c1\uc758 \ucd08\ubcf4\uc801 \uc6d0\ub9ac\uc5d0 \ub300\ud574\uc11c \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \uc8fd\uc5c8\ub2e4\uba74 \uc5b4\uc9f8\uc11c \uc138\uc0c1\uc5d0 \uc18d\ud55c \uc0ac\ub78c\ub4e4\ucc98\ub7fc \u201c\uc190\ub300\uc9c0 \ub9d0\uc544\ub77c! \ub9db\ubcf4\uc9c0 \ub9d0\uc544\ub77c! \ub9cc\uc9c0\uc9c0 \ub9d0\uc544\ub77c!\u201d \ub294 \uc728\ubc95\uc801 \uaddc\uc815\uc5d0 \ubcf5\uc885\ud569\ub2c8\uae4c?"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\uc5ec\ub7ec\ubd84\uc774 \uc774 \uc138\uc0c1\uc758 \ucd08\ubcf4\uc801 \uc6d0\ub9ac\uc5d0 \ub300\ud574\uc11c \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \uc8fd\uc5c8\ub2e4\uba74 \uc5b4\uc9f8\uc11c \uc138\uc0c1\uc5d0 \uc18d\ud55c \uc0ac\ub78c\ub4e4\ucc98\ub7fc \u201c\uc190\ub300\uc9c0 \ub9d0\uc544\ub77c! \ub9db\ubcf4\uc9c0 \ub9d0\uc544\ub77c! \ub9cc\uc9c0\uc9c0 \ub9d0\uc544\ub77c!\u201d \ub294 \uc728\ubc95\uc801 \uaddc\uc815\uc5d0 \ubcf5\uc885\ud569\ub2c8\uae4c?"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\uc774\ub7f0 \uac83\uc740 \uc0ac\ub78c\uc758 \uacc4\uba85\uacfc \uac00\ub974\uce68\uc5d0 \uadfc\uac70\ud55c \uac83\uc785\ub2c8\ub2e4. \uc74c\uc2dd\ubb3c\uc740 \uba39\ub294 \ub300\ub85c \ubd80\ud328\ud558\uac8c \ub429\ub2c8\ub2e4."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\uc774\ub7f0 \uaddc\uc815\ub4e4\uc740 \uc81c\uba4b\ub300\ub85c \ub9cc\ub4e0 \uc885\uad50\uc801 \uc22d\ubc30\ub098 \uac70\uc9d3\ub41c \uacb8\uc190\uc774\ub098 \uc790\uae30 \ubab8\uc744 \uad34\ub86d\ud788\ub294 \ub370\ub294 \uc9c0\ud61c \uc788\ub294 \uac83\ucc98\ub7fc \ubcf4\uc774\uc9c0\ub9cc \uc721\uccb4\uc758 \uc815\uc695\uc744 \ub9c9\ub294 \ub370\ub294 \uc544\ubb34 \ud6a8\uacfc\uac00 \uc5c6\uc2b5\ub2c8\ub2e4."
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\ud558\ub298 \ub098\ub77c \uc77c\ub9cc \uc0dd\uac01\ud558\ub77c - \uadf8\ub7ec\ubbc0\ub85c \uc5ec\ub7ec\ubd84\uc774 \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \ub2e4\uc2dc \uc0b4\ub9ac\uc2ec\uc744 \ubc1b\uc558\uc73c\uba74 \uc704\uc5d0 \uc788\ub294 \uac83\uc744 \ucc3e\uc73c\uc2ed\uc2dc\uc624. \uac70\uae30\ub294 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \ud558\ub098\ub2d8\uc758 \uc624\ub978\ud3b8\uc5d0 \uc549\uc544 \uacc4\uc2dc\ub294 \uacf3\uc785\ub2c8\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \ud558\ub298\uc5d0 \uc788\ub294 \uac83\uc744 \uc0dd\uac01\ud558\uace0 \ub545\uc5d0 \uc788\ub294 \uac83\uc744 \uc0dd\uac01\ud558\uc9c0 \ub9c8\uc2ed\uc2dc\uc624."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uc774\uac83\uc740 \uc5ec\ub7ec\ubd84\uc774 \uc8fd\uc5c8\uace0 \uc5ec\ub7ec\ubd84\uc758 \uc0dd\uba85\uc774 \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \ud558\ub098\ub2d8 \uc548\uc5d0 \uac10\ucd94\uc5b4\uc838 \uc788\uae30 \ub54c\ubb38\uc785\ub2c8\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uc6b0\ub9ac\uc758 \uc0dd\uba85\uc774\uc2e0 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \ub098\ud0c0\ub098\uc2e4 \ub54c\uc5d0\ub294 \uc5ec\ub7ec\ubd84\ub3c4 \uadf8\ubd84\uacfc \ud568\uaed8 \uc601\uad11 \uac00\uc6b4\ub370 \ub098\ud0c0\ub098\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \uc5ec\ub7ec\ubd84\uc740 \ub545\uc5d0 \uc788\ub294 \uc721\uccb4\uc758 \uc695\ub9dd\uc744 \uc8fd\uc774\uc2ed\uc2dc\uc624. \uc74c\ub780\uacfc \ub354\ub7ec\uc6b4 \uc9d3\uacfc \uc815\uc695\uacfc \uc545\ud55c \uc695\ub9dd\uacfc \uc9c0\ub098\uce5c \uc695\uc2ec\uc740 \ubc84\ub824\uc57c \ud569\ub2c8\ub2e4. \uc9c0\ub098\uce5c \uc695\uc2ec\uc740 \uc6b0\uc0c1 \uc22d\ubc30\uc785\ub2c8\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uc774\ub7f0 \uac83 \ub54c\ubb38\uc5d0 \ud558\ub098\ub2d8\uc758 \ub178\uc5ec\uc6b0\uc2ec\uc744 \uc0ac\uac8c \ub429\ub2c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uc5ec\ub7ec\ubd84\ub3c4 \uc804\uc5d0 \uc774\ub7f0 \uc695\ub9dd\uc73c\ub85c \uc0b4 \ub54c\uc5d0\ub294 \uadf8\ub7f0 \uc9d3\uc744 \ud558\uc600\uc73c\ub098"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uc774\uc81c\ub294 \ubd84\ud55c \uc0dd\uac01\uacfc \ud654\ub0b4\ub294 \uac83\uacfc \uc545\uc758\uc640 \ube44\ubc29\uacfc \uc785\uc5d0\uc11c \ub098\uc624\ub294 \ub354\ub7ec\uc6b4 \ub9d0\uc744 \ubc84\ub9ac\uc2ed\uc2dc\uc624."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc11c\ub85c \uac70\uc9d3\ub9d0\uc744 \ud558\uc9c0 \ub9c8\uc2ed\uc2dc\uc624. \uc61b\ub0a0\uc758 \uc5ec\ub7ec\ubd84\uc740 \uc774\ubbf8 \uc8fd\uc5c8\uace0"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uc774\uc81c\ub294 \uc0c8\uc0ac\ub78c\uc774 \ub418\uc5c8\uc2b5\ub2c8\ub2e4. \uc774 \uc0c8\uc0ac\ub78c\uc740 \uc5ec\ub7ec\ubd84 \uc548\uc5d0 \uc0c8 \uc0dd\uba85\uc744 \ucc3d\uc870\ud558\uc2e0 \ud558\ub098\ub2d8\uc758 \ubaa8\uc2b5\uc744 \ub530\ub77c \ucc38\ub41c \uc9c0\uc2dd\uc5d0 \uc774\ub974\ub3c4\ub85d \uc0c8\ub86d\uac8c \ub418\uc5b4\uac00\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc5ec\uae30\uc5d0\ub294 \uadf8\ub9ac\uc2a4 \uc0ac\ub78c\uc774\ub098 \uc720\ub300\uc778\uc774\ub098 \ud560\ub840\ub97c \ubc1b\uc740 \uc0ac\ub78c\uc774\ub098 \ud560\ub840\ub97c \ubc1b\uc9c0 \uc54a\uc740 \uc0ac\ub78c\uc774\ub098 \uc57c\ub9cc\uc778\uc774\ub098 \ubbf8\uac1c\uc778\uc774\ub098 \uc885\uc774\ub098 \uc790\uc720\uc778\uc774\ub098 \ucc28\ubcc4\uc774 \uc5c6\uc2b5\ub2c8\ub2e4. \uadf8\ub9ac\uc2a4\ub3c4\ub294 \ubaa8\ub4e0 \uac83 \uadf8 \uc790\uccb4\uc774\uba70 \ubaa8\ub4e0 \uac83 \uc548\uc5d0 \uacc4\uc2ed\ub2c8\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \ud558\ub098\ub2d8\uc774 \ud0dd\ud558\uc2e0 \uc0ac\ub791\ubc1b\ub294 \uac70\ub8e9\ud55c \uc0ac\ub78c\ub4e4\uc785\ub2c8\ub2e4. \uadf8\ub7ec\ubbc0\ub85c \ubd88\uc30d\ud788 \uc5ec\uae30\ub294 \ub9c8\uc74c\uacfc \uce5c\uc808\uacfc \uacb8\uc190\uacfc \ubd80\ub4dc\ub7ec\uc6c0\uacfc \uc778\ub0b4\ub85c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uc11c\ub85c \ucc38\uc73c\uba70 \uc5b8\uc9e2\uc740 \uc77c\uc774 \uc788\ub354\ub77c\ub3c4 \uc8fc\ub2d8\uaed8\uc11c \uc5ec\ub7ec\ubd84\uc744 \uc6a9\uc11c\ud558\uc2e0 \uac83\uac19\uc774 \uc11c\ub85c \uc6a9\uc11c\ud558\uc2ed\uc2dc\uc624."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uadf8\ub9ac\uace0 \uc774 \ubaa8\ub4e0 \uac83 \uc704\uc5d0 \uc0ac\ub791\uc744 \ub354\ud558\uc2ed\uc2dc\uc624. \uc0ac\ub791\uc740 \uadf8 \ubaa8\ub4e0 \uac83\uc744 \uc644\uc804\ud788 \ud558\ub098\ub85c \ubb36\uc5b4 \uc8fc\ub294 \ub760\uc640 \uac19\uc740 \uac83\uc785\ub2c8\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uc758 \ud3c9\uc548\uc774 \uc5ec\ub7ec\ubd84\uc758 \ub9c8\uc74c\uc744 \ub2e4\uc2a4\ub9ac\uac8c \ud558\uc2ed\uc2dc\uc624. \ud3c9\uc548\uc744 \uc704\ud574 \uc5ec\ub7ec\ubd84\uc740 \ud55c\ubab8\uc73c\ub85c \ubd80\ub974\uc2ec\uc744 \ubc1b\uc558\uc2b5\ub2c8\ub2e4. \uadf8\ub9ac\uace0 \uc5ec\ub7ec\ubd84\uc740 \uac10\uc0ac\ud558\ub294 \uc0ac\ub78c\uc774 \ub418\uc2ed\uc2dc\uc624."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uc758 \ub9d0\uc500\uc774 \uc5ec\ub7ec\ubd84 \uac00\uc6b4\ub370 \ud48d\uc131\ud558\uac8c \ud558\uc5ec \ubaa8\ub4e0 \uc9c0\ud61c\ub85c \uc11c\ub85c \uac00\ub974\uce58\uace0 \uad8c\uba74\ud558\uba70 \uc2dc\uc640 \ucc2c\ubbf8\uc640 \uc601\uc801\uc778 \ub178\ub798\ub97c \ubd80\ub974\uba70 \uac10\uc0ac\ud558\ub294 \ub9c8\uc74c\uc73c\ub85c \ud558\ub098\ub2d8\uc744 \ucc2c\uc591\ud558\uc2ed\uc2dc\uc624."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uadf8\ub9ac\uace0 \ubb34\uc5c7\uc744 \ud558\ub4e0\uc9c0 \ub9d0\uacfc \ud589\ub3d9\uc5d0 \uc8fc \uc608\uc218\ub2d8\uc758 \uc774\ub984\uc73c\ub85c \ud558\uace0 \uadf8\ubd84\uc744 \ud1b5\ud574 \ud558\ub098\ub2d8 \uc544\ubc84\uc9c0\uaed8 \uac10\uc0ac\ud558\uc2ed\uc2dc\uc624."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uc544\ub984\ub2e4\uc6b4 \uac00\uc815 \uc0dd\ud65c - \uc544\ub0b4\ub4e4\uc740 \ub0a8\ud3b8\uc5d0\uac8c \ubcf5\uc885\ud558\uc2ed\uc2dc\uc624. \uc774\uac83\uc740 \uc8fc\ub2d8\uc744 \ubbff\ub294 \uc0ac\ub78c\uc73c\ub85c\uc11c \ub9c8\ub545\ud788 \ud574\uc57c \ud560 \uc77c\uc785\ub2c8\ub2e4."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\ub0a8\ud3b8\ub4e4\uc740 \uc544\ub0b4\ub97c \uc0ac\ub791\ud558\uace0 \uad34\ub86d\ud788\uc9c0 \ub9c8\uc2ed\uc2dc\uc624."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\uc790\ub140\ub4e4\uc740 \ubaa8\ub4e0 \uc77c\uc5d0 \ubd80\ubaa8\uc5d0\uac8c \uc21c\uc885\ud558\uc2ed\uc2dc\uc624. \uc774\uac83\uc774 \uc8fc\ub2d8\uc744 \uae30\uc058\uc2dc\uac8c \ud558\ub294 \uc77c\uc785\ub2c8\ub2e4."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\ubd80\ubaa8\ub4e4\uc740 \uc790\ub140\ub4e4\uc758 \uac10\uc815\uc744 \uac74\ub4dc\ub824 \ud654\ub098\uac8c \ud558\uc9c0 \ub9c8\uc2ed\uc2dc\uc624. \uadf8\ub807\uac8c \ud558\uba74 \uadf8\ub4e4\uc774 \ub099\uc2ec\ud558\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\uc885\ub4e4\uc740 \ubaa8\ub4e0 \uc77c\uc5d0 \uc790\uae30 \uc8fc\uc778\uc5d0\uac8c \uc21c\uc885\ud558\uc2ed\uc2dc\uc624. \ub2e8\uc21c\ud788 \uc0ac\ub78c\uc744 \uae30\uc058\uac8c \ud558\ub294 \uc0ac\ub78c\ub4e4\ucc98\ub7fc \ub208\uac00\ub9bc\ub9cc \ud558\uc9c0 \ub9d0\uace0 \uc8fc\ub2d8\uc744 \ub450\ub824\uc6cc\ud558\uba70 \uc131\uc2e4\ud55c \ub9c8\uc74c\uc73c\ub85c \uc8fc\uc778\uc744 \uc12c\uae30\uc2ed\uc2dc\uc624."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\ubb34\uc2a8 \uc77c\uc744 \ud558\ub4e0\uc9c0 \uc0ac\ub78c\uc5d0\uac8c \ud558\ub4ef \ud558\uc9c0 \ub9d0\uace0 \uc8fc\ub2d8\uaed8 \ud558\ub4ef \uc131\uc2e4\ud558\uac8c \ud558\uc2ed\uc2dc\uc624."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\uc5ec\ub7ec\ubd84\uc740 \uc8fc\ub2d8\uc5d0\uac8c \ud558\ub298\uc758 \ucd95\ubcf5\uc744 \uc0c1\uc73c\ub85c \ubc1b\uac8c \ub420 \uac83\uc744 \uae30\uc5b5\ud558\uc2ed\uc2dc\uc624. \uc5ec\ub7ec\ubd84\uc740 \uc8fc\ub2d8\uc774\uc2e0 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc12c\uae30\ub294 \uc0ac\ub78c\ub4e4\uc785\ub2c8\ub2e4."
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\uadf8\ub9ac\uace0 \ub098\uc05c \uc9d3\uc744 \ud558\ub294 \uc0ac\ub78c\uc740 \ubc18\ub4dc\uc2dc \uadf8 \ub300\uac00\ub97c \ubc1b\uc744 \uac83\uc785\ub2c8\ub2e4. \ud558\ub098\ub2d8\uc740 \uc678\ud615\uc801\uc778 \uac83\uc744 \ubcf4\uace0 \uc0ac\ub78c\uc744 \ucc28\ubcc4\ud558\uc9c0 \uc54a\uc73c\uc2ed\ub2c8\ub2e4."
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uc8fc\uc778 \ub41c \uc0ac\ub78c\ub4e4\uc740 \ud558\ub298\uc5d0\ub3c4 \uc790\uae30 \uc8fc\uc778\uc774 \uacc4\uc2e0\ub2e4\ub294 \uac83\uc744 \uc54c\uace0 \uc885\ub4e4\uc744 \uacf5\uc815\ud558 \uace0 \uc815\ub2f9\ud558\uac8c \ub300\uc6b0\ud558\uc2ed\uc2dc\uc624."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ud56d\uc0c1 \uae30\ub3c4\uc5d0 \ud798\uc4f0\uc2ed\uc2dc\uc624. \uae30\ub3c4\ud560 \ub54c \uc815\uc2e0\uc744 \ubc14\uc9dd \ucc28\ub9ac\uace0 \ud558\ub098\ub2d8\uaed8 \uac10\uc0ac\ud558\uc2ed\uc2dc\uc624."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uadf8\ub9ac\uace0 \ud558\ub098\ub2d8\uc774 \uc6b0\ub9ac\uc5d0\uac8c \uc804\ub3c4\uc758 \ubb38\uc744 \uc5f4\uc5b4 \uc8fc\uc154\uc11c \uadf8\ub9ac\uc2a4\ub3c4\uc758 \ube44\ubc00\uc744 \ub9d0\ud560 \uc218 \uc788\ub3c4\ub85d \uc6b0\ub9ac\ub97c \uc704\ud574 \uae30\ub3c4\ud574 \uc8fc\uc2ed\uc2dc\uc624. \ub098\ub294 \uc774 \uc77c \ub54c\ubb38\uc5d0 \uc9c0\uae08 \uac07\ud600 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub0b4\uac00 \uc774 \ube44\ubc00\uc744 \uba85\ud655\ud558\uac8c \uc804\ud558\ub3c4\ub85d \uae30\ub3c4\ud574 \uc8fc\uc2ed\uc2dc\uc624."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\ubbff\uc9c0 \uc54a\ub294 \uc0ac\ub78c\ub4e4\uc744 \ub300\ud560 \ub54c\ub294 \uc9c0\ud61c\ub86d\uac8c \ud589\ub3d9\ud558\uace0 \uae30\ud68c\ub97c \ucd5c\ub300\ud55c \uc774\uc6a9\ud558\uc2ed\uc2dc\uc624."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uadf8\ub9ac\uace0 \ub9d0\uc740 \uc5b8\uc81c\ub098 \uce5c\uc808\ud558\uace0 \uc7ac\uce58 \uc788\uac8c \ud558\uc2ed\uc2dc\uc624. \uadf8\ub7ec\uba74 \uac01 \uc0ac\ub78c\uc5d0\uac8c \ubc14\ub978 \ub300\ub2f5\uc744 \ud560 \uc218 \uc788\uc744 \uac83\uc785\ub2c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ub9c8\uc9c0\ub9c9 \uc778\uc0ac - \ub450\uae30\uace0\uac00 \ub098\uc5d0 \uad00\ud55c \ubaa8\ub4e0 \uc18c\uc2dd\uc744 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \ub9d0\ud574 \uc904 \uac83\uc785\ub2c8\ub2e4. \uadf8\ub294 \uc6b0\ub9ac\uc758 \uc0ac\ub791\ud558\ub294 \ud615\uc81c\uc774\uba70 \uc2e0\uc2e4\ud55c \uc77c\uafbc\uc774\uc694 \ub098\uc640 \ud568\uaed8 \uc8fc\ub2d8\uc758 \uc885\uc774 \ub41c \uc0ac\ub78c\uc785\ub2c8\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub0b4\uac00 \ud2b9\ubcc4\ud788 \uadf8\ub97c \uc5ec\ub7ec\ubd84\uc5d0\uac8c \ubcf4\ub0b4\ub294 \uac83\uc740 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \uc6b0\ub9ac \uc0ac\uc815\uc744 \uc54c\uac8c \ud558\uc5ec \uc5ec\ub7ec\ubd84\uc744 \uaca9\ub824\ud558\uae30 \uc704\ud574\uc11c\uc785\ub2c8\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uadf8\ub9ac\uace0 \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc1b\ub294 \uc2e0\uc2e4\ud55c \ud615\uc81c \uc624\ub124\uc2dc\ubaa8\ub3c4 \ud568\uaed8 \ubcf4\ub0c5\ub2c8\ub2e4. \uadf8\ub294 \uc5ec\ub7ec\ubd84\uacfc \ud568\uaed8 \uc788\ub358 \uc0ac\ub78c\uc785\ub2c8\ub2e4. \uadf8\ub4e4\uc774 \uc774 \uacf3 \uc0ac\uc815\uc744 \ubaa8\ub450 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \ub9d0\ud574 \uc904 \uac83\uc785\ub2c8\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub098\uc640 \ud568\uaed8 \uac07\ud600 \uc788\ub294 \uc544\ub9ac\uc2a4\ub2e4\uace0\uac00 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \ubb38\uc548\ud558\uace0 \ubc14\ub098\ubc14\uc758 \uc870\uce74 \ub9c8\uac00\ub3c4 \ubb38\uc548\ud569\ub2c8\ub2e4. \uc5ec\ub7ec\ubd84\uc774 \ub9c8\uac00\uc5d0 \ub300\ud55c \uc9c0\uc2dc\ub97c \uc774\ubbf8 \ubc1b\uc558\uaca0\uc9c0\ub9cc \uadf8\uac00 \uac00\uac70\ub4e0 \ub530\ub73b\ud558\uac8c \ub9de\uc544 \uc8fc\uc2ed\uc2dc\uc624."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uadf8\ub9ac\uace0 \uc720\uc2a4\ub3c4\ub77c\ub294 \uc608\uc218\ub3c4 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \ubb38\uc548\ud569\ub2c8\ub2e4. \uc720\ub300\uc778 \uac00\uc6b4\ub370 \ud558\ub098\ub2d8\uc758 \ub098\ub77c\ub97c \uc704\ud574 \ub098\uc640 \ud568\uaed8 \uc77c\ud558\ub294 \uc0ac\ub78c\uc740 \uc774 \uc0ac\ub78c\ub4e4\ubc16\uc5d0 \uc5c6\ub294\ub370 \uc774\ub4e4\uc774 \ub098\uc5d0\uac8c \ud070 \uc704\ub85c\uac00 \ub418\uc5c8\uc2b5\ub2c8\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ub610 \uc5ec\ub7ec\ubd84\uc5d0\uac8c\uc11c \uc628 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub2d8\uc758 \uc885, \uc5d0\ubc14\ube0c\ub77c\uac00 \ubb38\uc548\ud569\ub2c8\ub2e4. \uadf8\ub294 \uc5ec\ub7ec\ubd84\uc774 \ud558\ub098\ub2d8\uc758 \ub73b\uc744 \ud655\uc2e0\ud558\uace0 \uad73\uac8c \uc11c\uc11c \uc131\uc219\ud55c \uc2e0\uc559 \uc0dd\ud65c\uc744 \ud558\uac8c \ud574 \ub2ec\ub77c\uace0 \uc5b8\uc81c\ub098 \uc5f4\uc2ec\ud788 \uae30\ub3c4\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uadf8\ub294 \uc5ec\ub7ec\ubd84\ubfd0\ub9cc \uc544\ub2c8\ub77c \ub77c\uc624\ub514\uac8c\uc544\uc640 \ud788\uc5d0\ub77c\ubcfc\ub9ac\uc5d0 \uc788\ub294 \uc0ac\ub78c\ub4e4\uc744 \uc704\ud574 \ub9ce\uc740 \uc218\uace0\ub97c \ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uc0ac\ub791\ud558\ub294 \uc758\uc0ac \ub204\uac00\uac00 \uc5ec\ub7ec\ubd84\uc5d0\uac8c \ubb38\uc548\ud558\uba70 \ub370\ub9c8\ub3c4 \ubb38\uc548\ud569\ub2c8\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\ub77c\uc624\ub514\uac8c\uc544\uc5d0 \uc788\ub294 \ud615\uc81c\ub4e4\uacfc \ub214\ubc14\uc640 \uadf8\ub140\uc758 \uc9d1\uc5d0\uc11c \ubaa8\uc774\ub294 \uad50\ud68c\uc5d0 \uc548\ubd80\ub97c \uc804\ud574 \uc8fc\uc2ed\uc2dc\uc624."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uc5ec\ub7ec\ubd84\uc774 \uc774 \ud3b8\uc9c0\ub97c \uc77d\uc740 \ud6c4\uc5d0 \ub77c\uc624\ub514\uac8c\uc544 \uad50\ud68c\ub3c4 \uc77d\uac8c \ud558\uace0 \ub610 \ub77c\uc624\ub514\uac8c\uc544\uc5d0\uc11c \uc624\ub294 \ud3b8\uc9c0\ub3c4 \uc77d\uc73c\uc2ed\uc2dc\uc624."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uadf8\ub9ac\uace0 \uc544\ud0b5\ubcf4\uc5d0\uac8c\ub294 \uc8fc\ub2d8\uc5d0\uac8c \ubc1b\uc740 \uc9c1\ubb34\ub97c \uc131\uc2e4\ud788 \uc218\ud589\ud558\ub77c\uace0 \uc804\ud574 \uc8fc\uc2ed\uc2dc\uc624."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\ub098 \ubc14\uc6b8\uc740 \uc190\uc218 \uc774 \uc778\uc0ac\ub9d0\uc744 \uc368\uc11c \ubb38\uc548\ud569\ub2c8\ub2e4. \uc5ec\ub7ec\ubd84\uc740 \ub0b4\uac00 \uc9c0\uae08 \uac07\ud600 \uc788\ub2e4\ub294 \uc0ac\uc2e4\uc744 \uc78a\uc9c0 \ub9c8\uc2ed\uc2dc\uc624. \ud558\ub098\ub2d8\uc758 \uc740\ud61c\uac00 \uc5ec\ub7ec\ubd84\uacfc \ud568\uaed8\ud558\uae30\ub97c \uae30\ub3c4\ud569\ub2c8\ub2e4."
                }
            }
        }
    }
}