{
    "version": "klb",
    "book_name": "Song of Solomon",
    "book_nr": "22",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uc774\uac83\uc740 \uc194\ub85c\ubaac\uc774 \uc9c0\uc740 \ub178\ub798 \uc911\uc758 \ub178\ub798\uc774\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uccab\ubc88\uc9f8 \ub178\ub798 - (\uc5ec\uc790) \ub098\uc5d0\uac8c \uc785\ub9de\ucdb0 \uc8fc\uc138\uc694. \ub2f9\uc2e0\uc758 \uc0ac\ub791\uc740 \ud3ec\ub3c4\uc8fc\ubcf4\ub2e4 \ub354 \ub2ec\ucf64\ud569\ub2c8\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\ub2f9\uc2e0\uc758 \uae30\ub984\uc774 \ud5a5\uae30\ub86d\uace0 \ub2f9\uc2e0\uc758 \uc774\ub984\uc774 \uc3df\uc740 \ud5a5\uc218\uc640 \uac19\uc73c\ubbc0\ub85c \ucc98\ub140\ub4e4\uc774 \ub2f9\uc2e0\uc744 \uc0ac\ub791\ud569\ub2c8\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub098\ub97c \ub370\ub824\uac00 \uc8fc\uc138\uc694. \uc790, \ud568\uaed8 \ub2ec\ub824\uac11\uc2dc\ub2e4. \uc655\uc774 \ub098\ub97c \uc790\uae30 \uad81\uc804\uc73c\ub85c \ub370\ub824\uac00\uc168\uc73c\ub2c8 \uc6b0\ub9ac\ub294 \uc815\ub9d0 \ud589\ubcf5\ud560 \uac83\uc785\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \uc0ac\ub791\uc774 \ud3ec\ub3c4\uc8fc\ubcf4\ub2e4 \ub098\uc73c\ubbc0\ub85c \ubaa8\ub4e0 \uc5ec\uc790\ub4e4\uc774 \ub2f9\uc2e0\uc744 \uc0ac\ub791\ud558\ub294 \uac83\ub3c4 \ub2f9\uc5f0\ud569\ub2c8\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc608\ub8e8\uc0b4\ub818\uc758 \uc5ec\uc790\ub4e4\uc544, \ub098\ub294 \ube44\ub85d \uac80\uc9c0\ub9cc \uc544\ub984\ub2f5\ub2e8\ub2e4. \ub0b4\uac00 \uac80\uc740 \ucc9c\ub9c9\ucc98\ub7fc \uadf8\uc744\ub838\uc5b4\ub3c4 \uc194\ub85c\ubaac \uad81\uc804\uc758 \ud718\uc7a5\ucc98\ub7fc \uc544\ub984\ub2f5\ub2e8\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\ub0b4 \ud53c\ubd80\uac00 \uac80\ub2e4\uace0 \ub098\ub97c \ud758\uaca8\ubcf4\uc9c0 \ub9d0\uc544\ub77c. \ud587\ubcd5\uc5d0 \uadf8\uc744\ub838\uc744 \ubfd0\uc774\ub780\ub2e4. \ub0b4 \uc624\ube60\ub4e4\uc774 \ud654\ub97c \ub0b4\uc5b4 \ub098\ub97c \ud3ec\ub3c4\uc6d0\uc5d0\uc11c \uc77c\ud558\uac8c \ud558\uc600\uc73c\ubbc0\ub85c \ub0b4\uac00 \ubab8\uc744 \ub3cc\ubcfc \uc2dc\uac04\uc774 \uc5c6\uc5c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ub0b4\uac00 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc2dc\uc5ec, \uc624\ub298\uc740 \ub2f9\uc2e0\uc774 \uc591\ub5bc\ub97c \uc5b4\ub514\uc11c \uba39\uc774\uba70 \uc815\uc624\uc5d0\ub294 \uc5b4\ub514\uc5d0\uc11c \uc26c\ub294\uc9c0 \ub9d0\ud574 \uc8fc\uc138\uc694. \uc5b4\uc9f8\uc11c \ub0b4\uac00 \uc774\ub9ac\uc800\ub9ac \ubc29\ud669\ud558\uba70 \ub2f9\uc2e0 \uce5c\uad6c\ub4e4\uc758 \uc591\ub5bc \uac00\uc6b4\ub370\uc11c \ub2f9\uc2e0\uc744 \ucc3e\uc544\uc57c \ud569\ub2c8\uae4c?"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "(\ub0a8\uc790) \uc138\uc0c1\uc5d0\uc11c \uac00\uc7a5 \uc544\ub984\ub2e4\uc6b4 \uc5ec\uc778\uc774\uc5ec, \uadf8\ub300\uac00 \ub098 \uc788\ub294 \uacf3\uc744 \ubaa8\ub978\ub2e4\uba74 \ub0b4 \uc591\ub5bc\uc758 \ubc1c\uc790\ucde8\ub97c \ub530\ub77c \ubaa9\uc790\ub4e4\uc758 \ucc9c\ub9c9 \uacc1\uc5d0\uc11c \uadf8\ub300\uc758 \uc5fc\uc18c \uc0c8\ub07c\ub97c \uba39\uc774\uc2dc\uc624."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub098\uc758 \uc0ac\ub791\uc774\uc5ec, \uadf8\ub300\ub294 \uc554\ub9dd\uc544\uc9c0\ucc98\ub7fc \uc0ac\ub791\uc2a4\ub7fd\uad6c\ub824."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uba38\ub9ac\ub97c \ub298\uc5b4\ub728\ub9b0 \uadf8\ub300\uc758 \ubea8\uacfc \ubaa9\uac78\uc774\ub85c \uc7a5\uc2dd\ud55c \uadf8\ub300\uc758 \ubaa9\uc774 \ucc38\uc73c\ub85c \uc544\ub984\ub2f5\uc18c\uc774\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc6b0\ub9ac\uac00 \uadf8\ub300\ub97c \uc704\ud574 \uae08\uc73c\ub85c \uadc0\uace0\ub9ac\ub97c \ub9cc\ub4e4\uc5b4 \uc740\uc744 \ubc15\uc544 \uc8fc\uaca0\uc18c."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "(\uc5ec\uc790) \uc655\uc774 \uc2dd\ud0c1\uc5d0 \uc549\uc558\uc744 \ub54c \ub0b4 \ud5a5\uc218 \ub0c4\uc0c8\uac00 \uc9c4\ub3d9\ud558\uc600\ub124."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc740 \ub0b4 \ud488\uc18d\uc758 \ubab0\uc57d \uc8fc\uba38\ub2c8 \uac19\uace0"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uc5d4 \u2013 \uac8c\ub514 \uc815\uc6d0\uc758 \uaf43\uc1a1\uc774 \uac19\uad6c\ub098."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "(\ub0a8\uc790) \ub098\uc758 \uc0ac\ub791\uc774\uc5ec, \uadf8\ub300\ub294 \uc815\ub9d0 \uc544\ub984\ub2f5\uad6c\ub824. \uadf8\ub300\uc758 \ub208\uc740 \ube44\ub458\uae30 \uac19\uc18c\uc774\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "(\uc5ec\uc790) \ub098\uc758 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc2dc\uc5ec, \ub2f9\uc2e0\uc740 \uc815\ub9d0 \uba4b\uc788\uace0 \uc798\uc0dd\uacbc\uc2b5\ub2c8\ub2e4. \uc6b0\ub9ac \uce68\ub300\ub294 \ud478\ub978 \ud480\ubc2d\uc774\uc694"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uc6b0\ub9ac \uc9d1\uc758 \ub4e4\ubcf4\ub294 \ubc31\ud5a5\ubaa9\uc774\uba70 \uc11c\uae4c\ub798\ub294 \uc7a3\ub098\ubb34\uc785\ub2c8\ub2e4."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\uc5ec\uc790) \ub098\ub294 \uc0ac\ub860\uc758 \uc7a5\ubbf8\uc694 \uace8\uc9dc\uae30\uc758 \ubc31\ud569\ud654\ub77c\ub124."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "(\ub0a8\uc790) \ub0b4\uac00 \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \ub2e4\ub978 \uc5ec\uc790\uc640 \ube44\uad50\ud574 \ubcf4\ub2c8 \uac00\uc2dc\ub098\ubb34 \uac00\uc6b4\ub370 \uc788\ub294 \ubc31\ud569\ud654 \uac19\uad6c\ub098."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "(\uc5ec\uc790) \ub0b4\uac00 \uc0ac\ub791\ud558\ub294 \uc0ac\ub78c\uc744 \ub2e4\ub978 \ub0a8\uc790\uc640 \ube44\uad50\ud574 \ubcf4\ub2c8 \uc232\uc18d\uc758 \uc0ac\uacfc\ub098\ubb34 \uac19\uad6c\ub098. \ub0b4\uac00 \uadf8\uc758 \uadf8\ub298\uc5d0 \uc549\uc544\uc11c \uae30\ubed0\ud558\uba70 \uadf8\uc758 \uc5f4\ub9e4\ub97c \ub9db\uc788\uac8c \uba39\ub294\uad6c\ub098."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uadf8\uac00 \ub098\ub97c \ub370\ub9ac\uace0 \uc5f0\ud68c\uc7a5\uc73c\ub85c \ub4e4\uc5b4\uac00\uc11c \ub098\uc5d0\uac8c \uc0ac\ub791\uc758 \uae30\ub97c \uce58\ucf1c\uc62c\ub838\ub124."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uac74\ud3ec\ub3c4\ub85c \ub0b4 \ud798\uc744 \ud68c\ubcf5\uc2dc\ud0a4\uace0 \uc0ac\uacfc\ub85c \ub098\ub97c \uc2dc\uc6d0\ud558\uac8c \ud574 \ub2e4\uc624. \ub0b4\uac00 \uc0ac\ub791 \ub54c\ubb38\uc5d0 \ubcd1\uc774 \ub4e4\uc5c8\ub2e8\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uadf8\uac00 \ub098\uc5d0\uac8c \uadf8\uc758 \uc67c\uc190\uc744 \ubca0\uac8c \ud558\uace0 \uc624\ub978\uc190\uc73c\ub85c \ub098\ub97c \uc548\ub294\uad6c\ub098."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4\uc544, \ub0b4\uac00 \ub108\ud76c\uc5d0\uac8c \uac04\uc808\ud788 \ubd80\ud0c1\ud55c\ub2e4. \uc81c\ubc1c \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc29\ud574\ud558\uc9c0 \ub9d0\uc544\ub2e4\uc624."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub450 \ubc88\uc9f8 \ub178\ub798 - (\uc5ec\uc790) \uc0ac\ub791\ud558\ub294 \ub2d8\uc758 \ubaa9\uc18c\ub9ac\uac00 \ub4e4\ub9ac\ub294\uad6c\ub098. \uadf8\uac00 \uc0b0\uc744 \ub118\uace0 \ub4e4\uc744 \uc9c0\ub098 \ub2ec\ub824\uc628\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc740 \ub178\ub8e8\uc640 \uc5b4\ub9b0 \uc0ac\uc2b4 \uac19\uad6c\ub098. \uadf8\uac00 \ubcbd \ub4a4\uc5d0 \uc11c\uc11c \ucc3d\ud2c8\uc73c\ub85c \ub4e4\uc5ec\ub2e4\ubcf8\ub2e4\ub124."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774 \ub9d0\ud55c\ub2e4. (\ub0a8\uc790) \ub098\uc758 \uc0ac\ub791, \ub098\uc758 \ub2d8\uc774\uc5ec, \uc77c\uc5b4\ub098 \ud568\uaed8 \uac11\uc2dc\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uaca8\uc6b8\ub3c4 \uc9c0\ub098\uace0 \ube44\ub3c4 \uadf8\ucce4\uc73c\uba70"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uaf43\uc774 \ud53c\uace0 \uc0c8\uac00 \ub178\ub798\ud558\ub294 \ub54c\uac00 \ub418\uc5b4 \ube44\ub458\uae30 \uc18c\ub9ac\uac00 \ub4e4\ub9ac\uace0 \uc788\uc18c."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ubb34\ud654\uacfc\uac00 \ub9fa\ud788\uae30 \uc2dc\uc791\ud558\uace0 \ud3ec\ub3c4\ub098\ubb34\uac00 \uaf43\uc774 \ud53c\uc5b4 \ud5a5\uae30\ub97c \ub0a0\ub9bd\ub2c8\ub2e4. \ub098\uc758 \uc0ac\ub791, \ub098\uc758 \ub2d8\uc774\uc5ec, \uc77c\uc5b4\ub098 \ud568\uaed8 \uac11\uc2dc\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\ubc14\uc704 \ud2c8\uc5d0 \uc228\uc740 \ube44\ub458\uae30 \uac19\uc740 \ub2d8\uc774\uc5ec, \uadf8\ub300\uc758 \uc0ac\ub791\uc2a4\ub7f0 \uc5bc\uad74\uc744 \ubcf4\uc774\uace0 \uadf8\ub300\uc758 \uc544\ub984\ub2e4\uc6b4 \ubaa9\uc18c\ub9ac\ub97c \ub4e4\ub824 \uc8fc\uc624."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uc5ec\uc6b0\ub97c \uc7a1\uc544\ub77c. \ud3ec\ub3c4\uc6d0\uc744 \ud574\uce58\ub294 \uc791\uc740 \uc5ec\uc6b0\ub97c \uc7a1\uc544\ub77c. \uc6b0\ub9ac\uc758 \ud3ec\ub3c4\uc6d0\uc5d0 \uaf43\uc774 \ud53c\uc5c8\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "(\uc5ec\uc790) \ub2d8\uc740 \ub098\uc758 \uc0ac\ub791, \ub098\ub294 \ub2d8\uc758 \uc0ac\ub791, \uadf8\uac00 \ubc31\ud569\ud654 \uac00\uc6b4\ub370\uc11c \uc591\ub5bc\ub97c \uba39\uc774\ub294\uad6c\ub098."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc2dc\uc5ec, \ub0a0\uc774 \uc0c8\uae30 \uc804, \uc5b4\ub450\uc6c0\uc774 \uc0ac\ub77c\uc9c0\uae30 \uc804\uc5d0 \ub3cc\uc544\uc640\uc11c \ubca0\ub378\uc0b0\uc758 \ub178\ub8e8\uc640 \uc5b4\ub9b0 \uc0ac\uc2b4\ucc98\ub7fc \ub418\uc5b4 \uc8fc\uc138\uc694."
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\uc5ec\uc790) \ub0b4\uac00 \uc7a0\uc790\ub9ac\uc5d0\uc11c \ubc24\uc0c8\ub3c4\ub85d \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \uadf8\ub9ac\uc6cc\ud558\uace0 \ucc3e\uc544\ub3c4 \ucc3e\uc9c0 \ubabb \ud558\uc600\ub124."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ub0b4\uac00 \uc77c\uc5b4\ub098 \uac70\ub9ac\uc640 \uad11\uc7a5\uc73c\ub85c \ub098\uac00\uc11c \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \ucc3e\uc558\uc73c\ub098 \ub9cc\ub098\uc9c0 \ubabb\ud558\uc600\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uc57c\uacbd\uafbc\ub4e4\uc744 \ub9cc\ub098 \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \ubcf4\uc558\ub290\ub0d0\uace0 \ubb3c\uc5c8\uc9c0."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uadf8\ub4e4\uc744 \ub5a0\ub09c \uc989\uc2dc \ub098\ub294 \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \ub9cc\ub0ac\ub124. \uadf8\ub798\uc11c \uadf8\ub97c \ubd99\uc7a1\uc544 \uac00\uc9c0 \ubabb\ud558\uac8c \ud558\uace0 \ub0b4\uac00 \ud0dc\uc5b4\ub09c \uc6b0\ub9ac \uc9d1 \ubc29\uc73c\ub85c \ub370\ub9ac\uace0 \uac14\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4\uc544, \ub0b4\uac00 \ub108\ud76c\uc5d0\uac8c \uac04\uc808\ud788 \ubd80\ud0c1\ud55c\ub2e4. \uc81c\ubc1c \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc29\ud574\ud558\uc9c0 \ub9d0\uc544\ub2e4\uc624."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uc138 \ubc88\uc9f8 \ub178\ub798 - (\uc5ec\uc790) \uc0c1\uc778\ub4e4\uc758 \ud5a5\ud488\uc73c\ub85c \ub9cc\ub4e0 \ubab0\uc57d\uacfc \uc720\ud5a5\uc758 \ud5a5\uae30\ub97c \ub0a0\ub9ac\uba70 \uc5f0\uae30 \uae30\ub465\ucc98\ub7fc \uc0ac\ub9c9\uc5d0\uc11c \uc624\ub294 \uc790\uac00 \ub204\uad6c\uc778\uac00?"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ubcf4\ub77c, \uc774\uc2a4\ub77c\uc5d8\uc758 \uc6a9\uac10\ud55c \ubcd1\uc0ac 60\uba85\uc758 \ud638\uc704\ub97c \ubc1b\uc73c\uba70 \uc194\ub85c\ubaac\uc758 \ub9c8\ucc28\uac00 \uc624\uace0 \uc788\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uadf8 \uacbd\ud638\ubcd1\ub4e4\uc740 \uce7c\uc744 \uc798 \uc4f0\ub294 \uc5ed\uc804\uc758 \uc6a9\uc0ac\ub4e4\uc774\uba70 \ubc24\uc5d0\ub3c4 \uc655\uc744 \uacbd\ud638\ud558\ub824\uace0 \ubaa8\ub450 \ud5c8\ub9ac\uc5d0 \uce7c\uc744 \ucc3c\uad6c\ub098."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uc194\ub85c\ubaac\uc655\uc774 \ub808\ubc14\ub17c\uc758 \ub098\ubb34\ub85c \uc790\uae30 \ub9c8\ucc28\ub97c \ub9cc\ub4e4\uc5c8\uc73c\ub2c8"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uadf8 \uae30\ub465\uc740 \uc740\uc774\uc694 \ubc14\ub2e5\uc740 \uae08\uc774\uba70 \uc549\ub294 \uc790\ub9ac\ub294 \uc790\uc0c9 \ucc9c\uc73c\ub85c \ub9cc\ub4e4\uc5c8\uace0 \uadf8 \ub0b4\ubd80\ub294 \uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4\uc758 \uc0ac\ub791\uc73c\ub85c \uafb8\uba70\uc84c\ub124."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc2dc\uc628\uc758 \uc5ec\uc790\ub4e4\uc544, \ub098\uc640\uc11c \uc194\ub85c\ubaac\uc655\uc744 \ubcf4\uc544\ub77c. \uacb0\ud63c\uc2dd \ub0a0 \uadf8\uc758 \ub9c8\uc74c\uc774 \uae30\uc060 \ub54c \uadf8 \uc5b4\uba38\ub2c8\uac00 \uc50c\uc6cc \uc900 \uba74\ub958\uad00\uc744 \ubcf4\uc544\ub77c."
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\ub0a8\uc790) \ub098\uc758 \uc0ac\ub791, \uadf8\ub300\ub294 \uc815\ub9d0 \uc544\ub984\ub2f5\uad6c\ub824. \ubca0\uc77c\uc5d0 \uac00\ub9ac\uc5b4\uc9c4 \uadf8\ub300\uc758 \ub208\uc740 \ube44 \ub458\uae30 \uac19\uace0 \uadf8\ub300\uc758 \uba38\ub9ac\ud138\uc740 \uae38\ub974\uc557\uc0b0\uc5d0\uc11c \ub6f0\uc5b4\ub0b4\ub824\uc624\ub294 \uc5fc\uc18c\ub5bc \uac19\uc18c\uc774\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uadf8\ub300\uc758 \uc774\ub294 \uc774\uc81c \ub9c9 \ud138\uc744 \uae4e\uace0 \ubaa9\uc695\uc744 \ud55c \uc591\uc758 \ud138\ucc98\ub7fc \ud76c\uace0 \ud558\ub098\ub3c4 \ube60\uc9c4 \uac83 \uc5c6\uc774 \uac00\uc9c0\ub7f0\ud558\uba70"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uc575\ub450 \uac19\uc740 \uadf8\ub300\uc758 \uc785\uc220\uc740 \uc815\ub9d0 \uc544\ub984\ub2f5\uad6c\ub824. \ubca0\uc77c\uc5d0 \uac00\ub9ac\uc5b4\uc9c4 \uadf8\ub300\uc758 \ubea8\uc740 \ucabc\uac1c \ub193\uc740 \uc11d\ub958 \uac19\uc73c\uba70"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uadf8\ub300\uc758 \ubaa9\uc740 \uad70\uc778\ub4e4\uc758 \ubc29\ud328 1,000\uac1c\uac00 \ub2ec\ub9b0 \ub2e4\uc717\uc758 \uc6b0\uc544\ud55c \ub9dd\ub300 \uac19\uace0"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uadf8\ub300\uc758 \uc816\uac00\uc2b4\uc740 \ubc31\ud569\ud654 \uac00\uc6b4\ub370\uc11c \uaf34\uc744 \uba39\ub294 \uc30d\ud0dc \ub178\ub8e8 \uc0c8\ub07c \uac19\uc18c\uc774\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\ub0a0\uc774 \uc0c8\uace0 \uc5b4\ub450\uc6c0\uc774 \uc0ac\ub77c\uc9c8 \ub54c\uae4c\uc9c0 \ub0b4\uac00 \uc720\ud5a5 \uc0b0\uacfc \ubab0\uc57d \uc0b0\uc73c\ub85c \uac00\uc624\ub9ac\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ub098\uc758 \uc0ac\ub791, \uadf8\ub300\ub294 \uc815\ub9d0 \uc544\ub984\ub2f5\uace0 \ud760 \ud558\ub098 \uc5c6\uad6c\ub824."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub098\uc758 \uc2e0\ubd80\uc5ec, \ub808\ubc14\ub17c\uc744 \ub5a0\ub098 \ub098\uc640 \ud568\uaed8 \uac11\uc2dc\ub2e4. \uc544\ub9c8\ub098\uc0b0\uacfc \uc2a4\ub2d0\uc0b0\uacfc \ud5e4\ub974\ubaac\uc0b0\uc5d0\uc11c \ub0b4\ub824\uc624\uc2dc\uc624. \uac70\uae30\ub294 \uc0ac\uc790\uc640 \ud45c\ubc94\uc774 \uc0b4\uace0 \uc788\uc18c."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub098\uc758 \ub204\uc774, \ub098\uc758 \uc2e0\ubd80\uc5ec, \uadf8\ub300\uac00 \ub0b4 \ub9c8\uc74c\uc744 \ube7c\uc557\uc544 \uac14\uc18c\uc774\ub2e4. \uadf8\ub300\uac00 \ud55c \ubc88\uc758 \ub208\uc9d3\uacfc \ubaa9\uac78\uc774\uc758 \ubcf4\uc11d \ud558\ub098\ub85c \ub0b4 \ub9c8\uc74c\uc744 \ud6d4\uccd0 \uac00\uace0 \ub9d0\uc558\uc18c."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub098\uc758 \ub204\uc774, \ub098\uc758 \uc2e0\ubd80\uc5ec, \uadf8\ub300\uc758 \uc0ac\ub791\uc774 \uc5b4\ucc0c \uadf8\ub9ac \uc544\ub984\ub2e4\uc6b4\uc9c0\uc694! \uadf8\ub300\uc758 \uc0ac\ub791\uc740 \ud3ec\ub3c4\uc8fc\ubcf4\ub2e4 \ub2ec\ucf64\ud558\uace0 \uadf8\ub300\uc758 \ud5a5\uae30\ub294 \uadf8 \uc5b4\ub5a4 \ud5a5\uc218\ubcf4\ub2e4 \ub0ab\ub2f5\ub2c8\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\ub098\uc758 \uc2e0\ubd80\uc5ec, \uadf8\ub300\uc758 \uc785\uc220\uc5d0\uc11c\ub294 \uafc0\uc774 \ub5a8\uc5b4\uc9c0\uace0 \uadf8\ub300\uc758 \ud600 \ubc11\uc5d0\ub3c4 \uafc0\uacfc \uc6b0\uc720\uac00 \uc788\uc73c\uba70 \uadf8\ub300\uc758 \uc637\uc5d0\uc11c\ub294 \ub808\ubc14\ub17c\uc758 \ud5a5\uae30\uac00 \ud48d\uae30\ub294\uad6c\ub824."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ub098\uc758 \ub204\uc774, \ub098\uc758 \uc2e0\ubd80\uc5ec, \uadf8\ub300\ub294 \uacf5\uac1c\ub418\uc9c0 \uc54a\uc740 \ub3d9\uc0b0\uc774\uba70 \ub098 \ud63c\uc790\ub9cc\uc774 \ub9c8\uc2e4 \uc218 \uc788\ub294 \uc6b0\ubb3c\uc774\uc694 \uc0d8\uc774\ub77c\uc624."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uadf8\ub300\ub294 \uc11d\ub958\uc640 \uac19\uc740"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uac01\uc885 \uc544\ub984\ub2e4\uc6b4 \uacfc\uc77c \u2013 \ud5e4\ub108, \ub098\uc544\ub4dc, \uc0ac\ud504\ub780, \ucc3d\ud3ec\uc640 \uacc4\ud53c, \uc628\uac16 \uc720\ud5a5\ubaa9\uacfc \ubab0\uc57d\uacfc \uce68\ud5a5, \uac00\uc7a5 \uc88b\uc740 \ubaa8\ub4e0 \ud5a5\ud488\uacfc\ub3c4 \uac19\uc18c\uc774\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uadf8\ub300\ub294 \uc815\uc6d0\uc758 \uc0d8\uc774\uc694 \uc0dd\uc218\uc758 \uc6b0\ubb3c\uc774\uba70 \ub808\ubc14\ub17c \uc0b0\uc5d0\uc11c \ud750\ub974\ub294 \uc2dc\ub0b4\ub78d\ub2c8\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "(\uc5ec\uc790) \ubd81\ud48d\uc544, \uc77c\uc5b4\ub77c. \ub0a8\ud48d\uc544, \uc624\ub108\ub77c. \ub0b4 \ub3d9\uc0b0\uc5d0 \ubd88\uc5b4\uc640\uc11c \ud5a5\uae30\ub97c \ub0a0\ub824\ub77c. \ub0b4 \ub2d8\uc774 \uc790\uae30 \ub3d9\uc0b0\uc5d0 \ub4e4\uc5b4\uac00\uc11c \uc81c\uc77c \uc88b\uc740 \uacfc\uc77c\uc744 \uba39\uac8c \ud574 \ub2e4\uc624."
                }
            }
        },
        "5": {
            "chapter_nr": "5",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\ub0a8\uc790) \ub098\uc758 \ub204\uc774, \ub098\uc758 \uc2e0\ubd80\uc5ec, \ub0b4\uac00 \ub0b4 \ub3d9\uc0b0\uc5d0 \ub4e4\uc5b4\uc640\uc11c \ud5a5\ud488\uacfc \ubab0\uc57d\uc744 \uac70\ub450 \uace0 \ub0b4 \uafc0\uc744 \uba39\uc73c\uba70 \ub0b4 \ud3ec\ub3c4\uc8fc\uc640 \uc816\uc744 \ub9c8\uc2e0\ub2e4\uc624. (\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4) \uce5c\uad6c\ub4e4\uc544, \uba39\uace0 \ub9c8\uc154\ub77c. \uc5f0\uc778\ub4e4\uc544, \uc0ac\ub791\uc5d0 \ucde8\ud560 \ub54c\uae4c\uc9c0 \ub9c8\uc154\ub77c."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ub124 \ubc88\uc9f8 \ub178\ub798 - (\uc5ec\uc790) \ub0b4\uac00 \uc790\uba74\uc11c\ub3c4 \ub9c8\uc74c\uc740 \uae68\uc5b4 \uc788\uc73c\ub2c8 \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774 \ubb38\uc744 \ub450\ub4dc\ub9ac\ub294 \uc18c\ub9ac\uac00 \ub4e4\ub9ac\ub294\uad6c\ub098. (\ub0a8\uc790) \ub098\uc758 \ub204\uc774, \ub098\uc758 \uc0ac\ub791, \ub098\uc758 \uc544\ub984\ub2e4\uc6b4 \ube44\ub458\uae30\uc5ec, \ubb38\uc744 \uc5f4\uc5b4 \uc8fc\uc2dc\uc624. \ub0b4 \uba38\ub9ac\uac00 \uc774\uc2ac\uc5d0 \uc816\uace0 \ub0b4 \uba38\ub9ac\ud138\uc774 \ubc24 \uc548\uac1c\uc5d0 \uc816\uc5c8\ub2e4\uc624."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "(\uc5ec\uc790) \ub0b4\uac00 \uc637\uc744 \ubc97\uc5c8\ub294\ub370 \uc5b4\ub5bb\uac8c \ub2e4\uc2dc \uc785\uaca0\uc2b5\ub2c8\uae4c? \ub0b4\uac00 \ubc1c\uc744 \uc53b\uc5c8\ub294\ub370 \uc5b4\ub5bb\uac8c \ub354\ub7fd\ud790 \uc218 \uc788\uaca0\uc2b5\ub2c8\uae4c?"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774 \ubb38\ud2c8\uc73c\ub85c \uc190\uc744 \ub0b4\uc5b4\ubc00\uc790 \ub0b4 \uac00\uc2b4\uc774 \ub450\uadfc\uac70\ub838\ub124."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\ub0b4\uac00 \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \uc704\ud574 \ubb38\uc744 \uc5f4\ub824\uace0 \uc77c\uc5b4\ub098 \ubb38\uace0\ub9ac\ub97c \uc7a1\uc73c\ub2c8 \ub0b4 \uc190\uc5d0\uc11c \ubab0\uc57d\uc774, \ub0b4 \uc190\uac00\ub77d\uc5d0\uc11c \ud5a5\uc218\uac00 \ub5a8\uc5b4\uc9c0\ub294\uad6c\ub098."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\ub0b4\uac00 \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \uc704\ud574 \ubb38\uc744 \uc5f4\uc5c8\uc73c\ub098 \uadf8\ub294 \uc774\ubbf8 \uac00\uace0 \uc5c6\uc5c8\ub2e4. \ub0b4\uac00 \uc5bc\ub9c8\ub098 \ub2d8\uc758 \ubaa9\uc18c\ub9ac\ub97c \ub4e3\uace0 \uc2f6\uc5b4\ud588\ub358\uac00! \ub0b4\uac00 \uadf8\ub97c \ucc3e\uc544\ub3c4 \ub9cc\ub098\uc9c0 \ubabb\ud558\uc600\uace0 \uadf8\ub97c \ubd88\ub7ec\ub3c4 \ub300\ub2f5\uc774 \uc5c6\uc5c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uc57c\uacbd\uafbc\ub4e4\uc774 \ub098\ub97c \ubcf4\uc790 \ub098\ub97c \uccd0\uc11c \ubd80\uc0c1\uc744 \uc785\ud788\uace0 \uc131\uc744 \uc9c0\ud0a4\ub294 \uacbd\ube44\ubcd1\ub4e4\uc774 \ub0b4 \uc6c3\uc637\uc744 \ubc97\uaca8 \uac14\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4\uc544, \ub0b4\uac00 \ub108\ud76c\uc5d0\uac8c \ubd80\ud0c1\ud55c\ub2e4. \ub108\ud76c\uac00 \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc744 \ub9cc\ub098\uac70\ub4e0 \ub0b4\uac00 \uc0ac\ub791 \ub54c\ubb38\uc5d0 \ubcd1\uc774 \ub0ac\ub2e4\uace0 \ub9d0\ud574 \ub2e4\uc624."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "(\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4) \uc5ec\uc790 \uc911\uc5d0 \uac00\uc7a5 \uc544\ub984\ub2e4\uc6b4 \ubbf8\uc778\uc774\uc5ec, \ub124 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774 \ub2e4\ub978 \uc0ac\ub78c\ubcf4\ub2e4 \ub098\uc740 \uac83\uc774 \ubb34\uc5c7\uc778\uac00? \uadf8\uac00 \uc5bc\ub9c8\ub098 \uc798\ub0ac\ub2e4\uace0 \ub124\uac00 \uc6b0\ub9ac\uc5d0\uac8c \uc774\ub7f0 \ubd80\ud0c1\uc744 \ud558\ub294\uac00?"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "(\uc5ec\uc790) \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc740 \ud608\uc0c9\uc774 \uc88b\uace0 \uac74\uc7a5\ud558\uba70 \ubb47 \ub0a8\uc131 \uac00\uc6b4\ub370\uc11c\ub3c4 \uac00\uc7a5 \ub6f0\uc5b4\ub09c \ubbf8\ub0a8\uc790\ub780\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uadf8\uc758 \uba38\ub9ac\ub294 \uc21c\uae08 \uac19\uace0 \uadf8\uc758 \uba38\ub9ac\ud138\uc740 \uace0\ubd88\uace0\ubd88\ud558\uba70 \uae4c\ub9c8\uadc0\ucc98\ub7fc \uac80\uace0"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uadf8\uc758 \ub208\uc740 \uc2dc\ub0c7\uac00\uc758 \ube44\ub458\uae30 \uac19\uc544\uc11c \uc6b0\uc720\uc5d0 \uc53b\uc740 \ub4ef\ud558\uace0 \ubcf4\uc11d\uc774 \ubc15\ud78c \ub4ef\uc774 \uc544\ub984\ub2e4\uc6b0\uba70"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ubea8\uc740 \ud5a5\uae30\ub85c\uc6b4 \uaf43\ubc2d\uacfc \ud5a5\ub0b4 \ub098\ub294 \ud480 \uc5b8\ub355 \uac19\uace0 \uc785\uc220\uc740 \ubc31\ud569\ud654 \uac19\uc73c\uba70 \ubab0\uc57d\uc774 \ub69d\ub69d \ub5a8\uc5b4\uc9c4\ub2e8\ub2e4."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\uadf8\uc758 \ud314\uc740 \ubcf4\uc11d\uc774 \ubc15\ud78c \ud669\uae08 \uc9c0\ud321\uc774 \uac19\uace0 \uadf8\uc758 \ubab8\uc740 \uccad\uc625\uc744 \ubc15\uc740 \uc724\ub098\ub294 \uc0c1\uc544 \uac19\uc73c\uba70"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\ub2e4\ub9ac\ub294 \uc815\uae08 \ubc1b\uce68\uc5d0 \uc138\uc6b4 \ub300\ub9ac\uc11d \uae30\ub465 \uac19\uace0 \uc678\ubaa8\ub294 \ub808\ubc14\ub17c\uc758 \ubc31\ud5a5\ubaa9\ucc98\ub7fc \uc798\uc0dd\uacbc\uc73c\uba70"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uadf8\uc758 \uc785\ub3c4 \ub2ec\ucf64\ud558\uc5ec \ubaa8\ub4e0 \uac83\uc774 \uc0ac\ub791\uc2a4\ub7fd\uae30\ub9cc \ud558\uad6c\ub098. \uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4\uc544, \uc774 \uc0ac\ub78c\uc774 \ubc14\ub85c \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\ub780\ub2e4."
                }
            }
        },
        "6": {
            "chapter_nr": "6",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4) \uc5ec\uc790 \uc911\uc5d0 \uac00\uc7a5 \uc544\ub984\ub2e4\uc6b4 \ubbf8\uc778\uc774\uc5ec, \ub108\uc758 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774 \uc5b4\ub514\ub85c \uac14\ub294\uac00? \uadf8\uac00 \uc5b4\ub290 \uae38\ub85c \uac14\ub294\uc9c0 \ub9d0\ud574 \ub2e4\uc624. \uc6b0\ub9ac\uac00 \ub108\uc640 \ud568\uaed8 \ucc3e\uc544\ubcf4\uaca0\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "(\uc5ec\uc790) \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc740 \ud5a5\ub098\ubb34\uac00 \uc790\ub77c\ub294 \uc790\uae30 \uc815\uc6d0\uc73c\ub85c \ub0b4\ub824\uac00 \ub3d9\uc0b0\uc5d0\uc11c \uc591\ub5bc\ub97c \uba39\uc774\uba70 \ubc31\ud569\ud654\ub97c \ub530\ub294\uad6c\ub098."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\ub2d8\uc740 \ub098\uc758 \uc0ac\ub791, \ub098\ub294 \ub2d8\uc758 \uc0ac\ub791, \uadf8 \ub2d8\uc774 \ubc31\ud569\ud654 \uac00\uc6b4\ub370\uc11c \uc790\uae30 \uc591\ub5bc\ub97c \uba39\uc778\ub2e4\ub124."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub2e4\uc12f \ubc88\uc9f8 \ub178\ub798 - (\ub0a8\uc790) \ub098\uc758 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc5ec, \uadf8\ub300\ub294 \ub514\ub974\uc0ac\ucc98\ub7fc \uc544\ub984\ub2f5\uace0 \uc608\ub8e8\uc0b4\ub818\ucc98\ub7fc \uc0ac\ub791\uc2a4\ub7ec\uc6b0\uba70 \uae30\ub97c \ub192\uc774 \ub4e0 \uad70\ub300\ucc98\ub7fc \uc704\uc5c4\uc774 \uc788\uc18c\uc774\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uadf8\ub300\uc758 \ub208\uc774 \ub098\ub97c \uc0ac\ub85c\uc7a1\uace0 \uc788\uc18c. \ub208\uae38\uc744 \ub0b4\uac8c\uc11c \ub3cc\ub824 \uc8fc\uc2dc\uc624. \uadf8\ub300\uc758 \uba38\ub9ac\ud138\uc740 \uae38\ub974\uc557\uc0b0\uc5d0\uc11c \ub6f0\uc5b4\ub0b4\ub824\uc624\ub294 \uc5fc\uc18c\ub5bc \uac19\uc73c\uba70"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uadf8\ub300\uc758 \uc774\ub294 \uc774\uc81c \ub9c9 \ubaa9\uc695\ud55c \uc591\uc758 \ud138\ucc98\ub7fc \ud76c\uace0 \ud558\ub098\ub3c4 \ube60\uc9c4 \uac83 \uc5c6\uc774 \uac00\uc9c0\ub7f0\ud558\uba70"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ubca0\uc77c\uc5d0 \uac00\ub9ac\uc5b4\uc9c4 \uadf8\ub300\uc758 \ubea8\uc740 \ucabc\uac1c \ub193\uc740 \uc11d\ub958 \uac19\uad6c\ub824."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub0b4\uac8c\ub294 \uc655\ud6c4\uac00 60\uba85\uc774\uc694, \ud6c4\uad81\uc774 80\uba85\uc774\uba70 \uadf8 \ubc16\uc5d0 \uc218\ub9ce\uc740 \uc2dc\ub140\ub4e4\uc774 \uc788\uc73c\ub098"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub098\uc758 \ube44\ub458\uae30, \ub098\uc758 \uc644\uc804\ud55c \uc0ac\ub791\uc740 \ud558\ub098\ubfd0\uc774\uad6c\ub098. \uadf8\ub140\ub294 \uc790\uae30 \uc5b4\uba38\ub2c8\uc758 \uc0ac\ub791\uc744 \ud55c\ubab8\uc5d0 \ubc1b\uace0 \uc788\ub294 \uc678\ub538\uc774\ub2e4. \ubaa8\ub4e0 \uc5ec\uc790\ub4e4\uc774 \uadf8\ub140\ub97c \ubcf4\uace0 \ubcf5\ubc1b\uc558\ub2e4\uace0 \ub9d0\ud558\ub2c8 \uc655\ud6c4\uc640 \ud6c4\uad81\ub4e4\ub3c4 \uadf8\ub140\ub97c \uce6d\ucc2c\ud558\ub294\uad6c\ub098."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uc0c8\ubcbd\ucc98\ub7fc \ub098\ud0c0\ub098\uba70 \ub2ec\ub369\uc774\ucc98\ub7fc \uc544\ub984\ub2f5\uace0 \ud574\uac19\uc774 \ubc1d\uc73c\uba70 \uae30\ub97c \ub192\uc774 \ub4e0 \uad70\ub300\ucc98\ub7fc \uc704\uc5c4 \uc788\ub294 \uc5ec\uc790\uac00 \ub204\uad6c\uc778\uac00?"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "(\uc5ec\uc790) \ub0b4\uac00 \uace8\uc9dc\uae30\uc5d0 \uc0c8\ub85c \ub3cb\uc544\ub09c \ucd08\ubaa9\uc744 \ubcf4\ub824\uace0, \ud3ec\ub3c4\ub098\ubb34\uac00 \uc21c\uc774 \ub0ac\ub294\uc9c0 \uc11d\ub958\ub098\ubb34\uac00 \uaf43\uc774 \ud53c\uc5c8\ub294\uc9c0 \uc54c\uc544\ubcf4\ub824\uace0 \ud638\ub3c4\ub098\ubb34 \uc232\uc73c\ub85c \ub0b4\ub824\uac14\uc5c8\uc9c0."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ub098\ub3c4 \ubaa8\ub974\uac8c \uac11\uc790\uae30 \uc0ac\ub791\uc774 \uadf8\ub9ac\uc6cc \ub0b4 \ubc31\uc131 \uac00\uc6b4\ub370\ub85c \ub3cc\uc544\uac00\uace0 \uc2f6\uc5c8\ub2e4\ub124."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "(\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4) \ub3cc\uc544\uc624\ub77c, \ub3cc\uc544\uc624\ub77c, \uc220\ub78c\ubbf8 \uc5ec\uc790\uc5ec, \uc6b0\ub9ac\uac00 \ub108\ub97c \ub2e4\uc2dc \ubcfc \uc218 \uc788\ub3c4\ub85d \ub3cc\uc544\uc624\ub77c, \ub3cc\uc544\uc624\ub77c. (\uc5ec\uc790) \ucc9c\uc0ac\ub4e4\uc774 \ucda4\ucd94\ub294 \uac83\uc744 \ubcf4\ub824\ub294 \ub4ef \uc5b4\uc9f8\uc11c \ub108\ud76c\uac00 \ub098\ub97c \ubcf4\uace0\uc790 \ud558\ub290\ub0d0?"
                }
            }
        },
        "7": {
            "chapter_nr": "7",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\ub0a8\uc790) \uadc0\ud55c \uc9d1\uc548\uc758 \ub538\uc774\uc5ec, \uc2e0\ubc1c\uc744 \uc2e0\uc740 \uadf8\ub300\uc758 \ubc1c\uc774 \uc5b4\uca4c\uba74 \uadf8\ucc98\ub7fc \uc544\ub984\ub2e4 \uc6b4\uac00\uc694! \uadf8\ub300\uc758 \ub2e4\ub9ac\ub294 \uc608\uc220\uac00\uc758 \uc791\ud488\ucc98\ub7fc \uc6b0\uc544\ud558\uace0"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uadf8\ub300\uc758 \ubc30\uaf3d\uc740 \ud63c\ud569 \ud3ec\ub3c4\uc8fc\ub97c \uac00\ub4dd \ubd80\uc740 \ub465\uadfc \uc220\uc794 \uac19\uc73c\uba70 \uadf8\ub300\uc758 \ud5c8\ub9ac\ub294 \ubc31\ud569\ud654\ub85c \ub450\ub978 \ubc00\uc9da\ub2e8 \uac19\uad6c\ub824."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\uadf8\ub300\uc758 \uc816\uac00\uc2b4\uc740 \uc30d\ud0dc \ub178\ub8e8 \uc0c8\ub07c \uac19\uace0"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ubaa9\uc740 \uc0c1\uc544\ud0d1 \uac19\uc73c\uba70 \ub208\uc740 \ubc14\ub4dc \u2013 \ub78d\ube54 \uc131\ubb38 \uacc1\uc758 \ud5e4\uc2a4\ubcf8 \uc5f0\ubabb \uac19\uace0 \ucf54\ub294 \ub2e4\ub9c8\uc2a4\ucee4\uc2a4\ub97c \uad7d\uc5b4\ubcf4\ub294 \ub808\ubc14\ub17c\uc758 \ub9dd\ub300 \uac19\uc18c\uc774\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uadf8\ub300\uc758 \uba38\ub9ac\ub294 \uac08\uba5c\uc0b0 \uac19\uace0 \uba38\ub9ac\ud138\uc740 \ube44\ub2e8\ucc98\ub7fc \uace0\uc640 \uc655\uc774 \uadf8 \ub9e4\ub825\uc5d0 \uc0ac\ub85c\uc7a1\ud788\uace0 \ub9d0\uc558\uc18c."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uc5b4\uca4c\uba74 \uadf8\ub300\ub294 \uadf8\ucc98\ub7fc \uc608\uc058\uace0 \uc544\ub984\ub2e4\uc6b4\uac00\uc694! \ub0b4 \uc0ac\ub791\uc774\uc5ec, \uadf8\ub300\uc758 \ub9e4\ub825\uc774 \ub300\ub2e8\ud558\uad6c\ub824."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\uadf8\ub300\ub294 \uc885\ub824\ub098\ubb34\ucc98\ub7fc \ud0a4\uac00 \ud06c\uace0 \ub298\uc52c\ud558\uba70 \uadf8\ub300\uc758 \uc816\uac00\uc2b4\uc740 \uadf8 \uc5f4\ub9e4 \uc1a1\uc774 \uac19\uc18c\uc774\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\ub0b4\uac00 \uadf8 \uc885\ub824\ub098\ubb34\uc5d0 \uc62c\ub77c\uac00\uc11c \uc5f4\ub9e4\ub97c \uc7a1\uc73c\uc624\ub9ac\ub2e4. \uadf8\ub300\uc758 \uc720\ubc29\uc740 \ud3ec\ub3c4\uc1a1\uc774 \uac19\uace0 \uadf8\ub300\uc758 \uc228\uc5d0\uc11c\ub294 \uc0ac\uacfc \ub0c4\uc0c8\uac00 \ub098\uba70"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uadf8\ub300\uc758 \uc785\uc220\uc740 \uc81c\uc77c \uc88b\uc740 \ud3ec\ub3c4\uc8fc\ucc98\ub7fc \ub2ec\ucf64\ud558\uad6c\ub824. (\uc5ec\uc790) \uc774 \ud3ec\ub3c4\uc8fc\uac00 \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc758 \uc785\uc220\ub85c \ubd80\ub4dc\ub7fd\uac8c \ud758\ub7ec\ub4e4\uc5b4\uac00\uae30\ub97c \uc6d0\ud558\ub178\ub77c."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\ub098\ub294 \ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc758 \uac83\uc774\ubbc0\ub85c \uadf8\uac00 \ub098\ub97c \uc0ac\ubaa8\ud558\ub294\uad6c\ub098."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\ub0b4 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc2dc\uc5ec, \uc6b0\ub9ac \ud568\uaed8 \uc2dc\uace8\ub85c \uac00\uc11c \ub9c8\uc744\uc5d0\uc11c \ubc24\uc744 \ubcf4\ub0c5\uc2dc\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uc6b0\ub9ac \uc77c\ucc0d \uc77c\uc5b4\ub098 \ud3ec\ub3c4\uc6d0\uc73c\ub85c \uac00\uc11c \ud3ec\ub3c4 \uc21c\uc774 \ub3cb\uc558\ub294\uc9c0, \uaf43\ubd09\uc624\ub9ac\uac00 \ub9fa\ud614\ub294\uc9c0, \uc11d\ub958\uaf43\uc774 \ud53c\uc5c8\ub294\uc9c0 \uc54c\uc544\ubd05\uc2dc\ub2e4. \ub0b4\uac00 \uac70\uae30\uc11c \ub2f9\uc2e0\uc5d0\uac8c \ub0b4 \uc0ac\ub791\uc744 \ubc14\uce58\uaca0\uc5b4\uc694."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ub9cc\ub2e4\ub77c\ud654\uac00 \ud5a5\uae30\ub97c \ud1a0\ud558\uace0 \uc6b0\ub9ac \ubb38 \uc55e\uc5d0\ub294 \uc628\uac16 \uacfc\uc77c\uc774 \uac00\ub4dd\ud558\ub2c8 \ub0b4\uac00 \uc0ac\ub791\ud558\ub294 \ub2f9\uc2e0\uc744 \uc704\ud574 \uc313\uc544 \ub454 \uac83\uc774\ub78d\ub2c8\ub2e4."
                }
            }
        },
        "8": {
            "chapter_nr": "8",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "(\uc5ec\uc790) \ub9cc\uc77c \ub2f9\uc2e0\uc774 \uc6b0\ub9ac \uc5b4\uba38\ub2c8\uc758 \uc816\uc744 \uba39\uace0 \uc790\ub780 \uc624\ube60\ub77c\uba74 \ub0b4\uac00 \uae38\uac70\ub9ac\uc5d0 \uc11c \ub2f9\uc2e0\uc744 \ub9cc\ub098 \uc785\uc744 \ub9de\ucd94\uc5b4\ub3c4 \ub098\ub97c \ube44\uc6c3\ub294 \uc790\uac00 \uc5c6\uc744 \uac83\uc785\ub2c8\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ub0b4\uac00 \ub2f9\uc2e0\uc744 \uc6b0\ub9ac \uc9d1\uc73c\ub85c \ub370\ub824\uc640\uc11c \ub2f9\uc2e0\uc758 \uac00\ub974\uce68\uc744 \ubc1b\uace0 \uc2f6\uc5b4\uc694. \ub098\ub294 \ub2f9\uc2e0\uc5d0\uac8c \uc11d\ub958\uc999\uc73c\ub85c \ub9cc\ub4e0 \ud5a5\uae30\ub85c\uc6b4 \uc220\uc744 \uac16\ub2e4 \ub4dc\ub9ac\uace0"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\ub2f9\uc2e0\uc740 \ub098\ub97c \uc67c\ud314\uc5d0 \ub215\ud600 \uc624\ub978\uc190\uc73c\ub85c \uaef4\uc548\uc544 \uc900\ub2e4\uba74 \uc5bc\ub9c8\ub098 \uc88b\uaca0\uc5b4\uc694!"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4\uc544, \ub0b4\uac00 \ub108\ud76c\uc5d0\uac8c \ubd80\ud0c1\ud55c\ub2e4. \ub108\ud76c\ub294 \uc81c\ubc1c \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc29\ud574\ud558\uc9c0 \ub9d0\uc544\ub2e4\uc624."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc5ec\uc12f \ubc88\uc9f8 \ub178\ub798 - (\uc608\ub8e8\uc0b4\ub818 \uc5ec\uc790\ub4e4) \uc0ac\ub791\ud558\ub294 \ub2d8\uc758 \ud314\uc744 \ub07c\uace0 \uc0ac\ub9c9\uc5d0\uc11c \uc62c\ub77c\uc624\ub294 \uc5ec\uc790\uac00 \ub204\uad6c\uc778\uac00? (\ub0a8\uc790) \uadf8\ub300\uc758 \uc5b4\uba38\ub2c8\uac00 \uc0b0\ud1b5\uc744 \uacaa\uc73c\uba70 \uadf8\ub300\ub97c \ud574\uc0b0\ud55c \uadf8 \uc0ac\uacfc\ub098\ubb34 \uc544\ub798\uc11c \ub0b4\uac00 \uadf8\ub300\ub97c \uae68\uc6e0\ub178\ub77c."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "(\uc5ec\uc790) \ub2f9\uc2e0\uc740 \ub098\ub97c \uc778\uc7a5 \ubc18\uc9c0\ucc98\ub7fc \ub2f9\uc2e0\uc758 \ub9c8\uc74c\uc5d0 \uac04\uc9c1\ud558\uace0 \ub3c4\uc7a5\ucc98\ub7fc \ub2f9\uc2e0\uc758 \ud314\uc5d0 \uc0c8\uae30\uc138\uc694. \uc0ac\ub791\uc740 \uc8fd\uc74c\ucc98\ub7fc \uac15\ud558\uba70 \uc9c8\ud22c\ub294 \ubb34\ub364\ucc98\ub7fc \uc794\uc778\ud558\uc5ec \ub9f9\ub82c\ud55c \ubd88\ucc98\ub7fc \ud0c0\uc624\ub985\ub2c8\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ub9ce\uc740 \ubb3c\ub3c4 \uc0ac\ub791\uc758 \ubd88\uc744 \ub04c \uc218 \uc5c6\uace0 \ud64d\uc218\ub77c\ub3c4 \uadf8\uac83\uc744 \uc18c\uba78\ud560 \uc218 \uc5c6\uc73c\ub2c8 \uc0ac\ub78c\uc774 \uc790\uae30 \uc7ac\uc0b0\uc744 \ub2e4 \uc8fc\uace0 \uc0ac\ub791\uc744 \uc0ac\ub824\uace0 \ud574\ub3c4 \uc624\ud788\ub824 \ube44\uc6c3\uc74c\ub9cc \uc0b4 \uac83\uc785\ub2c8\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "(\uc5ec\uc790\uc758 \uc624\ube60\ub4e4) \uc6b0\ub9ac \uc5b4\ub9b0 \ub204\uc774\ub3d9\uc0dd\uc740 \uc544\uc9c1\ub3c4 \uc720\ubc29\uc774 \uc5c6\uad6c\ub098. \ub3d9\uc0dd\uc774 \uccad\ud63c\uc744 \ubc1b\uc73c\uba74 \uc6b0\ub9ac\ub294 \ubb34\uc5c7\uc744 \ud574\uc57c \ud560\uae4c?"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub9cc\uc77c \ub3d9\uc0dd\uc774 \uc131\ubcbd\uc774\ub77c\uba74 \uc6b0\ub9ac\ub294 \ub3d9\uc0dd\uc5d0\uac8c \uc740\ub9dd\ub300\ub97c \uc138\uc6cc \uc904 \uac83\uc774\uba70 \ub9cc\uc77c \ub3d9\uc0dd\uc774 \ubb38\uc774\ub77c\uba74 \uc6b0\ub9ac\uac00 \ubc31\ud5a5\ubaa9 \ud310\uc790\ub97c \ub458\ub7ec \uc8fc\ub9ac\ub77c."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "(\uc5ec\uc790) \ub098\ub294 \uc131\ubcbd\uc774\uc694 \ub0b4 \uc720\ubc29\uc740 \ub9dd\ub300\uc640 \uac19\uc73c\ubbc0\ub85c \ub0b4\uac00 \ub2d8\uc758 \ubcf4\uae30\uc5d0 \ud3c9\uc548\uc744 \uc5bb\uc740 \uc0ac\ub78c\ucc98\ub7fc \ub418\uc5c8\uad6c\ub098."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\ubc14\uc54c \u2013 \ud558\ubaac\uc774\ub77c\ub294 \uacf3\uc5d0 \ud3ec\ub3c4\uc6d0\uc774 \uc788\uc5b4\uc11c \uc194\ub85c\ubaac\uc774 \uadf8\uac83\uc744 \uc18c\uc791\uc778\ub4e4\uc5d0\uac8c \ub0b4\uc5b4\uc8fc\uace0 \ud55c \uc0ac\ub78c \uc55e\uc5d0 \uc740\ud654 1,000\uac1c\uc529 \ubc14\uce58\uac8c \ud558\uc600\ub124."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\uadf8\ub7ec\ub098 \ub0b4 \ud3ec\ub3c4\uc6d0\uc740 \ub0b4\uac00 \ub9c8\uc74c\ub300\ub85c \ud560 \uc218 \uc788\uc73c\ub2c8 \uc194\ub85c\ubaac \ub2f9\uc2e0\uc740 \uc740\ud654 1,000\uac1c\ub97c \uac00\uc9c8 \uac83\uc774\uba70 \ud3ec\ub3c4\uc6d0\uc744 \uac00\uafb8\ub294 \uc790\ub4e4\uc5d0\uac8c\ub294 \ub0b4\uac00 \uc740\ud654 200\uac1c\ub97c \uc8fc\ub9ac\ub77c."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "(\ub0a8\uc790) \ub3d9\uc0b0\uc5d0 \uc549\uc544 \uc788\ub294 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc5ec, \uadf8\ub300\uc758 \uc74c\uc131\uc744 \ub4e4\ub824 \uc8fc\uc2dc\uc624. \ub0b4 \uce5c\uad6c\ub4e4\uc774 \uadf8\ub300\uc758 \ubaa9\uc18c\ub9ac\ub97c \ub4e3\uace0 \uc2f6\uc5b4 \uae30\ub2e4\ub9ac\uace0 \uc788\ub2e4\uc624."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "(\uc5ec\uc790) \ub098\uc758 \uc0ac\ub791\ud558\ub294 \ub2d8\uc774\uc2dc\uc5ec, \ub178\ub8e8\ucc98\ub7fc \ub2ec\ub824\uc624\uc138\uc694. \ud5a5\uae30\ub85c\uc6b4 \uc0b0\uc758 \uc5b4\ub9b0 \uc0ac\uc2b4\ucc98\ub7fc \uc18d\ud788 \ub2ec\ub824\uc624\uc138\uc694."
                }
            }
        }
    }
}