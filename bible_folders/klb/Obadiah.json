{
    "version": "klb",
    "book_name": "Obadiah",
    "book_nr": "31",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uc5d0\ub3d4\uc5d0 \ub2e4\uac00\uc62c \uc2ec\ud310 - \uc774\uac83\uc740 \uc8fc \uc5ec\ud638\uc640\uaed8\uc11c \uc5d0\ub3d4\uc5d0 \ub300\ud558\uc5ec \ub9d0\uc500\ud558\uc2e0 \uc624\ubc14\ub31c\uc758 \uc608\uc5b8\uc774\ub2e4: \uc5ec\ud638\uc640\uaed8\uc11c \uc790\uae30 \uc0ac\uc790\ub97c \uc5ec\ub7ec \ub098\ub77c\uc5d0 \ubcf4\ub0b4\uc168\ub2e4. \uc6b0\ub9ac\ub294 \uadf8\uc758 \uc0ac\uc790\uac00 \u201c\uc77c\uc5b4\ub098\ub77c. \uc5d0\ub3d4\uc744 \uce58\ub7ec \uac00\uc790!\u201d \ud558\uace0 \ub9d0\ud558\ub294 \uc18c\ub9ac\ub97c \ub4e4\uc5c8\ub2e4."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\uc5ec\ud638\uc640\uaed8\uc11c \uc5d0\ub3d4\uc5d0\uac8c \ub9d0\uc500\ud558\uc2e0\ub2e4. \u201c\ub0b4\uac00 \uc5ec\ub7ec \ub098\ub77c \uac00\uc6b4\ub370\uc11c \ub108\ub97c \ubcf4\uc798\uac83\uc5c6\ub294 \ub098\ub77c\ub85c \ub9cc\ub4e4\uaca0\ub2e4. \ub124\uac00 \ud06c\uac8c \uba78\uc2dc\ub97c \ub2f9\ud560 \uac83\uc774\ub2e4."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\ub124\uac00 \ub192\uc740 \uc0b0\uc545 \uc9c0\ub300\uc5d0 \uc0b0\ub2e4\uace0 \ud574\uc11c \u2018\ub204\uac00 \ub098\ub97c \uc544\ub798\ub85c \ub04c\uc5b4\ub0b4\ub9b4 \uc218 \uc788\uaca0\ub290\ub0d0?\u2019 \ud558\uace0 \ub9d0\ud558\uc9c0\ub9cc \ub108\ub294 \ub124 \uad50\ub9cc\uc5d0 \uc18d\uace0 \uc788\ub2e4."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub124\uac00 \ube44\ub85d \ub3c5\uc218\ub9ac\ucc98\ub7fc \ub192\uc774 \uc62c\ub77c \ub124 \ubcf4\uae08\uc790\ub9ac\ub97c \ubcc4 \uc0ac\uc774\uc5d0 \ub9cc\ub4e4\uc9c0\ub77c\ub3c4 \ub0b4\uac00 \uac70\uae30\uc11c \ub108\ub97c \ub04c\uc5b4\ub0b4\ub9b4 \uac83\uc774\ub2e4."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u201c\ubc24\uc5d0 \ub3c4\ub451\uc774 \ub4e4\uc5b4\ub3c4 \uadf8\ub4e4\uc740 \uc790\uae30\ub4e4\uc774 \uc6d0\ud558\ub294 \uac83\ub9cc \uac00\uc838\uac00\uba70 \uc0ac\ub78c\ub4e4\uc774 \ud3ec\ub3c4\ub97c \ub530\ub3c4 \ubaa8\uc870\ub9ac \ub530\uc9c0 \uc54a\uace0 \uc5bc\ub9c8\ucbe4\uc740 \ub0a8\uaca8 \ub454\ub2e4. \uadf8\ub7ec\ub098 \ub124 \uc6d0\uc218\ub4e4\uc740 \ub108\ub97c \uc644\uc804\ud788 \ud729\uc4f8\uc5b4 \ubc84\ub9b4 \uac83\uc774\ub2e4."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uc5d0\uc11c\uc758 \ud6c4\uc190\uc544, \ub124 \ubcf4\ubb3c\uc774 \ubaa8\uc870\ub9ac \uc57d\ud0c8\ub2f9\ud560 \uac83\uc774\ub2e4."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ub108\uc758 \ub3d9\ub9f9\uad6d\ub4e4\uc774 \ub108\ub97c \ub124 \uad6d\uacbd \ubc16\uc73c\ub85c \ubc00\uc5b4\ub0bc \uac83\uc774\uba70 \ub108\uc758 \uc6b0\ubc29\ub4e4\uc774 \ub108\ub97c \uc18d\uc5ec \uc815\ubcf5\ud558\uace0 \ub108\uc640 \uc2dd\uc0ac\ub97c \uac19\uc774\ud558\ub294 \ub124 \uce5c\uad6c\ub4e4\uc774 \ub108\ub97c \uc4f0\ub7ec\ub728\ub9b4 \ud568\uc815\uc744 \ud314 \uac83\uc774\ub098 \ub108\ub294 \uc9c0\uac01\uc774 \uc5c6\uc73c\ubbc0\ub85c \uadf8\uac83\uc744 \uc54c\uc9c0 \ubabb\ud560 \uac83\uc774\ub2e4."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u201c\uadf8 \ub0a0\uc5d0 \ub0b4\uac00 \uc5d0\ub3d4\uc744 \ubc8c\ud558\uc5ec \uc9c0\ud61c\ub85c\uc6b4 \uc790\ub4e4\uc744 \ubaa8\uc870\ub9ac \uc5c6\uc560 \ubc84\ub9ac\uaca0\ub2e4."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\ub370\ub9cc\uc544, \ub108\uc758 \uc6a9\uc0ac\ub4e4\uc774 \ub180\ub784 \uac83\uc774\uba70 \uc5d0\ub3d4\uc5d0 \uc788\ub294 \ubaa8\ub4e0 \uc0ac\ub78c\ub4e4\uc774 \uc8fd\uc74c\uc744 \ub2f9\ud560 \uac83\uc774\ub2e4."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u201c\ub124\uac00 \uc57c\uacf1\uc758 \ud6c4\uc190\uc778 \ub124 \ud615\uc81c\ub4e4\uc744 \uc57d\ud0c8\ud558\uace0 \uc8fd\uc600\uc73c\ubbc0\ub85c \ub124\uac00 \uba78\ub9dd\ud558\uc5ec \uc601\uc6d0\ud788 \uc218\uce58\ub97c \ub2f9\ud560 \uac83\uc774\ub2e4."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc6d0\uc218\ub4e4\uc774 \uc608\ub8e8\uc0b4\ub818\uc744 \uce68\ub7b5\ud558\uc5ec \uadf8 \uc7ac\ubb3c\uc744 \uc57d\ud0c8\ud558\uace0 \uadf8 \uc131\uc744 \uc81c\ube44 \ubf51\uc544 \ub098\ub20c \ub54c\uc5d0 \ub124\uac00 \uadf8 \uacc1\uc5d0 \uc788\uc5c8\uc73c\ub2c8 \ub108\ub3c4 \uadf8\ub4e4\uc758 \uc6d0\uc218\uc640 \ub2e4\ub97c \uac83\uc774 \uc5c6\ub2e4."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ub108\ub294 \ub124 \ud615\uc81c\ub4e4\uc758 \ubd88\ud589\uc744 \ub9cc\uc871\uc2a4\ub7ec\uc6b4 \ub4ef\uc774 \ubc14\ub77c\ubcf4\uac70\ub098 \uc720\ub2e4 \ubc31\uc131\uc758 \ud328\ub9dd\uc744 \uae30\ubed0\ud558\uac70\ub098 \uadf8\ub4e4\uc774 \uace0\ub09c\uc744 \ub2f9\ud560 \ub54c \uc6b0\ucb50\ub300\uc9c0 \ub9d0\uc558\uc5b4\uc57c \ud588\ub2e4."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\ub0b4 \ubc31\uc131\uc774 \uc7ac\uc559\uc744 \ub2f9\ud558\ub294 \ub0a0\uc5d0 \ub108\ub294 \uadf8\ub4e4\uc758 \uc131\uc5d0 \ub4e4\uc5b4\uac00\uc11c \ub9cc\uc871\uc2a4\ub7ec\uc6b4 \ub4ef\uc774 \uadf8\ub4e4\uc758 \uace0\ud1b5\uc744 \ubc14\ub77c\ubcf4\uba70 \uadf8\ub4e4\uc758 \uc7ac\ubb3c\uc5d0 \uc190\uc744 \ub300\uace0"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\ub610 \uadf8\ub4e4\uc774 \uace0\ub09c\uc744 \ub2f9\ud560 \ub54c\uc5d0 \uc0ac\uac70\ub9ac\uc5d0 \uc11c\uc11c \ub3c4\ub9dd\ud558\ub294 \uc790\ub4e4\uc744 \ubd99\uc7a1\uc544 \uadf8\ub4e4\uc744 \uc6d0\uc218\ub4e4\uc5d0\uac8c \ub118\uaca8 \uc8fc\uc5c8\ub2e4."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u201c\ub098 \uc5ec\ud638\uc640\uac00 \uc628 \uc138\uc0c1\uc744 \uc2ec\ud310\ud560 \ub0a0\uc774 \ub2e4\uac00\uc624\uace0 \uc788\ub2e4. \uc5d0\ub3d4\uc544, \ub124\uac00 \ud589\ud55c \ub300\ub85c \ub0b4\uac00 \ub108\uc5d0\uac8c \uac1a\uc544 \uc8fc\uaca0\ub2e4. \ub124 \ud589\uc704\uac00 \ub124 \uba38\ub9ac\ub85c \ub3cc\uc544\uac08 \uac83\uc774\ub2e4."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\ub124\uac00 \ub098\uc758 \uac70\ub8e9\ud55c \uc0b0\uc5d0\uc11c \ud615\ubc8c\uc758 \uc4f4 \uc794\uc744 \ub9c8\uc2e0 \uac83\ucc98\ub7fc \ub124 \uc8fc\ubcc0\uc758 \ubaa8\ub4e0 \ub098\ub77c\ub4e4\ub3c4 \uadf8 \uc794\uc744 \ub9c8\uc2e4 \uac83\uc774\ub2e4. \uadf8\ub4e4\uc740 \ud55c\uc5c6\uc774 \ub9c8\uc2dc\uace0 \ube44\ud2c0\uac70\ub9ac\ub2e4\uac00 \uc544\uc8fc \uc0ac\ub77c\uc9c0\uace0 \ub9d0 \uac83\uc774\ub2e4.\u201d"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uc774\uc2a4\ub77c\uc5d8\uc758 \uc2b9\ub9ac - \u201c\uadf8\ub7ec\ub098 \uc2dc\uc628\uc0b0\uc5d0 \uad6c\uc6d0\uc774 \uc788\uc744 \uac83\uc774\uba70 \uadf8 \uacf3\uc774 \uac70\ub8e9\ud558\uac8c \ub418\uc5b4 \uc774\uc2a4\ub77c\uc5d8\uc774 \uadf8 \ub545\uc744 \ub2e4\uc2dc \uc18c\uc720\ud560 \uac83\uc774\ub2e4."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uc57c\uacf1\uc758 \ud6c4\uc190\uc740 \ubd88\uacfc \uac19\uace0 \uc694\uc149\uc758 \ud6c4\uc190\uc740 \ubd88\uaf43\uacfc \uac19\uc73c\uba70 \uc5d0\uc11c\uc758 \ud6c4\uc190\uc740 \uc9c0\ud478\ub77c\uae30\uc640 \uac19\uc744 \uac83\uc774\ub2e4. \uadf8\ub798\uc11c \uadf8\ub4e4\uc774 \uadf8\uac83\uc744 \ud0dc\uc6b8 \uac83\uc774\ub2c8 \uc5d0\uc11c\uc758 \ud6c4\uc190 \uac00\uc6b4\ub370 \uc0b4\uc544 \ub0a8\uc744 \uc790\uac00 \uc5c6\uc744 \uac83\uc774\ub2e4. \uc774\uac83\uc740 \ub098 \uc5ec\ud638\uc640\uc758 \ub9d0\uc774\ub2e4.\u201d"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\ub0a8\ucabd \ub124\uac9d \uc9c0\ubc29\uc5d0 \uc0ac\ub294 \uc0ac\ub78c\ub4e4\uc740 \uc5d0\ub3d4\uc758 \uc0b0\uc545 \uc9c0\ub300\ub97c \uc810\ub839\ud560 \uac83\uc774\uba70 \uc720\ub2e4 \uc800\uc9c0\ub300\uc5d0 \uc0ac\ub294 \uc0ac\ub78c\ub4e4\uc740 \ube14\ub808\uc14b \ub545\uc744 \uc18c\uc720\ud558\uace0 \uadf8\ub4e4\uc740 \ub610 \uc5d0\ube0c\ub77c\uc784 \ub4e4\uacfc \uc0ac\ub9c8\ub9ac\uc544 \ub4e4\uc744 \uc18c\uc720\ud560 \uac83\uc774\uba70 \ubca0\ub0d0\ubbfc \uc0ac\ub78c\ub4e4\uc740 \uae38\ub974\uc557 \ub545\uc744 \ucc28\uc9c0\ud560 \uac83\uc774\ub2e4."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\uc774\uc2a4\ub77c\uc5d8\uc758 \ud3ec\ub85c\ub4e4\uc740 \ub3cc\uc544\uc640\uc11c \ubd81\uc73c\ub85c \uc0ac\ub974\ubc27\uc5d0 \uc774\ub974\ub294 \ud398\ub2c8\ud0a4\uc544 \uc5f0\uc548 \uc77c\ub300\ub97c \uc810\ub839\ud560 \uac83\uc774\uba70 \uc2a4\ubc14\ub78f\uc5d0 \uc788\ub294 \uc608\ub8e8\uc0b4\ub818\uc758 \ud3ec\ub85c\ub4e4\uc740 \ub124\uac9d \uc9c0\ubc29\uc758 \uc131\ub4e4\uc744 \ucc28\uc9c0\ud560 \uac83\uc774\ub2e4."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\uad6c\uc6d0\uc790\ub4e4\uc774 \uc2dc\uc628\uc0b0\uc73c\ub85c \uc62c\ub77c\uc640\uc11c \uc5d0\ub3d4\uc744 \ub2e4\uc2a4\ub9b4 \uac83\uc774\uba70 \uc5ec\ud638\uc640\uaed8\uc11c \uc655\uc774 \ub418\uc2e4 \uac83\uc774\ub2e4."
                }
            }
        }
    }
}