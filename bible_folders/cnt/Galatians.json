                    "verse": "\u5c6c\u57fa\u7763\u8036\u7a4c\u7684\u4eba\uff0c\u662f\u5df2\u7d93\u628a\u8089\u9ad4\u548c\u90aa\u60c5\u79c1\u617e\u90fd\u91d8\u5728\u5341\u5b57\u67b6\u4e0a\u4e86\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u5982\u679c\u6211\u5011\u9760\u8056\u9748\u6d3b\u8457\uff0c\u5c31\u61c9\u8a72\u9806\u8457\u8056\u9748\u884c\u4e8b\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u6211\u5011\u4e0d\u53ef\u8caa\u5716\u865b\u69ae\uff0c\u5f7c\u6b64\u89f8\u6012\uff0c\u4e92\u76f8\u5ac9\u5992\u3002\n"
                }
            }
        },
        "6": {
            "chapter_nr": 6,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4fe1\u5f92\u7684\u8cac\u4efb\u5f1f\u5144\u5011\uff0c\u5982\u679c\u6709\u4eba\u9677\u5728\u4e00\u4e9b\u904e\u72af\u88e1\uff0c\u4f60\u5011\u5c6c\u9748\u7684\u4eba\uff0c\u8981\u7528\u6eab\u67d4\u7684\u5fc3\u4f7f\u4ed6\u56de\u8f49\u904e\u4f86\uff0c\u81ea\u5df1\u537b\u8981\u5c0f\u5fc3\uff0c\u514d\u5f97\u4e5f\u88ab\u5f15\u8a98\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4f60\u5011\u5404\u4eba\u7684\u91cd\u64d4\u8981\u4e92\u76f8\u64d4\u7576\uff0c\u9019\u6a23\u5c31\u6210\u5168\u4e86\u57fa\u7763\u7684\u5f8b\u6cd5\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5982\u679c\u6709\u4eba\u672c\u4f86\u6c92\u6709\u751a\u9ebc\u4e86\u4e0d\u8d77\uff0c\u537b\u81ea\u4ee5\u70ba\u662f\u4e86\u4e0d\u8d77\u7684\uff0c\u5c31\u662f\u6b3a\u9a19\u81ea\u5df1\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5404\u4eba\u61c9\u8a72\u7701\u5bdf\u81ea\u5df1\u6240\u4f5c\u7684\uff0c\u9019\u6a23\uff0c\u4ed6\u5f15\u4ee5\u70ba\u69ae\u7684\uff0c\u5c31\u53ea\u5728\u81ea\u5df1\u800c\u4e0d\u5728\u5225\u4eba\u4e86\uff1b\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0\u70ba\u5404\u4eba\u7684\u64d4\u5b50\uff0c\u662f\u8981\u81ea\u5df1\u64d4\u7576\u7684\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5728\u8056\u9053\u4e0a\u53d7\u6559\u7684\uff0c\u61c9\u8a72\u548c\u65bd\u6559\u7684\u4eba\u5206\u4eab\u81ea\u5df1\u7684\u4e00\u5207\u7f8e\u7269\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4e0d\u8981\u81ea\u6b3a\uff0c\u3000\u795e\u662f\u4e0d\u53ef\u8f15\u6162\u7684\u3002\u4eba\u7a2e\u7684\u662f\u751a\u9ebc\uff0c\u6536\u7684\u4e5f\u662f\u751a\u9ebc\uff1a\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u9806\u8457\u81ea\u5df1\u7684\u8089\u9ad4\u6492\u7a2e\u7684\uff0c\u5fc5\u5b9a\u5f9e\u8089\u9ad4\u6536\u53d6\u6557\u58de\uff1b\u9806\u8457\u8056\u9748\u6492\u7a2e\u7684\uff0c\u5fc5\u5b9a\u5f9e\u8056\u9748\u6536\u53d6\u6c38\u751f\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u5011\u884c\u5584\uff0c\u4e0d\u8981\u89ba\u5f97\u53ad\u7169\uff1b\u5982\u679c\u4e0d\u9b06\u61c8\uff0c\u5230\u4e86\u9069\u7576\u7684\u6642\u5019\uff0c\u5c31\u6709\u6536\u6210\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6240\u4ee5\uff0c\u6211\u5011\u4e00\u6709\u6a5f\u6703\uff0c\u5c31\u61c9\u8a72\u5c0d\u773e\u4eba\u884c\u5584\uff0c\u5c0d\u4fe1\u5f92\u66f4\u8981\u9019\u6a23\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u52f8\u52c9\u548c\u795d\u798f\u8acb\u770b\uff0c\u6211\u89aa\u624b\u5beb\u7d66\u4f60\u5011\u7684\u5b57\u662f\u591a\u9ebc\u7684\u5927\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u90a3\u4e9b\u5728\u5916\u8868\u4e0a\u8981\u9ad4\u9762\u7684\u4eba\uff0c\u4ed6\u5011\u52c9\u5f37\u4f60\u5011\u53d7\u5272\u79ae\uff0c\u4e0d\u904e\u662f\u6015\u70ba\u4e86\u57fa\u7763\u7684\u5341\u5b57\u67b6\u53d7\u8feb\u5bb3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u90a3\u4e9b\u53d7\u5272\u79ae\u7684\u4eba\uff0c\u81ea\u5df1\u4e5f\u4e0d\u9075\u5b88\u5f8b\u6cd5\uff0c\u53cd\u800c\u8981\u4f60\u5011\u53d7\u5272\u79ae\uff0c\u70ba\u7684\u662f\u8981\u85c9\u8457\u4f60\u5011\u7684\u8089\u8eab\u8a87\u53e3\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u81f3\u65bc\u6211\uff0c\u6211\u7d55\u4e0d\u4ee5\u5225\u7684\u8a87\u53e3\uff0c\u53ea\u8a87\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u5341\u5b57\u67b6\u3002\u85c9\u8457\u9019\u5341\u5b57\u67b6\uff0c\u5728\u6211\u4f86\u8aaa\uff0c\u4e16\u754c\u5df2\u7d93\u91d8\u5728\u5341\u5b57\u67b6\u4e0a\u4e86\uff1b\u5c31\u4e16\u754c\u4f86\u8aaa\uff0c\u6211\u4e5f\u5df2\u7d93\u91d8\u5728\u5341\u5b57\u67b6\u4e0a\u4e86\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u53d7\u5272\u79ae\u6216\u4e0d\u53d7\u5272\u79ae\uff0c\u90fd\u7b97\u4e0d\u5f97\u751a\u9ebc\uff0c\u8981\u7dca\u7684\u662f\u4f5c\u65b0\u9020\u7684\u4eba\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6240\u6709\u7167\u9019\u6e96\u5247\u800c\u884c\u7684\u4eba\uff0c\u9858\u5e73\u5b89\u6190\u61ab\u81e8\u5230\u4ed6\u5011\uff0c\u5c31\u662f\u81e8\u5230\u3000\u795e\u7684\u4ee5\u8272\u5217\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u5f9e\u4eca\u4ee5\u5f8c\uff0c\u8ab0\u4e5f\u4e0d\u8981\u652a\u64fe\u6211\uff0c\u56e0\u70ba\u6211\u8eab\u4e0a\u5e36\u8457\u8036\u7a4c\u7684\u70d9\u75d5\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5f1f\u5144\u5011\uff0c\u9858\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u6069\u60e0\u8207\u4f60\u5011\u540c\u5728\uff08\u201c\u8207\u4f60\u5011\u540c\u5728\u201d\u539f\u6587\u4f5c\u201c\u8207\u4f60\u5011\u7684\u5fc3\u9748\u540c\u5728\u201d\uff09\u3002\u963f\u5011\u3002\n"
                }
            }
        }
    }
