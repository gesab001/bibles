{
    "version": "cnt",
    "book_name": "Habakkuk",
    "book_nr": "35",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5148\u77e5\u9996\u6b21\u7591\u554f\u54c8\u5df4\u8c37\u5148\u77e5\u6240\u5f97\u7684\u9ed8\u793a\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u83ef\u554a\uff01\u6211\u61c7\u6c42\uff0c\u4f60\u4e0d\u5782\u807d\uff0c\u8981\u5230\u5e7e\u6642\u5462\uff1f\u6211\u5411\u4f60\u547c\u53eb\u201c\u6709\u72c2\u66b4\u7684\u4e8b\u201d\uff0c\u4f60\u537b\u4e0d\u62ef\u6551\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u70ba\u751a\u9ebc\u4f7f\u6211\u770b\u898b\u60e1\u884c\uff1f\u6709\u5978\u60e1\u7684\u4e8b\uff0c\u4f60\u70ba\u751a\u9ebc\u898b\u800c\u4e0d\u7406\uff1f\u6bc0\u6ec5\u548c\u5f37\u66b4\u5728\u6211\u9762\u524d\uff0c\u7d1b\u722d\u548c\u76f8\u9b25\u5e38\u5e38\u767c\u751f\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u56e0\u6b64\u5f8b\u6cd5\u4e0d\u80fd\u751f\u6548\uff08\u201c\u5f8b\u6cd5\u4e0d\u80fd\u751f\u6548\u201d\u539f\u6587\u4f5c\u201c\u5f8b\u6cd5\u9b06\u61c8\u201d\uff09\uff0c\u516c\u7406\u7121\u6cd5\u5f70\u986f\u3002\u56e0\u70ba\u60e1\u4eba\u628a\u7fa9\u4eba\u5305\u570d\uff0c\u6240\u4ee5\u516c\u7406\u985b\u5012\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8036\u548c\u83ef\u7684\u56de\u7b54\u4f60\u5011\u7576\u770b\u5217\u570b\uff0c\u8981\u5b9a\u775b\u89c0\u770b\uff0c\u5c31\u6703\u5927\u5927\u9a5a\u5947\uff0c\u56e0\u70ba\u5728\u4f60\u5011\u7684\u65e5\u5b50\uff0c\u6211\u8981\u4f5c\u4e00\u4ef6\u4e8b\uff0c\u5373\u4f7f\u6709\u4eba\u8aaa\u4e86\u51fa\u4f86\uff0c\u4f60\u5011\u4e5f\u4e0d\u6703\u76f8\u4fe1\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5c31\u662f\u8981\u8208\u8d77\u8fe6\u52d2\u5e95\u4eba\uff0c\u90a3\u6b98\u5fcd\u5147\u66b4\u7684\u6c11\uff1b\u4ed6\u5011\u904d\u884c\u5168\u5730\uff0c\u4f54\u9818\u5225\u4eba\u7684\u5bb6\u5712\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u5011\u6050\u6016\u53ef\u6015\uff0c\u81ea\u4ee5\u70ba\u7fa9\uff0c\u8dbe\u9ad8\u6c23\u63da\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ed6\u5011\u7684\u99ac\u6bd4\u8c79\u66f4\u5feb\uff0c\u6bd4\u665a\u4e0a\u7684\u8c7a\u72fc\u66f4\u731b\u3002\u4ed6\u5011\u7684\u9a0e\u5175\u5954\u99b3\uff0c\u81ea\u9060\u800c\u4f86\uff1b\u4ed6\u5011\u5982\u9df9\u98db\u7fd4\uff0c\u8fc5\u901f\u541e\u566c\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u5011\u5b9a\u8457\u81c9\u9762\u5411\u524d\uff0c\u9f4a\u4f86\u884c\u66b4\uff0c\u64c4\u7372\u6230\u4fd8\u591a\u5982\u5875\u6c99\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4ed6\u5011\u6232\u5f04\u541b\u738b\uff0c\u4ee5\u638c\u6b0a\u7684\u70ba\u7b11\u67c4\uff1b\u4ed6\u5011\u55e4\u7b11\u4e00\u5207\u57ce\u5821\uff0c\u7bc9\u58d8\u653b\u53d6\uff1b\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u7136\u5f8c\u6383\u8569\u5982\u98a8\u5439\u904e\u3002\u4ed6\u5011\u662f\u6709\u7f6a\u7684\uff0c\u56e0\u4ed6\u5011\u4ee5\u81ea\u5df1\u7684\u52e2\u529b\u70ba\u795e\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5148\u77e5\u7b2c\u4e8c\u6b21\u7684\u7591\u554f\u8036\u548c\u83ef\u6211\u7684\u3000\u795e\uff0c\u6211\u7684\u8056\u8005\u554a\uff01\u4f60\u4e0d\u662f\u81ea\u53e4\u5c31\u6709\u7684\u55ce\uff1f\u6211\u5011\u4e0d\u6703\u6b7b\u7684\u3002\u8036\u548c\u83ef\u554a\uff01\u4f60\u6d3e\u4ed6\u5011\u884c\u5be9\u5224\u3002\u78d0\u77f3\u554a\uff01\u4f60\u7acb\u4ed6\u5011\u65bd\u61f2\u7f70\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u7684\u773c\u76ee\u7d14\u6f54\uff0c\u4e0d\u770b\u90aa\u60e1\uff0c\u4e0d\u80fd\u5750\u8996\u5978\u60e1\uff1b\u70ba\u751a\u9ebc\u898b\u884c\u8a6d\u8a50\u7684\u4eba\u800c\u4e0d\u7406\uff1f\u60e1\u4eba\u541e\u6ec5\u6bd4\u81ea\u5df1\u516c\u7fa9\u7684\u4eba\uff0c\u4f60\u70ba\u751a\u9ebc\u7dd8\u9ed8\u5462\uff1f\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u7adf\u4f7f\u4eba\u50cf\u6d77\u88e1\u7684\u9b5a\uff0c\u50cf\u7121\u4eba\u7ba1\u8f44\u7684\u722c\u87f2\u985e\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8fe6\u52d2\u5e95\u4eba\u65e2\u7528\u9264\u628a\u4ed6\u5011\u91e3\u8d77\u4f86\uff0c\u7528\u7db2\u62d6\u8d70\uff0c\u7528\u9b5a\u7db2\u6536\u805a\u5728\u4e00\u8655\uff0c\u5c31\u6b61\u559c\u5feb\u6a02\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5411\u81ea\u5df1\u7684\u7db2\u737b\u796d\uff0c\u5c0d\u9b5a\u7db2\u71d2\u9999\uff0c\u56e0\u4ed6\u5011\u85c9\u6b64\u6536\u7a6b\u8c50\u5bcc\uff0c\u98f2\u98df\u5145\u88d5\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u9019\u6a23\uff0c\u4ed6\u5011\u5012\u7a7a\u81ea\u5df1\u7684\u7db2\uff0c\u6beb\u4e0d\u7559\u60c5\u5730\u7e7c\u7e8c\u6bba\u622e\u5217\u570b\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u83ef\u7684\u56de\u7b54\u6211\u8981\u7ad9\u5728\u54e8\u5d17\uff0c\u7acb\u5728\u57ce\u6a13\uff0c\u7559\u5fc3\u770b\u8036\u548c\u83ef\u5728\u6211\u88e1\u9762\u8aaa\u751a\u9ebc\uff0c\u600e\u6a23\u4f7f\u6211\u7b54\u8986\u81ea\u5df1\u7684\u6028\u8a34\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u83ef\u56de\u7b54\u6211\u8aaa\uff1a\u201c\u628a\u7570\u8c61\u5beb\u4e0b\uff0c\u8a18\u5728\u6ce5\u7248\u4e0a\uff0c\u4f7f\u8b80\u7684\u4eba\u5bb9\u6613\u660e\u767d\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56e0\u9019\u7570\u8c61\u95dc\u4e4e\u4e00\u5b9a\u7684\u65e5\u671f\uff0c\u5f88\u5feb\u5c31\u8981\u5be6\u73fe\uff0c\u6c7a\u4e0d\u5f92\u7136\uff1b\u7e31\u6709\u9072\u5ef6\uff0c\u4ecd\u7576\u7b49\u5019\uff0c\u56e0\u70ba\u5b83\u4e00\u5b9a\u6703\u4f86\u5230\uff0c\u7d55\u4e0d\u803d\u8aa4\u3002\u201d\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8fe6\u52d2\u5e95\u4eba\u81ea\u9ad8\u81ea\u5927\uff0c\u5fc3\u4e0d\u6b63\u76f4\uff1b\u7136\u800c\u7fa9\u4eba\u5fc5\u56e0\u4fe1\u5f97\u751f\uff08\u201c\u5fc5\u56e0\u4fe1\u5f97\u751f\u201d\u6216\u8b6f\uff1a\u201c\u5fc5\u56e0\u4ed6\u7684\u4fe1\u5be6\u5f97\u751f\u201d\uff09\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u8ca1\u5bcc\uff08\u50b3\u7d71\u4f5c\u201c\u9152\u201d\uff0c\u4eca\u7167\u6b7b\u6d77\u53e4\u5377\u8b6f\u4f5c\u201c\u8ca1\u5bcc\u201d\uff09\u4f7f\u4eba\u5978\u8a50\u72c2\u50b2\uff0c\u4e0d\u5f97\u5b89\u5be7\uff1b\u64f4\u5f35\u617e\u671b\uff0c\u5982\u540c\u9670\u9593\uff1b\u53c8\u50cf\u6b7b\u4ea1\uff0c\u6c38\u4e0d\u6eff\u8db3\u3002\u4ed6\u5011\u62db\u805a\u842c\u570b\uff0c\u96c6\u5408\u842c\u6c11\uff0c\u90fd\u6b78\u81ea\u5df1\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u7b2c\u4e00\u798d\u9019\u4e9b\u570b\u6c11\u8c48\u4e0d\u90fd\u7528\u8b6c\u55bb\u3001\u8af7\u523a\u3001\u6697\u8a9e\u6307\u8457\u4ed6\u5011\u8aaa\uff1a\u201c\u90a3\u4e9b\u6feb\u5f97\u4ed6\u4eba\u8ca1\u7269\u7684\uff0c\u6eff\u8f09\u5225\u4eba\u62b5\u62bc\u54c1\u7684\uff0c\u6709\u798d\u4e86\uff01\u4ed6\u5011\u9019\u6a23\u8981\u5230\u5e7e\u6642\u5462\uff1f\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u7684\u50b5\u4e3b\uff08\u201c\u4f60\u7684\u50b5\u4e3b\u201d\u6216\u8b6f\uff1a\u201c\u54ac\u4f60\u7684\u201d\uff09\u8c48\u4e0d\u5ffd\u7136\u8d77\u4f86\uff1f\u64fe\u4e82\u4f60\u7684\u8c48\u4e0d\u9192\u8d77\uff1f\u4f60\u5c31\u6210\u4e86\u4ed6\u5011\u7684\u64c4\u7269\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u56e0\u4f60\u6436\u63a0\u591a\u570b\uff0c\u6bba\u4eba\u6d41\u8840\uff0c\u5411\u5168\u5730\u5404\u57ce\u65bd\u884c\u66b4\u529b\uff0c\u6240\u4ee5\u5269\u4e0b\u7684\u4eba\u4e5f\u5fc5\u6436\u63a0\u4f60\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7b2c\u4e8c\u798d\u4f60\u9019\u70ba\u81ea\u5df1\u7684\u5bb6\u7a4d\u805a\u4e0d\u7fa9\u4e4b\u8ca1\uff0c\u5728\u9ad8\u8655\u642d\u7aa9\uff0c\u4ee5\u9003\u907f\u707d\u5bb3\u7684\uff0c\u6709\u798d\u4e86\uff01\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f60\u526a\u9664\u591a\u570b\u7684\u6c11\uff0c\u53cd\u5bb3\u4e86\u81ea\u5df1\uff1b\u4f60\u6240\u8b00\u7b97\u7684\uff0c\u4f7f\u4f60\u7684\u5bb6\u8499\u7f9e\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u77f3\u982d\u5fc5\u5f9e\u7246\u88e1\u547c\u53eb\uff0c\u6881\u6728\u5fc5\u5728\u6a39\u4e2d\u61c9\u8072\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u7b2c\u4e09\u798d\u4f60\u9019\u7528\u4eba\u8840\u505a\u57ce\uff0c\u6191\u90aa\u60e1\u7acb\u93ae\u7684\uff0c\u6709\u798d\u4e86\uff01\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u773e\u6c11\u52de\u788c\u5f97\u4f86\u7684\uff0c\u88ab\u706b\u711a\u71d2\uff1b\u5217\u570b\u8f9b\u52de\u800c\u5f97\u7684\uff0c\u7d42\u6b78\u7121\u6709\u3002\u9019\u4e0d\u90fd\u51fa\u65bc\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u55ce\uff1f\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u8a8d\u8b58\u8036\u548c\u83ef\u4e4b\u69ae\u8000\u7684\u77e5\u8b58\uff0c\u5fc5\u5145\u6eff\u5168\u5730\uff0c\u597d\u50cf\u773e\u6c34\u906e\u84cb\u6d77\u6d0b\u4e00\u6a23\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u7b2c\u56db\u798d\u4f60\u9019\u8acb\u9130\u820d\u559d\u9152\uff0c\u537b\u628a\u6bd2\u7269\u6df7\u5165\uff0c\u4f7f\u4ed6\u9189\u5012\uff0c\u70ba\u8981\u898b\u4ed6\u8d64\u88f8\u7684\uff0c\u6709\u798d\u4e86\uff01\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f60\u6eff\u6709\u7f9e\u8fb1\uff0c\u6c92\u6709\u5c0a\u69ae\uff0c\u4f60\u4e5f\u559d\u5230\u9732\u51fa\u4e0b\u9ad4\u4f86\u5427\uff01\u8036\u548c\u83ef\u53f3\u624b\u7684\u676f\u5fc5\u50b3\u56de\u4f86\u7d66\u4f60\uff0c\u4f7f\u6975\u5927\u7684\u7f9e\u8fb1\u53d6\u4ee3\u4f60\u7684\u5c0a\u69ae\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u56e0\u4f60\u5411\u9ece\u5df4\u5ae9\u4f7f\u7528\u66b4\u529b\uff0c\u6bba\u6ec5\u9a5a\u5687\u91ce\u7378\uff1b\u53c8\u6bba\u4eba\u6d41\u8840\uff0c\u5411\u5168\u5730\u5404\u57ce\u5c45\u6c11\u65bd\u884c\u66b4\u529b\uff0c\u5c31\u5fc5\u53d7\u5230\u5831\u61c9\uff08\u201c\u5831\u61c9\u201d\u539f\u6587\u4f5c\u201c\u906e\u84cb\u201d\uff09\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u7b2c\u4e94\u798d\u96d5\u523b\u7684\u50cf\u6709\u751a\u9ebc\u7528\u8655\u5462\uff1f\u4e0d\u904e\u662f\u5320\u4eba\u96d5\u523b\u51fa\u4f86\u7684\uff1b\u9444\u9020\u7684\u50cf\u3001\u865b\u5047\u7684\u6559\u5e2b\uff0c\u6709\u751a\u9ebc\u7528\u8655\u5462\uff1f\u5320\u4eba\u7adf\u501a\u9760\u81ea\u5df1\u6240\u505a\uff0c\u90a3\u4e0d\u80fd\u8aaa\u8a71\u7684\u5047\u795e\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u4f60\u9019\u5c0d\u6728\u982d\u8aaa\u201c\u8d77\u4f86\u201d\uff0c\u5c0d\u555e\u77f3\u8aaa\u201c\u9192\u5427\u201d\u7684\u4eba\uff0c\u6709\u798d\u4e86\uff01\u5b83\u600e\u80fd\u6559\u5c0e\u4f60\u5462\uff1f\u770b\u54ea\uff01\u5b83\u5305\u91d1\u9472\u9280\uff0c\u88e1\u9762\u537b\u5168\u7121\u6c23\u606f\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u7136\u800c\u8036\u548c\u83ef\u5728\u4ed6\u7684\u8056\u6bbf\u88e1\uff0c\u5168\u5730\u7576\u5728\u4ed6\u9762\u524d\u8085\u975c\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u83ef\u7684\u5a01\u56b4\u548c\u6b0a\u80fd\u54c8\u5df4\u8c37\u5148\u77e5\u7684\u79b1\u544a\uff0c\u914d\u6fc0\u52d5\u7684\u97f3\u8abf\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8036\u548c\u83ef\u554a\uff01\u6211\u807d\u898b\u4f60\u7684\u8072\u97f3\uff0c\u61fc\u6015\u4f60\u7684\u4f5c\u70ba\uff1b\u6c42\u4f60\u5728\u9019\u4e9b\u5e74\u9593\u5fa9\u8208\uff0c\u5728\u9019\u4e9b\u5e74\u9593\u5f70\u986f\uff0c\u767c\u6012\u7684\u6642\u5019\u4ee5\u6190\u61ab\u70ba\u61f7\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
