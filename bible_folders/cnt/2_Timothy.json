{
    "version": "cnt",
    "book_name": "2 Timothy",
    "book_nr": "55",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u5949\u3000\u795e\u65e8\u610f\uff0c\u6191\u8457\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u7684\u751f\u547d\u7684\u61c9\u8a31\uff0c\u4f5c\u57fa\u7763\u8036\u7a4c\u4f7f\u5f92\u7684\u4fdd\u7f85\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5beb\u4fe1\u7d66\u89aa\u611b\u7684\u5152\u5b50\u63d0\u6469\u592a\uff0c\u9858\u6069\u60e0\u3001\u6190\u61ab\u3001\u5e73\u5b89\u5f9e\u7236\u3000\u795e\u548c\u6211\u5011\u7684\u4e3b\u57fa\u7763\u8036\u7a4c\u81e8\u5230\u4f60\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u70ba\u4e3b\u4f5c\u898b\u8b49\u4e0d\u8981\u7576\u4f5c\u7f9e\u6065\u6211\u611f\u8b1d\u3000\u795e\uff0c\u5c31\u662f\u6211\u50cf\u6211\u7956\u5148\u4e00\u6a23\uff0c\u4ee5\u6e05\u6f54\u7684\u826f\u5fc3\u6240\u4e8b\u5949\u7684\u3000\u795e\uff0c\u5728\u79b1\u544a\u4e2d\u665d\u591c\u4e0d\u65b7\u5730\u8a18\u5ff5\u4f60\uff1b\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4e00\u60f3\u8d77\u4f60\u6d41\u7684\u773c\u6dda\uff0c\u6211\u5c31\u6e34\u671b\u898b\u4f60\uff0c\u597d\u53eb\u6211\u6eff\u6709\u559c\u6a02\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6211\u8a18\u5f97\u4f60\u5fc3\u88e1\u7121\u507d\u7684\u4fe1\u5fc3\uff0c\u9019\u4fe1\u5fc3\u539f\u5148\u662f\u5728\u4f60\u5916\u7956\u6bcd\u7f85\u4ee5\u548c\u4f60\u6bcd\u89aa\u53cb\u5c3c\u57fa\u5fc3\u88e1\u7684\uff0c\u6211\u6df1\u4fe1\u4e5f\u5728\u4f60\u7684\u5fc3\u88e1\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u70ba\u4e86\u9019\u7de3\u6545\uff0c\u6211\u63d0\u9192\u4f60\uff0c\u8981\u628a\u3000\u795e\u85c9\u8457\u6211\u6309\u624b\u7d66\u4f60\u7684\u6069\u8cdc\uff0c\u50cf\u706b\u4e00\u6a23\u518d\u6311\u65fa\u8d77\u4f86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u70ba\u3000\u795e\u6240\u8cdc\u7d66\u6211\u5011\u7684\uff0c\u4e0d\u662f\u81bd\u602f\u7684\u9748\uff0c\u800c\u662f\u6709\u80fd\u529b\u3001\u4ec1\u611b\u3001\u81ea\u5f8b\u7684\u9748\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6240\u4ee5\uff0c\u4e0d\u8981\u4ee5\u7d66\u6211\u5011\u7684\u4e3b\u4f5c\u898b\u8b49\u7576\u4f5c\u7f9e\u6065\uff0c\u4e5f\u4e0d\u8981\u4ee5\u6211\u9019\u70ba\u4e3b\u88ab\u56da\u7684\u7576\u4f5c\u7f9e\u6065\uff0c\u537b\u8981\u70ba\u4e86\u798f\u97f3\u7684\u7de3\u6545\uff0c\u9760\u8457\u3000\u795e\u7684\u5927\u80fd\uff0c\u8207\u6211\u540c\u53d7\u78e8\u96e3\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u3000\u795e\u6551\u4e86\u6211\u5011\uff0c\u4ee5\u8056\u53ec\u547c\u53ec\u6211\u5011\uff0c\u4e0d\u662f\u6309\u7167\u6211\u5011\u7684\u884c\u70ba\uff0c\u537b\u662f\u6309\u7167\u4ed6\u81ea\u5df1\u7684\u8a08\u5283\u548c\u6069\u5178\uff1b\u9019\u6069\u5178\u662f\u5728\u6c38\u4e16\u4e4b\u5148\uff0c\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u8cdc\u7d66\u4e86\u6211\u5011\u7684\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f46\u73fe\u5728\u85c9\u8457\u6211\u5011\u6551\u4e3b\u57fa\u7763\u8036\u7a4c\u7684\u986f\u73fe\uff0c\u624d\u8868\u660e\u51fa\u4f86\u3002\u4ed6\u5ee2\u6389\u4e86\u6b7b\u4ea1\uff0c\u85c9\u8457\u798f\u97f3\u628a\u751f\u547d\u548c\u4e0d\u673d\u5f70\u986f\u51fa\u4f86\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u70ba\u9019\u798f\u97f3\uff0c\u6211\u88ab\u6d3e\u4f5c\u50b3\u9053\u7684\u3001\u4f7f\u5f92\u548c\u6559\u5e2b\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u70ba\u4e86\u9019\u7de3\u6545\uff0c\u6211\u4e5f\u53d7\u9019\u4e9b\u82e6\uff0c\u4f46\u6211\u4e0d\u4ee5\u70ba\u6065\uff0c\u56e0\u70ba\u6211\u77e5\u9053\u6211\u6240\u4fe1\u7684\u662f\u8ab0\uff0c\u4e5f\u6df1\u4fe1\u4ed6\u80fd\u4fdd\u5b88\u6211\u6240\u4ea4\u8a17\u4ed6\u7684\uff0c\u76f4\u5230\u90a3\u65e5\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u8981\u9760\u8457\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u7684\u4fe1\u5fc3\u548c\u611b\u5fc3\uff0c\u5e38\u5e38\u5805\u5b88\u5f9e\u6211\u9019\u88e1\u807d\u904e\u7684\u8a71\uff0c\u4f5c\u70ba\u7d14\u6b63\u8a71\u8a9e\u7684\u6a21\u7bc4\uff1b\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u53c8\u8981\u9760\u8457\u90a3\u4f4f\u5728\u6211\u5011\u88e1\u9762\u7684\u8056\u9748\uff0c\u4fdd\u5b88\u6240\u4ea4\u8a17\u4f60\u7684\u5584\u9053\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u77e5\u9053\u6240\u6709\u5728\u4e9e\u897f\u4e9e\u7684\u4eba\u90fd\u96e2\u68c4\u4e86\u6211\uff0c\u4ed6\u5011\u7576\u4e2d\u6709\u8153\u5409\u8def\u548c\u9ed1\u6469\u5176\u5c3c\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u9858\u4e3b\u8cdc\u6190\u61ab\u7d66\u963f\u5c3c\u8272\u5f17\u4e00\u5bb6\u7684\u4eba\uff0c\u56e0\u70ba\u4ed6\u591a\u6b21\u4f7f\u6211\u66a2\u5feb\uff0c\u4e5f\u4e0d\u4ee5\u6211\u7684\u9396\u934a\u70ba\u6065\uff1b\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ed6\u7adf\u7136\u4f86\u5230\u7f85\u99ac\uff0c\u8feb\u5207\u5730\u5c0b\u627e\u6211\uff0c\u7d50\u679c\u627e\u5230\u4e86\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\uff08\u9858\u4e3b\u5728\u90a3\u65e5\u4f7f\u4ed6\u5f9e\u4e3b\u90a3\u88e1\u5f97\u8457\u6190\u61ab\u3002\uff09\u4f60\u4e5f\u6e05\u695a\u77e5\u9053\u4ed6\u5728\u4ee5\u5f17\u6240\u662f\u600e\u6a23\u591a\u65b9\u670d\u4e8b\u6211\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4f5c\u57fa\u7763\u7684\u7cbe\u5175\u6240\u4ee5\uff0c\u6211\u5152\u554a\uff0c\u4f60\u61c9\u7576\u5728\u57fa\u7763\u8036\u7a4c\u7684\u6069\u5178\u88e1\u525b\u5f37\u8d77\u4f86\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u53c8\u61c9\u7576\u628a\u4f60\u5728\u8a31\u591a\u898b\u8b49\u4eba\u9762\u524d\u5f9e\u6211\u9019\u88e1\u807d\u898b\u7684\uff0c\u4ea4\u8a17\u7d66\u90a3\u4e9b\u53c8\u5fe0\u5fc3\u53c8\u80fd\u5920\u6559\u5c0e\u5225\u4eba\u7684\u4eba\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u61c9\u7576\u548c\u6211\u540c\u53d7\u78e8\u96e3\uff0c\u597d\u50cf\u57fa\u7763\u8036\u7a4c\u7684\u7cbe\u5175\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u7576\u5175\u7684\u4eba\u4e0d\u8b93\u4e16\u52d9\u7e8f\u8eab\uff0c\u70ba\u8981\u4f7f\u90a3\u62db\u5175\u7684\u4eba\u6b61\u559c\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7af6\u8cfd\u7684\u4eba\u82e5\u4e0d\u9075\u5b88\u898f\u5247\uff0c\u5c31\u4e0d\u80fd\u5f97\u51a0\u5195\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u52de\u529b\u7684\u8fb2\u592b\u7406\u7576\u5148\u5617\u679c\u5be6\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u8981\u60f3\u60f3\u6211\u7684\u8a71\uff0c\u56e0\u70ba\u51e1\u4e8b\u4e3b\u5fc5\u7d66\u4f60\u9818\u609f\u529b\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u8981\u8a18\u5f97\u90a3\u5f9e\u6b7b\u4eba\u4e2d\u5fa9\u6d3b\u7684\u8036\u7a4c\u57fa\u7763\uff0c\u4ed6\u662f\u5927\u885b\u7684\u5f8c\u88d4\u6240\u751f\u7684\uff0c\u9019\u5c31\u662f\u6211\u6240\u50b3\u7684\u798f\u97f3\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u70ba\u4e86\u9019\u798f\u97f3\u53d7\u4e86\u78e8\u96e3\uff0c\u751a\u81f3\u50cf\u72af\u4eba\u4e00\u6a23\u88ab\u6346\u7d81\u8d77\u4f86\uff1b\u53ef\u662f\uff0c\u3000\u795e\u7684\u8a71\u537b\u4e0d\u88ab\u6346\u7d81\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u6b64\uff0c\u6211\u70ba\u4e86\u9078\u6c11\u5fcd\u53d7\u4e00\u5207\uff0c\u597d\u53eb\u4ed6\u5011\u4e5f\u53ef\u4ee5\u5f97\u8457\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u7684\u6551\u6069\uff0c\u548c\u6c38\u9060\u7684\u69ae\u8000\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u201c\u6211\u5011\u82e5\u8207\u57fa\u7763\u540c\u6b7b\uff0c\u5c31\u5fc5\u8207\u4ed6\u540c\u6d3b\uff1b\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u5011\u82e5\u80fd\u5805\u5fcd\uff0c\u5c31\u5fc5\u8207\u4ed6\u4e00\u540c\u4f5c\u738b\uff1b\u6211\u5011\u82e5\u4e0d\u8a8d\u4ed6\uff0c\u4ed6\u5fc5\u4e0d\u8a8d\u6211\u5011\uff1b\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u5011\u7e31\u7136\u4e0d\u4fe1\uff0c\u4ed6\u4ecd\u7136\u662f\u4fe1\u5be6\u7684\uff0c\u56e0\u70ba\u4ed6\u4e0d\u80fd\u5426\u5b9a\u81ea\u5df1\u3002\u201d\u9019\u8a71\u662f\u53ef\u4fe1\u7684\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f5c\u7121\u6127\u7684\u5de5\u4eba\u4f60\u8981\u5728\u3000\u795e\u9762\u524d\u628a\u9019\u4e9b\u4e8b\u63d0\u9192\u773e\u4eba\uff0c\u53ee\u56d1\u4ed6\u5011\u4e0d\u8981\u4f5c\u7121\u76ca\u7684\u722d\u8faf\uff1b\u9019\u53ea\u80fd\u6557\u58de\u807d\u898b\u7684\u4eba\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u61c9\u7576\u7aed\u529b\u5728\u3000\u795e\u9762\u524d\u4f5c\u4e00\u500b\u8499\u7a31\u8a31\u3001\u7121\u6127\u7684\u5de5\u4eba\uff0c\u6b63\u78ba\u5730\u8b1b\u89e3\u771f\u7406\u7684\u9053\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u7e3d\u8981\u9060\u907f\u4e16\u4fd7\u7684\u7a7a\u8ac7\uff0c\u56e0\u70ba\u9019\u4e9b\u5fc5\u6703\u5f15\u4eba\u9032\u5230\u66f4\u4e0d\u656c\u8654\u7684\u5730\u6b65\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ed6\u5011\u7684\u8a71\u597d\u50cf\u6bd2\u7624\u4e00\u6a23\u8513\u5ef6\uff1b\u4ed6\u5011\u7576\u4e2d\u6709\u8a31\u7c73\u4e43\u548c\u8153\u7406\u5f92\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6\u5011\u504f\u96e2\u4e86\u771f\u9053\uff0c\u8aaa\u5fa9\u6d3b\u7684\u4e8b\u5df2\u7d93\u904e\u53bb\u4e86\uff0c\u65bc\u662f\u6bc0\u58de\u4e86\u4e00\u4e9b\u4eba\u7684\u4fe1\u5fc3\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u7136\u800c\uff0c\u3000\u795e\u5805\u56fa\u7684\u6839\u57fa\u5df2\u7d93\u7acb\u5b9a\uff0c\u4e0a\u9762\u523b\u8457\u9019\u6a23\u7684\u5370\uff1a\u201c\u4e3b\u8a8d\u8b58\u5c6c\u65bc\u4ed6\u7684\u4eba\u201d\u548c\u201c\u51e1\u7a31\u547c\u4e3b\u540d\u7684\u4eba\u90fd\u61c9\u7576\u96e2\u958b\u4e0d\u7fa9\u201d\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u5728\u5bcc\u8cb4\u4eba\u7684\u5bb6\u88e1\uff08\u201c\u5bcc\u8cb4\u4eba\u7684\u5bb6\u88e1\u201d\u539f\u6587\u4f5c\u201c\u5927\u623f\u5b50\u201d\uff09\uff0c\u4e0d\u4f46\u6709\u91d1\u5668\u3001\u9280\u5668\uff0c\u4e5f\u6709\u6728\u5668\u3001\u74e6\u5668\uff1b\u6709\u8cb4\u91cd\u7684\uff0c\u4e5f\u6709\u5351\u8ce4\u7684\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u4eba\u82e5\u81ea\u6f54\uff0c\u96e2\u958b\u5351\u8ce4\u7684\u4e8b\uff0c\u5c31\u5fc5\u4f5c\u8cb4\u91cd\u7684\u5668\u76bf\uff0c\u6210\u70ba\u8056\u6f54\uff0c\u5408\u4e3b\u4f7f\u7528\uff0c\u9810\u5099\u884c\u5404\u6a23\u7684\u5584\u4e8b\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4f60\u61c9\u7576\u9003\u907f\u5e74\u8f15\u4eba\u7684\u79c1\u617e\uff0c\u8981\u548c\u90a3\u4e9b\u4ee5\u6e05\u6f54\u7684\u5fc3\u6c42\u544a\u4e3b\u7684\u4eba\uff0c\u4e00\u540c\u8ffd\u6c42\u516c\u7fa9\u3001\u4fe1\u5fc3\u3001\u611b\u5fc3\u3001\u548c\u5e73\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u4f60\u8981\u62d2\u7d55\u611a\u8822\u7121\u77e5\u7684\u554f\u96e3\uff1b\u4f60\u77e5\u9053\u9019\u4e9b\u4e8b\u6703\u5f15\u8d77\u722d\u8ad6\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u4f46\u4e3b\u7684\u50d5\u4eba\u537b\u4e0d\u53ef\u722d\u8ad6\uff0c\u7e3d\u8981\u5f85\u4eba\u6eab\u548c\uff0c\u5584\u65bc\u6559\u5c0e\uff0c\u5b58\u5fc3\u5fcd\u8010\uff0c\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u4ee5\u6eab\u67d4\u52f8\u5c0e\u90a3\u4e9b\u5c0d\u6297\u7684\u4eba\uff0c\u6216\u8a31\u3000\u795e\u7d66\u4ed6\u5011\u6094\u6539\u7684\u5fc3\uff0c\u53ef\u4ee5\u8a8d\u8b58\u771f\u7406\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u4ed6\u5011\u96d6\u7136\u66fe\u7d93\u88ab\u9b54\u9b3c\u64c4\u53bb\uff0c\u96a8\u5f9e\u4ed6\u7684\u610f\u601d\u800c\u884c\uff0c\u6216\u8a31\u4e5f\u80fd\u9192\u609f\u904e\u4f86\uff0c\u812b\u96e2\u9b54\u9b3c\u7684\u9677\u9631\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u672b\u4e16\u7684\u60c5\u6cc1\u4f60\u61c9\u7576\u77e5\u9053\uff0c\u672b\u5f8c\u7684\u65e5\u5b50\u5fc5\u6709\u8271\u96e3\u7684\u6642\u671f\u4f86\u5230\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u90a3\u6642\uff0c\u4eba\u6703\u5c08\u611b\u81ea\u5df1\u3001\u8caa\u611b\u9322\u8ca1\u3001\u81ea\u8a87\u3001\u9ad8\u50b2\u3001\u893b\u7006\u3001\u6096\u9006\u7236\u6bcd\u3001\u5fd8\u6069\u8ca0\u7fa9\u3001\u4e0d\u8056\u6f54\u3001\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6c92\u6709\u89aa\u60c5\u3001\u4e0d\u80af\u548c\u89e3\u3001\u60e1\u8a00\u4e2d\u50b7\u3001\u4e0d\u80fd\u81ea\u5f8b\u3001\u6a6b\u883b\u5147\u66b4\u3001\u4e0d\u611b\u826f\u5584\u3001\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8ce3\u4e3b\u8ce3\u53cb\u3001\u5bb9\u6613\u885d\u52d5\u3001\u50b2\u6162\u81ea\u5927\u3001\u611b\u4eab\u6a02\u904e\u65bc\u611b\u3000\u795e\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6709\u656c\u8654\u7684\u5f62\u5f0f\uff0c\u537b\u5426\u5b9a\u656c\u8654\u7684\u80fd\u529b\uff1b\u9019\u4e9b\u4eba\u4f60\u61c9\u7576\u907f\u958b\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u5011\u4e2d\u9593\u6709\u4eba\u6f5b\u5165\u5225\u4eba\u7684\u5bb6\u88e1\uff0c\u64cd\u7e31\u7121\u77e5\u7684\u5a66\u5973\u3002\u9019\u4e9b\u5a66\u5973\u88ab\u7f6a\u58d3\u4f4f\uff0c\u53c8\u88ab\u5404\u7a2e\u79c1\u617e\u62d6\u7d2f\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u96d6\u7136\u5e38\u5e38\u5b78\u7fd2\uff0c\u7e3d\u4e0d\u80fd\u5145\u5206\u660e\u767d\u771f\u7406\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5f9e\u524d\u96c5\u5c3c\u548c\u694a\u84d3\u600e\u6a23\u53cd\u5c0d\u6469\u897f\uff0c\u9019\u4e9b\u4eba\u4e5f\u600e\u6a23\u53cd\u5c0d\u771f\u7406\u3002\u4ed6\u5011\u7684\u5fc3\u601d\u6557\u58de\uff0c\u4fe1\u4ef0\u4e5f\u7d93\u4e0d\u8d77\u8003\u9a57\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7136\u800c\u4ed6\u5011\u4e0d\u80fd\u518d\u9019\u6a23\u4e0b\u53bb\u4e86\uff0c\u56e0\u70ba\u4ed6\u5011\u7684\u611a\u6627\u7d42\u5fc5\u5728\u773e\u4eba\u9762\u524d\u986f\u9732\u51fa\u4f86\uff0c\u597d\u50cf\u90a3\u5169\u500b\u4eba\u4e00\u6a23\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8056\u7d93\u90fd\u662f\u3000\u795e\u9ed8\u793a\u7684\u4f46\u662f\uff0c\u4f60\u5df2\u7d93\u4e86\u89e3\u6211\u7684\u6559\u8a13\u3001\u54c1\u884c\u3001\u5fd7\u5411\u3001\u4fe1\u5fc3\u3001\u5bec\u5bb9\u3001\u611b\u5fc3\u548c\u5fcd\u8010\uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ee5\u53ca\u6211\u5728\u5b89\u63d0\u963f\u3001\u4ee5\u54e5\u5ff5\u3001\u8def\u53f8\u5f97\u6240\u53d7\u7684\u82e6\u548c\u8feb\u5bb3\u3002\u6211\u6240\u5fcd\u53d7\u7684\u662f\u600e\u6a23\u7684\u8feb\u5bb3\uff1b\u9019\u4e00\u5207\u4e3b\u90fd\u6551\u6211\u812b\u96e2\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5176\u5be6\uff0c\u6240\u6709\u7acb\u5fd7\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u904e\u656c\u8654\u751f\u6d3b\u7684\uff0c\u90fd\u5fc5\u906d\u53d7\u8feb\u5bb3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f46\u60e1\u4eba\u548c\u9a19\u5b50\u5fc5\u8d8a\u4f86\u8d8a\u58de\uff0c\u4ed6\u5011\u6b3a\u9a19\u4eba\uff0c\u4e5f\u5fc5\u53d7\u6b3a\u9a19\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u61c9\u7576\u9075\u5b88\u6240\u5b78\u548c\u78ba\u4fe1\u7684\uff1b\u4f60\u77e5\u9053\u662f\u8ddf\u8ab0\u5b78\u7684\uff0c\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4e26\u4e14\u77e5\u9053\u81ea\u5df1\u5f9e\u5c0f\u5c31\u660e\u767d\u8056\u7d93\uff0c\u9019\u8056\u7d93\u80fd\u5920\u4f7f\u4f60\u6709\u667a\u6167\uff0c\u53ef\u4ee5\u56e0\u4fe1\u57fa\u7763\u8036\u7a4c\u5f97\u8457\u6551\u6069\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5168\u90e8\u8056\u7d93\u90fd\u662f\u3000\u795e\u6240\u9ed8\u793a\u7684\uff0c\u5728\u6559\u8a13\u3001\u8cac\u5099\u3001\u77ef\u6b63\u548c\u516c\u7fa9\u7684\u8a13\u7df4\u5404\u65b9\u9762\uff0c\u90fd\u662f\u6709\u76ca\u7684\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u70ba\u8981\u4f7f\u5c6c\u3000\u795e\u7684\u4eba\u88dd\u5099\u597d\uff0c\u53ef\u4ee5\u5b8c\u6210\u5404\u6a23\u7684\u5584\u5de5\u3002\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u52d9\u8981\u50b3\u9053\u6211\u5728\u3000\u795e\u9762\u524d\uff0c\u4e26\u4e14\u5728\u90a3\u5c07\u8981\u5be9\u5224\u6d3b\u4eba\u6b7b\u4eba\u7684\u57fa\u7763\u8036\u7a4c\u9762\u524d\uff0c\u6191\u8457\u4ed6\u7684\u986f\u73fe\u548c\u570b\u5ea6\u53ee\u56d1\u4f60\uff1a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u52d9\u8981\u50b3\u9053\uff1b\u7121\u8ad6\u6642\u6a5f\u662f\u5426\u9069\u5408\uff0c\u90fd\u8981\u5e38\u4f5c\u6e96\u5099\uff1b\u8981\u4ee5\u591a\u65b9\u7684\u5fcd\u8010\u548c\u6559\u8a13\u8cac\u5099\u4eba\u3001\u8b66\u6212\u4eba\u3001\u52f8\u52c9\u4eba\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56e0\u70ba\u6642\u5019\u5feb\u8981\u5230\u4e86\uff0c\u4eba\u5fc5\u5bb9\u4e0d\u4e0b\u7d14\u6b63\u7684\u9053\u7406\uff0c\u53cd\u800c\u8033\u6735\u767c\u7662\uff0c\u96a8\u8457\u81ea\u5df1\u7684\u79c1\u617e\uff0c\u589e\u6dfb\u8a31\u591a\u6559\u5e2b\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u800c\u4e14\u8f49\u96e2\u4e0d\u807d\u771f\u7406\uff0c\u53cd\u5012\u8da8\u5411\u7121\u7a3d\u4e4b\u8ac7\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u537b\u8981\u51e1\u4e8b\u8b39\u614e\uff0c\u5fcd\u53d7\u78e8\u96e3\u3001\u4f5c\u50b3\u798f\u97f3\u8005\u7684\u5de5\u4f5c\uff0c\u5b8c\u6210\u4f60\u7684\u8077\u52d9\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u5df2\u7d93\u88ab\u6f86\u5960\uff1b\u6211\u96e2\u4e16\u7684\u6642\u5019\u4e5f\u5230\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u90a3\u7f8e\u597d\u7684\u4ed7\u6211\u5df2\u7d93\u6253\u904e\u4e86\uff0c\u7576\u8dd1\u7684\u8def\u6211\u5df2\u7d93\u8dd1\u76e1\u4e86\uff0c\u6240\u6301\u7684\u4fe1\u4ef0\u6211\u5df2\u7d93\u5b88\u4f4f\u4e86\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u5f9e\u6b64\u4ee5\u5f8c\uff0c\u6709\u516c\u7fa9\u7684\u51a0\u5195\u70ba\u6211\u5b58\u7559\uff0c\u5c31\u662f\u6309\u516c\u7fa9\u5be9\u5224\u7684\u4e3b\u5728\u90a3\u65e5\u8981\u8cde\u7d66\u6211\u7684\uff1b\u4e0d\u4f46\u8cde\u7d66\u6211\uff0c\u4e5f\u8cde\u7d66\u6240\u6709\u611b\u6155\u4ed6\u986f\u73fe\u7684\u4eba\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6700\u5f8c\u7684\u5429\u5490\u4f60\u8981\u76e1\u5feb\u5230\u6211\u9019\u88e1\u4f86\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u70ba\u5e95\u99ac\u8caa\u611b\u73fe\u4eca\u7684\u4e16\u754c\uff0c\u5c31\u96e2\u68c4\u6211\u5230\u5e16\u6492\u7f85\u5c3c\u8fe6\u53bb\u4e86\u3002\u9769\u52d2\u58eb\u53bb\u4e86\u52a0\u62c9\u592a\uff0c\u63d0\u591a\u53bb\u4e86\u9054\u99ac\u592a\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u53ea\u6709\u8def\u52a0\u5728\u6211\u9019\u88e1\u3002\u4f60\u627e\u5230\u4e86\u99ac\u53ef\uff0c\u5c31\u628a\u4ed6\u5e36\u4f86\uff0c\u56e0\u70ba\u4ed6\u5728\u8056\u5de5\u4e0a\u5c0d\u6211\u6709\u76ca\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u5dee\u6d3e\u4e86\u63a8\u57fa\u53e4\u5230\u4ee5\u5f17\u6240\u53bb\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u5728\u7279\u7f85\u4e9e\u7559\u5728\u52a0\u5e03\u90a3\u88e1\u7684\u5916\u8863\uff0c\u4f60\u4f86\u7684\u6642\u5019\u8981\u628a\u5b83\u5e36\u4f86\uff0c\u9084\u6709\u90a3\u4e9b\u66f8\u5377\uff0c\u7279\u5225\u662f\u90a3\u4e9b\u7f8a\u76ae\u5377\uff0c\u4e5f\u8981\u5e36\u4f86\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u9285\u5320\u4e9e\u6b77\u5c71\u5927\u4f5c\u4e86\u8a31\u591a\u60e1\u4e8b\u9677\u5bb3\u6211\uff0c\u4e3b\u5fc5\u6309\u8457\u4ed6\u6240\u884c\u7684\u5831\u61c9\u4ed6\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u4e5f\u8981\u63d0\u9632\u4ed6\uff0c\u56e0\u70ba\u4ed6\u6975\u529b\u62b5\u64cb\u6211\u5011\u7684\u8a71\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6211\u7b2c\u4e00\u6b21\u7533\u8faf\u7684\u6642\u5019\uff0c\u6c92\u6709\u4e00\u500b\u4eba\u652f\u6301\u6211\uff0c\u53cd\u800c\u96e2\u68c4\u6211\u3002\u4f46\u9858\u9019\u7f6a\u4e0d\u8981\u6b78\u5728\u4ed6\u5011\u8eab\u4e0a\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u7136\u800c\u4e3b\u7ad9\u5728\u6211\u65c1\u908a\uff0c\u52a0\u7d66\u6211\u529b\u91cf\uff0c\u4f7f\u798f\u97f3\u7684\u4fe1\u606f\u85c9\u8457\u6211\u76e1\u90fd\u50b3\u958b\uff0c\u842c\u570b\u7684\u4eba\u90fd\u53ef\u4ee5\u807d\u898b\uff1b\u4ed6\u4e26\u4e14\u628a\u6211\u5f9e\u7345\u5b50\u53e3\u4e2d\u6551\u51fa\u4f86\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4e3b\u5fc5\u6551\u6211\u812b\u96e2\u4e00\u5207\u90aa\u60e1\u7684\u4e8b\uff0c\u4e5f\u5fc5\u6551\u6211\u9032\u5165\u4ed6\u5929\u4e0a\u7684\u570b\u3002\u9858\u69ae\u8000\u6b78\u7d66\u4ed6\uff0c\u76f4\u5230\u6c38\u6c38\u9060\u9060\u3002\u963f\u5011\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u554f\u5019\u7684\u8a71\u554f\u5019\u767e\u57fa\u62c9\u3001\u4e9e\u5c45\u62c9\u548c\u963f\u5c3c\u8272\u5f17\u4e00\u5bb6\u7684\u4eba\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4ee5\u62c9\u90fd\u5728\u54e5\u6797\u591a\u4f4f\u4e0b\u4e86\u3002\u7279\u7f85\u975e\u6469\u75c5\u4e86\uff0c\u6211\u5c31\u7559\u4ed6\u5728\u7c73\u5229\u90fd\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u4f60\u8981\u8d95\u7dca\u5728\u51ac\u5929\u4ee5\u524d\u5230\u6211\u9019\u88e1\u4f86\u3002\u53cb\u5e03\u7f85\u3001\u5e03\u7530\u3001\u5229\u5974\u3001\u9769\u8001\u5e95\u4e9e\u548c\u773e\u5f1f\u5144\u90fd\u554f\u5019\u4f60\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u4e3b\u8207\u4f60\u540c\u5728\u3002\u9858\u6069\u60e0\u8207\u4f60\u5011\u540c\u5728\u3002\n"
                }
            }
        }
    }
}