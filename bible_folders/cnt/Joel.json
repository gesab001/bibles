{
    "version": "cnt",
    "book_name": "Joel",
    "book_nr": "29",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u56b4\u91cd\u7684\u8757\u707d\u8036\u548c\u83ef\u7684\u8a71\u81e8\u5230\u6bd7\u571f\u73e5\u7684\u5152\u5b50\u7d04\u73e5\uff1a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u8001\u5e74\u4eba\u54ea\uff01\u4f60\u5011\u8981\u807d\u9019\u8a71\u3002\u9019\u5730\u6240\u6709\u7684\u5c45\u6c11\u554a\uff01\u4f60\u5011\u8981\u7559\u5fc3\u807d\u3002\u5728\u4f60\u5011\u7684\u65e5\u5b50\uff0c\u6216\u5728\u4f60\u5011\u7956\u5148\u7684\u65e5\u5b50\uff0c\u66fe\u6709\u9019\u6a23\u7684\u4e8b\u55ce\uff1f\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u5011\u8981\u628a\u9019\u4e8b\u50b3\u8207\u5b50\uff0c\u5b50\u50b3\u8207\u5b6b\uff0c\u5b6b\u50b3\u8207\u5f8c\u4ee3\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u526a\u87f2\u5269\u4e0b\u7684\uff0c\u8757\u87f2\u5403\u4e86\uff1b\u8757\u87f2\u5269\u4e0b\u7684\uff0c\u877b\u5b50\u5403\u4e86\uff1b\u877b\u5b50\u5269\u4e0b\u7684\uff0c\u879e\u86b1\u5403\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9152\u9189\u7684\u4eba\u54ea\uff01\u4f60\u5011\u8981\u9192\u904e\u4f86\uff1b\u8981\u54c0\u54ed\uff01\u6240\u6709\u55dc\u9152\u7684\u4eba\u54ea\uff01\u4f60\u5011\u90fd\u8981\u70ba\u751c\u9152\u54c0\u865f\uff0c\u56e0\u70ba\u751c\u9152\u5f9e\u4f60\u5011\u7684\u53e3\u88e1\u65b7\u7d55\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u56e0\u70ba\u6709\u4e00\u6c11\u65cf\u4e0a\u4f86\u4fb5\u72af\u6211\u7684\u570b\u571f\uff0c\u4ed6\u5011\u5f37\u5927\u53c8\u7121\u6578\uff1b\u4ed6\u5011\u7684\u7259\u9f52\u50cf\u7345\u5b50\u7684\u7259\u9f52\uff0c\u4ed6\u5011\u6709\u6bcd\u7345\u7684\u5927\u7259\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u5011\u4f7f\u6211\u7684\u8461\u8404\u6a39\u8352\u6dbc\uff0c\u6298\u65b7\u4e86\u6211\u7684\u7121\u82b1\u679c\u6a39\uff0c\u628a\u6a39\u76ae\u525d\u76e1\uff0c\u4e1f\u5728\u4e00\u65c1\uff0c\u4f7f\u679d\u689d\u9732\u767d\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u52f8\u4eba\u6c11\u54c0\u54ed\u4f60\u5011\u8981\u54c0\u865f\uff0c\u50cf\u5c11\u5973\u8170\u675f\u9ebb\u5e03\uff0c\u70ba\u5979\u5e7c\u5e74\u6642\u8a31\u914d\u7684\u4e08\u592b\u54c0\u865f\u4e00\u6a23\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7d20\u796d\u548c\u5960\u796d\u90fd\u5f9e\u8036\u548c\u83ef\u7684\u6bbf\u88e1\u65b7\u7d55\u4e86\uff0c\u4e8b\u5949\u8036\u548c\u83ef\u7684\u796d\u53f8\u4e5f\u90fd\u60b2\u54c0\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u7530\u91ce\u8352\u5ee2\uff0c\u571f\u5730\u6dd2\u6dbc\uff0c\u56e0\u70ba\u4e94\u7a40\u6bc0\u58de\uff0c\u65b0\u9152\u4e7e\u7aed\uff0c\u6cb9\u4e5f\u7f3a\u4e4f\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8fb2\u592b\u554a\uff01\u4f60\u5011\u8981\u70ba\u5927\u9ea5\u548c\u5c0f\u9ea5\u7f9e\u6127\u3002\u4fee\u7406\u8461\u8404\u5712\u7684\u554a\uff01\u4f60\u5011\u8981\u54c0\u865f\uff0c\u56e0\u70ba\u7530\u9593\u7684\u838a\u7a3c\u90fd\u7834\u58de\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u8461\u8404\u6a39\u67af\u4e7e\uff0c\u7121\u82b1\u679c\u6a39\u8870\u6b98\uff1b\u77f3\u69b4\u6a39\u3001\u68d5\u6a39\u3001\u860b\u679c\u6a39\uff0c\u7530\u91ce\u6240\u6709\u7684\u6a39\u6728\u90fd\u67af\u4e7e\uff0c\u56e0\u6b64\u6b61\u6a02\u5f9e\u4eba\u9593\u6d88\u5931\u4e86\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u547c\u7c72\u4eba\u6c11\u7981\u98df\u79b1\u544a\u796d\u53f8\u554a\uff01\u4f60\u5011\u8981\u8170\u675f\u9ebb\u5e03\uff0c\u4e26\u4e14\u75db\u54ed\u3002\u5728\u796d\u58c7\u4f8d\u5019\u7684\u554a\uff01\u4f60\u5011\u8981\u54c0\u865f\u3002\u4e8b\u5949\u6211\u7684\u3000\u795e\u7684\u554a\uff01\u4f60\u5011\u62ab\u4e0a\u9ebb\u5e03\u9032\u53bb\u904e\u591c\u5427\uff0c\u56e0\u70ba\u7d20\u796d\u548c\u5960\u796d\uff0c\u90fd\u5f9e\u4f60\u5011\u7684\u3000\u795e\u7684\u6bbf\u4e2d\u6b62\u606f\u4e86\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u5011\u8981\u628a\u7981\u98df\u7684\u65e5\u5b50\u5206\u5225\u70ba\u8056\uff0c\u53ec\u958b\u56b4\u8085\u6703\uff0c\u805a\u96c6\u773e\u9577\u8001\u548c\u570b\u4e2d\u6240\u6709\u7684\u5c45\u6c11\uff0c\u5230\u4f60\u5011\u7684\u3000\u795e\u8036\u548c\u83ef\u7684\u6bbf\u88e1\uff0c\u5411\u8036\u548c\u83ef\u547c\u6c42\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u54c0\u54c9\u90a3\u65e5\uff01\u56e0\u70ba\u8036\u548c\u83ef\u7684\u65e5\u5b50\u81e8\u8fd1\u4e86\uff1b\u90a3\u65e5\u4f86\u5230\uff0c\u597d\u50cf\u6bc0\u6ec5\u5f9e\u5168\u80fd\u8005\u81e8\u5230\u4e00\u6a23\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u7ce7\u98df\u4e0d\u662f\u5728\u6211\u5011\u7684\u773c\u524d\u65b7\u7d55\u4e86\u55ce\uff1f\u6b61\u559c\u8207\u5feb\u6a02\u4e0d\u662f\u5f9e\u6211\u5011\u7684\u3000\u795e\u7684\u6bbf\u4e2d\u6b62\u606f\u4e86\u55ce\uff1f\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u7a40\u7a2e\u5728\u571f\u584a\u5e95\u4e0b\u673d\u721b\uff0c\u5009\u5eab\u8352\u6dbc\uff0c\u7a40\u5009\u7834\u721b\u88ab\u62c6\u6bc0\uff0c\u56e0\u70ba\u4e94\u7a40\u90fd\u67af\u4e7e\u4e86\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u7272\u755c\u767c\u51fa\u54c0\u9cf4\uff0c\u725b\u7fa4\u5230\u8655\u6d41\u8569\uff0c\u56e0\u70ba\u6c92\u6709\u8349\u5834\u7d66\u7260\u5011\uff0c\u7f8a\u7fa4\u4e5f\u540c\u6a23\u53d7\u82e6\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8036\u548c\u83ef\u554a\uff01\u6211\u5411\u4f60\u547c\u6c42\uff0c\u56e0\u70ba\u706b\u541e\u6ec5\u4e86\u66e0\u91ce\u7684\u8349\u5834\uff0c\u706b\u71c4\u71d2\u76e1\u4e86\u7530\u9593\u6240\u6709\u7684\u6a39\u6728\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u7530\u91ce\u7684\u8d70\u7378\u4e5f\u5411\u4f60\u767c\u5598\uff0c\u56e0\u70ba\u6eaa\u6c34\u90fd\u4e7e\u6db8\u4e86\uff0c\u706b\u541e\u6ec5\u4e86\u66e0\u91ce\u7684\u8349\u5834\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8036\u548c\u83ef\u7684\u65e5\u5b50\u5feb\u4f86\u4f60\u5011\u8981\u5728\u932b\u5b89\u5439\u865f\uff0c\u4e26\u5728\u6211\u7684\u8056\u5c71\u767c\u51fa\u8b66\u8a0a\uff0c\u8b93\u570b\u4e2d\u6240\u6709\u7684\u5c45\u6c11\u90fd\u6230\u6144\uff0c\u56e0\u70ba\u8036\u548c\u83ef\u7684\u65e5\u5b50\u5feb\u5230\uff0c\u5fc5\u7136\u81e8\u8fd1\u4e86\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u90a3\u65e5\u662f\u9ed1\u6697\u5e7d\u51a5\u7684\u65e5\u5b50\uff0c\u662f\u5bc6\u96f2\u6f06\u9ed1\u7684\u65e5\u5b50\u3002\u6709\u4e00\u968a\u773e\u591a\u5f37\u76db\u7684\u6c11\u4f86\u5230\uff0c\u597d\u50cf\u6668\u5149\u6eff\u5e03\u7fa4\u5c71\uff1b\u9019\u6a23\u7684\u4e8b\u5f9e\u524d\u6c92\u6709\u767c\u751f\u904e\uff0c\u4ee5\u5f8c\u76f4\u5230\u842c\u4ee3\uff0c\u4e5f\u4e0d\u6703\u767c\u751f\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5728\u4ed6\u5011\u524d\u9762\u6709\u541e\u6ec5\u7684\u706b\uff0c\u5728\u4ed6\u5011\u5f8c\u9762\u6709\u71c3\u71d2\u7684\u706b\u71c4\uff1b\u4ed6\u5011\u672a\u5230\u4ee5\u524d\uff0c\u5927\u5730\u50cf\u4f0a\u7538\u5712\uff0c\u4ed6\u5011\u904e\u53bb\u4ee5\u5f8c\uff0c\u5927\u5730\u6210\u4e86\u8352\u6dbc\u7684\u66e0\u91ce\uff0c\u6c92\u6709\u4e00\u6a23\u80fd\u9003\u907f\u4ed6\u5011\u7684\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u5011\u7684\u5f62\u72c0\u50cf\u99ac\u7684\u5f62\u72c0\uff0c\u6230\u99ac\u600e\u6a23\u5954\u8dd1\uff0c\u4ed6\u5011\u4e5f\u600e\u6a23\u5954\u8dd1\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4ed6\u5011\u5728\u7fa4\u5c71\u9802\u4e0a\u8df3\u8e8d\u7684\u8072\u97f3\uff0c\u5982\u6230\u8eca\u7684\u97ff\u8072\uff0c\u53c8\u5982\u706b\u71c4\u541e\u6ec5\u788e\u79f8\u7684\u97ff\u8072\uff0c\u50cf\u5f37\u76db\u7684\u6c11\u64fa\u9663\u9810\u5099\u4f5c\u6230\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5728\u4ed6\u5011\u9762\u524d\uff0c\u842c\u6c11\u82e6\u60f1\uff0c\u4eba\u4eba\u81c9\u7d05\u8033\u8d64\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4ed6\u5011\u5954\u8dd1\u50cf\u52c7\u58eb\uff0c\u53c8\u50cf\u6230\u58eb\u722c\u4e0a\u57ce\u7246\uff1b\u4ed6\u5011\u5404\u6309\u81ea\u5df1\u7684\u8def\u524d\u884c\uff0c\u4e26\u4e0d\u504f\u96e2\u8def\u7dda\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4ed6\u5011\u5f7c\u6b64\u4e0d\u64c1\u64e0\uff0c\u5404\u6309\u81ea\u5df1\u7684\u9053\u524d\u884c\uff1b\u751a\u81f3\u4ed6\u5011\u6301\u8457\u5175\u5668\u8dcc\u5012\uff0c\u4e5f\u4e0d\u6703\u88ab\u526a\u9664\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4ed6\u5011\u885d\u5165\u57ce\u93ae\uff0c\u5954\u4e0a\u57ce\u7246\uff0c\u722c\u9032\u623f\u5c4b\uff0c\u50cf\u76dc\u8cca\u4e00\u6a23\u5f9e\u7a97\u6236\u722c\u9032\u53bb\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5728\u4ed6\u5011\u9762\u524d\uff0c\u5927\u5730\u9707\u52d5\uff0c\u8af8\u5929\u6416\u64bc\uff0c\u65e5\u6708\u660f\u6697\uff0c\u661f\u661f\u7121\u5149\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8036\u548c\u83ef\u5728\u4ed6\u7684\u8ecd\u968a\u524d\u982d\u767c\u51fa\u8072\u97f3\uff1b\u4ed6\u7684\u968a\u4f0d\u6975\u5927\uff0c\u6210\u5c31\u4ed6\u8a71\u8a9e\u7684\uff0c\u662f\u5f37\u76db\u7684\u4eba\u3002\u8036\u548c\u83ef\u7684\u65e5\u5b50\u5049\u5927\uff0c\u975e\u5e38\u53ef\u754f\uff0c\u8ab0\u80fd\u5fcd\u53d7\u5f97\u4f4f\u5462\uff1f\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u547c\u7c72\u4eba\u6c11\u6094\u6539\u6b78\u5411\u3000\u795e\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u73fe\u5728\u96d6\u7136\u5982\u6b64\uff0c\u4f60\u5011\u4ecd\u8981\u5168\u5fc3\u5168\u610f\uff0c\u4ee5\u7981\u98df\u3001\u54ed\u6ce3\u3001\u54c0\u865f\uff0c\u6b78\u5411\u6211\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u5011\u8981\u6495\u88c2\u4f60\u5011\u7684\u5fc3\u8178\uff0c\u4e0d\u8981\u6495\u88c2\u4f60\u5011\u7684\u8863\u670d\u3002\u4e26\u8981\u6b78\u5411\u8036\u548c\u83ef\u4f60\u5011\u7684\u3000\u795e\uff0c\u56e0\u70ba\u4ed6\u6709\u6069\u5178\u6709\u6190\u61ab\uff0c\u4e0d\u8f15\u6613\u767c\u6012\uff0c\u4e26\u4e14\u6709\u8c50\u76db\u7684\u6148\u611b\uff0c\u96a8\u6642\u8f49\u610f\u4e0d\u964d\u707d\u798d\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6709\u8ab0\u77e5\u9053\u6216\u8a31\u4ed6\u6703\u8f49\u610f\uff0c\u4e26\u4e14\u65bd\u6148\u611b\uff0c\u7559\u4e0b\u9918\u798f\uff0c\u5c31\u662f\u7559\u4e0b\u737b\u7d66\u8036\u548c\u83ef\u4f60\u5011\u7684\u3000\u795e\u7684\u7d20\u796d\u548c\u5960\u796d\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u5011\u8981\u5728\u932b\u5b89\u5439\u865f\u89d2\uff0c\u628a\u7981\u98df\u7684\u65e5\u5b50\u5206\u5225\u70ba\u8056\uff0c\u53ec\u958b\u56b4\u8085\u6703\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f60\u5011\u8981\u53ec\u96c6\u6c11\u773e\uff0c\u4f7f\u6703\u773e\u5206\u5225\u70ba\u8056\uff1b\u62db\u805a\u8001\u4eba\uff0c\u805a\u96c6\u5b69\u7ae5\uff0c\u548c\u5728\u61f7\u4e2d\u5403\u5976\u7684\u5b30\u5b69\uff1b\u8b93\u65b0\u90ce\u5f9e\u65b0\u623f\u51fa\u4f86\uff0c\u65b0\u5a66\u5f9e\u5167\u5ba4\u51fa\u4f86\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8b93\u4e8b\u5949\u8036\u548c\u83ef\u7684\u796d\u53f8\u5728\u8056\u6bbf\u7684\u5eca\u5b50\u548c\u796d\u58c7\u4e4b\u9593\u54ed\u6ce3\uff0c\u8b93\u4ed6\u5011\u8aaa\uff1a\u201c\u8036\u548c\u83ef\u554a\uff01\u6c42\u4f60\u6190\u6064\u4f60\u7684\u5b50\u6c11\u3002\u4e0d\u8981\u4f7f\u4f60\u7684\u7522\u696d\u53d7\u7f9e\u8fb1\uff0c\u4f7f\u4ed6\u5011\u5728\u5217\u570b\u4e2d\u6210\u70ba\u7b11\u8ac7\u3002\u70ba\u751a\u9ebc\u5bb9\u4eba\u5728\u842c\u6c11\u4e2d\u8b4f\u7b11\u8aaa\uff1a\u2018\u4ed6\u5011\u7684\u3000\u795e\u5728\u54ea\u88e1\u5462\uff1f\u2019\u201d\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u518d\u8cdc\u6069\u798f\u65bc\u662f\u8036\u548c\u83ef\u70ba\u4e86\u81ea\u5df1\u7684\u5730\u767c\u71b1\u5fc3\uff0c\u6190\u6064\u4ed6\u7684\u5b50\u6c11\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u8036\u548c\u83ef\u61c9\u5141\u4ed6\u7684\u5b50\u6c11\uff0c\u5c0d\u4ed6\u5011\u8aaa\uff1a\u201c\u770b\u54ea\uff01\u6211\u5fc5\u8cdc\u7d66\u4f60\u5011\u4e94\u7a40\u3001\u65b0\u9152\u548c\u65b0\u6cb9\uff0c\u4f60\u5011\u5fc5\u5403\u5f97\u98fd\u8db3\uff0c\u6211\u5fc5\u4e0d\u518d\u4f7f\u4f60\u5011\u6210\u70ba\u5217\u570b\u4e2d\u7684\u7f9e\u8fb1\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u6211\u8981\u4f7f\u5317\u65b9\u4f86\u7684\u8ecd\u968a\u9060\u96e2\u4f60\u5011\uff1b\u6211\u8981\u628a\u4ed6\u5011\u9a45\u9010\u5230\u4e7e\u65f1\u8352\u6dbc\u4e4b\u5730\uff0c\u4ed6\u5011\u7684\u524d\u968a\u8d95\u5165\u6771\u6d77\uff0c\u4ed6\u5011\u7684\u5f8c\u968a\u8d95\u5165\u897f\u6d77\uff1b\u4e26\u4e14\u4ed6\u5011\u7684\u81ed\u6c23\u4e0a\u5347\uff0c\u4ed6\u5011\u7684\u8165\u5473\u4e0a\u9a30\uff0c\u56e0\u70ba\u8036\u548c\u83ef\u884c\u4e86\u5927\u4e8b\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u5730\u571f\u554a\uff01\u4f60\u4e0d\u8981\u61fc\u6015\uff1b\u4f60\u8981\u6b61\u559c\u5feb\u6a02\uff0c\u56e0\u70ba\u8036\u548c\u83ef\u884c\u4e86\u5927\u4e8b\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u7530\u91ce\u7684\u8d70\u7378\u554a\uff0c\u4f60\u5011\u4e0d\u8981\u61fc\u6015\uff0c\u56e0\u70ba\u66e0\u91ce\u7684\u8349\u5834\u53c8\u767c\u9752\u4e86\uff0c\u6a39\u6728\u53c8\u7d50\u679c\u5b50\u4e86\uff0c\u7121\u82b1\u679c\u6a39\u548c\u8461\u8404\u6a39\u4e5f\u90fd\u6548\u529b\u4e86\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u932b\u5b89\u7684\u4eba\u6c11\u54ea\uff01\u4f60\u5011\u8981\u6b61\u559c\uff0c\u8981\u9760\u8036\u548c\u83ef\u4f60\u5011\u7684\u3000\u795e\u5feb\u6a02\uff0c\u56e0\u70ba\u4ed6\u8cdc\u7d66\u4e86\u4f60\u5011\u5408\u6642\u7684\u79cb\u96e8\u3002\u4ed6\u7d66\u4f60\u5011\u964d\u4e0b\u6642\u96e8\uff0c\u5c31\u662f\u79cb\u96e8\u6625\u96e8\uff0c\u50cf\u4ee5\u524d\u4e00\u6a23\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u79be\u5834\u4e0a\u5fc5\u6eff\u4e86\u4e94\u7a40\uff1b\u69a8\u9152\u6c60\u8207\u6cb9\u91a1\u5fc5\u6ea2\u51fa\u65b0\u9152\u8207\u65b0\u6cb9\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u6211\u5dee\u9063\u5230\u4f60\u5011\u4e2d\u9593\u53bb\u7684\uff0c\u6211\u7684\u5927\u8ecd\u968a\uff0c\u5c31\u662f\u8757\u87f2\u3001\u877b\u5b50\u3001\u879e\u86b1\u548c\u526a\u87f2\uff0c\u5728\u90a3\u4e9b\u5e74\u9593\u5403\u76e1\u7684\uff0c\u6211\u5fc5\u88dc\u7d66\u4f60\u5011\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u4f60\u5011\u5fc5\u5403\u5f97\u8c50\u5bcc\uff0c\u4e26\u4e14\u98fd\u8db3\uff0c\u4f60\u5011\u5c31\u8b9a\u7f8e\u8036\u548c\u83ef\u4f60\u5011\u7684\u3000\u795e\u7684\u540d\uff0c\u56e0\u70ba\u4ed6\u70ba\u4f60\u5011\u884c\u4e86\u5947\u5999\u7684\u4e8b\u3002\u6211\u7684\u5b50\u6c11\u5fc5\u6c38\u9060\u4e0d\u6703\u7f9e\u6127\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u9019\u6a23\uff0c\u4f60\u5011\u5fc5\u77e5\u9053\u6211\u662f\u5728\u4ee5\u8272\u5217\u4e2d\u9593\uff0c\u53c8\u77e5\u9053\u6211\u662f\u8036\u548c\u83ef\u4f60\u5011\u7684\u3000\u795e\uff0c\u9664\u4e86\u6211\u518d\u6c92\u6709\u5225\u7684\u795e\u3002\u6211\u7684\u5b50\u6c11\u5fc5\u6c38\u9060\u4e0d\u6703\u7f9e\u6127\u3002\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\u61c9\u8a31\u8cdc\u4e0b\u8056\u9748\u201c\u4ee5\u5f8c\uff0c\u6211\u8981\u628a\u6211\u7684\u9748\u6f86\u704c\u6240\u6709\u7684\u4eba\u3002\u4f60\u5011\u7684\u5152\u5973\u8981\u8aaa\u9810\u8a00\uff0c\u4f60\u5011\u7684\u8001\u5e74\u4eba\u8981\u4f5c\u7570\u5922\uff0c\u4f60\u5011\u7684\u5c11\u5e74\u4eba\u8981\u898b\u7570\u8c61\u3002\n"
                },
                "29": {
                    "verse_nr": 29,
                    "verse": "\u5728\u90a3\u4e9b\u65e5\u5b50\uff0c\u6211\u4e5f\u8981\u628a\u6211\u7684\u9748\u6f86\u704c\u6211\u7684\u50d5\u4eba\u548c\u5a62\u5973\u3002\n"
                },
                "30": {
                    "verse_nr": 30,
                    "verse": "\u6211\u8981\u5728\u5929\u4e0a\u5730\u4e0b\u986f\u51fa\u795e\u8e5f\u5947\u4e8b\uff0c\u6709\u8840\u3001\u6709\u706b\u3001\u6709\u7159\u67f1\u3002\n"
                },
                "31": {
                    "verse_nr": 31,
                    "verse": "\u592a\u967d\u5c07\u8b8a\u70ba\u9ed1\u6697\uff0c\u6708\u4eae\u5c07\u8b8a\u70ba\u8840\u7d05\u3002\u5728\u8036\u548c\u83ef\u5049\u5927\u53ef\u754f\u7684\u65e5\u5b50\u81e8\u5230\u4ee5\u524d\uff0c\u9019\u4e00\u5207\u90fd\u8981\u767c\u751f\u3002\n"
                },
                "32": {
                    "verse_nr": 32,
                    "verse": "\u90a3\u6642\uff0c\u6c42\u544a\u8036\u548c\u83ef\u7684\u540d\u7684\uff0c\u90fd\u5fc5\u5f97\u6551\uff1b\u56e0\u70ba\u6b63\u5982\u8036\u548c\u83ef\u6240\u8aaa\u7684\uff0c\u5728\u932b\u5b89\u5c71\u548c\u8036\u8def\u6492\u51b7\u5fc5\u6709\u5f97\u6551\u7684\u4eba\uff0c\u5728\u52ab\u5f8c\u9918\u751f\u7684\u4eba\u4e2d\uff0c\u5fc5\u6709\u8036\u548c\u83ef\u6240\u547c\u53ec\u7684\u3002\u201d\uff08\u672c\u7ae0\u7b2c28~32\u7bc0\u5728\u300a\u99ac\u7d22\u62c9\u6284\u672c\u300b\u70ba3:1~5\uff09\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u5be9\u5224\u842c\u570b\u201c\u770b\u54ea\uff01\u5230\u4e86\u90a3\u4e9b\u65e5\u5b50\uff0c\u5728\u90a3\u500b\u6642\u671f\uff0c\u6211\u4f7f\u7336\u5927\u548c\u8036\u8def\u6492\u51b7\u88ab\u64c4\u7684\u4eba\u6b78\u56de\uff0c\uff08\u672c\u7bc0\u5728\u300a\u99ac\u7d22\u62c9\u6284\u672c\u300b\u70ba4:1\uff09\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6211\u5fc5\u62db\u805a\u5217\u570b\uff0c\u9818\u4ed6\u5011\u4e0b\u5230\u7d04\u6c99\u6cd5\u8c37\uff0c\u70ba\u4e86\u6211\u7684\u5b50\u6c11\uff0c\u5c31\u662f\u6211\u7684\u7522\u696d\u4ee5\u8272\u5217\uff0c\u6211\u8981\u5728\u90a3\u88e1\u89aa\u81e8\u5be9\u5224\u4ed6\u5011\uff0c\u56e0\u70ba\u4ed6\u5011\u628a\u6211\u7684\u5b50\u6c11\u5206\u6563\u5728\u5217\u570b\u4e2d\uff0c\u53c8\u5206\u53d6\u4e86\u6211\u7684\u5730\u571f\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4ed6\u5011\u70ba\u6211\u7684\u5b50\u6c11\u62bd\u7c64\uff0c\u7528\u7537\u7ae5\u4ea4\u63db\u5993\u5973\uff0c\u5973\u7ae5\u88ab\u8ce3\u63db\u9152\u559d\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u201c\u63a8\u7f85\u3001\u897f\u9813\u548c\u975e\u5229\u58eb\u56db\u5883\u7684\u4eba\u54ea\uff01\u4f60\u5011\u60f3\u5c0d\u6211\u4f5c\u751a\u9ebc\u5462\uff1f\u4f60\u5011\u60f3\u5411\u6211\u5831\u5fa9\u55ce\uff1f\u4f60\u5011\u82e5\u5411\u6211\u5831\u5fa9\uff0c\u6211\u5f88\u5feb\u5c31\u4f7f\u5831\u61c9\u6b78\u5728\u4f60\u5011\u7684\u982d\u4e0a\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u5011\u62ff\u53bb\u4e86\u6211\u7684\u91d1\u9280\uff0c\u53c8\u628a\u6211\u7684\u73cd\u5bf6\u5e36\u9032\u4e86\u4f60\u5011\u7684\u5edf\u5b87\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u5011\u628a\u7336\u5927\u4eba\u548c\u8036\u8def\u6492\u51b7\u4eba\u8ce3\u7d66\u5e0c\u81d8\u4eba\uff0c\u4f7f\u4ed6\u5011\u9060\u96e2\u81ea\u5df1\u7684\u5883\u754c\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u770b\u54ea\uff01\u6211\u8981\u6fc0\u52d5\u4ed6\u5011\u96e2\u958b\u88ab\u4f60\u5011\u8ce3\u5230\u7684\u5730\u65b9\uff0c\u6211\u4e5f\u8981\u4f7f\u5831\u61c9\u6b78\u5230\u4f60\u5011\u7684\u982d\u4e0a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u5fc5\u628a\u4f60\u5011\u7684\u5152\u5973\u8ce3\u5728\u7336\u5927\u4eba\u7684\u624b\u4e2d\uff0c\u7336\u5927\u4eba\u5fc5\u628a\u4ed6\u5011\u8ce3\u7d66\u9060\u65b9\u793a\u5df4\u570b\u7684\u4eba\uff0c\u56e0\u70ba\u9019\u662f\u8036\u548c\u83ef\u8aaa\u7684\u3002\u201d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u5011\u8981\u5728\u5217\u570b\u4e2d\u5ba3\u544a\u4ee5\u4e0b\u9019\u8a71\uff1a\u201c\u4f60\u5011\u8981\u9810\u5099\u722d\u6230\uff0c\u8981\u6fc0\u52d5\u52c7\u58eb\u3002\u8b93\u6240\u6709\u7684\u6230\u58eb\u90fd\u8fd1\u524d\u4f86\uff0c\u8b93\u4ed6\u5011\u90fd\u4e0a\u53bb\u653b\u64ca\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u8981\u628a\u7281\u982d\u6253\u6210\u5200\u528d\uff0c\u628a\u942e\u5200\u6253\u6210\u77db\u69cd\uff1b\u8edf\u5f31\u7684\u8981\u8aaa\uff1a\u2018\u6211\u662f\u52c7\u58eb\u3002\u2019\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56db\u570d\u7684\u5217\u570b\u554a\uff01\u4f60\u5011\u5feb\u4f86\u5e6b\u52a9\uff0c\u5728\u90a3\u88e1\u4e00\u540c\u805a\u96c6\u3002\u8036\u548c\u83ef\u554a\uff01\u6c42\u4f60\u53eb\u4f60\u7684\u52c7\u58eb\u4e0b\u4f86\u5427\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5728\u7d04\u6c99\u6cd5\u8c37\u5be9\u5224\u842c\u6c11\u201c\u842c\u570b\u90fd\u7576\u596e\u8d77\uff0c\u4e0a\u5230\u7d04\u6c99\u6cd5\u8c37\u53bb\u3002\u56e0\u70ba\u6211\u5fc5\u5750\u5728\u90a3\u88e1\uff0c\u5be9\u5224\u56db\u570d\u7684\u5217\u570b\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u5011\u8981\u4f38\u51fa\u942e\u5200\uff0c\u56e0\u70ba\u838a\u7a3c\u5df2\u7d93\u719f\u4e86\u3002\u4f86\u8e10\u8e0f\u5427\uff0c\u56e0\u70ba\u58d3\u9152\u6c60\u6eff\u4e86\uff0c\u9152\u6c60\u76c8\u6ea2\uff0c\u56e0\u70ba\u4ed6\u5011\u7684\u7f6a\u60e1\u6975\u5927\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u7121\u6578\u7684\u4eba\u7fa4\u9f4a\u96c6\u5728\u5224\u6c7a\u8c37\uff0c\u56e0\u70ba\u8036\u548c\u83ef\u7684\u65e5\u5b50\u81e8\u8fd1\u4e86\u5224\u6c7a\u8c37\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u65e5\u6708\u660f\u6697\uff0c\u661f\u661f\u7121\u5149\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u8036\u548c\u83ef\u5f9e\u932b\u5b89\u543c\u53eb\uff0c\u4e26\u5f9e\u8036\u8def\u6492\u51b7\u767c\u8072\uff0c\u5929\u5730\u5c31\u9707\u52d5\u3002\u8036\u548c\u83ef\u537b\u8981\u4f5c\u4ed6\u5b50\u6c11\u7684\u907f\u96e3\u6240\uff0c\u4f5c\u4ee5\u8272\u5217\u773e\u5b50\u7684\u4fdd\u969c\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60\u5011\u77e5\u9053\u6211\u662f\u8036\u548c\u83ef\u4f60\u5011\u7684\u3000\u795e\uff0c\u662f\u4f4f\u5728\u932b\u5b89\u6211\u7684\u8056\u5c71\u4e0a\u3002\u90a3\u6642\uff0c\u8036\u8def\u6492\u51b7\u5fc5\u6210\u70ba\u8056\u3002\u5916\u65cf\u4eba\u5fc5\u4e0d\u5f97\u5f9e\u5176\u4e2d\u7d93\u904e\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5230\u90a3\u65e5\uff0c\u5927\u5c71\u90fd\u5fc5\u6ef4\u4e0b\u751c\u9152\uff0c\u5c0f\u5c71\u90fd\u5fc5\u6d41\u51fa\u5976\uff0c\u7336\u5927\u7684\u4e00\u5207\u6eaa\u6d41\uff0c\u90fd\u5fc5\u6c34\u6d41\u4e0d\u606f\u3002\u5fc5\u6709\u6cc9\u6e90\u5f9e\u8036\u548c\u83ef\u7684\u6bbf\u88e1\u6d41\u51fa\u4f86\uff0c\u704c\u6e89\u6cb3\u8c37\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u57c3\u53ca\u5c07\u6703\u8352\u6dbc\uff0c\u4ee5\u6771\u5fc5\u8b8a\u6210\u6dd2\u6dbc\u7684\u66e0\u91ce\uff0c\u90fd\u56e0\u70ba\u4ed6\u5011\u5411\u7336\u5927\u4eba\u6240\u884c\u7684\u5f37\u66b4\uff0c\u53c8\u56e0\u70ba\u5728\u4ed6\u5011\u7684\u570b\u4e2d\u6d41\u4e86\u7121\u8f9c\u4eba\u8840\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u7336\u5927\u5fc5\u6c38\u5b58\u201c\u4f46\u7336\u5927\u5fc5\u5b58\u5230\u6c38\u9060\uff0c\u8036\u8def\u6492\u51b7\u4e5f\u5fc5\u5b58\u5230\u842c\u4ee3\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u73fe\u5728\u6211\u8981\u8ffd\u8a0e\u6211\u9084\u672a\u6709\u8ffd\u8a0e\u7684\u6d41\u4eba\u8840\u7684\u7f6a\u3002\u8036\u548c\u83ef\u5728\u932b\u5b89\u5c45\u4f4f\u3002\u201d\n"
                }
            }
        }
    }
}