{
    "version": "cnt",
    "book_name": "2 Thessalonians",
    "book_nr": "53",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u4fdd\u7f85\u3001\u897f\u62c9\u548c\u63d0\u6469\u592a\uff0c\u5beb\u4fe1\u7d66\u5e16\u6492\u7f85\u5c3c\u8fe6\u3001\u5728\u6211\u5011\u7684\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u88e1\u7684\u6559\u6703\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u9858\u6069\u60e0\u5e73\u5b89\u5f9e\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u81e8\u5230\u4f60\u5011\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e3b\u518d\u4f86\u6642\u5fc5\u65bd\u884c\u5be9\u5224\u5f1f\u5144\u5011\uff0c\u6211\u5011\u61c9\u8a72\u5e38\u5e38\u70ba\u4f60\u5011\u611f\u8b1d\u3000\u795e\uff0c\u9019\u662f\u5408\u9069\u7684\u3002\u56e0\u70ba\u4f60\u5011\u7684\u4fe1\u5fc3\u683c\u5916\u9577\u9032\uff0c\u4f60\u5011\u773e\u4eba\u5f7c\u6b64\u76f8\u611b\u7684\u5fc3\u4e5f\u5728\u589e\u52a0\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6240\u4ee5\u6211\u5011\u5728\u3000\u795e\u7684\u773e\u6559\u6703\u88e1\uff0c\u89aa\u81ea\u8a87\u734e\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u5728\u6240\u53d7\u7684\u4e00\u5207\u8feb\u5bb3\u60a3\u96e3\u4e2d\uff0c\u4ecd\u7136\u5b58\u8457\u5805\u5fcd\u548c\u4fe1\u5fc3\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9019\u6b63\u662f\u3000\u795e\u516c\u7fa9\u5224\u65b7\u7684\u660e\u8b49\uff0c\u4f7f\u4f60\u5011\u53ef\u4ee5\u7b97\u662f\u914d\u5f97\u4e0a\u4ed6\u7684\u570b\uff1b\u4f60\u5011\u4e5f\u662f\u70ba\u4e86\u3000\u795e\u7684\u570b\u800c\u53d7\u82e6\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u3000\u795e\u65e2\u7136\u662f\u516c\u7fa9\u7684\uff0c\u4e3b\u8036\u7a4c\u548c\u4ed6\u6709\u80fd\u529b\u7684\u5929\u4f7f\u5f9e\u5929\u4e0a\u986f\u73fe\u5728\u706b\u71c4\u4e2d\u7684\u6642\u5019\uff0c\u5c31\u4f7f\u4f60\u5011\u9019\u4e9b\u53d7\u707d\u96e3\u7684\u4eba\uff0c\u53ef\u4ee5\u548c\u6211\u5011\u540c\u4eab\u5b89\u606f\uff1b\u537b\u4ee5\u707d\u96e3\u5831\u61c9\u90a3\u4e9b\u628a\u707d\u96e3\u52a0\u7d66\u4f60\u5011\u7684\u4eba\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": " \n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u53c8\u8981\u5831\u61c9\u90a3\u4e9b\u4e0d\u8a8d\u8b58\u3000\u795e\u3001\u4e0d\u807d\u5f9e\u6211\u5011\u4e3b\u8036\u7a4c\u7684\u798f\u97f3\u7684\u4eba\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7576\u4e3b\u4f86\u7684\u6642\u5019\uff0c\u4ed6\u5011\u8981\u53d7\u6c38\u9060\u6c89\u6dea\u7684\u61f2\u7f70\uff0c\u5c31\u662f\u96e2\u958b\u4e3b\u7684\u9762\u548c\u4ed6\u6b0a\u80fd\u7684\u69ae\u5149\u3002\u5728\u90a3\u4e00\u5929\uff0c\u4ed6\u8981\u5728\u8056\u5f92\u8eab\u4e0a\u5f97\u8457\u69ae\u8000\uff0c\u53c8\u8981\u5728\u6240\u6709\u4fe1\u5f92\u8eab\u4e0a\u53d7\u5230\u5c0a\u5d07\uff08\u4f60\u5011\u4e5f\u5728\u4ed6\u5011\u7576\u4e2d\uff0c\u56e0\u70ba\u4f60\u5011\u4fe1\u4e86\u6211\u5011\u5411\u4f60\u5011\u6240\u4f5c\u7684\u898b\u8b49\uff09\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": " \n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56e0\u6b64\uff0c\u6211\u5011\u5e38\u5e38\u70ba\u4f60\u5011\u79b1\u544a\uff0c\u9858\u6211\u5011\u7684\u3000\u795e\u770b\u4f60\u5011\u662f\u914d\u5f97\u4e0a\u6240\u8499\u7684\u53ec\uff0c\u53c8\u7528\u5927\u80fd\u6210\u5c31\u4f60\u5011\u6240\u7fa8\u6155\u7684\u4e00\u5207\u826f\u5584\u548c\u4fe1\u5fc3\u7684\u5de5\u4f5c\uff0c\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4f7f\u6211\u5011\u4e3b\u8036\u7a4c\u7684\u540d\uff0c\u7167\u8457\u6211\u5011\u7684\u3000\u795e\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u6069\uff0c\u5728\u4f60\u5011\u8eab\u4e0a\u5f97\u8457\u69ae\u8000\uff0c\u4f60\u5011\u4e5f\u5728\u4ed6\u8eab\u4e0a\u5f97\u8457\u69ae\u8000\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4e0d\u6cd5\u7684\u4eba\u5fc5\u8981\u986f\u9732\u5f1f\u5144\u5011\uff0c\u95dc\u65bc\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u518d\u4f86\uff0c\u548c\u6211\u5011\u5230\u4ed6\u90a3\u88e1\u805a\u96c6\u7684\u4e8b\uff0c\u6211\u5011\u6c42\u4f60\u5011\uff1a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u7121\u8ad6\u6709\u9748\u3001\u6709\u8a71\u3001\u6709\u5192\u6211\u5011\u7684\u540d\u7684\u66f8\u4fe1\uff0c\u8aaa\u4e3b\u7684\u65e5\u5b50\u73fe\u5728\u5230\u4e86\uff0c\u4f60\u5011\u90fd\u4e0d\u8981\u8f15\u6613\u52d5\u5fc3\uff0c\u4e5f\u4e0d\u8981\u9a5a\u614c\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e0d\u8981\u8b93\u4eba\u7528\u4efb\u4f55\u65b9\u6cd5\u8ff7\u60d1\u4e86\u4f60\u5011\uff0c\u56e0\u70ba\u4e3b\u7684\u65e5\u5b50\u4f86\u5230\u4ee5\u524d\uff0c\u5fc5\u5b9a\u6709\u80cc\u9053\u7684\u4e8b\uff0c\u4e26\u4e14\u90a3\u4e0d\u6cd5\u7684\u4eba\uff0c\u5c31\u662f\u90a3\u6c89\u6dea\u4e4b\u5b50\uff0c\u5fc5\u5b9a\u986f\u9732\u51fa\u4f86\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u62b5\u64cb\u3000\u795e\uff0c\u62ac\u8209\u81ea\u5df1\uff0c\u9ad8\u904e\u4e00\u5207\u7a31\u70ba\u795e\u548c\u53d7\u4eba\u656c\u62dc\u7684\uff0c\u751a\u81f3\u5750\u5728\u3000\u795e\u7684\u6bbf\u4e2d\uff0c\u81ea\u7a31\u70ba\u3000\u795e\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u6211\u9084\u5728\u4f60\u5011\u90a3\u88e1\u7684\u6642\u5019\uff0c\u66fe\u7d93\u628a\u9019\u4e9b\u4e8b\u544a\u8a34\u4f60\u5011\uff0c\u4f60\u5011\u4e0d\u8a18\u5f97\u55ce\uff1f\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u73fe\u5728\u4f60\u5011\u4e5f\u77e5\u9053\uff0c\u90a3\u7b9d\u5236\u4ed6\uff0c\u4f7f\u4ed6\u5230\u4e86\u6642\u5019\u624d\u53ef\u4ee5\u986f\u9732\u51fa\u4f86\u7684\u662f\u751a\u9ebc\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u70ba\u90a3\u4e0d\u6cd5\u7684\u6f5b\u529b\u5df2\u7d93\u767c\u52d5\uff0c\u53ea\u662f\u73fe\u5728\u6709\u4e00\u500b\u7b9d\u5236\u4ed6\u7684\u5728\u90a3\u88e1\uff0c\u76f4\u7b49\u5230\u90a3\u7b9d\u5236\u89e3\u9664\u4e86\uff0c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u90a3\u6642\uff0c\u9019\u4e0d\u6cd5\u7684\u4eba\u5fc5\u8981\u986f\u9732\u51fa\u4f86\u3002\u4e3b\u8036\u7a4c\u8981\u7528\u81ea\u5df1\u53e3\u4e2d\u7684\u6c23\u9664\u6389\u4ed6\uff0c\u4ee5\u81ea\u5df1\u518d\u4f86\u6240\u986f\u73fe\u7684\u5149\u8f1d\u6d88\u6ec5\u4ed6\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u9019\u4e0d\u6cd5\u7684\u4eba\u4f86\u5230\uff0c\u662f\u7167\u8457\u6492\u4f46\u7684\u884c\u52d5\uff0c\u884c\u5404\u6a23\u7684\u7570\u80fd\u5947\u8e5f\u548c\u8352\u8a95\u7684\u4e8b\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4e26\u4e14\u5728\u90a3\u4e9b\u6c89\u6dea\u7684\u4eba\u8eab\u4e0a\uff0c\u884c\u5404\u6a23\u4e0d\u7fa9\u7684\u6b3a\u8a50\uff0c\u56e0\u70ba\u4ed6\u5011\u4e0d\u9818\u53d7\u611b\u771f\u7406\u7684\u5fc3\uff0c\u4f7f\u4ed6\u5011\u5f97\u6551\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56e0\u6b64\uff0c\u3000\u795e\u5c31\u4f7f\u932f\u8b2c\u7684\u601d\u60f3\u904b\u884c\u5728\u4ed6\u5011\u7576\u4e2d\uff0c\u8b93\u4ed6\u5011\u76f8\u4fe1\u865b\u8b0a\uff0c\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u53eb\u6240\u6709\u4e0d\u4fe1\u771f\u7406\u5012\u559c\u611b\u4e0d\u7fa9\u7684\u4eba\uff0c\u90fd\u88ab\u5b9a\u7f6a\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u7ad9\u7acb\u5f97\u7a69\u6301\u5b88\u4fe1\u4ef0\u4e3b\u6240\u611b\u7684\u5f1f\u5144\u5011\uff0c\u6211\u5011\u61c9\u8a72\u5e38\u5e38\u70ba\u4f60\u5011\u611f\u8b1d\u3000\u795e\uff0c\u56e0\u70ba\u4ed6\u5f9e\u8d77\u521d\u5c31\u63c0\u9078\u4e86\u4f60\u5011\uff0c\u85c9\u8457\u8056\u9748\u6210\u8056\u7684\u5de5\u4f5c\uff0c\u548c\u4f60\u5011\u5c0d\u771f\u9053\u7684\u4fe1\u5fc3\uff0c\u4f7f\u4f60\u5011\u53ef\u4ee5\u5f97\u6551\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u56e0\u6b64\uff0c\u3000\u795e\u85c9\u8457\u6211\u5011\u6240\u50b3\u7684\u798f\u97f3\u547c\u53ec\u4f60\u5011\uff0c\u4f7f\u4f60\u5011\u5f97\u8457\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u69ae\u8000\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u6240\u4ee5\u5f1f\u5144\u5011\uff0c\u4f60\u5011\u8981\u7ad9\u7acb\u5f97\u7a69\uff1b\u4f60\u5011\u6240\u9818\u53d7\u7684\u6559\u8a13\uff0c\u7121\u8ad6\u662f\u6211\u5011\u53e3\u50b3\u7684\uff0c\u6216\u662f\u4fe1\u4e0a\u5beb\u7684\uff0c\u90fd\u8981\u6301\u5b88\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u9858\u6211\u5011\u7684\u4e3b\u8036\u7a4c\u57fa\u7763\u81ea\u5df1\uff0c\u548c\u90a3\u611b\u6211\u5011\u3001\u958b\u6069\u628a\u6c38\u9060\u7684\u5b89\u6170\u548c\u7f8e\u597d\u7684\u76fc\u671b\u8cdc\u7d66\u6211\u5011\u7684\u7236\u3000\u795e\uff0c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u5b89\u6170\u4f60\u5011\u7684\u5fc3\uff0c\u4e26\u4e14\u5728\u4e00\u5207\u5584\u884c\u5584\u8a00\u4e0a\uff0c\u5805\u5b9a\u4f60\u5011\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u7948\u6c42\u4e3b\u7684\u9053\u5feb\u5feb\u50b3\u958b\u6700\u5f8c\uff0c\u5f1f\u5144\u5011\uff0c\u8acb\u70ba\u6211\u5011\u79b1\u544a\uff0c\u597d\u53eb\u4e3b\u7684\u9053\u4e5f\u50cf\u5728\u4f60\u5011\u90a3\u88e1\u4e00\u6a23\u5feb\u5feb\u50b3\u958b\uff0c\u5f97\u8457\u69ae\u8000\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4e5f\u4f7f\u6211\u5011\u80fd\u5920\u812b\u96e2\u90a3\u4e9b\u4e0d\u8b1b\u7406\u7684\u60e1\u4eba\uff0c\u56e0\u70ba\u4e0d\u662f\u4eba\u4eba\u90fd\u6709\u4fe1\u5fc3\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e3b\u662f\u4fe1\u5be6\u7684\uff0c\u4ed6\u5fc5\u5805\u5b9a\u4f60\u5011\uff0c\u4fdd\u8b77\u4f60\u5011\u812b\u96e2\u90a3\u60e1\u8005\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u5011\u9760\u8457\u4e3b\u6df1\u6df1\u76f8\u4fe1\uff0c\u4f60\u5011\u73fe\u5728\u4ee5\u53ca\u5c07\u4f86\u90fd\u6703\u9075\u884c\u6211\u5011\u6240\u5429\u5490\u7684\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9858\u4e3b\u5f15\u5c0e\u4f60\u5011\u7684\u5fc3\uff0c\u4f7f\u4f60\u5011\u6709\u3000\u795e\u7684\u611b\u548c\u57fa\u7763\u7684\u5805\u5fcd\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4e0d\u53ef\u904a\u624b\u597d\u9592\u5f1f\u5144\u5011\uff0c\u6211\u5011\u5949\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u540d\u5429\u5490\u4f60\u5011\uff0c\u51e1\u662f\u904a\u624b\u597d\u9592\u7684\u5f1f\u5144\uff0c\u4e0d\u6309\u8457\u4f60\u5011\u5f9e\u6211\u5011\u6240\u9818\u53d7\u7684\u6559\u8a13\u53bb\u884c\uff0c\u5c31\u61c9\u7576\u9060\u96e2\u4ed6\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u5011\u81ea\u5df1\u672c\u4f86\u5c31\u77e5\u9053\u61c9\u7576\u600e\u6a23\u6548\u6cd5\u6211\u5011\uff0c\u56e0\u70ba\u6211\u5011\u5728\u4f60\u5011\u4e2d\u9593\u4e26\u6c92\u6709\u904a\u624b\u597d\u9592\uff0c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4e5f\u6c92\u6709\u767d\u5403\u904e\u8ab0\u7684\u98ef\uff0c\u53cd\u800c\u8f9b\u82e6\u52de\u788c\uff0c\u665d\u591c\u4f5c\u5de5\uff0c\u514d\u5f97\u52a0\u91cd\u4f60\u5011\u4efb\u4f55\u4e00\u4eba\u7684\u8ca0\u64d4\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u9019\u4e0d\u662f\u56e0\u70ba\u6211\u5011\u6c92\u6709\u6b0a\u5229\uff0c\u800c\u662f\u8981\u7d66\u4f60\u5011\u4f5c\u699c\u6a23\uff0c\u597d\u8b93\u4f60\u5011\u6548\u6cd5\u6211\u5011\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u5011\u5728\u4f60\u5011\u90a3\u88e1\u7684\u6642\u5019\uff0c\u66fe\u7d93\u5429\u5490\u904e\u4f60\u5011\uff0c\u5982\u679c\u6709\u4eba\u4e0d\u80af\u4f5c\u5de5\uff0c\u5c31\u4e0d\u53ef\u5403\u98ef\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
