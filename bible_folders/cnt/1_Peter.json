                    "verse": "\u5982\u679c\u4f60\u5011\u71b1\u5fc3\u884c\u5584\uff0c\u6709\u8ab0\u80fd\u5bb3\u4f60\u5011\u5462\uff1f\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5c31\u7b97\u4f60\u5011\u8981\u70ba\u7fa9\u53d7\u82e6\uff0c\u4e5f\u662f\u6709\u798f\u7684\u3002\u201c\u4e0d\u8981\u6015\u4eba\u7684\u6050\u5687\uff0c\u4e5f\u4e0d\u8981\u754f\u61fc\u3002\u201d\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u53ea\u8981\u5fc3\u88e1\u5c0a\u57fa\u7763\u70ba\u8056\uff0c\u4ee5\u4ed6\u70ba\u4e3b\uff1b\u5e38\u5e38\u4f5c\u597d\u6e96\u5099\uff0c\u53bb\u56de\u7b54\u90a3\u4e9b\u554f\u4f60\u5011\u70ba\u751a\u9ebc\u61f7\u6709\u76fc\u671b\u7684\u4eba\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f46\u8981\u7528\u6eab\u67d4\u656c\u754f\u7684\u5fc3\u56de\u7b54\u3002\u7576\u5b58\u7121\u6127\u7684\u826f\u5fc3\uff0c\u4f7f\u90a3\u4e9b\u8aa3\u8cf4\u4f60\u5011\u9019\u5728\u57fa\u7763\u88e1\u6709\u597d\u54c1\u884c\u7684\u4eba\uff0c\u5728\u6bc0\u8b17\u4f60\u5011\u7684\u4e8b\u4e0a\u8499\u7f9e\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u5982\u679c\u3000\u795e\u7684\u65e8\u610f\u662f\u8981\u4f60\u5011\u53d7\u82e6\uff0c\u90a3\u9ebc\u70ba\u884c\u5584\u53d7\u82e6\uff0c\u7e3d\u6bd4\u70ba\u884c\u60e1\u53d7\u82e6\u597d\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u56e0\u70ba\u57fa\u7763\u4e5f\u66fe\u4e00\u6b21\u70ba\u4f60\u5011\u7684\u7f6a\u6b7b\u4e86\uff0c\u5c31\u662f\u7fa9\u7684\u4ee3\u66ff\u4e0d\u7fa9\u7684\uff0c\u70ba\u8981\u9818\u4f60\u5011\u5230\u3000\u795e\u9762\u524d\u3002\u5c31\u8089\u9ad4\u7684\u65b9\u9762\u8aaa\uff0c\u4ed6\u66fe\u6b7b\u53bb\uff1b\u5c31\u9748\u7684\u65b9\u9762\u8aaa\uff0c\u4ed6\u5fa9\u6d3b\u4e86\uff1b\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u4ed6\u85c9\u9019\u9748\u4e5f\u66fe\u53bb\u5411\u90a3\u4e9b\u5728\u76e3\u7ba1\u4e2d\u7684\u9748\u5ba3\u8b1b\uff0c\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4ed6\u5011\u5c31\u662f\u632a\u4e9e\u5efa\u9020\u65b9\u821f\u7684\u65e5\u5b50\u3001\u3000\u795e\u5bb9\u5fcd\u7b49\u5f85\u7684\u6642\u5019\uff0c\u90a3\u4e9b\u4e0d\u9806\u5f9e\u7684\u4eba\u3002\u7576\u6642\u9032\u5165\u65b9\u821f\u3001\u85c9\u8457\u6c34\u5f97\u6551\u7684\u4eba\u4e0d\u591a\uff0c\u53ea\u6709\u516b\u500b\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u9019\u6c34\u9810\u8868\u7684\u6d17\u79ae\uff0c\u73fe\u5728\u4e5f\u62ef\u6551\u4f60\u5011\uff1a\u4e0d\u662f\u9664\u53bb\u8089\u9ad4\u7684\u6c61\u7a62\uff0c\u800c\u662f\u85c9\u8457\u8036\u7a4c\u57fa\u7763\u7684\u5fa9\u6d3b\uff0c\u5411\u3000\u795e\u8a31\u9858\u5e38\u5b58\u7d14\u6f54\u7684\u826f\u5fc3\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u57fa\u7763\u5df2\u9032\u5230\u5929\u4e0a\uff0c\u5728\u3000\u795e\u53f3\u908a\uff0c\u773e\u5929\u4f7f\u3001\u6709\u6b0a\u52e2\u7684\u3001\u6709\u80fd\u529b\u7684\uff0c\u90fd\u670d\u5f9e\u4e86\u4ed6\u3002\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8981\u6709\u53d7\u82e6\u7684\u5fc3\u5fd7\u57fa\u7763\u65e2\u7136\u5728\u8089\u8eab\u53d7\u904e\u82e6\uff0c\u4f60\u5011\u4e5f\u61c9\u7576\u4ee5\u540c\u6a23\u7684\u5fc3\u5fd7\u88dd\u5099\u81ea\u5df1\uff08\u56e0\u70ba\u5728\u8089\u8eab\u53d7\u904e\u82e6\u7684\uff0c\u5c31\u5df2\u7d93\u8207\u7f6a\u65b7\u7d55\u4e86\uff09\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u597d\u53eb\u4f60\u5011\u4e0d\u518d\u96a8\u5f9e\u4eba\u7684\u79c1\u617e\uff0c\u53ea\u9806\u5f9e\u3000\u795e\u7684\u65e8\u610f\uff0c\u5728\u4e16\u4e0a\u5ea6\u9918\u4e0b\u7684\u5149\u9670\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u56e0\u70ba\u4f60\u5011\u904e\u53bb\u96a8\u5f9e\u6559\u5916\u4eba\u7684\u5fc3\u610f\uff0c\u884c\u90aa\u6deb\u3001\u79c1\u617e\u3001\u9189\u9152\u3001\u8352\u5bb4\u3001\u72c2\u98f2\u548c\u53ef\u618e\u62dc\u5076\u50cf\u7684\u4e8b\uff0c\u6642\u5019\u5df2\u7d93\u5920\u4e86\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u5011\u898b\u4f60\u5011\u4e0d\u518d\u8207\u4ed6\u5011\u540c\u5954\u90a3\u7e31\u60c5\u653e\u8569\u7684\u8def\uff0c\u5c31\u89ba\u5f97\u5947\u602a\uff0c\u6bc0\u8b17\u4f60\u5011\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4ed6\u5011\u5fc5\u8981\u5411\u90a3\u4f4d\u9810\u5099\u8981\u5be9\u5224\u6d3b\u4eba\u6b7b\u4eba\u7684\u4e3b\u4ea4\u5e33\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u56e0\u6b64\uff0c\u90a3\u4e9b\u6b7b\u4eba\u4e5f\u66fe\u6709\u798f\u97f3\u50b3\u7d66\u4ed6\u5011\uff0c\u597d\u4f7f\u4ed6\u5011\u7684\u8089\u9ad4\u53d7\u4e86\u4eba\u8981\u53d7\u7684\u5be9\u5224\uff0c\u4ed6\u5011\u7684\u9748\u537b\u9760\u3000\u795e\u6d3b\u8457\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6309\u7167\u6069\u8cdc\u5f7c\u6b64\u670d\u4e8b\u842c\u7269\u7684\u7d50\u5c40\u8fd1\u4e86\uff0c\u6240\u4ee5\u4f60\u5011\u8981\u8b39\u614e\u8b66\u9192\u5730\u79b1\u544a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6700\u91cd\u8981\u7684\u662f\u8981\u5f7c\u6b64\u5207\u5be6\u76f8\u611b\uff0c\u56e0\u70ba\u611b\u80fd\u906e\u84cb\u8a31\u591a\u7684\u7f6a\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u5011\u8981\u4e92\u76f8\u63a5\u5f85\uff0c\u4e0d\u767c\u6028\u8a00\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f60\u5011\u8981\u4f5c\u3000\u795e\u5404\u6a23\u6069\u8cdc\u7684\u597d\u7ba1\u5bb6\uff0c\u5404\u4eba\u7167\u8457\u6240\u9818\u53d7\u7684\u6069\u8cdc\u5f7c\u6b64\u670d\u4e8b\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8b1b\u9053\u7684\uff0c\u61c9\u7576\u6309\u8457\u3000\u795e\u7684\u8056\u8a00\u8b1b\uff1b\u670d\u4e8b\u7684\uff0c\u61c9\u7576\u6309\u8457\u3000\u795e\u6240\u8cdc\u7684\u529b\u91cf\u670d\u4e8b\uff0c\u70ba\u8981\u5728\u4e00\u5207\u4e8b\u4e0a\uff0c\u4f7f\u3000\u795e\u85c9\u8036\u7a4c\u57fa\u7763\u5f97\u69ae\u8000\u3002\u69ae\u8000\u3001\u6b0a\u80fd\u90fd\u662f\u4ed6\u7684\uff0c\u76f4\u5230\u6c38\u6c38\u9060\u9060\u3002\u963f\u5011\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u70ba\u57fa\u7763\u53d7\u82e6\u7684\u6709\u798f\u4e86\u89aa\u611b\u7684\uff0c\u6709\u706b\u7149\u7684\u8a66\u9a57\u81e8\u5230\u4f60\u5011\uff0c\u4e0d\u8981\u4ee5\u70ba\u5947\u602a\uff0c\u597d\u50cf\u662f\u906d\u9047\u975e\u5e38\u7684\u4e8b\uff0c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u5012\u8981\u6b61\u559c\uff0c\u56e0\u70ba\u4f60\u5011\u65e2\u7136\u5728\u57fa\u7763\u7684\u53d7\u82e6\u4e0a\u6709\u5206\uff0c\u5c31\u5728\u4ed6\u69ae\u8000\u986f\u73fe\u7684\u6642\u5019\uff0c\u53ef\u4ee5\u6b61\u559c\u5feb\u6a02\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u5011\u8981\u662f\u70ba\u57fa\u7763\u7684\u540d\u53d7\u8fb1\u7f75\uff0c\u5c31\u6709\u798f\u4e86\uff01\u56e0\u70ba\u3000\u795e\u69ae\u8000\u7684\u9748\uff0c\u4f4f\u5728\u4f60\u5011\u8eab\u4e0a\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u5011\u4e2d\u9593\u4e0d\u53ef\u6709\u4eba\u56e0\u70ba\u6bba\u4eba\u3001\u6216\u5077\u7aca\u3001\u6216\u884c\u60e1\u3001\u6216\u597d\u7ba1\u9592\u4e8b\u800c\u53d7\u82e6\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u5982\u679c\u56e0\u70ba\u4f5c\u57fa\u7763\u5f92\u800c\u53d7\u82e6\uff0c\u4e0d\u8981\u4ee5\u70ba\u7f9e\u6065\uff0c\u5012\u8981\u85c9\u8457\u9019\u540d\u5b57\u69ae\u8000\u3000\u795e\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u56e0\u70ba\u5be9\u5224\u5f9e\u3000\u795e\u7684\u5bb6\u958b\u59cb\uff0c\u5c31\u5728\u9019\u6642\u5019\u4e86\u3002\u5982\u679c\u5148\u5f9e\u6211\u5011\u8d77\u982d\uff0c\u90a3\u4e0d\u4fe1\u5f9e\u3000\u795e\u798f\u97f3\u7684\u4eba\uff0c\u7d50\u5c40\u5c07\u8981\u600e\u6a23\u5462\uff1f\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u201c\u5982\u679c\u7fa9\u4eba\u50c5\u50c5\u5f97\u6551\uff0c\u4e0d\u656c\u8654\u548c\u72af\u7f6a\u7684\u4eba\uff0c\u53c8\u6703\u8b8a\u6210\u600e\u6a23\u5462\uff1f\u201d\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6240\u4ee5\u90a3\u9806\u8457\u3000\u795e\u7684\u65e8\u610f\u800c\u53d7\u82e6\u7684\u4eba\uff0c\u8981\u7e7c\u7e8c\u5730\u884c\u5584\uff0c\u628a\u81ea\u5df1\u7684\u751f\u547d\u4ea4\u8a17\u90a3\u4fe1\u5be6\u7684\u5275\u9020\u8005\u3002\n"
                }
            }
        },
        "5": {
            "chapter_nr": 5,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u52d9\u8981\u7267\u990a\u3000\u795e\u7684\u7f8a\u7fa4\u6211\u9019\u540c\u4f5c\u9577\u8001\uff0c\u4e5f\u662f\u70ba\u57fa\u7763\u53d7\u82e6\u4f5c\u898b\u8b49\uff0c\u53c8\u662f\u540c\u4eab\u5c07\u4f86\u6240\u8981\u986f\u73fe\u7684\u69ae\u8000\u7684\uff0c\u52f8\u4f60\u5011\u7576\u4e2d\u4f5c\u9577\u8001\u7684\uff1a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u52d9\u8981\u7267\u990a\u4f60\u5011\u4e2d\u9593\u7684\u7f8a\u7fa4\uff0c\u6309\u8457\u3000\u795e\u7684\u65e8\u610f\u7167\u9867\u4ed6\u5011\u3002\u4e0d\u662f\u51fa\u65bc\u52c9\u5f37\uff0c\u800c\u662f\u51fa\u65bc\u7518\u5fc3\uff1b\u4e0d\u662f\u56e0\u70ba\u8caa\u8ca1\uff0c\u800c\u662f\u51fa\u65bc\u71b1\u8aa0\uff1b\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e5f\u4e0d\u662f\u8981\u8f44\u5236\u8a17\u4ed8\u4f60\u5011\u7684\u7f8a\u7fa4\uff0c\u800c\u662f\u4f5c\u4ed6\u5011\u7684\u699c\u6a23\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5230\u4e86\u7267\u9577\u986f\u73fe\u7684\u6642\u5019\uff0c\u4f60\u5011\u5fc5\u5b9a\u5f97\u8457\u90a3\u6c38\u4e0d\u8870\u6b98\u7684\u69ae\u8000\u51a0\u5195\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7167\u6a23\uff0c\u4f60\u5011\u9752\u5e74\u4eba\u8981\u9806\u670d\u5e74\u9577\u7684\u3002\u5c31\u662f\u4f60\u5011\u5404\u4eba\u4e5f\u8981\u5f7c\u6b64\u4ee5\u8b19\u5351\u70ba\u88dd\u675f\uff0c\u56e0\u70ba\u201c\u3000\u795e\u6575\u64cb\u9a55\u50b2\u7684\u4eba\uff0c\u8cdc\u6069\u7d66\u8b19\u5351\u7684\u4eba\u3002\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6240\u4ee5\u4f60\u5011\u8981\u8b19\u5351\uff0c\u670d\u5728\u3000\u795e\u5927\u80fd\u7684\u624b\u4e0b\uff0c\u5230\u4e86\u6642\u5019\uff0c\u4ed6\u5fc5\u53eb\u4f60\u5011\u5347\u9ad8\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f60\u5011\u8981\u628a\u4e00\u5207\u6182\u616e\u5378\u7d66\u3000\u795e\uff0c\u56e0\u70ba\u4ed6\u9867\u5ff5\u4f60\u5011\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u5011\u8981\u8b39\u5b88\u3001\u8b66\u9192\u3002\u4f60\u5011\u7684\u4ec7\u6575\u9b54\u9b3c\uff0c\u597d\u50cf\u543c\u53eb\u7684\u7345\u5b50\u8d70\u4f86\u8d70\u53bb\uff0c\u5c0b\u627e\u53ef\u4ee5\u541e\u5403\u7684\u4eba\uff1b\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u5011\u8981\u7528\u5805\u5f37\u7684\u4fe1\u5fc3\u62b5\u64cb\u4ed6\uff0c\u56e0\u70ba\u77e5\u9053\u4f60\u5011\u5728\u4e16\u4e0a\u7684\u5f1f\u5144\uff0c\u4e5f\u7d93\u6b77\u904e\u540c\u6a23\u7684\u82e6\u96e3\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f46\u6eff\u6709\u6069\u5178\u7684\u3000\u795e\uff0c\u5c31\u662f\u5728\u57fa\u7763\u88e1\u53ec\u4f60\u5011\u9032\u5165\u4ed6\u6c38\u9060\u69ae\u8000\u7684\u90a3\u4e00\u4f4d\uff0c\u5728\u4f60\u5011\u53d7\u4e86\u77ed\u66ab\u7684\u82e6\u96e3\u4e4b\u5f8c\uff0c\u5fc5\u5b9a\u89aa\u81ea\u6210\u5168\u4f60\u5011\uff0c\u5805\u56fa\u4f60\u5011\uff0c\u8cdc\u529b\u91cf\u7d66\u4f60\u5011\uff0c\u5efa\u7acb\u4f60\u5011\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u9858\u6b0a\u80fd\u6b78\u7d66\u4ed6\uff0c\u76f4\u5230\u6c38\u9060\u3002\u963f\u5011\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u52f8\u52c9\u548c\u795d\u798f\u6211\u85c9\u8457\u6211\u8a8d\u70ba\u5fe0\u5fc3\u7684\u5f1f\u5144\u897f\u62c9\uff0c\u7c21\u7565\u5730\u5beb\u4e86\u9019\u5c01\u4fe1\u52f8\u52c9\u4f60\u5011\uff0c\u53c8\u5411\u4f60\u5011\u8b49\u5be6\u9019\u662f\u3000\u795e\u771f\u6b63\u7684\u6069\u5178\uff1b\u4f60\u5011\u8981\u5728\u9019\u6069\u5178\u4e0a\u7ad9\u7acb\u5f97\u4f4f\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u5728\u5df4\u6bd4\u502b\u8207\u4f60\u5011\u540c\u8499\u63c0\u9078\u7684\uff0c\u548c\u6211\u5152\u5b50\u99ac\u53ef\u90fd\u554f\u5019\u4f60\u5011\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u5011\u8981\u7528\u611b\u5fc3\u5f7c\u6b64\u89aa\u5634\u554f\u5b89\u3002\u9858\u5e73\u5b89\u6b78\u8207\u4f60\u5011\u6240\u6709\u5728\u57fa\u7763\u88e1\u7684\u4eba\u3002\n"
                }
            }
        }
    }
