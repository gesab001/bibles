        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8cac\u5099\u4eba\u6c11\u62d6\u5ef6\u91cd\u5efa\u8056\u6bbf\u5927\u5229\u70cf\u738b\u7b2c\u4e8c\u5e74\u516d\u6708\u4e00\u65e5\uff0c\u8036\u548c\u83ef\u7684\u8a71\u85c9\u8457\u54c8\u8a72\u5148\u77e5\u81e8\u5230\u6492\u62c9\u9435\u7684\u5152\u5b50\u3001\u7336\u5927\u7701\u9577\u6240\u7f85\u5df4\u4f2f\uff0c\u548c\u7d04\u6492\u7b54\u7684\u5152\u5b50\u5927\u796d\u53f8\u7d04\u66f8\u4e9e\uff0c\u8aaa\uff1a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u201c\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u9019\u6a23\u8aaa\uff1a\u2018\u9019\u6c11\u8aaa\uff0c\u91cd\u5efa\u8036\u548c\u83ef\u6bbf\u5b87\u7684\u6642\u5019\u9084\u6c92\u6709\u5230\u3002\u2019\u201d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u65bc\u662f\uff0c\u8036\u548c\u83ef\u7684\u8a71\u81e8\u5230\u54c8\u8a72\u5148\u77e5\u8aaa\uff1a\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u201c\u9019\u6bbf\u5b87\u4ecd\u7136\u8352\u5ee2\uff0c\u8c48\u662f\u4f60\u5011\u4f4f\u5728\u6709\u5929\u82b1\u677f\u7684\u623f\u5c4b\u7684\u6642\u5019\u55ce\uff1f\u201d\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u73fe\u5728\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u9019\u6a23\u8aaa\uff1a\u201c\u4f60\u5011\u8981\u53cd\u7701\u81ea\u5df1\u7684\u884c\u70ba\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u5011\u6492\u7684\u7a2e\u591a\uff0c\u6536\u5272\u7684\u537b\u5c11\uff1b\u4f60\u5011\u5403\uff0c\u537b\u5403\u4e0d\u98fd\uff1b\u4f60\u5011\u559d\uff0c\u537b\u559d\u4e0d\u8db3\uff1b\u4f60\u5011\u7a7f\u8863\u670d\uff0c\u537b\u7a7f\u4e0d\u6696\uff1b\u5f97\u5de5\u9322\u7684\uff0c\u537b\u628a\u5de5\u9322\u653e\u5728\u7834\u888b\u88e1\u3002\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u52f8\u52c9\u4eba\u6c11\u52d5\u5de5\u91cd\u5efa\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u9019\u6a23\u8aaa\uff1a\u201c\u4f60\u5011\u8981\u53cd\u7701\u81ea\u5df1\u7684\u884c\u70ba\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u5011\u8981\u4e0a\u5c71\u53d6\u6728\u6750\uff0c\u5efa\u9020\u6bbf\u5b87\uff1b\u6211\u5fc5\u56e0\u6b64\u559c\u6085\uff0c\u4e26\u4e14\u5f97\u69ae\u8000\u3002\u201d\u8036\u548c\u83ef\u8aaa\uff1a\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u201c\u4f60\u5011\u671f\u671b\u8c50\u6536\uff0c\u53cd\u5012\u6b49\u6536\uff1b\u4f60\u5011\u6536\u5230\u5bb6\u88e1\u7684\uff0c\u6211\u5c31\u5439\u53bb\u3002\u201d\u70ba\u751a\u9ebc\u5462\uff1f\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u56e0\u70ba\u6211\u7684\u6bbf\u5b87\u8352\u5ee2\uff0c\u4f60\u5011\u5404\u4eba\u537b\u70ba\u81ea\u5df1\u7684\u623f\u5c4b\u5954\u99b3\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6240\u4ee5\uff0c\u56e0\u4f60\u5011\u7684\u7de3\u6545\uff0c\u5929\u5c31\u4e0d\u964d\u7518\u9732\uff0c\u5730\u4e5f\u4e0d\u51fa\u571f\u7522\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u6211\u53eb\u4e7e\u65f1\u81e8\u5230\u5927\u5730\u3001\u7fa4\u5c71\u3001\u4e94\u7a40\u3001\u65b0\u9152\u3001\u65b0\u6cb9\u3001\u5730\u4e0a\u7684\u51fa\u7522\u3001\u4eba\u755c\u548c\u4eba\u624b\u52de\u788c\u5f97\u4f86\u7684\u4e00\u5207\u3002\u201d\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u52f8\u52c9\u7684\u679c\u6548\u90a3\u6642\uff0c\u6492\u62c9\u9435\u7684\u5152\u5b50\u6240\u7f85\u5df4\u4f2f\u3001\u7d04\u6492\u7b54\u7684\u5152\u5b50\u5927\u796d\u53f8\u7d04\u66f8\u4e9e\u548c\u6240\u6709\u9918\u5269\u7684\u5b50\u6c11\uff0c\u90fd\u807d\u5f9e\u4e86\u8036\u548c\u83ef\u4ed6\u5011\u3000\u795e\u7684\u8a71\uff0c\u807d\u5f9e\u4e86\u8036\u548c\u83ef\u4ed6\u5011\u3000\u795e\u6240\u5dee\u4f86\u7684\u54c8\u8a72\u5148\u77e5\u7684\u8a71\u3002\u773e\u6c11\u5728\u8036\u548c\u83ef\u9762\u524d\u90fd\u5b58\u8457\u656c\u754f\u7684\u5fc3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8036\u548c\u83ef\u7684\u4f7f\u8005\u54c8\u8a72\uff0c\u5949\u8036\u548c\u83ef\u7684\u547d\uff0c\u5c0d\u773e\u6c11\u8aaa\uff1a\u201c\u8036\u548c\u83ef\u8aaa\uff1a\u2018\u6211\u8207\u4f60\u5011\u540c\u5728\u3002\u2019\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u65bc\u662f\u8036\u548c\u83ef\u6fc0\u767c\u4e86\u6492\u62c9\u9435\u7684\u5152\u5b50\u3001\u7336\u5927\u7701\u9577\u6240\u7f85\u5df4\u4f2f\u7684\u5fc3\uff0c\u548c\u7d04\u6492\u7b54\u7684\u5152\u5b50\u5927\u796d\u53f8\u7d04\u66f8\u4e9e\u7684\u5fc3\uff0c\u4ee5\u53ca\u6240\u6709\u9918\u5269\u5b50\u6c11\u7684\u5fc3\uff0c\u4ed6\u5011\u5c31\u4f86\u8208\u5de5\u5efa\u9020\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u4ed6\u5011\u3000\u795e\u7684\u6bbf\u5b87\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u90a3\u6642\u662f\u5927\u5229\u70cf\u738b\u7b2c\u4e8c\u5e74\u516d\u6708\u4e8c\u5341\u56db\u65e5\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u65b0\u6bbf\u7684\u69ae\u8000\u52dd\u904e\u524d\u6bbf\u4e03\u6708\u4e8c\u5341\u4e00\u65e5\uff0c\u8036\u548c\u83ef\u7684\u8a71\u81e8\u5230\u54c8\u8a72\u5148\u77e5\u8aaa\uff1a\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u201c\u4f60\u8981\u5c0d\u6492\u62c9\u9435\u7684\u5152\u5b50\u7336\u5927\u7701\u9577\u6240\u7f85\u5df4\u4f2f\uff0c\u548c\u7d04\u6492\u7b54\u7684\u5152\u5b50\u5927\u796d\u53f8\u7d04\u66f8\u4e9e\uff0c\u4ee5\u53ca\u6240\u6709\u9918\u5269\u7684\u5b50\u6c11\uff0c\u8aaa\uff1a\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u2018\u4f60\u5011\u4e2d\u9593\u5269\u4e0b\u7684\u4eba\uff0c\u6709\u8ab0\u898b\u904e\u9019\u6bbf\u5b87\u5f9e\u524d\u7684\u69ae\u8000\u5462\uff1f\u73fe\u5728\u4f60\u5011\u53c8\u770b\u5b83\u600e\u6a23\uff1f\u8c48\u4e0d\u662f\u8996\u5982\u7121\u7269\u4e00\u6a23\u55ce\uff1f\u2019\u201d\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u6240\u7f85\u5df4\u4f2f\u554a\uff01\u73fe\u5728\u4f60\u8981\u525b\u5f37\u3002\u201d\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u7d04\u6492\u7b54\u7684\u5152\u5b50\u5927\u796d\u53f8\u7d04\u66f8\u4e9e\u554a\uff01\u4f60\u8981\u525b\u5f37\uff1b\u9019\u5730\u7684\u4e00\u5207\u5b50\u6c11\u554a\uff01\u4f60\u5011\u4e5f\u8981\u525b\u5f37\uff0c\u4e26\u4e14\u8981\u4f5c\u5de5\u3002\u201d\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u56e0\u70ba\u6211\u8207\u4f60\u5011\u540c\u5728\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9019\u5c31\u662f\u4f60\u5011\u5f9e\u57c3\u53ca\u51fa\u4f86\u7684\u6642\u5019\uff0c\u6211\u8207\u4f60\u5011\u7acb\u7d04\u7684\u8a71\u3002\u73fe\u5728\u6211\u7684\u9748\u4f4f\u5728\u4f60\u5011\u4e2d\u9593\uff1b\u4f60\u5011\u4e0d\u8981\u61fc\u6015\u3002\u201d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u56e0\u70ba\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u9019\u6a23\u8aaa\uff1a\u201c\u518d\u904e\u4e0d\u591a\u7684\u6642\u5019\uff0c\u6211\u8981\u9707\u52d5\u5929\u3001\u5730\u3001\u6d77\u6d0b\u548c\u65f1\u5730\u3002\u201d\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u6211\u8981\u9707\u52d5\u842c\u570b\uff0c\u842c\u570b\u7684\u73cd\u5bf6\u5c31\u5fc5\u904b\u4f86\uff1b\u6211\u8981\u4f7f\u9019\u6bbf\u5b87\u5145\u6eff\u69ae\u8000\u3002\u201d\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u9280\u5b50\u662f\u6211\u7684\uff0c\u91d1\u5b50\u4e5f\u662f\u6211\u7684\u3002\u201d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u9019\u6bbf\u5b87\u5f8c\u4f86\u7684\u69ae\u8000\uff0c\u5fc5\u5927\u904e\u5148\u524d\u7684\u69ae\u8000\u3002\u201d\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u8aaa\uff1a\u201c\u5728\u9019\u5730\u65b9\uff0c\u6211\u5fc5\u8cdc\u4e0b\u5e73\u5b89\u3002\u201d\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6307\u51fa\u4eba\u6c11\u7684\u60e1\u884c\u5927\u5229\u70cf\u738b\u7b2c\u4e8c\u5e74\u4e5d\u6708\u4e8c\u5341\u56db\u65e5\uff0c\u8036\u548c\u83ef\u7684\u8a71\u81e8\u5230\u54c8\u8a72\u5148\u77e5\u8aaa\uff1a\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u201c\u842c\u8ecd\u4e4b\u8036\u548c\u83ef\u9019\u6a23\u8aaa\uff1a\u4f60\u8981\u5411\u796d\u53f8\u8a62\u554f\u6709\u95dc\u5f8b\u6cd5\u7684\u4e8b\u8aaa\uff1a\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4eba\u82e5\u7528\u81ea\u5df1\u7684\u8863\u895f\u76db\u8f09\u8056\u8089\uff0c\u5f8c\u4f86\u53c8\u4f7f\u81ea\u5df1\u7684\u8863\u895f\u89f8\u8457\u9905\u3001\u6e6f\u3001\u9152\u3001\u6cb9\u6216\u5225\u7684\u98df\u7269\uff0c\u9019\u5c31\u7b97\u70ba\u8056\u55ce\uff1f\u201d\u796d\u53f8\u90fd\u56de\u7b54\u8aaa\uff1a\u201c\u4e0d\u7b97\u70ba\u8056\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u54c8\u8a72\u53c8\u8aaa\uff1a\u201c\u4eba\u82e5\u56e0\u89f8\u8457\u5c4d\u9ad4\u800c\u6210\u70ba\u4e0d\u6f54\uff0c\u53c8\u89f8\u8457\u9019\u4e9b\u6771\u897f\u7684\u4efb\u4f55\u4e00\u6a23\uff0c\u9019\u5c31\u7b97\u70ba\u4e0d\u6f54\u55ce\uff1f\u201d\u796d\u53f8\u90fd\u56de\u7b54\u8aaa\uff1a\u201c\u8981\u7b97\u70ba\u4e0d\u6f54\u6de8\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u65bc\u662f\u54c8\u8a72\u56de\u7b54\uff0c\u8aaa\uff1a\u201c\u8036\u548c\u83ef\u8aaa\uff1a\u2018\u9019\u6c11\u4e5f\u662f\u9019\u6a23\uff0c\u9019\u570b\u5728\u6211\u9762\u524d\u4e5f\u662f\u9019\u6a23\uff1b\u4ed6\u5011\u624b\u88e1\u7684\u4e00\u5207\u5de5\u4f5c\uff0c\u90fd\u662f\u9019\u6a23\uff1b\u4ed6\u5011\u5728\u90a3\u88e1\u6240\u737b\u7684\uff0c\u90fd\u662f\u4e0d\u6f54\u6de8\u7684\u3002\u2019\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u8036\u548c\u83ef\u61c9\u8a31\u8cdc\u798f\u201c\u73fe\u5728\u4f60\u5011\u8981\u53cd\u7701\uff0c\u5f9e\u4eca\u65e5\u8d77\u56de\u60f3\u904e\u5f80\uff0c\u5c31\u662f\u5728\u8036\u548c\u83ef\u7684\u6bbf\u9084\u6c92\u6709\u4e00\u584a\u77f3\u982d\u758a\u5728\u53e6\u4e00\u584a\u77f3\u982d\u4e0a\u9762\u4ee5\u524d\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u90a3\u6642\u4f60\u5011\u600e\u6a23\u5462\uff1f\u6709\u4eba\u5230\u7a40\u5806\u4f86\uff0c\u60f3\u5f97\u5169\u767e\u516c\u65a4\u7a40\uff0c\u537b\u53ea\u5f97\u4e00\u767e\u516c\u65a4\uff1b\u6709\u4eba\u5230\u69a8\u9152\u6c60\u53bb\uff0c\u60f3\u5f9e\u9152\u69fd\u5f97\u4e00\u767e\u516c\u5347\u9152\uff0c\u537b\u53ea\u5f97\u56db\u5341\u516c\u5347\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u8036\u548c\u83ef\u8aaa\uff1a\u2018\u6211\u7528\u65f1\u98a8\u3001\u9709\u721b\u548c\u51b0\u96f9\u64ca\u6253\u4f60\u5011\uff0c\u548c\u4f60\u5011\u624b\u88e1\u7684\u4e00\u5207\u5de5\u4f5c\uff0c\u4f60\u5011\u9084\u662f\u4e0d\u6b78\u5411\u6211\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4f60\u5011\u8981\u53cd\u7701\uff0c\u5f9e\u4eca\u65e5\u8d77\u56de\u60f3\u904e\u5f80\uff0c\u5c31\u662f\u81ea\u4e5d\u6708\u4e8c\u5341\u56db\u65e5\u8d77\uff0c\u5f9e\u8036\u548c\u83ef\u6bbf\u7684\u5960\u57fa\u65e5\u958b\u59cb\uff0c\u4f60\u5011\u8981\u53cd\u7701\uff1a\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u5009\u88e1\u9084\u6709\u7a2e\u5b50\u55ce\uff1f\u8461\u8404\u6a39\u3001\u7121\u82b1\u679c\u6a39\u3001\u77f3\u69b4\u6a39\u548c\u6a44\u6b16\u6a39\uff0c\u8c48\u662f\u9084\u6c92\u6709\u7d50\u679c\u5b50\u55ce\uff1f\u5f9e\u4eca\u65e5\u8d77\uff0c\u6211\u5fc5\u8cdc\u798f\u3002\u2019\u201d\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u61c9\u8a31\u63c0\u9078\u6240\u7f85\u5df4\u4f2f\u540c\u6708\u4e8c\u5341\u56db\u65e5\uff0c\u8036\u548c\u83ef\u7684\u8a71\u518d\u6b21\u81e8\u5230\u54c8\u8a72\uff0c\u8aaa\uff1a\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u201c\u4f60\u8981\u5411\u7336\u5927\u7701\u9577\u6240\u7f85\u5df4\u4f2f\u8aaa\uff1a\u2018\u6211\u8981\u9707\u52d5\u5929\u5730\uff0c\n"
                },
                "22": {
