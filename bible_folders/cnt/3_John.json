{
    "version": "cnt",
    "book_name": "3 John",
    "book_nr": "64",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u6211\u9019\u4f5c\u9577\u8001\u7684\u5beb\u4fe1\u7d66\u89aa\u611b\u7684\u8a72\u7336\uff0c\u5c31\u662f\u6211\u5728\u771f\u7406\u4e2d\u6240\u611b\u7684\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u89aa\u611b\u7684\uff0c\u6211\u795d\u4f60\u51e1\u4e8b\u4ea8\u901a\uff0c\u8eab\u9ad4\u5065\u58ef\uff0c\u6b63\u5982\u4f60\u7684\u9748\u9b42\u5b89\u6cf0\u4e00\u6a23\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6709\u4e9b\u5f1f\u5144\u4f86\u5230\uff0c\u8b49\u5be6\u4f60\u5fc3\u4e2d\u5b58\u6709\u771f\u7406\uff0c\u5c31\u662f\u4f60\u6309\u771f\u7406\u884c\u4e8b\uff0c\u6211\u5c31\u975e\u5e38\u6b23\u6170\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u807d\u898b\u6211\u7684\u5152\u5973\u6309\u771f\u7406\u884c\u4e8b\uff0c\u6211\u7684\u559c\u6a02\u6c92\u6709\u6bd4\u9019\u66f4\u5927\u7684\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7a31\u8b9a\u8a72\u7336\u63a5\u5f85\u5f1f\u5144\u89aa\u611b\u7684\uff0c\u4f60\u5411\u5f1f\u5144\u6240\u884c\u7684\uff0c\u7279\u5225\u662f\u5411\u5916\u5730\u4f86\u7684\u5f1f\u5144\u6240\u884c\u7684\uff0c\u90fd\u662f\u51fa\u65bc\u5fe0\u5fc3\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u5011\u5728\u6559\u6703\u9762\u524d\u8b49\u5be6\u4e86\u4f60\u7684\u611b\uff1b\u4f60\u7167\u8457\u3000\u795e\u6240\u559c\u6085\u7684\uff0c\u8cc7\u52a9\u4ed6\u5011\u7684\u65c5\u7a0b\uff0c\u9019\u6a23\u662f\u597d\u7684\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u70ba\u4ed6\u5011\u70ba\u4e3b\u7684\u540d\u51fa\u5916\uff0c\u4e26\u6c92\u6709\u5f9e\u6559\u5916\u4eba\u63a5\u53d7\u751a\u9ebc\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6240\u4ee5\u6211\u5011\u61c9\u7576\u63a5\u5f85\u9019\u6a23\u7684\u4eba\uff0c\u597d\u8b93\u6211\u5011\u70ba\u4e86\u771f\u7406\u6210\u70ba\u540c\u5de5\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4e0d\u8981\u6548\u6cd5\u60e1\uff0c\u61c9\u8a72\u6548\u6cd5\u5584\u6211\u66fe\u7d93\u7565\u7565\u5beb\u4fe1\u7d66\u4f60\u90a3\u88e1\u7684\u6559\u6703\uff0c\u4f46\u4ed6\u5011\u4e2d\u9593\u90a3\u597d\u4f5c\u9818\u8896\u7684\u4e1f\u7279\u8153\u4e0d\u63a5\u5f85\u6211\u5011\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u6b64\uff0c\u6211\u4f86\u7684\u6642\u5019\uff0c\u5fc5\u8981\u63d0\u8d77\u4ed6\u6240\u4f5c\u7684\u4e8b\uff0c\u5c31\u662f\u4ed6\u7528\u60e1\u8a00\u4e2d\u50b7\u6211\u5011\uff1b\u9019\u9084\u4e0d\u5920\uff0c\u4ed6\u4e0d\u4f46\u4e0d\u63a5\u5f85\u5f1f\u5144\uff0c\u9084\u8981\u963b\u6b62\u90a3\u4e9b\u60f3\u8981\u63a5\u5f85\u7684\u4eba\uff0c\u751a\u81f3\u628a\u4ed6\u5011\u8d95\u51fa\u6559\u6703\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u89aa\u611b\u7684\uff0c\u4e0d\u8981\u6548\u6cd5\u60e1\uff0c\u61c9\u8a72\u6548\u6cd5\u5584\u3002\u884c\u5584\u7684\u5c6c\u65bc\u3000\u795e\uff0c\u4f5c\u60e1\u7684\u6c92\u6709\u898b\u904e\u3000\u795e\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u4f4e\u7c73\u4e1f\u884c\u5584\uff0c\u6709\u773e\u4eba\u70ba\u4ed6\u4f5c\u8b49\uff0c\u771f\u7406\u672c\u8eab\u4e5f\u70ba\u4ed6\u4f5c\u8b49\u3002\u6211\u5011\u4e5f\u70ba\u4ed6\u4f5c\u8b49\uff0c\u4f60\u77e5\u9053\u6211\u5011\u7684\u898b\u8b49\u662f\u771f\u7684\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u554f\u5019\u7684\u8a71\u6211\u9084\u6709\u8a31\u591a\u8a71\u8981\u5beb\u7d66\u4f60\uff0c\u53ef\u662f\u6211\u4e0d\u9858\u501f\u7528\u7b46\u58a8\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6211\u76fc\u671b\u5f88\u5feb\u5c31\u898b\u5230\u4f60\uff0c\u7576\u9762\u8ac7\u8ac7\u3002 [ (III John 1:15) \u9858\u4f60\u5e73\u5b89\u3002\u9019\u88e1\u7684\u670b\u53cb\u90fd\u554f\u5019\u4f60\u3002\u8acb\u4f60\u4e00\u4e00\u63d0\u540d\u554f\u5019\u4f60\u90a3\u88e1\u7684\u670b\u53cb\u3002 ]\n"
                }
            }
        }
    }
}