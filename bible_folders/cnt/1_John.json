    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u751f\u547d\u4e4b\u9053\u8ad6\u5230\u592a\u521d\u5c31\u5df2\u7d93\u5b58\u5728\u7684\u751f\u547d\u4e4b\u9053\uff0c\u5c31\u662f\u6211\u5011\u6240\u807d\u898b\uff0c\u89aa\u773c\u6240\u770b\u898b\uff0c\u4ed4\u7d30\u89c0\u5bdf\u904e\uff0c\u89aa\u624b\u6478\u904e\u7684\uff1b\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u9019\u751f\u547d\u5df2\u7d93\u986f\u73fe\u51fa\u4f86\uff0c\u6211\u5011\u898b\u904e\u4e86\uff0c\u73fe\u5728\u4e5f\u4f5c\u898b\u8b49\uff0c\u4e26\u4e14\u5411\u4f60\u5011\u5ba3\u63da\u9019\u672c\u4f86\u8207\u7236\u540c\u5728\uff0c\u53c8\u5411\u6211\u5011\u986f\u73fe\u904e\u7684\u6c38\u9060\u7684\u751f\u547d\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u5011\u628a\u6240\u770b\u898b\u6240\u807d\u898b\u7684\u5411\u4f60\u5011\u5ba3\u63da\uff0c\u4f7f\u4f60\u5011\u4e5f\u53ef\u4ee5\u548c\u6211\u5011\u5fc3\u9748\u76f8\u901a\u3002\u6211\u5011\u662f\u8207\u7236\u548c\u4ed6\u7684\u5152\u5b50\u8036\u7a4c\u57fa\u7763\u5fc3\u9748\u76f8\u901a\u7684\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u6211\u5011\u5beb\u9019\u4e9b\u4e8b\uff0c\u662f\u8981\u4f7f\u6211\u5011\u7684\u559c\u6a02\u5145\u8db3\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u3000\u795e\u662f\u5149\u3000\u795e\u662f\u5149\uff0c\u5728\u4ed6\u88e1\u9762\u6beb\u7121\u9ed1\u6697\uff1b\u9019\u5c31\u662f\u6211\u5011\u5f9e\u4ed6\u90a3\u88e1\u807d\u898b\uff0c\u73fe\u5728\u50b3\u7d66\u4f60\u5011\u7684\u4fe1\u606f\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u5011\u82e5\u8aaa\u81ea\u5df1\u8207\u4ed6\u5fc3\u9748\u76f8\u901a\uff0c\u537b\u884c\u5728\u9ed1\u6697\u88e1\uff0c\u5c31\u662f\u8aaa\u8b0a\u8a71\uff0c\u4e0d\u5be6\u884c\u771f\u7406\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6211\u5011\u82e5\u884c\u5728\u5149\u4e2d\uff0c\u50cf\u4ed6\u5728\u5149\u4e2d\u4e00\u6a23\uff0c\u5c31\u5f7c\u6b64\u5fc3\u9748\u76f8\u901a\uff0c\u4ed6\u5152\u5b50\u8036\u7a4c\u7684\u8840\u4e5f\u6f54\u6de8\u6211\u5011\u812b\u96e2\u4e00\u5207\u7f6a\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u5011\u82e5\u8aaa\u81ea\u5df1\u6c92\u6709\u7f6a\uff0c\u5c31\u662f\u81ea\u6b3a\uff0c\u771f\u7406\u5c31\u4e0d\u5728\u6211\u5011\u88e1\u9762\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u6211\u5011\u82e5\u627f\u8a8d\u81ea\u5df1\u7684\u7f6a\uff0c\u3000\u795e\u662f\u4fe1\u5be6\u7684\u3001\u516c\u7fa9\u7684\uff0c\u5fc5\u5b9a\u8d66\u514d\u6211\u5011\u7684\u7f6a\uff0c\u6f54\u6de8\u6211\u5011\u812b\u96e2\u4e00\u5207\u4e0d\u7fa9\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6211\u5011\u82e5\u8aaa\u81ea\u5df1\u6c92\u6709\u72af\u904e\u7f6a\uff0c\u5c31\u662f\u628a\u3000\u795e\u7576\u4f5c\u8aaa\u8b0a\u7684\uff0c\u4ed6\u7684\u9053\u5c31\u4e0d\u5728\u6211\u5011\u5fc3\u88e1\u4e86\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u6211\u5011\u7684\u7dad\u8b77\u8005\u57fa\u7763\u6211\u7684\u5b69\u5b50\u5011\uff0c\u6211\u5beb\u9019\u4e9b\u7d66\u4f60\u5011\uff0c\u662f\u8981\u4f60\u5011\u4e0d\u72af\u7f6a\u3002\u5982\u679c\u6709\u4eba\u72af\u4e86\u7f6a\uff0c\u5728\u7236\u7684\u9762\u524d\u6211\u5011\u6709\u4e00\u4f4d\u7dad\u8b77\u8005\uff0c\u5c31\u662f\u90a3\u7fa9\u8005\u8036\u7a4c\u57fa\u7763\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4ed6\u70ba\u6211\u5011\u7684\u7f6a\u4f5c\u4e86\u8d16\u7f6a\u796d\uff0c\u4e0d\u50c5\u70ba\u6211\u5011\u7684\u7f6a\uff0c\u4e5f\u70ba\u5168\u4eba\u985e\u7684\u7f6a\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u5011\u82e5\u9075\u5b88\u3000\u795e\u7684\u547d\u4ee4\uff0c\u9019\u6a23\uff0c\u5c31\u77e5\u9053\u6211\u5011\u5df2\u7d93\u8a8d\u8b58\u4ed6\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u51e1\u662f\u8aaa\u201c\u6211\u5df2\u7d93\u8a8d\u8b58\u4ed6\u201d\uff0c\u537b\u4e0d\u9075\u5b88\u4ed6\u547d\u4ee4\u7684\uff0c\u5c31\u662f\u8aaa\u8b0a\u7684\u4eba\uff0c\u771f\u7406\u5c31\u4e0d\u5728\u4ed6\u88e1\u9762\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u7136\u800c\u51e1\u662f\u9075\u5b88\u4ed6\u7684\u9053\u7684\uff0c\u4ed6\u611b\u3000\u795e\u7684\u5fc3\u5c31\u7684\u78ba\u5728\u4ed6\u88e1\u9762\u5b8c\u5168\u4e86\u3002\u9019\u6a23\uff0c\u6211\u5011\u5c31\u77e5\u9053\u6211\u5011\u662f\u5728\u3000\u795e\u88e1\u9762\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u51e1\u8aaa\u81ea\u5df1\u662f\u4f4f\u5728\u4ed6\u88e1\u9762\u7684\uff0c\u5c31\u61c9\u8a72\u7167\u8457\u4ed6\u6240\u884c\u7684\u53bb\u884c\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u611b\u5f1f\u5144\u7684\u5c31\u662f\u4f4f\u5728\u5149\u660e\u4e2d\u89aa\u611b\u7684\uff0c\u6211\u5beb\u7d66\u4f60\u5011\u7684\uff0c\u4e0d\u662f\u4e00\u689d\u65b0\u547d\u4ee4\uff0c\u800c\u662f\u4f60\u5011\u672c\u4f86\u5c31\u6709\u7684\u820a\u547d\u4ee4\u3002\u9019\u820a\u547d\u4ee4\u5c31\u662f\u4f60\u5011\u807d\u904e\u7684\u9053\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u7136\u800c\uff0c\u56e0\u70ba\u9ed1\u6697\u6f38\u6f38\u6d88\u901d\uff0c\u771f\u5149\u5df2\u5728\u7167\u8000\uff0c\u6211\u5beb\u7d66\u4f60\u5011\u7684\u4e5f\u662f\u4e00\u689d\u65b0\u547d\u4ee4\uff0c\u9019\u5728\u3000\u795e\u548c\u5728\u4f60\u5011\u4f86\u8aaa\uff0c\u90fd\u662f\u771f\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u51e1\u662f\u8aaa\u81ea\u5df1\u5728\u5149\u660e\u4e2d\uff0c\u537b\u6068\u4ed6\u7684\u5f1f\u5144\u7684\uff0c\u4ed6\u5230\u73fe\u5728\u9084\u5728\u9ed1\u6697\u88e1\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u51e1\u662f\u611b\u5f1f\u5144\u7684\uff0c\u5c31\u662f\u4f4f\u5728\u5149\u660e\u4e2d\uff0c\u5728\u5149\u660e\u4e2d\u4ed6\u5c31\u4e0d\u6703\u8dcc\u5012\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u51e1\u662f\u6068\u5f1f\u5144\u7684\uff0c\u5c31\u662f\u5728\u9ed1\u6697\u88e1\uff0c\u4e5f\u5728\u9ed1\u6697\u88e1\u884c\u4e8b\u3002\u4ed6\u4e0d\u77e5\u9053\u5f80\u54ea\u88e1\u53bb\uff0c\u56e0\u70ba\u9ed1\u6697\u5f04\u778e\u4e86\u4ed6\u7684\u773c\u775b\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u5b69\u5b50\u5011\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u7684\u7f6a\u56e0\u8457\u4e3b\u7684\u540d\u5df2\u7d93\u5f97\u5230\u8d66\u514d\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u7236\u8001\u5011\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u8a8d\u8b58\u592a\u521d\u5c31\u5b58\u5728\u7684\u90a3\u4e00\u4f4d\u3002\u9752\u5e74\u5011\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u5df2\u7d93\u52dd\u904e\u90a3\u60e1\u8005\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5b69\u5b50\u5011\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u8a8d\u8b58\u7236\u3002\u7236\u8001\u5011\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u8a8d\u8b58\u592a\u521d\u5c31\u5b58\u5728\u7684\u90a3\u4e00\u4f4d\u3002\u9752\u5e74\u5011\uff0c\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u56e0\u70ba\u4f60\u5011\u5f37\u58ef\uff0c\u3000\u795e\u7684\u9053\u5728\u4f60\u5011\u5fc3\u88e1\uff0c\u4f60\u5011\u4e5f\u52dd\u904e\u4e86\u90a3\u60e1\u8005\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4e0d\u8981\u611b\u4e16\u754c\u548c\u4e16\u4e0a\u7684\u6771\u897f\u3002\u4eba\u82e5\u611b\u4e16\u754c\uff0c\u611b\u7236\u7684\u5fc3\u5c31\u4e0d\u5728\u4ed6\u88e1\u9762\u4e86\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u539f\u4f86\u4e16\u4e0a\u7684\u4e00\u5207\uff0c\u5c31\u5982\u8089\u9ad4\u7684\u79c1\u617e\uff0c\u773c\u76ee\u7684\u79c1\u617e\u548c\u4eca\u751f\u7684\u9a55\u50b2\uff0c\u90fd\u4e0d\u662f\u51fa\u65bc\u7236\uff0c\u800c\u662f\u5f9e\u4e16\u754c\u4f86\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u9019\u4e16\u754c\u548c\u4e16\u4e0a\u7684\u79c1\u617e\u90fd\u8981\u6f38\u6f38\u904e\u53bb\uff0c\u4f46\u90a3\u9075\u884c\u3000\u795e\u65e8\u610f\u7684\u537b\u5b58\u5230\u6c38\u9060\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u6575\u57fa\u7763\u8005\u5426\u8a8d\u8036\u7a4c\u662f\u57fa\u7763\u5b69\u5b50\u5011\uff0c\u73fe\u5728\u662f\u672b\u4e16\u7684\u6642\u5019\u4e86\u3002\u4f60\u5011\u807d\u904e\u6575\u57fa\u7763\u8005\u8981\u4f86\uff0c\u73fe\u5728\u5df2\u7d93\u6709\u4e0d\u5c11\u6575\u57fa\u7763\u8d77\u4f86\u4e86\uff1b\u56e0\u6b64\u6211\u5011\u5c31\u77e5\u9053\u9019\u662f\u672b\u4e16\u7684\u6642\u5019\u4e86\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u4ed6\u5011\u5f9e\u6211\u5011\u4e2d\u9593\u96e2\u53bb\uff0c\u9019\u5c31\u8868\u660e\u4ed6\u5011\u662f\u4e0d\u5c6c\u65bc\u6211\u5011\u7684\u3002\u5176\u5be6\u4ed6\u5011\u4e26\u4e0d\u5c6c\u65bc\u6211\u5011\uff0c\u5982\u679c\u771f\u7684\u5c6c\u65bc\u6211\u5011\uff0c\u5c31\u4e00\u5b9a\u6703\u7559\u5728\u6211\u5011\u4e2d\u9593\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4f60\u5011\u5f9e\u90a3\u8056\u8005\u5f97\u8457\u818f\u62b9\uff0c\u9019\u662f\u4f60\u5011\u90fd\u77e5\u9053\u7684\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211\u5beb\u4fe1\u7d66\u4f60\u5011\uff0c\u4e0d\u662f\u56e0\u70ba\u4f60\u5011\u4e0d\u660e\u767d\u771f\u7406\uff0c\u800c\u662f\u56e0\u70ba\u4f60\u5011\u660e\u767d\uff0c\u53c8\u56e0\u70ba\u8b0a\u8a00\u7d55\u4e0d\u6703\u51fa\u65bc\u771f\u7406\u7684\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u90a3\u9ebc\uff0c\u8ab0\u662f\u8aaa\u8b0a\u7684\u5462\uff1f\u4e0d\u5c31\u662f\u90a3\u5426\u8a8d\u8036\u7a4c\u662f\u57fa\u7763\u7684\u55ce\uff1f\u5426\u8a8d\u7236\u548c\u5b50\u7684\uff0c\u4ed6\u5c31\u662f\u6575\u57fa\u7763\u7684\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u51e1\u5426\u8a8d\u5b50\u7684\uff0c\u5c31\u6c92\u6709\u7236\uff1b\u627f\u8a8d\u5b50\u7684\uff0c\u9023\u7236\u4e5f\u6709\u4e86\u3002\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u81f3\u65bc\u4f60\u5011\uff0c\u7e3d\u8981\u628a\u8d77\u521d\u6240\u807d\u898b\u7684\u5b58\u8a18\u5728\u4f60\u5011\u5fc3\u88e1\uff1b\u4f60\u5011\u82e5\u628a\u8d77\u521d\u6240\u807d\u898b\u7684\u5b58\u8a18\u5728\u5fc3\u88e1\uff0c\u4f60\u5011\u4e5f\u5c31\u4f4f\u5728\u5b50\u548c\u7236\u88e1\u9762\u4e86\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u4ed6\u61c9\u8a31\u7d66\u6211\u5011\u7684\uff0c\u5c31\u662f\u6c38\u9060\u7684\u751f\u547d\u3002\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\u6211\u628a\u9019\u4e9b\u4e8b\u5beb\u7d66\u4f60\u5011\uff0c\u662f\u6307\u8457\u90a3\u4e9b\u6b3a\u9a19\u4f60\u5011\u7684\u4eba\u8aaa\u7684\u3002\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\u4f60\u5011\u65e2\u7136\u5728\u4f60\u5011\u88e1\u9762\u6709\u5f9e\u4e3b\u9818\u53d7\u7684\u818f\u62b9\uff0c\u5c31\u4e0d\u9700\u8981\u5225\u4eba\u6559\u5c0e\u4f60\u5011\u4e86\uff0c\u56e0\u70ba\u5728\u4e00\u5207\u4e8b\u4e0a\u6709\u4e3b\u7684\u818f\u62b9\u6559\u5c0e\u4f60\u5011\uff1b\u9019\u818f\u62b9\u662f\u771f\u7684\uff0c\u4e0d\u662f\u5047\u7684\uff0c\u4f60\u5011\u61c9\u8a72\u6309\u8457\u4ed6\u6240\u6559\u5c0e\u7684\u4f4f\u5728\u4ed6\u88e1\u9762\u3002\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\u3000\u795e\u7684\u5152\u5973\u5c31\u4e0d\u72af\u7f6a\u5b69\u5b50\u5011\uff0c\u4f60\u5011\u8981\u4f4f\u5728\u4e3b\u88e1\u9762\u3002\u9019\u6a23\uff0c\u4ed6\u82e5\u986f\u73fe\uff0c\u6211\u5011\u5c31\u53ef\u4ee5\u5766\u7136\u7121\u61fc\uff1b\u4ed6\u4f86\u7684\u6642\u5019\uff0c\u6211\u5011\u4e5f\u4e0d\u81f3\u615a\u6127\u5730\u8eb2\u907f\u4ed6\u4e86\u3002\n"
                },
                "29": {
                    "verse_nr": 29,
