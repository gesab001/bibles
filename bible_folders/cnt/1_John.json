                "12": {
                    "verse_nr": 12,
                    "verse": "\u51e1\u6709\u3000\u795e\u5152\u5b50\u7684\uff0c\u5c31\u6709\u751f\u547d\uff1b\u6c92\u6709\u3000\u795e\u5152\u5b50\u7684\uff0c\u5c31\u6c92\u6709\u751f\u547d\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u8981\u7167\u3000\u795e\u7684\u65e8\u610f\u7948\u6c42\u6211\u628a\u9019\u4e9b\u4e8b\u5beb\u7d66\u4f60\u5011\u4fe1\u3000\u795e\u7684\u5152\u5b50\u4e4b\u540d\u7684\u4eba\uff0c\u662f\u8981\u4f60\u5011\u77e5\u9053\u81ea\u5df1\u6709\u6c38\u751f\u3002\n"
                },
                "14": {
