                "8": {
                    "verse_nr": 8,
                    "verse": "\u5c31\u662f\u8056\u9748\u3001\u6c34\u548c\u8840\uff0c\u9019\u4e09\u6a23\u662f\u4e00\u81f4\u7684\u3002\n"
