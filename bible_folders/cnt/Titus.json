{
    "version": "cnt",
    "book_name": "Titus",
    "book_nr": "56",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u3000\u795e\u7684\u50d5\u4eba\u3001\u8036\u7a4c\u57fa\u7763\u7684\u4f7f\u5f92\u4fdd\u7f85\uff08\u9019\u662f\u70ba\u8457\u3000\u795e\u9078\u6c11\u7684\u4fe1\u5fc3\uff0c\u548c\u90a3\u5408\u4e4e\u656c\u8654\u771f\u7406\u7684\u77e5\u8b58\uff1b\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u9019\u90fd\u662f\u6839\u64da\u6c38\u751f\u7684\u76fc\u671b\uff0c\u9019\u6c38\u751f\u5c31\u662f\u6c92\u6709\u8b0a\u8a00\u7684\u3000\u795e\u5728\u842c\u4e16\u4ee5\u524d\u6240\u61c9\u8a31\u7684\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5230\u4e86\u9069\u7576\u7684\u6642\u5019\uff0c\u3000\u795e\u5c31\u85c9\u8457\u798f\u97f3\u7684\u5ba3\u8b1b\uff0c\u628a\u4ed6\u7684\u9053\u986f\u660e\u51fa\u4f86\u3002\u7167\u8457\u3000\u795e\u6211\u5011\u6551\u4e3b\u7684\u547d\u4ee4\uff0c\u9019\u5ba3\u8b1b\u7684\u4f7f\u547d\u4ea4\u8a17\u4e86\u6211\uff09\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u5beb\u4fe1\u7d66\u63d0\u591a\uff0c\u5c31\u662f\u90a3\u5728\u5171\u540c\u7684\u4fe1\u4ef0\u4e0a\u4f5c\u6211\u771f\u5152\u5b50\u7684\u3002\u9858\u6069\u60e0\u5e73\u5b89\u5f9e\u7236\u3000\u795e\u548c\u6211\u5011\u7684\u6551\u4e3b\u57fa\u7763\u8036\u7a4c\u81e8\u5230\u4f60\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u63d0\u591a\u5728\u514b\u91cc\u7279\u7684\u5de5\u4f5c\u6211\u5f9e\u524d\u7559\u4f60\u5728\u514b\u91cc\u7279\u5cf6\uff0c\u662f\u8981\u4f60\u8fa6\u597d\u6c92\u6709\u8fa6\u5b8c\u7684\u4e8b\uff0c\u4e26\u4e14\u7167\u6211\u6240\u5429\u5490\u7684\uff0c\u5728\u5404\u57ce\u8a2d\u7acb\u9577\u8001\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5982\u679c\u6709\u4eba\u7121\u53ef\u6307\u6458\uff0c\u53ea\u4f5c\u4e00\u500b\u59bb\u5b50\u7684\u4e08\u592b\uff0c\u5152\u5973\u90fd\u4fe1\u4e3b\uff0c\u4e5f\u6c92\u6709\u4eba\u63a7\u544a\u4ed6\u5011\u653e\u8569\u6216\u4e0d\u53d7\u7d04\u675f\uff0c\u624d\u53ef\u4ee5\u4f5c\u9577\u8001\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u70ba\u76e3\u7763\u662f\u3000\u795e\u7684\u7ba1\u5bb6\uff0c\u6240\u4ee5\u5fc5\u9808\u7121\u53ef\u6307\u6458\u3001\u4e0d\u4efb\u6027\u3001\u4e0d\u96a8\u4fbf\u52d5\u6012\u3001\u4e0d\u597d\u9152\u3001\u4e0d\u6253\u4eba\u3001\u4e0d\u8caa\u4e0d\u7fa9\u4e4b\u8ca1\uff1b\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u537b\u8981\u63a5\u5f85\u5ba2\u65c5\u3001\u559c\u611b\u826f\u5584\u3001\u81ea\u5f8b\u3001\u516c\u6b63\u3001\u8056\u6f54\u3001\u81ea\u5236\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5805\u5b88\u90a3\u5408\u4e4e\u6559\u7fa9\u3001\u53ef\u9760\u7684\u771f\u9053\uff0c\u597d\u4f7f\u4ed6\u80fd\u5920\u7528\u7d14\u6b63\u7684\u9053\u7406\u52f8\u52c9\u4eba\uff0c\u4e26\u4e14\u80fd\u5920\u6298\u670d\u53cd\u5c0d\u7684\u4eba\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u70ba\u6709\u8a31\u591a\u4e0d\u53d7\u7d04\u675f\u3001\u597d\u8b1b\u7a7a\u8a71\u548c\u6b3a\u9a19\u4eba\u7684\uff0c\u5c24\u5176\u662f\u90a3\u4e9b\u5b88\u5272\u79ae\u7684\u4eba\uff0c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6\u5011\u70ba\u4e86\u53ef\u6065\u7684\u5229\u76ca\uff0c\u6559\u8a13\u4e00\u4e9b\u4e0d\u61c9\u8a72\u6559\u5c0e\u7684\u4e8b\uff0c\u6557\u58de\u4eba\u7684\u5168\u5bb6\uff0c\u4f60\u52d9\u8981\u5835\u4f4f\u4ed6\u5011\u7684\u5634\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6709\u4e00\u500b\u514b\u91cc\u7279\u672c\u5730\u7684\u5148\u77e5\u8aaa\u904e\uff1a\u201c\u514b\u91cc\u7279\u4eba\u662f\u5e38\u5e38\u8aaa\u8b0a\u7684\uff0c\u662f\u60e1\u7378\uff0c\u597d\u5403\u61f6\u4f5c\u3002\u201d\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u9019\u898b\u8b49\u662f\u771f\u7684\u3002\u70ba\u4e86\u9019\u7de3\u6545\uff0c\u4f60\u8981\u56b4\u53b2\u5730\u8cac\u5099\u4ed6\u5011\uff0c\u597d\u4f7f\u4ed6\u5011\u5728\u4fe1\u4ef0\u4e0a\u5065\u5168\uff0c\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e0d\u518d\u7406\u6703\u7336\u592a\u4eba\u7684\u7121\u7a3d\u4e4b\u8ac7\u548c\u504f\u96e2\u6b63\u9053\u7684\u4eba\u7684\u898f\u689d\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5728\u6e05\u6f54\u7684\u4eba\uff0c\u4e00\u5207\u90fd\u662f\u6e05\u6f54\u7684\uff1b\u4f46\u5728\u6c61\u7a62\u548c\u4e0d\u4fe1\u7684\u4eba\uff0c\u6c92\u6709\u4e00\u6a23\u662f\u6e05\u6f54\u7684\uff0c\u9023\u4ed6\u5011\u7684\u610f\u5ff5\u548c\u826f\u5fc3\u90fd\u6c61\u7a62\u4e86\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4ed6\u5011\u8072\u7a31\u8a8d\u8b58\u3000\u795e\uff0c\u537b\u5728\u884c\u70ba\u4e0a\u5426\u8a8d\u4ed6\u3002\u4ed6\u5011\u662f\u53ef\u618e\u7684\u3001\u6096\u9006\u7684\uff0c\u5728\u5404\u6a23\u7684\u5584\u4e8b\u4e0a\uff0c\u662f\u6beb\u7121\u7528\u8655\u7684\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8981\u50b3\u8b1b\u7d14\u6b63\u7684\u9053\u7406\u81f3\u65bc\u4f60\uff0c\u4f60\u61c9\u7576\u8b1b\u5408\u4e4e\u7d14\u6b63\u7684\u9053\u7406\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u52f8\u8001\u5e74\u4eba\u8981\u6709\u7bc0\u5236\u3001\u838a\u91cd\u3001\u81ea\u5f8b\uff0c\u5728\u4fe1\u5fc3\u3001\u611b\u5fc3\u3001\u5fcd\u8010\u4e0a\u90fd\u8981\u5065\u5168\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u7167\u6a23\uff0c\u52f8\u5e74\u8001\u7684\u5a66\u5973\u8981\u751f\u6d3b\u656c\u8654\uff0c\u4e0d\u8aaa\u8b92\u8a00\uff0c\u4e0d\u88ab\u9152\u5974\u5f79\uff0c\u7528\u5584\u9053\u6559\u5c0e\u4eba\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u597d\u63d0\u9192\u5e74\u8f15\u7684\u5a66\u5973\u611b\u4e08\u592b\u611b\u5152\u5973\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4e26\u4e14\u81ea\u5f8b\u3001\u8c9e\u6f54\u3001\u7406\u5bb6\u3001\u5584\u826f\u3001\u9806\u5f9e\u81ea\u5df1\u7684\u4e08\u592b\uff0c\u514d\u5f97\u3000\u795e\u7684\u9053\u53d7\u6bc0\u8b17\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u7167\u6a23\uff0c\u52f8\u5e74\u8f15\u7684\u7537\u5b50\u8981\u81ea\u5f8b\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u7121\u8ad6\u5728\u751a\u9ebc\u4e8b\u4e0a\u4f60\u90fd\u8981\u986f\u51fa\u597d\u884c\u70ba\u7684\u699c\u6a23\uff0c\u5728\u6559\u5c0e\u4e0a\u8981\u7d14\u5168\uff0c\u8981\u838a\u91cd\uff0c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u8a00\u8a5e\u8981\u7d14\u6b63\uff0c\u7121\u53ef\u6307\u6458\uff0c\u4f7f\u53cd\u5c0d\u7684\u4eba\u56e0\u70ba\u7121\u5f9e\u6bc0\u8b17\uff0c\u5c31\u81ea\u89ba\u615a\u6127\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u52f8\u50d5\u4eba\u8981\u51e1\u4e8b\u9806\u670d\u81ea\u5df1\u7684\u4e3b\u4eba\uff0c\u8a0e\u4ed6\u6b61\u559c\uff0c\u4e0d\u8981\u9802\u5634\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4e0d\u8981\u79c1\u53d6\u8ca1\u7269\uff0c\u537b\u8981\u986f\u793a\u7d55\u5c0d\u7684\u8aa0\u5be6\uff0c\u597d\u4f7f\u6211\u5011\u6551\u4e3b\u3000\u795e\u7684\u9053\u7406\uff0c\u5728\u51e1\u4e8b\u4e0a\u90fd\u5f97\u8457\u5c0a\u69ae\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u3000\u795e\u62ef\u6551\u842c\u4eba\u7684\u6069\u5178\u5df2\u7d93\u986f\u660e\u51fa\u4f86\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u9019\u6069\u5178\u8a13\u7df4\u6211\u5011\u9664\u53bb\u4e0d\u656c\u8654\u7684\u5fc3\uff0c\u548c\u5c6c\u4e16\u7684\u79c1\u617e\uff0c\u5728\u4eca\u751f\u904e\u8457\u81ea\u5f8b\u3001\u516c\u6b63\u3001\u656c\u8654\u7684\u751f\u6d3b\uff0c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u7b49\u5019\u90a3\u6709\u798f\u7684\u76fc\u671b\uff0c\u5c31\u662f\u6211\u5011\u5049\u5927\u7684\u3000\u795e\uff0c\u6551\u4e3b\u8036\u7a4c\u57fa\u7763\u69ae\u8000\u7684\u986f\u73fe\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4ed6\u70ba\u6211\u5011\u6368\u5df1\uff0c\u70ba\u7684\u662f\u8981\u6551\u8d16\u6211\u5011\u812b\u96e2\u4e00\u5207\u4e0d\u6cd5\u7684\u4e8b\uff0c\u4e26\u4e14\u6f54\u6de8\u6211\u5011\u4f5c\u4ed6\u81ea\u5df1\u7684\u5b50\u6c11\uff0c\u71b1\u5fc3\u5584\u5de5\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u8981\u50b3\u8b1b\u9019\u4e9b\u4e8b\uff0c\u904b\u7528\u5404\u6a23\u7684\u6b0a\u67c4\u53bb\u52f8\u6212\u4eba\uff0c\u8cac\u5099\u4eba\uff1b\u4e0d\u8981\u8b93\u4eba\u8f15\u770b\u4f60\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8981\u5e38\u5e38\u7559\u5fc3\u4f5c\u5584\u5de5\u4f60\u8981\u63d0\u9192\u4ed6\u5011\u670d\u5f9e\u57f7\u653f\u7684\u548c\u638c\u6b0a\u7684\uff0c\u807d\u5f9e\u4ed6\u5011\uff0c\u96a8\u6642\u6e96\u5099\u4f5c\u5404\u7a2e\u5584\u5de5\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4e0d\u53ef\u6bc0\u8b17\u4eba\uff0c\u8981\u8207\u4eba\u7121\u722d\uff0c\u8b19\u606d\u6709\u79ae\uff0c\u5411\u773e\u4eba\u8868\u73fe\u5145\u5206\u6eab\u67d4\u7684\u5fc3\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u6211\u5011\u5f9e\u524d\u4e5f\u662f\u7121\u77e5\u3001\u4e0d\u9806\u670d\u3001\u53d7\u4e86\u8ff7\u60d1\u3001\u88ab\u5404\u7a2e\u79c1\u617e\u548c\u9038\u6a02\u6240\u5974\u5f79\uff0c\u751f\u6d3b\u5728\u60e1\u6bd2\u548c\u5ac9\u5992\u4e4b\u4e2d\uff0c\u662f\u53ef\u618e\u53ef\u60e1\u7684\uff0c\u4e26\u4e14\u4e92\u76f8\u4ec7\u8996\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u7136\u800c\uff0c\u5230\u4e86\u3000\u795e\u6211\u5011\u7684\u6551\u4e3b\u986f\u660e\u4ed6\u7684\u6069\u6148\u548c\u6190\u611b\u7684\u6642\u5019\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4ed6\u5c31\u6551\u4e86\u6211\u5011\uff0c\u4e26\u4e0d\u662f\u7531\u65bc\u6211\u5011\u6240\u884c\u7684\u7fa9\uff0c\u800c\u662f\u7167\u8457\u4ed6\u7684\u6190\u61ab\uff0c\u85c9\u8457\u91cd\u751f\u7684\u6d17\u548c\u8056\u9748\u7684\u66f4\u65b0\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u8056\u9748\u5c31\u662f\u3000\u795e\u85c9\u8457\u6211\u5011\u7684\u6551\u4e3b\u8036\u7a4c\u57fa\u7763\u8c50\u8c50\u5bcc\u5bcc\u6f86\u704c\u5728\u6211\u5011\u8eab\u4e0a\u7684\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f7f\u6211\u5011\u65e2\u7136\u56e0\u8457\u4ed6\u7684\u6069\u5178\u5f97\u7a31\u70ba\u7fa9\uff0c\u5c31\u53ef\u4ee5\u6191\u8457\u6c38\u751f\u7684\u76fc\u671b\u6210\u70ba\u5f8c\u55e3\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u9019\u8a71\u662f\u53ef\u4fe1\u7684\uff0c\u6211\u9858\u4f60\u78ba\u5be6\u5730\u5f37\u8abf\u9019\u4e9b\u4e8b\uff0c\u4f7f\u4fe1\u3000\u795e\u7684\u4eba\u5e38\u5e38\u7559\u5fc3\u4f5c\u5584\u5de5\uff1b\u9019\u4e9b\u90fd\u662f\u7f8e\u4e8b\uff0c\u4e26\u4e14\u662f\u5c0d\u4eba\u6709\u76ca\u7684\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f60\u8981\u9060\u907f\u611a\u6627\u7684\u8faf\u8ad6\u3001\u5bb6\u8b5c\u3001\u7d1b\u722d\u548c\u5f8b\u6cd5\u4e0a\u7684\u722d\u57f7\uff0c\u56e0\u70ba\u9019\u90fd\u662f\u865b\u5984\u7121\u76ca\u7684\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5206\u9580\u7d50\u9ee8\u7684\u4eba\uff0c\u8b66\u6212\u4e00\u5169\u6b21\u4e4b\u5f8c\uff0c\u5c31\u8981\u548c\u4ed6\u7d55\u4ea4\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4f60\u77e5\u9053\u9019\u7a2e\u4eba\u5df2\u7d93\u80cc\u9053\uff0c\u5e38\u5e38\u72af\u7f6a\uff0c\u5b9a\u4e86\u81ea\u5df1\u7684\u7f6a\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u56d1\u5490\u548c\u554f\u5b89\u6211\u6d3e\u4e9e\u63d0\u99ac\u6216\u63a8\u57fa\u53e4\u5230\u4f60\u90a3\u88e1\u53bb\u7684\u6642\u5019\uff0c\u4f60\u8981\u8d95\u5feb\u5230\u5c3c\u54e5\u6ce2\u7acb\u4f86\u898b\u6211\uff0c\u56e0\u70ba\u6211\u5df2\u6c7a\u5b9a\u5728\u90a3\u88e1\u904e\u51ac\u3002\n"
                },
                "13": {
