{
    "version": "cnt",
    "book_name": "2 John",
    "book_nr": "63",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u6211\u9019\u4f5c\u9577\u8001\u7684\uff0c\u5beb\u4fe1\u7d66\u8499\u63c0\u9078\u7684\u592b\u4eba\u548c\u5979\u7684\u5152\u5973\u3002\u4f60\u5011\u662f\u6211\u5728\u771f\u7406\u4e2d\u6240\u611b\u7684\uff1b\u4e0d\u55ae\u662f\u6211\uff0c\u51e1\u8a8d\u8b58\u771f\u7406\u7684\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4e5f\u56e0\u70ba\u771f\u7406\u7684\u7de3\u6545\u611b\u4f60\u5011\u3002\u9019\u771f\u7406\u5728\u6211\u5011\u88e1\u9762\uff0c\u4e5f\u5fc5\u8207\u6211\u5011\u540c\u5728\uff0c\u76f4\u5230\u6c38\u9060\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u9858\u6069\u60e0\u3001\u6190\u61ab\u3001\u5e73\u5b89\uff0c\u5f9e\u7236\u3000\u795e\u548c\u4ed6\u5152\u5b50\u8036\u7a4c\u57fa\u7763\u5728\u771f\u7406\u548c\u611b\u4e2d\u8207\u6211\u5011\u540c\u5728\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u8981\u6309\u771f\u7406\u6191\u611b\u5fc3\u884c\u4e8b\u6211\u770b\u898b\u4f60\u7684\u5152\u5973\u4e2d\uff0c\u6709\u4eba\u7167\u8457\u6211\u5011\u5f9e\u7236\u9818\u53d7\u7684\u547d\u4ee4\u5728\u771f\u7406\u4e2d\u884c\u4e8b\uff0c\u6211\u5c31\u975e\u5e38\u6b23\u6170\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u592b\u4eba\u54ea\uff0c\u6211\u73fe\u5728\u8acb\u6c42\u4f60\uff0c\u6211\u5011\u8981\u5f7c\u6b64\u76f8\u611b\u3002\u6211\u5beb\u7d66\u4f60\u7684\uff0c\u4e0d\u662f\u4e00\u689d\u65b0\u547d\u4ee4\uff0c\u800c\u662f\u6211\u5011\u5f9e\u8d77\u521d\u5c31\u6709\u7684\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6211\u5011\u8981\u7167\u8457\u4ed6\u7684\u547d\u4ee4\u884c\u4e8b\uff0c\u9019\u5c31\u662f\u611b\u3002\u4f60\u5011\u5f9e\u8d77\u521d\u6240\u807d\u898b\u7684\u90a3\u547d\u4ee4\uff0c\u5c31\u662f\u8981\u4f60\u5011\u6191\u8457\u611b\u5fc3\u884c\u4e8b\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u6709\u8a31\u591a\u6b3a\u9a19\u4eba\u7684\u5df2\u7d93\u5728\u4e16\u4e0a\u51fa\u73fe\uff0c\u4ed6\u5011\u5426\u8a8d\u8036\u7a4c\u57fa\u7763\u662f\u6210\u4e86\u8089\u8eab\u4f86\u7684\uff1b\u9019\u5c31\u662f\u90a3\u9a19\u4eba\u7684\u548c\u6575\u57fa\u7763\u7684\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u5011\u8981\u5c0f\u5fc3\uff0c\u4e0d\u8981\u62c6\u6bc0\u6211\u5011\u5df2\u7d93\u5b8c\u6210\u7684\u5de5\u4f5c\uff0c\u537b\u8981\u5f97\u8457\u7f8e\u6eff\u7684\u8cde\u8cdc\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u51e1\u8d8a\u904e\u57fa\u7763\u7684\u6559\u8a13\u53c8\u4e0d\u6301\u5b88\u7684\uff0c\u5c31\u6c92\u6709\u3000\u795e\uff1b\u6301\u5b88\u9019\u6559\u8a13\u7684\uff0c\u5c31\u6709\u7236\u548c\u5b50\u4e86\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5982\u679c\u6709\u4eba\u5230\u4f60\u5011\u90a3\u88e1\uff0c\u4e0d\u50b3\u9019\u6559\u8a13\uff0c\u4f60\u5011\u5c31\u4e0d\u8981\u63a5\u5f85\u4ed6\u5230\u5bb6\u88e1\uff0c\u4e5f\u4e0d\u8981\u554f\u5019\u4ed6\uff1b\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u56e0\u70ba\u554f\u5019\u4ed6\u7684\uff0c\u5c31\u5728\u4ed6\u7684\u60e1\u4e8b\u4e0a\u6709\u5206\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u554f\u5019\u7684\u8a71\u6211\u9084\u6709\u8a31\u591a\u8a71\u8981\u5beb\u7d66\u4f60\u5011\uff0c\u53ef\u662f\u6211\u4e0d\u60f3\u501f\u7528\u7d19\u58a8\uff0c\u53ea\u662f\u76fc\u671b\u5230\u4f60\u5011\u90a3\u88e1\u7576\u9762\u8ac7\u8ac7\uff0c\u597d\u8b93\u6211\u5011\u7684\u559c\u6a02\u5145\u8db3\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u4f60\u90a3\u8499\u63c0\u9078\u7684\u59ca\u59b9\u7684\u5152\u5973\u90fd\u554f\u5019\u4f60\u3002\n"
                }
            }
        }
    }
}