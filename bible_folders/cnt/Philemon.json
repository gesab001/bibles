{
    "version": "cnt",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u70ba\u57fa\u7763\u8036\u7a4c\u88ab\u56da\u7981\u7684\u4fdd\u7f85\uff0c\u548c\u63d0\u6469\u592a\u5f1f\u5144\uff0c\u5beb\u4fe1\u7d66\u6211\u5011\u6240\u611b\u7684\uff0c\u53c8\u4e00\u540c\u4f5c\u5de5\u7684\u8153\u5229\u9580\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u548c\u4e9e\u8153\u4e9e\u59ca\u59b9\uff0c\u4e26\u6211\u5011\u7684\u6230\u53cb\u4e9e\u57fa\u5e03\uff0c\u4ee5\u53ca\u5728\u4f60\u5bb6\u88e1\u7684\u6559\u6703\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u9858\u6069\u60e0\u5e73\u5b89\u5f9e\u6211\u5011\u7684\u7236\u3000\u795e\u548c\u4e3b\u8036\u7a4c\u57fa\u7763\u81e8\u5230\u4f60\u5011\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u7a31\u8b9a\u8153\u5229\u9580\u6211\u807d\u898b\u4f60\u5c0d\u4e3b\u8036\u7a4c\u548c\u773e\u8056\u5f92\u6709\u611b\u5fc3\u548c\u4fe1\u5fc3\uff0c\u6211\u6bcf\u9022\u79b1\u544a\u63d0\u5230\u4f60\u7684\u6642\u5019\uff0c\u5c31\u5e38\u5e38\u611f\u8b1d\u6211\u7684\u3000\u795e\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": " \n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u9858\u4f60\u8207\u773e\u4eba\u5206\u4eab\u4f60\u7684\u4fe1\u5fc3\u7684\u6642\u5019\uff0c\u6703\u7522\u751f\u529f\u6548\uff0c\u4f7f\u6211\u5011\u53ef\u4ee5\u77e5\u9053\u5728\u6211\u5011\u4e2d\u9593\u7684\u4e00\u5207\u5584\u4e8b\uff0c\u90fd\u662f\u70ba\u57fa\u7763\u4f5c\u7684\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5f1f\u5144\u554a\uff0c\u773e\u8056\u5f92\u7684\u5fc3\u65e2\u7136\u5f9e\u4f60\u5f97\u5230\u8212\u66a2\uff0c\u6211\u4e5f\u56e0\u4f60\u7684\u611b\u5fc3\uff0c\u5f97\u5230\u66f4\u5927\u7684\u559c\u6a02\u548c\u5b89\u6170\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u70ba\u6b50\u5c3c\u897f\u6155\u8acb\u6c42\u6211\u5728\u57fa\u7763\u88e1\u96d6\u7136\u53ef\u4ee5\u653e\u81bd\u5429\u5490\u4f60\u4f5c\u61c9\u4f5c\u7684\u4e8b\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7136\u800c\u50cf\u6211\u9019\u4e0a\u4e86\u5e74\u7d00\u7684\u4fdd\u7f85\uff0c\u73fe\u5728\u53c8\u662f\u70ba\u57fa\u7763\u8036\u7a4c\u88ab\u56da\u7981\u7684\uff0c\u5be7\u9858\u6191\u8457\u611b\u5fc3\u8acb\u6c42\u4f60\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5c31\u662f\u70ba\u6211\u5728\u56da\u7981\u6642\u6240\u751f\u7684\u5152\u5b50\u6b50\u5c3c\u897f\u6155\u6c42\u4f60\uff1b\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4ed6\u5f9e\u524d\u5c0d\u4f60\u6c92\u6709\u751a\u9ebc\u597d\u8655\uff0c\u4f46\u73fe\u5728\u5c0d\u4f60\u6211\u90fd\u6709\u597d\u8655\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u73fe\u5728\u6253\u767c\u4ed6\u89aa\u81ea\u56de\u5230\u4f60\u90a3\u88e1\u53bb\uff1b\u4ed6\u662f\u6211\u6240\u5fc3\u611b\u7684\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u672c\u4f86\u60f3\u628a\u4ed6\u7559\u5728\u6211\u9019\u88e1\uff0c\u4f7f\u4ed6\u5728\u6211\u70ba\u798f\u97f3\u88ab\u56da\u7981\u6642\uff0c\u53ef\u4ee5\u66ff\u4f60\u670d\u4e8b\u6211\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f46\u9084\u6c92\u6709\u5f97\u5230\u4f60\u7684\u540c\u610f\uff0c\u6211\u5c31\u4e0d\u9858\u610f\u9019\u6a23\u4f5c\uff0c\u597d\u53eb\u4f60\u7684\u5584\u884c\u4e0d\u662f\u51fa\u65bc\u52c9\u5f37\uff0c\u800c\u662f\u51fa\u65bc\u7518\u5fc3\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4e5f\u8a31\u4ed6\u66ab\u6642\u96e2\u958b\u4f60\uff0c\u6b63\u662f\u70ba\u4e86\u4f7f\u4f60\u6c38\u9060\u5f97\u8457\u4ed6\uff0c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4e0d\u518d\u662f\u5974\u50d5\uff0c\u800c\u662f\u9ad8\u904e\u5974\u50d5\uff0c\u662f\u89aa\u611b\u7684\u5f1f\u5144\u3002\u5c0d\u6211\u56fa\u7136\u662f\u9019\u6a23\uff0c\u5c0d\u4f60\u4f86\u8aaa\uff0c\u4e0d\u8ad6\u6309\u8089\u8eab\u6216\u5728\u4e3b\u5167\u7684\u95dc\u4fc2\uff0c\u66f4\u662f\u9019\u6a23\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u6240\u4ee5\uff0c\u4f60\u8981\u662f\u628a\u6211\u770b\u4f5c\u540c\u4f34\uff0c\u5c31\u63a5\u7d0d\u4ed6\u597d\u50cf\u63a5\u7d0d\u6211\u4e00\u6a23\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u5982\u679c\u4ed6\u4f7f\u4f60\u53d7\u4e86\u640d\u5931\uff0c\u6216\u6b20\u4f60\u751a\u9ebc\uff0c\u90fd\u8a18\u5728\u6211\u7684\u5e33\u4e0a\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u201c\u6211\u5fc5\u511f\u9084\u201d\uff0c\u9019\u662f\u6211\u4fdd\u7f85\u89aa\u624b\u5beb\u7684\u3002\u7528\u4e0d\u8457\u6211\u8aaa\uff0c\u751a\u81f3\u4f60\u7684\u751f\u547d\uff0c\u4f60\u4e5f\u662f\u6b20\u6211\u7684\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u6240\u4ee5\u5f1f\u5144\u554a\uff01\u8b93\u6211\u5728\u4e3b\u88e1\u5f97\u5230\u4f60\u7684\u5e6b\u52a9\uff0c\u4f7f\u6211\u7684\u5fc3\u5728\u57fa\u7763\u88e1\u5f97\u8457\u66a2\u5feb\u3002\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6211\u6df1\u4fe1\u4f60\u6703\u807d\u5f9e\uff0c\u4e5f\u77e5\u9053\u4f60\u6240\u4f5c\u7684\u5fc5\u8d85\u904e\u6211\u6240\u8aaa\u7684\uff0c\u56e0\u6b64\u624d\u5beb\u4fe1\u7d66\u4f60\u3002\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u540c\u6642\uff0c\u9084\u8acb\u4f60\u70ba\u6211\u9810\u5099\u4f4f\u7684\u5730\u65b9\uff0c\u56e0\u70ba\u6211\u76fc\u671b\u85c9\u8457\u4f60\u5011\u7684\u79b1\u544a\uff0c\u53ef\u4ee5\u7372\u5f97\u91cb\u653e\u5230\u4f60\u5011\u90a3\u88e1\u53bb\u3002\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u554f\u5019\u7684\u8a71\u70ba\u57fa\u7763\u8036\u7a4c\u7684\u7de3\u6545\u548c\u6211\u4e00\u540c\u5750\u76e3\u7684\u4ee5\u5df4\u5f17\uff0c\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u4ee5\u53ca\u6211\u7684\u540c\u5de5\u99ac\u53ef\u3001\u4e9e\u91cc\u9054\u53e4\u3001\u5e95\u99ac\u3001\u8def\u52a0\u90fd\u554f\u5019\u4f60\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u9858\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u6069\u60e0\u5e38\u8207\u4f60\u5011\u540c\u5728\u3002\uff08\u201c\u8207\u4f60\u5011\u540c\u5728\u201d\u539f\u6587\u4f5c\u201c\u8207\u4f60\u5011\u7684\u5fc3\u9748\u540c\u5728\u201d\uff1b\u597d\u4e9b\u6284\u672c\u5728\u9019\u66f8\u672b\u6709\u201c\u963f\u5011\u201d\u4e00\u8a5e\u3002\uff09\n"
                }
            }
        }
    }
}