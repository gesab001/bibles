        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8207\u4e16\u4fd7\u70ba\u53cb\u5c31\u662f\u8207\u3000\u795e\u70ba\u6575\u4f60\u5011\u4e2d\u9593\u7684\u722d\u57f7\u548c\u6253\u9b25\u662f\u5f9e\u54ea\u88e1\u4f86\u7684\u5462\uff1f\u4e0d\u662f\u5f9e\u4f60\u5011\u80a2\u9ad4\u4e2d\u597d\u9b25\u7684\u79c1\u617e\u4f86\u7684\u55ce\uff1f\n"
