                "10": {
                    "verse_nr": 10,
                    "verse": "\u4f60\u5011\u52d9\u8981\u5728\u4e3b\u9762\u524d\u8b19\u5351\uff0c\u4ed6\u5c31\u4f7f\u4f60\u5011\u9ad8\u5347\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u4e0d\u53ef\u5f7c\u6b64\u6279\u8a55\u5f1f\u5144\u5011\uff0c\u4e0d\u8981\u4e92\u76f8\u6bc0\u8b17\uff1b\u4eba\u82e5\u6bc0\u8b17\u5f1f\u5144\uff0c\u6216\u5224\u65b7\u5f1f\u5144\uff0c\u5c31\u662f\u6bc0\u8b17\u5f8b\u6cd5\u3001\u5224\u65b7\u5f8b\u6cd5\u4e86\u3002\u5982\u679c\u4f60\u5224\u65b7\u5f8b\u6cd5\uff0c\u5c31\u4e0d\u662f\u5be6\u884c\u5f8b\u6cd5\u7684\u4eba\uff0c\u800c\u662f\u5be9\u5224\u5b98\u4e86\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u7acb\u6cd5\u7684\uff0c\u5be9\u5224\u7684\uff0c\u53ea\u6709\u4e00\u4f4d\uff0c\u5c31\u662f\u90a3\u80fd\u62ef\u6551\u4eba\uff0c\u4e5f\u80fd\u6bc0\u6ec5\u4eba\u7684\u3000\u795e\uff1b\u4f60\u9019\u5224\u65b7\u9130\u820d\u7684\uff0c\u4f60\u662f\u8ab0\u5462\uff1f\n"
