                "13": {
                    "verse_nr": 13,
                    "verse": "\u4e0d\u53ef\u81ea\u8a87\u81ea\u6043\u4f60\u5011\u8aaa\uff1a\u201c\u4eca\u5929\u6216\u660e\u5929\uff0c\u6211\u5011\u8981\u5230\u67d0\u57ce\u53bb\uff0c\u5728\u90a3\u88e1\u4f4f\u4e00\u5e74\uff0c\u4f5c\u751f\u610f\u8cfa\u9322\u3002\u201d\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u5176\u5be6\u660e\u5929\u600e\u6a23\uff0c\u4f60\u5011\u4e26\u4e0d\u77e5\u9053\u3002\u4f60\u5011\u7684\u751f\u547d\u662f\u751a\u9ebc\u5462\uff1f\u4f60\u5011\u672c\u4f86\u662f\u904e\u773c\u96f2\u7159\uff0c\u8f49\u77ac\u4e4b\u9593\u5c31\u6d88\u901d\u4e86\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u4f60\u5011\u5012\u4e0d\u5982\u8aaa\uff1a\u201c\u4e3b\u82e5\u9858\u610f\uff0c\u6211\u5011\u5c31\u53ef\u4ee5\u6d3b\u8457\uff0c\u4f5c\u9019\u4e8b\u6216\u4f5c\u90a3\u4e8b\u3002\u201d\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u4f46\u73fe\u5728\u4f60\u5011\u7adf\u7136\u5f35\u72c2\u81ea\u8a87\uff1b\u9019\u4e00\u5207\u81ea\u8a87\uff0c\u90fd\u662f\u90aa\u60e1\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4eba\u82e5\u77e5\u9053\u8a72\u884c\u5584\u4e8b\uff0c\u537b\u4e0d\u53bb\u884c\uff0c\u9019\u5c31\u662f\u4ed6\u7684\u7f6a\u4e86\u3002\n"
                }
            }
        },
        "5": {
            "chapter_nr": 5,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8b66\u544a\u4e0d\u7fa9\u7684\u6709\u9322\u4eba\u4f60\u5011\u5bcc\u6709\u7684\u4eba\u54ea\uff0c\u61c9\u7576\u70ba\u90a3\u5c07\u8981\u81e8\u5230\u4f60\u5011\u7684\u707d\u798d\u54ed\u6ce3\u54c0\u865f\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u4f60\u5011\u7684\u8ca1\u7269\u673d\u58de\u4e86\uff0c\u4f60\u5011\u7684\u8863\u670d\u7d66\u86c0\u4e86\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4f60\u5011\u7684\u91d1\u9280\u751f\u93fd\uff0c\u9019\u93fd\u8981\u6210\u70ba\u63a7\u544a\u4f60\u5011\u7684\u9435\u8b49\uff0c\u53c8\u8981\u50cf\u706b\u4e00\u6a23\u541e\u5403\u4f60\u5011\u7684\u8089\u3002\u4f60\u5011\u7adf\u7136\u5728\u9019\u672b\u4e16\u7a4d\u805a\u8ca1\u5bf6\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u770b\u54ea\uff0c\u5de5\u4eba\u70ba\u4f60\u5011\u6536\u5272\u838a\u7a3c\uff0c\u4f60\u5011\u7adf\u7136\u524b\u6263\u4ed6\u5011\u7684\u5de5\u8cc7\uff1b\u90a3\u5de5\u8cc7\u5fc5\u70ba\u4ed6\u5011\u547c\u51a4\uff1b\u6536\u5272\u8005\u7684\u547c\u8072\uff0c\u5df2\u7d93\u9054\u5230\u842c\u8ecd\u4e4b\u4e3b\u7684\u8033\u4e2d\u4e86\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u4f60\u5011\u5728\u4e16\u4e0a\u7aae\u5962\u6975\u4f88\uff0c\u990a\u80a5\u4e86\u81ea\u5df1\uff0c\u7adf\u4e0d\u77e5\u5c60\u5bb0\u7684\u65e5\u5b50\u5230\u4e86\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4f60\u5011\u628a\u7fa9\u4eba\u5b9a\u7f6a\u6bba\u5bb3\uff0c\u4f46\u4ed6\u4e26\u6c92\u6709\u53cd\u6297\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u5fcd\u8010\u7684\u4eba\u6709\u798f\u4e86\u6240\u4ee5\uff0c\u5f1f\u5144\u5011\uff0c\u4f60\u5011\u61c9\u7576\u5fcd\u8010\uff0c\u76f4\u5230\u4e3b\u4f86\u3002\u770b\u54ea\uff0c\u8fb2\u592b\u7b49\u5f85\u8457\u5730\u88e1\u5bf6\u8cb4\u7684\u51fa\u7522\uff0c\u70ba\u5b83\u5fcd\u8010\uff0c\u76f4\u5230\u7372\u5f97\u79cb\u9716\u6625\u96e8\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f60\u5011\u4e5f\u61c9\u7576\u5fcd\u8010\uff0c\u5805\u5b9a\u81ea\u5df1\u7684\u5fc3\uff1b\u56e0\u70ba\u4e3b\u518d\u4f86\u7684\u65e5\u5b50\u8fd1\u4e86\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u5f1f\u5144\u5011\uff0c\u4e0d\u8981\u5f7c\u6b64\u62b1\u6028\uff0c\u514d\u5f97\u4f60\u5011\u53d7\u5be9\u5224\u3002\u770b\u54ea\uff0c\u5be9\u5224\u7684\u4e3b\u5df2\u7d93\u7ad9\u5728\u9580\u524d\u4e86\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u5f1f\u5144\u5011\uff0c\u4f60\u5011\u61c9\u7576\u6548\u6cd5\u5949\u4e3b\u7684\u540d\u8aaa\u8a71\u7684\u5148\u77e5\uff0c\u4ee5\u4ed6\u5011\u70ba\u53d7\u82e6\u5fcd\u8010\u7684\u699c\u6a23\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u770b\u54ea\uff0c\u90a3\u4e9b\u5fcd\u8010\u7684\u4eba\uff0c\u6211\u5011\u7a31\u4ed6\u5011\u662f\u6709\u798f\u7684\uff1b\u4f60\u5011\u807d\u904e\u7d04\u4f2f\u7684\u5fcd\u8010\uff0c\u4e5f\u770b\u898b\u4e86\u4e3b\u8cdc\u7d66\u4ed6\u7684\u7d50\u5c40\uff0c\u77e5\u9053\u4e3b\u662f\u6eff\u6709\u6190\u61ab\u548c\u4ec1\u6148\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u7684\u5f1f\u5144\u5011\uff0c\u6700\u8981\u7dca\u7684\u662f\u4e0d\u53ef\u8d77\u8a93\uff1a\u4e0d\u53ef\u6307\u8457\u5929\u8d77\u8a93\uff0c\u4e5f\u4e0d\u53ef\u6307\u8457\u5730\u8d77\u8a93\uff0c\u4efb\u4f55\u7684\u8a93\u90fd\u7576\u7981\u7d55\u3002\u4f60\u5011\u7684\u8a71\uff0c\u662f\u5c31\u8aaa\u662f\uff0c\u4e0d\u662f\u5c31\u8aaa\u4e0d\u662f\uff1b\u514d\u5f97\u4f60\u5011\u843d\u5728\u5be9\u5224\u4e4b\u4e0b\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u7fa9\u4eba\u7684\u7948\u79b1\u5927\u6709\u529f\u6548\u4f60\u5011\u4e2d\u9593\u6709\u4eba\u53d7\u82e6\u55ce\uff1f\u4ed6\u5c31\u61c9\u8a72\u79b1\u544a\u3002\u6709\u4eba\u5fc3\u60c5\u6109\u5feb\u55ce\uff1f\u4ed6\u5c31\u61c9\u8a72\u6b4c\u980c\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u5011\u4e2d\u9593\u6709\u4eba\u60a3\u75c5\u55ce\uff1f\u4ed6\u5c31\u61c9\u8a72\u8acb\u6559\u6703\u7684\u9577\u8001\u4f86\uff0c\u8b93\u4ed6\u5011\u5949\u4e3b\u7684\u540d\u70ba\u4ed6\u62b9\u6cb9\u7948\u79b1\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u51fa\u65bc\u4fe1\u5fc3\u7684\u7948\u79b1\uff0c\u53ef\u4ee5\u4f7f\u75c5\u4eba\u5eb7\u5fa9\uff0c\u4e3b\u5fc5\u53eb\u4ed6\u8d77\u4f86\uff1b\u4ed6\u82e5\u72af\u4e86\u7f6a\uff0c\u4e5f\u5fc5\u8499\u8d66\u514d\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u6240\u4ee5\u4f60\u5011\u61c9\u7576\u5f7c\u6b64\u8a8d\u7f6a\uff0c\u4e92\u76f8\u4ee3\u6c42\uff0c\u9019\u6a23\u4f60\u5011\u5c31\u53ef\u4ee5\u75ca\u6108\u3002\u7fa9\u4eba\u7948\u79b1\u6240\u767c\u51fa\u7684\u529b\u91cf\uff0c\u662f\u5927\u6709\u529f\u6548\u7684\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4ee5\u5229\u4e9e\u662f\u8207\u6211\u5011\u6027\u60c5\u76f8\u540c\u7684\u4eba\uff1b\u4ed6\u61c7\u5207\u7948\u6c42\u4e0d\u8981\u4e0b\u96e8\uff0c\u5730\u4e0a\u5c31\u4e09\u5e74\u96f6\u516d\u500b\u6708\u6c92\u6709\u96e8\uff1b\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u4ed6\u53c8\u7948\u79b1\uff0c\u5929\u5c31\u4e0b\u96e8\uff0c\u5730\u5c31\u751f\u51fa\u571f\u7522\u4f86\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u6211\u7684\u5f1f\u5144\u5011\uff0c\u4f60\u5011\u4e2d\u9593\u82e5\u6709\u4eba\u88ab\u9a19\u96e2\u958b\u4e86\u771f\u9053\uff0c\u5982\u679c\u6709\u4eba\u4f7f\u4ed6\u56de\u982d\uff0c\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4f60\u5011\u61c9\u8a72\u77e5\u9053\uff0c\u90a3\u4f7f\u7f6a\u4eba\u5f9e\u6b67\u9014\u4e0a\u8f49\u56de\u7684\uff0c\u5c31\u6703\u62ef\u6551\u4ed6\u7684\u9748\u9b42\u812b\u96e2\u6b7b\u4ea1\uff0c\u4e5f\u6703\u906e\u84cb\u8a31\u591a\u7f6a\u60e1\u3002\n"
                }
            }
        }
    }
