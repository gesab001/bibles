                "24": {
                    "verse_nr": 24,
                    "verse": "\u6709\u4e9b\u4eba\u7684\u7f6a\u662f\u660e\u986f\u7684\uff0c\u5c31\u5148\u53d7\u5be9\u5224\uff1b\u6709\u4e9b\u4eba\u7684\u7f6a\u662f\u5f8c\u4f86\u624d\u986f\u9732\u51fa\u4f86\u7684\u3002\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u7167\u6a23\uff0c\u5584\u884c\u4e5f\u662f\u660e\u986f\u7684\uff0c\u5c31\u7b97\u4e0d\u660e\u986f\uff0c\u4e5f\u4e0d\u80fd\u96b1\u85cf\u3002\n"
                }
            }
        },
        "6": {
            "chapter_nr": 6,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u51e1\u8ca0\u8edb\u4f5c\u5974\u50d5\u7684\uff0c\u61c9\u7576\u770b\u81ea\u5df1\u7684\u4e3b\u4eba\u662f\u914d\u53d7\u5341\u5206\u7684\u656c\u91cd\uff0c\u514d\u5f97\u3000\u795e\u7684\u540d\u548c\u9053\u7406\u88ab\u4eba\u893b\u7006\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5974\u50d5\u6709\u4fe1\u4e3b\u7684\u4e3b\u4eba\uff0c\u4e0d\u53ef\u56e0\u70ba\u4ed6\u5011\u662f\u5f1f\u5144\u800c\u8f15\u770b\u4ed6\u5011\uff1b\u5012\u8981\u52a0\u610f\u670d\u4e8b\u4ed6\u5011\uff0c\u56e0\u70ba\u9019\u4e9b\u53d7\u5230\u670d\u4e8b\u7684\u76ca\u8655\u7684\uff0c\u662f\u4fe1\u4e3b\u8499\u611b\u7684\u4eba\u3002\u4f60\u8981\u628a\u9019\u4e9b\u4e8b\u6559\u5c0e\u4eba\uff0c\u52f8\u52c9\u4eba\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u656c\u8654\u8207\u77e5\u8db3\u5982\u679c\u6709\u4eba\u50b3\u5225\u7684\u6559\u7fa9\uff0c\u4e0d\u63a5\u53d7\u6211\u5011\u7684\u4e3b\u8036\u7a4c\u57fa\u7763\u7d14\u6b63\u7684\u8a71\u8a9e\uff0c\u548c\u90a3\u656c\u8654\u7684\u9053\u7406\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u662f\u81ea\u9ad8\u81ea\u5927\uff0c\u4e00\u7121\u6240\u77e5\uff0c\u53cd\u800c\u5c08\u597d\u554f\u96e3\u722d\u8faf\uff0c\u7531\u6b64\u7522\u751f\u5992\u5fcc\u3001\u7d1b\u722d\u3001\u6bc0\u8b17\u3001\u60e1\u610f\u7684\u731c\u7591\uff0c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u5c31\u548c\u90a3\u4e9b\u5fc3\u8853\u6557\u58de\uff0c\u55aa\u5931\u771f\u7406\u7684\u4eba\u4e0d\u65b7\u5730\u722d\u7af6\u3002\u4ed6\u5011\u8996\u656c\u8654\u70ba\u5f97\u5229\u7684\u9580\u8def\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u5176\u5be6\u656c\u8654\u800c\u53c8\u77e5\u8db3\uff0c\u5c31\u662f\u5f97\u5927\u5229\u7684\u9014\u5f91\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u56e0\u70ba\u6211\u5011\u6c92\u6709\u5e36\u751a\u9ebc\u5230\u4e16\u4e0a\u4f86\uff0c\u4e5f\u4e0d\u80fd\u5e36\u751a\u9ebc\u53bb\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u53ea\u8981\u6709\u8863\u6709\u98df\uff0c\u5c31\u61c9\u7576\u77e5\u8db3\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u4f46\u90a3\u4e9b\u60f3\u8981\u767c\u8ca1\u7684\u4eba\uff0c\u5c31\u843d\u5728\u8a66\u63a2\u4e2d\u548c\u9677\u9631\u88e1\uff1b\u53c8\u843d\u5728\u8a31\u591a\u7121\u77e5\u800c\u6709\u5bb3\u7684\u79c1\u617e\u88e1\uff0c\u4f7f\u4eba\u6c89\u6dea\u5728\u6557\u58de\u548c\u6ec5\u4ea1\u4e2d\uff1b\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u56e0\u70ba\u8caa\u8ca1\u662f\u842c\u60e1\u4e4b\u6839\u3002\u6709\u4eba\u8caa\u611b\u9322\u8ca1\uff0c\u5c31\u88ab\u5f15\u8a98\u96e2\u958b\u771f\u9053\uff0c\u7528\u8a31\u591a\u75db\u82e6\u628a\u81ea\u5df1\u523a\u900f\u4e86\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u8981\u70ba\u4fe1\u4ef0\u6253\u90a3\u7f8e\u597d\u7684\u4ed7\u4f46\u4f60\u9019\u5c6c\u3000\u795e\u7684\u4eba\u554a\uff0c\u61c9\u8a72\u9003\u907f\u9019\u4e9b\u4e8b\uff0c\u8981\u8ffd\u6c42\u516c\u7fa9\u3001\u656c\u8654\u3001\u4fe1\u5fc3\u3001\u611b\u5fc3\u3001\u5fcd\u8010\u548c\u6eab\u67d4\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u8981\u70ba\u4fe1\u4ef0\u6253\u90a3\u7f8e\u597d\u7684\u4ed7\uff0c\u6301\u5b9a\u6c38\u751f\uff1b\u4f60\u662f\u70ba\u9019\u6c38\u751f\u800c\u8499\u53ec\u7684\uff0c\u53c8\u5728\u8a31\u591a\u7684\u8b49\u4eba\u9762\u524d\u627f\u8a8d\u904e\u7f8e\u597d\u7684\u4fe1\u4ef0\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u5728\u8cdc\u751f\u547d\u7d66\u842c\u7269\u7684\u3000\u795e\u9762\u524d\uff0c\u4e26\u90a3\u5728\u672c\u4e1f\uff0e\u5f7c\u62c9\u591a\u9762\u524d\u898b\u8b49\u904e\u7f8e\u597d\u7684\u4fe1\u4ef0\u7684\u57fa\u7763\u8036\u7a4c\u9762\u524d\u56d1\u5490\u4f60\uff0c\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4f60\u7576\u6beb\u7121\u73b7\u6c61\uff0c\u7121\u53ef\u6307\u6458\u5730\u6301\u5b88\u9019\u547d\u4ee4\uff0c\u76f4\u5230\u6211\u5011\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u986f\u73fe\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u5230\u4e86\u9069\u7576\u7684\u6642\u5019\uff0c\u90a3\u53ef\u7a31\u980c\u7684\u3001\u7368\u4e00\u7684\u5168\u80fd\u8005\uff0c\u842c\u738b\u4e4b\u738b\uff0c\u842c\u4e3b\u4e4b\u4e3b\uff0c\u5fc5\u628a\u57fa\u7763\u7684\u986f\u73fe\u8868\u660e\u51fa\u4f86\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u53ea\u6709\u4ed6\u6c38\u9060\u4e0d\u6b7b\uff0c\u4f4f\u5728\u4e0d\u80fd\u63a5\u8fd1\u7684\u5149\u88e1\uff0c\u6c92\u6709\u4eba\u898b\u904e\u4ed6\uff0c\u4eba\u4e5f\u4e0d\u80fd\u770b\u898b\u4ed6\u3002\u9858\u5c0a\u69ae\u548c\u6c38\u9060\u7684\u6b0a\u80fd\u90fd\u6b78\u7d66\u4ed6\u3002\u963f\u5011\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f60\u8981\u56d1\u5490\u90a3\u4e9b\u4eca\u4e16\u5bcc\u6709\u7684\u4eba\uff0c\u53eb\u4ed6\u5011\u4e0d\u8981\u5fc3\u9ad8\u6c23\u50b2\uff0c\u4e5f\u4e0d\u8981\u5bc4\u671b\u5728\u6d6e\u52d5\u7684\u8ca1\u5bcc\u4e0a\uff0c\u537b\u8981\u4ef0\u671b\u90a3\u539a\u8cdc\u767e\u7269\u7d66\u6211\u5011\u4eab\u7528\u7684\u3000\u795e\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u53c8\u8981\u56d1\u5490\u4ed6\u5011\u884c\u5584\uff0c\u5728\u5584\u4e8b\u4e0a\u5bcc\u8db3\uff0c\u6177\u6168\u597d\u65bd\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u9019\u6a23\uff0c\u5c31\u70ba\u81ea\u5df1\u5728\u4f86\u4e16\u7a4d\u805a\u8ca1\u5bcc\uff0c\u4f5c\u7f8e\u597d\u7684\u57fa\u790e\uff0c\u597d\u53eb\u4ed6\u5011\u80fd\u5920\u5f97\u8457\u90a3\u771f\u6b63\u7684\u751f\u547d\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u63d0\u6469\u592a\u554a\uff0c\u4f60\u8981\u4fdd\u5b88\u6240\u4ea4\u8a17\u4f60\u7684\uff0c\u907f\u514d\u4e16\u4fd7\u7684\u7a7a\u8ac7\u548c\u5192\u7a31\u662f\u77e5\u8b58\u7684\u90a3\u7a2e\u53cd\u8abf\uff1b\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u6709\u4e9b\u4eba\u81ea\u7a31\u6709\u9019\u77e5\u8b58\uff0c\u5c31\u504f\u96e2\u4e86\u771f\u9053\u3002\u9858\u6069\u60e0\u8207\u4f60\u5011\u540c\u5728\u3002\n"
                }
            }
        }
    }
}