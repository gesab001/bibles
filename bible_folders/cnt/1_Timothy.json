    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u554f\u5b89\u5949\u6211\u5011\u7684\u6551\u4e3b\u3000\u795e\uff0c\u548c\u6211\u5011\u7684\u76fc\u671b\u57fa\u7763\u8036\u7a4c\u7684\u547d\u4ee4\uff0c\u4f5c\u57fa\u7763\u8036\u7a4c\u4f7f\u5f92\u7684\u4fdd\u7f85\uff0c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u5beb\u4fe1\u7d66\u56e0\u4fe1\u4e3b\u800c\u4f5c\u6211\u771f\u5152\u5b50\u7684\u63d0\u6469\u592a\u3002\u9858\u6069\u60e0\u3001\u6190\u61ab\u3001\u5e73\u5b89\u5f9e\u7236\u3000\u795e\u548c\u6211\u5011\u7684\u4e3b\u57fa\u7763\u8036\u7a4c\u81e8\u5230\u4f60\u3002\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u63d0\u9632\u5225\u7684\u6559\u7fa9\u6211\u5f80\u99ac\u5176\u9813\u53bb\u7684\u6642\u5019\uff0c\u66fe\u7d93\u52f8\u4f60\u7559\u5728\u4ee5\u5f17\u6240\uff0c\u70ba\u8981\u56d1\u5490\u67d0\u4e9b\u4eba\uff0c\u4e0d\u53ef\u50b3\u5225\u7684\u6559\u7fa9\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4e5f\u4e0d\u53ef\u6c89\u8ff7\u65bc\u7121\u7a3d\u4e4b\u8ac7\u548c\u7121\u7aae\u7684\u5bb6\u8b5c\uff1b\u9019\u4e9b\u4e8b\u53ea\u80fd\u5f15\u8d77\u722d\u8ad6\uff0c\u5c0d\u65bc\u3000\u795e\u5728\u4fe1\u4ef0\u4e0a\u6240\u5b9a\u7684\u8a08\u5283\u662f\u6beb\u7121\u5e6b\u52a9\u7684\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u9019\u56d1\u5490\u7684\u76ee\u7684\u662f\u51fa\u65bc\u611b\uff1b\u9019\u611b\u662f\u767c\u81ea\u7d14\u6f54\u7684\u5fc3\u3001\u7121\u6127\u7684\u826f\u5fc3\u548c\u7121\u507d\u7684\u4fe1\u5fc3\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u6709\u4e9b\u4eba\u504f\u96e2\u4e86\u9019\u4e9b\uff0c\u5c31\u8f49\u5411\u7121\u610f\u7fa9\u7684\u8faf\u8ad6\uff0c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u60f3\u8981\u4f5c\u5f8b\u6cd5\u6559\u5e2b\uff0c\u537b\u4e0d\u660e\u767d\u81ea\u5df1\u8b1b\u7684\u662f\u751a\u9ebc\uff0c\u4e3b\u5f35\u7684\u53c8\u662f\u751a\u9ebc\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u6211\u5011\u77e5\u9053\u5f8b\u6cd5\u662f\u597d\u7684\uff0c\u53ea\u8981\u61c9\u7528\u5f97\u6070\u7576\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u8981\u77e5\u9053\u5f8b\u6cd5\u672c\u4f86\u4e0d\u662f\u70ba\u7fa9\u4eba\u8a2d\u7acb\u7684\uff0c\u800c\u662f\u70ba\u90a3\u4e9b\u7121\u6cd5\u7121\u5929\u548c\u653e\u8569\u4e0d\u7f88\u7684\u3001\u4e0d\u656c\u8654\u548c\u72af\u7f6a\u7684\u3001\u4e0d\u8056\u6f54\u548c\u4e16\u4fd7\u7684\u3001\u5f12\u7236\u6bcd\u548c\u6bba\u4eba\u7684\u3001\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u6deb\u4e82\u7684\u3001\u89aa\u7537\u8272\u7684\u3001\u62d0\u5e36\u4eba\u53e3\u7684\u3001\u8aaa\u8b0a\u8a71\u7684\u3001\u767c\u5047\u8a93\u7684\uff0c\u4ee5\u53ca\u70ba\u5176\u4ed6\u62b5\u64cb\u7d14\u6b63\u6559\u8a13\u7684\u4eba\u8a2d\u7acb\u7684\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u9019\u662f\u7167\u8457\u53ef\u7a31\u980c\u7684\u3001\u69ae\u8000\u7684\u3000\u795e\u6240\u4ea4\u8a17\u6211\u7684\u798f\u97f3\u8aaa\u7684\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u57fa\u7763\u8036\u7a4c\u964d\u4e16\u70ba\u8981\u62ef\u6551\u7f6a\u4eba\u6211\u611f\u8b1d\u90a3\u8cdc\u6211\u80fd\u529b\u7684\u3001\u6211\u5011\u7684\u4e3b\u57fa\u7763\u8036\u7a4c\uff0c\u56e0\u70ba\u4ed6\u8a8d\u70ba\u6211\u6709\u5fe0\u5fc3\uff0c\u6d3e\u6211\u670d\u4e8b\u4ed6\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u6211\u5f9e\u524d\u662f\u893b\u7006\u3000\u795e\u7684\u3001\u8feb\u5bb3\u4eba\u7684\u3001\u51cc\u8fb1\u4eba\u7684\uff0c\u7136\u800c\u6211\u9084\u8499\u4e86\u6190\u61ab\uff0c\u56e0\u70ba\u6211\u662f\u5728\u4e0d\u4fe1\u7684\u6642\u5019\uff0c\u7531\u65bc\u7121\u77e5\u800c\u4f5c\u7684\u3002\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u6211\u5011\u4e3b\u7684\u6069\u5178\uff0c\u96a8\u8457\u5728\u57fa\u7763\u8036\u7a4c\u88e1\u7684\u4fe1\u5fc3\u548c\u611b\u5fc3\uff0c\u5728\u6211\u8eab\u4e0a\u8d8a\u767c\u589e\u52a0\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u201c\u57fa\u7763\u8036\u7a4c\u964d\u4e16\uff0c\u70ba\u8981\u62ef\u6551\u7f6a\u4eba\u3002\u201d\u9019\u8a71\u662f\u53ef\u4fe1\u7684\uff0c\u662f\u503c\u5f97\u5b8c\u5168\u63a5\u7d0d\u7684\u3002\u5728\u7f6a\u4eba\u4e2d\u6211\u662f\u500b\u7f6a\u9b41\u3002\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u53ef\u662f\uff0c\u6211\u7adf\u7136\u8499\u4e86\u6190\u61ab\uff0c\u597d\u8b93\u57fa\u7763\u8036\u7a4c\u5728\u6211\u9019\u500b\u7f6a\u9b41\u8eab\u4e0a\uff0c\u986f\u660e\u4ed6\u5b8c\u5168\u7684\u5fcd\u8010\uff0c\u7d66\u5f8c\u4f86\u4fe1\u4ed6\u5f97\u6c38\u751f\u7684\u4eba\u4f5c\u699c\u6a23\u3002\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u4f46\u9858\u5c0a\u8cb4\u69ae\u8000\u6b78\u7d66\u842c\u4e16\u7684\u541b\u738b\uff0c\u5c31\u662f\u90a3\u4e0d\u673d\u58de\u3001\u4eba\u4e0d\u80fd\u898b\u3001\u7368\u4e00\u7684\u3000\u795e\uff0c\u76f4\u5230\u6c38\u6c38\u9060\u9060\u3002\u963f\u5011\u3002\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u6211\u5152\u63d0\u6469\u592a\u554a\uff01\u6211\u7167\u8457\u5f9e\u524d\u95dc\u65bc\u4f60\u7684\u9810\u8a00\uff0c\u628a\u9019\u547d\u4ee4\u4ea4\u8a17\u4f60\uff0c\u70ba\u7684\u662f\u8981\u53eb\u4f60\u85c9\u8457\u9019\u4e9b\u9810\u8a00\u6253\u90a3\u7f8e\u597d\u7684\u4ed7\u3002\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u5e38\u5e38\u5b58\u8457\u4fe1\u5fc3\u548c\u7121\u6127\u7684\u826f\u5fc3\u3002\u6709\u4e9b\u4eba\u4e1f\u68c4\u826f\u5fc3\uff0c\u5c31\u5728\u4fe1\u4ef0\u4e0a\u5931\u843d\u4e86\u3002\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u4ed6\u5011\u7576\u4e2d\u6709\u8a31\u7c73\u4e43\u548c\u4e9e\u6b77\u5c71\u5927\uff0c\u6211\u5df2\u7d93\u628a\u4ed6\u5011\u4ea4\u7d66\u6492\u4f46\uff0c\u4f7f\u4ed6\u5011\u53d7\u7ba1\u6559\u4e0d\u518d\u893b\u7006\u3002\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u8981\u70ba\u842c\u4eba\u79b1\u544a\u56e0\u6b64\uff0c\u6211\u52f8\u4f60\u6700\u91cd\u8981\u7684\u662f\u8981\u70ba\u842c\u4eba\u3001\u541b\u738b\u548c\u4e00\u5207\u6709\u6b0a\u4f4d\u7684\u61c7\u6c42\u3001\u79b1\u544a\u3001\u4ee3\u6c42\u548c\u611f\u6069\uff0c\u597d\u8b93\u6211\u5011\u53ef\u4ee5\u656c\u8654\u838a\u91cd\u5730\u904e\u5e73\u975c\u5b89\u7a69\u7684\u65e5\u5b50\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": " \n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u5728\u6211\u5011\u7684\u6551\u4e3b\u3000\u795e\u770b\u4f86\uff0c\u9019\u662f\u7f8e\u597d\u7684\uff0c\u53ef\u8499\u6085\u7d0d\u7684\u3002\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u4ed6\u9858\u610f\u842c\u4eba\u5f97\u6551\uff0c\u4e26\u4e14\u5145\u5206\u8a8d\u8b58\u771f\u7406\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u56e0\u70ba\u3000\u795e\u53ea\u6709\u4e00\u4f4d\uff0c\u5728\u3000\u795e\u548c\u4eba\u4e2d\u9593\u4e5f\u53ea\u6709\u4e00\u4f4d\u4e2d\u4fdd\uff0c\u5c31\u662f\u964d\u4e16\u70ba\u4eba\u7684\u57fa\u7763\u8036\u7a4c\u3002\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u4ed6\u6368\u4e86\u81ea\u5df1\u4f5c\u842c\u4eba\u7684\u8d16\u50f9\uff0c\u5230\u4e86\u9069\u7576\u7684\u6642\u5019\uff0c\u9019\u4e8b\u5c31\u8b49\u5be6\u4e86\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u70ba\u4e86\u9019\u4e8b\uff0c\u6211\u4e5f\u88ab\u6d3e\u4f5c\u50b3\u9053\u7684\u548c\u4f7f\u5f92\uff08\u6211\u8aaa\u7684\u662f\u771f\u8a71\uff0c\u4e0d\u662f\u8b0a\u8a00\uff09\uff0c\u5728\u4fe1\u4ef0\u548c\u771f\u7406\u4e0a\u4f5c\u5916\u65cf\u4eba\u7684\u6559\u5e2b\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u656c\u754f\u3000\u795e\u7684\u5973\u4eba\u56e0\u6b64\uff0c\u6211\u9858\u610f\u7537\u4eba\u6c92\u6709\u5fff\u6012\uff0c\u6c92\u6709\u722d\u8ad6\uff0c\u8209\u8d77\u8056\u6f54\u7684\u624b\u96a8\u8655\u79b1\u544a\u3002\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7167\u6a23\uff0c\u6211\u4e5f\u9858\u610f\u5973\u4eba\u4ee5\u7aef\u6b63\u3001\u5afb\u6dd1\u3001\u81ea\u5f8b\u70ba\u88dd\u98fe\uff1b\u4e0d\u8981\u4ee5\u9b08\u9aee\u3001\u91d1\u98fe\u3001\u73e0\u5bf6\uff0c\u6216\u540d\u8cb4\u8863\u88f3\u70ba\u88dd\u98fe\uff0c\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u537b\u8981\u4ee5\u5584\u884c\u4f5c\u88dd\u98fe\uff0c\u9019\u624d\u914d\u7a31\u70ba\u656c\u754f\u3000\u795e\u7684\u5973\u4eba\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u5973\u4eba\u61c9\u8a72\u5b89\u975c\u800c\u53c8\u5b8c\u5168\u9806\u670d\u5730\u5b78\u7fd2\u3002\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u6211\u4e0d\u51c6\u5973\u4eba\u6559\u8a13\u7537\u4eba\uff0c\u8f44\u5236\u7537\u4eba\uff1b\u5973\u4eba\u7e3d\u8981\u5b89\u975c\u3002\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u56e0\u70ba\u5148\u9020\u7684\u662f\u4e9e\u7576\uff0c\u5f8c\u9020\u7684\u662f\u590f\u5a03\uff1b\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u4e0d\u662f\u4e9e\u7576\u53d7\u4e86\u5f15\u8a98\uff0c\u800c\u662f\u5973\u4eba\u53d7\u4e86\u5f15\u8a98\uff0c\u9677\u5728\u904e\u72af\u88e1\u9762\u3002\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u7136\u800c\u5973\u4eba\u8981\u662f\u5e38\u5e38\u5b58\u8457\u4fe1\u5fc3\u3001\u611b\u5fc3\u3001\u8056\u6f54\u81ea\u5f8b\uff0c\u5728\u5979\u751f\u80b2\u7684\u4e8b\u4e0a\u5fc5\u5b9a\u5f97\u6551\u3002\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u4f5c\u76e3\u7763\u7684\u8cc7\u683c\u201c\u5982\u679c\u6709\u4eba\u6e34\u6155\u76e3\u7763\u7684\u8077\u5206\uff0c\u4ed6\u5c31\u662f\u611b\u6155\u5584\u5de5\u3002\u201d\u9019\u8a71\u662f\u53ef\u4fe1\u7684\u3002\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u6240\u4ee5\u4f5c\u76e3\u7763\u7684\uff0c\u5fc5\u9808\u7121\u53ef\u6307\u6458\uff0c\u53ea\u4f5c\u4e00\u500b\u59bb\u5b50\u7684\u4e08\u592b\uff0c\u6709\u7bc0\u5236\uff0c\u81ea\u5f8b\uff0c\u838a\u91cd\uff0c\u6a02\u610f\u63a5\u5f85\u5ba2\u65c5\uff0c\u5584\u65bc\u6559\u5c0e\uff0c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u4e0d\u597d\u9152\uff0c\u4e0d\u6253\u4eba\uff0c\u53ea\u8981\u6eab\u548c\uff0c\u8207\u4eba\u7121\u722d\uff0c\u4e0d\u8caa\u8ca1\uff0c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u597d\u597d\u7ba1\u7406\u81ea\u5df1\u7684\u5bb6\uff0c\u4f7f\u5152\u5973\u51e1\u4e8b\u656c\u91cd\u9806\u670d\u3002\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\uff08\u4eba\u82e5\u4e0d\u77e5\u9053\u600e\u6a23\u7ba1\u7406\u81ea\u5df1\u7684\u5bb6\uff0c\u600e\u80fd\u7167\u6599\u3000\u795e\u7684\u6559\u6703\u5462\uff1f\uff09\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u521d\u4fe1\u4e3b\u7684\u4e0d\u53ef\u4f5c\u76e3\u7763\uff0c\u6050\u6015\u4ed6\u9a55\u50b2\uff0c\u5c31\u843d\u5728\u9b54\u9b3c\u6240\u53d7\u7684\u5211\u7f70\u88e1\u3002\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u4f5c\u76e3\u7763\u7684\u4e5f\u5fc5\u9808\u5728\u6559\u5916\u6709\u597d\u8072\u8b7d\uff0c\u514d\u5f97\u4ed6\u88ab\u4eba\u6bc0\u8b17\uff0c\u5c31\u843d\u5728\u9b54\u9b3c\u7684\u9677\u9631\u88e1\u3002\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u4f5c\u57f7\u4e8b\u7684\u8cc7\u683c\u7167\u6a23\uff0c\u57f7\u4e8b\u4e5f\u5fc5\u9808\u838a\u91cd\uff0c\u4e0d\u4e00\u53e3\u5169\u820c\uff0c\u4e0d\u9157\u9152\uff0c\u4e0d\u8caa\u4e0d\u7fa9\u4e4b\u8ca1\uff0c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u7528\u6e05\u6f54\u7684\u826f\u5fc3\u6301\u5b88\u4fe1\u4ef0\u7684\u5967\u7955\u3002\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u4ed6\u5011\u4e5f\u5fc5\u9808\u5148\u53d7\u8003\u9a57\uff0c\u82e5\u6c92\u6709\u53ef\u8cac\u4e4b\u8655\uff0c\u7136\u5f8c\u624d\u8b93\u4ed6\u5011\u4f5c\u57f7\u4e8b\u3002\n"
                },
                "11": {
                    "verse_nr": 11,
