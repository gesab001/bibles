    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0423\u0432\u043e\u0434 - \u0408\u0443\u0434\u0430, \u0441\u043b\u0443\u0433\u0430 \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430, \u0430 \u0431\u0440\u0430\u0442 \u0408\u0430\u043a\u043e\u0432\u0459\u0435\u0432, \u043f\u043e\u0437\u0434\u0440\u0430\u0432\u0459\u0430 \u0432\u0430\u0441, \u0438\u0437\u0430\u0431\u0440\u0430\u043d\u0435, \u043a\u043e\u0458\u0438 \u0436\u0438\u0432\u0438\u0442\u0435 \u0443 \u0459\u0443\u0431\u0430\u0432\u0438 \u0411\u043e\u0433\u0430 \u041e\u0446\u0430, \u0438 \u043f\u043e\u0434 \u0437\u0430\u0448\u0442\u0438\u0442\u043e\u043c \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0418\u043c\u0430\u0458\u0442\u0435 \u043e\u0431\u0438\u0459\u0435 \u043c\u0438\u043b\u043e\u0441\u0440\u0452\u0430, \u043c\u0438\u0440\u0430 \u0438 \u0459\u0443\u0431\u0430\u0432\u0438."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0412\u043e\u0459\u0435\u043d\u0438 \u043c\u043e\u0458\u0438, \u0438\u0430\u043a\u043e \u0441\u0430\u043c \u0441\u0430 \u0432\u0435\u043b\u0438\u043a\u0438\u043c \u0436\u0430\u0440\u043e\u043c \u0445\u0442\u0435\u043e \u0434\u0430 \u0432\u0430\u043c \u043f\u0438\u0448\u0435\u043c \u043e \u043d\u0430\u0448\u0435\u043c \u0437\u0430\u0458\u0435\u0434\u043d\u0438\u0447\u043a\u043e\u043c \u0441\u043f\u0430\u0441\u0435\u045a\u0443, \u043e\u0441\u0435\u0442\u0438\u043e \u0441\u0430\u043c \u043f\u043e\u0442\u0440\u0435\u0431\u0443 \u0434\u0430 \u0432\u0430\u0441 \u0441\u0432\u043e\u0458\u0438\u043c \u043f\u0438\u0441\u0430\u045a\u0435\u043c \u043e\u0445\u0440\u0430\u0431\u0440\u0438\u043c \u0434\u0430 \u0441\u0435 \u0431\u043e\u0440\u0438\u0442\u0435 \u0437\u0430 \u0432\u0435\u0440\u0443 \u043a\u043e\u0458\u0430 \u0458\u0435 \u0458\u0435\u0434\u043d\u043e\u043c \u0437\u0430\u0443\u0432\u0435\u043a \u043f\u0440\u0435\u0434\u0430\u0442\u0430 \u0441\u0432\u0435\u0442\u0438\u043c\u0430."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0408\u0435\u0440 \u0443\u0448\u0443\u045a\u0430\u043b\u0438 \u0441\u0443 \u0441\u0435 \u043c\u0435\u0452\u0443 \u0432\u0430\u0441 \u043d\u0435\u043a\u0438 \u0459\u0443\u0434\u0438, \u043a\u043e\u0458\u0438 \u0441\u0443 \u0434\u0430\u0432\u043d\u043e \u043e\u0434\u0440\u0435\u0452\u0435\u043d\u0438 \u0437\u0430 \u0441\u0443\u0434. \u041e\u043d\u0438 \u0441\u0443 \u0431\u0435\u0437\u0431\u043e\u0436\u043d\u0438\u0446\u0438 \u043a\u043e\u0458\u0438 \u043c\u0438\u043b\u043e\u0441\u0442 \u043d\u0430\u0448\u0435\u0433\u0430 \u0411\u043e\u0433\u0430 \u0438\u0437\u0432\u0440\u045b\u0443 \u0443 \u0440\u0430\u0437\u0443\u0437\u0434\u0430\u043d\u043e\u0441\u0442 \u0438 \u043f\u043e\u0440\u0438\u0447\u0443 \u0458\u0435\u0434\u0438\u043d\u043e\u0433 \u0413\u043e\u0441\u043f\u043e\u0434\u0430\u0440\u0430, \u043d\u0430\u0448\u0435\u0433 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0416\u0435\u043b\u0438\u043c \u0434\u0430 \u0432\u0430\u0441 \u043f\u043e\u0434\u0441\u0435\u0442\u0438\u043c, \u0438\u0430\u043a\u043e \u0437\u043d\u0430\u0442\u0435 \u0441\u0432\u0435 \u043e\u0432\u043e, \u0434\u0430 \u0458\u0435 \u0413\u043e\u0441\u043f\u043e\u0434 \u0438\u0437\u0431\u0430\u0432\u0438\u043e \u0441\u0432\u043e\u0458 \u043d\u0430\u0440\u043e\u0434 \u0438\u0437 \u0415\u0433\u0438\u043f\u0442\u0430, \u0430 \u0437\u0430\u0442\u0438\u043c \u0443\u043d\u0438\u0448\u0442\u0438\u043e \u043e\u043d\u0435 \u043a\u043e\u0458\u0438 \u043d\u0438\u0441\u0443 \u0432\u0435\u0440\u043e\u0432\u0430\u043b\u0438."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0410 \u0430\u043d\u0452\u0435\u043b\u0435 \u043a\u043e\u0458\u0438 \u0441\u0435 \u043d\u0438\u0441\u0443 \u0434\u0440\u0436\u0430\u043b\u0438 \u0441\u0432\u043e\u0433 \u043f\u043e\u0440\u0435\u0442\u043a\u0430, \u043d\u0435\u0433\u043e \u0441\u0443 \u043d\u0430\u043f\u0443\u0441\u0442\u0438\u043b\u0438 \u043c\u0435\u0441\u0442\u043e \u0441\u0432\u043e\u0433\u0430 \u0431\u043e\u0440\u0430\u0432\u0438\u0448\u0442\u0430, \u0411\u043e\u0433 \u0447\u0443\u0432\u0430 \u0443 \u0432\u0435\u0447\u043d\u0438\u043c \u043e\u043a\u043e\u0432\u0438\u043c\u0430 \u043f\u043e\u0434 \u043c\u0440\u0430\u043a\u043e\u043c \u0437\u0430 \u0432\u0435\u043b\u0438\u043a\u0438 \u0434\u0430\u043d \u043a\u0430\u0434\u0430 \u045b\u0435 \u0431\u0438\u0442\u0438 \u043e\u0441\u0443\u0452\u0435\u043d\u0438."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0422\u0430\u043a\u043e \u0438 \u0421\u043e\u0434\u043e\u043c\u0430 \u0438 \u0413\u043e\u043c\u043e\u0440\u0430, \u0437\u0430\u0458\u0435\u0434\u043d\u043e \u0441\u0430 \u043e\u043a\u043e\u043b\u043d\u0438\u043c \u0433\u0440\u0430\u0434\u043e\u0432\u0438\u043c\u0430 \u043a\u043e\u0458\u0438 \u0441\u0443 \u0441\u0435 \u043d\u0430 \u0438\u0441\u0442\u0438 \u043d\u0430\u0447\u0438\u043d \u043e\u0434\u0430\u0432\u0430\u043b\u0438 \u0431\u043b\u0443\u0434\u0443 \u0438 \u0436\u0443\u0434\u0435\u043b\u0438 \u0437\u0430 \u0442\u0435\u043b\u043e\u043c \u0434\u0440\u0443\u0433\u0430\u0447\u0438\u0458\u0435 \u0432\u0440\u0441\u0442\u0435, \u0441\u0442\u043e\u0458\u0435 \u043a\u0430\u043e \u043f\u0440\u0438\u043c\u0435\u0440, \u0442\u0440\u043f\u0435\u045b\u0438 \u043a\u0430\u0437\u043d\u0443 \u0443 \u0432\u0435\u0447\u043d\u043e\u043c \u043e\u0433\u045a\u0443."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0423\u043f\u0440\u043a\u043e\u0441 \u0442\u043e\u043c\u0435, \u0442\u0438 \u0448\u0442\u043e \u0441\u0430\u045a\u0430\u0458\u0443 \u0432\u0438\u0452\u0435\u045a\u0430, \u043d\u0430 \u0438\u0441\u0442\u0438 \u043d\u0430\u0447\u0438\u043d \u043a\u0430\u0459\u0430\u0458\u0443 \u0441\u0432\u043e\u0458\u0435 \u0442\u0435\u043b\u043e, \u043f\u0440\u0435\u0437\u0438\u0440\u0443 \u0433\u043e\u0441\u043f\u043e\u0434\u0441\u0442\u0432\u043e, \u0438 \u0432\u0440\u0435\u0452\u0430\u0458\u0443 \u0441\u043b\u0430\u0432\u043d\u0435 \u0430\u043d\u0452\u0435\u043b\u0435."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0410 \u0447\u0430\u043a \u0441\u0435 \u043d\u0438 \u0430\u0440\u0445\u0430\u043d\u0452\u0435\u043b \u041c\u0438\u0445\u0430\u0438\u043b\u043e, \u0434\u043e\u043a \u0441\u0435 \u0443 \u0441\u0432\u0430\u0452\u0438 \u0441\u0430 \u0452\u0430\u0432\u043e\u043b\u043e\u043c \u043f\u0440\u0435\u043f\u0438\u0440\u0430\u043e \u043e\u043a\u043e \u041c\u043e\u0458\u0441\u0438\u0458\u0435\u0432\u043e\u0433 \u0442\u0435\u043b\u0430, \u043d\u0438\u0458\u0435 \u0443\u0441\u0443\u0434\u0438\u043e \u0434\u0430 \u0438\u0437\u043d\u0435\u0441\u0435 \u043f\u043e\u0433\u0440\u0434\u0430\u043d \u0441\u0443\u0434 \u043f\u0440\u043e\u0442\u0438\u0432 \u045a\u0435\u0433\u0430, \u043d\u0435\u0433\u043e \u0458\u0435 \u0440\u0435\u043a\u0430\u043e: \u201e\u041d\u0435\u043a\u0430 \u0442\u0435 \u0413\u043e\u0441\u043f\u043e\u0434 \u0443\u043a\u043e\u0440\u0438!\u201c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0410\u043b\u0438 \u043e\u0432\u0438 \u0459\u0443\u0434\u0438 \u0432\u0440\u0435\u0452\u0430\u0458\u0443 \u043e\u043d\u043e \u0448\u0442\u043e \u043d\u0435 \u0440\u0430\u0437\u0443\u043c\u0435\u0458\u0443; \u0430 \u043e\u043d\u043e \u0448\u0442\u043e \u0437\u043d\u0430\u0458\u0443 \u043d\u0430\u0433\u043e\u043d\u0441\u043a\u0438, \u043f\u043e\u043f\u0443\u0442 \u043d\u0435\u0440\u0430\u0437\u0443\u043c\u043d\u0438\u0445 \u0436\u0438\u0432\u043e\u0442\u0438\u045a\u0430, \u0443\u043f\u0440\u0430\u0432\u043e \u0458\u0435 \u043e\u043d\u043e \u0448\u0442\u043e \u0438\u0445 \u0432\u043e\u0434\u0438 \u0443 \u043f\u0440\u043e\u043f\u0430\u0441\u0442."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0422\u0435\u0448\u043a\u043e \u045a\u0438\u043c\u0430! \u041e\u043d\u0438 \u0441\u0443 \u043f\u043e\u0448\u043b\u0438 \u041a\u0430\u0458\u0438\u043d\u043e\u0432\u0438\u043c \u043f\u0443\u0442\u0435\u043c, \u0438 \u0437\u0431\u043e\u0433 \u043d\u043e\u0432\u0446\u0430 \u0443\u043f\u0430\u043b\u0438 \u0443 \u0437\u0430\u0431\u043b\u0443\u0434\u0443 \u043a\u0430\u043e \u0412\u0430\u043b\u0430\u043c, \u0438 \u0434\u0438\u0433\u043b\u0438 \u0431\u0443\u043d\u0443 \u043a\u0430\u043e \u041a\u043e\u0440\u0435\u0458. \u0417\u0430\u0442\u043e \u045b\u0435 \u0442\u0430\u043a\u043e \u0438 \u0438\u0437\u0433\u0438\u043d\u0443\u0442\u0438."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u041e\u043d\u0438 \u0441\u0443 \u043a\u0430\u043e \u0459\u0430\u0433\u0435 \u043d\u0430 \u0432\u0430\u0448\u0438\u043c \u0437\u0430\u0458\u0435\u0434\u043d\u0438\u0447\u043a\u0438\u043c \u043e\u0431\u0435\u0434\u0438\u043c\u0430; \u0431\u0435\u0441\u0442\u0438\u0434\u043d\u043e \u0441\u0435 \u0433\u043e\u0441\u0442\u0435 \u0441\u0430 \u0432\u0430\u043c\u0430, \u0430 \u0432\u043e\u0434\u0435 \u0431\u0440\u0438\u0433\u0443 \u0441\u0430\u043c\u043e \u0437\u0430 \u0441\u0435\u0431\u0435. \u041e\u043d\u0438 \u0441\u0443 \u043a\u0430\u043e \u0431\u0435\u0437\u0432\u043e\u0434\u043d\u0438 \u043e\u0431\u043b\u0430\u0446\u0438 \u0433\u043e\u045a\u0435\u043d\u0438 \u0432\u0435\u0442\u0440\u043e\u0432\u0438\u043c\u0430; \u043a\u0430\u043e \u0441\u0442\u0430\u0431\u043b\u0430 \u0443 \u043f\u043e\u0437\u043d\u0443 \u0458\u0435\u0441\u0435\u043d, \u0431\u0435\u0441\u043f\u043b\u043e\u0434\u043d\u0430, \u0434\u0432\u0430\u043f\u0443\u0442 \u0443\u0441\u0430\u0445\u043b\u0430 \u0438 \u0438\u0437 \u043a\u043e\u0440\u0435\u043d\u0430 \u0438\u0448\u0447\u0443\u043f\u0430\u043d\u0430."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0418 \u043a\u0430\u043e \u0448\u0442\u043e \u0431\u0435\u0441\u043d\u0438 \u043c\u043e\u0440\u0441\u043a\u0438 \u0442\u0430\u043b\u0430\u0441\u0438 \u0438\u0437\u0431\u0430\u0446\u0443\u0458\u0443 \u043f\u0435\u043d\u0443, \u0442\u0430\u043a\u043e \u043e\u043d\u0438 \u0438\u0437\u043d\u043e\u0441\u0435 \u0441\u0432\u043e\u0458\u0430 \u0441\u0440\u0430\u043c\u043e\u0442\u043d\u0430 \u0434\u0435\u043b\u0430. \u041e\u043d\u0438 \u0441\u0443 \u043a\u0430\u043e \u043b\u0443\u0442\u0430\u0458\u0443\u045b\u0435 \u0437\u0432\u0435\u0437\u0434\u0435 \u043a\u043e\u0458\u0438\u043c\u0430 \u0458\u0435 \u043f\u0440\u0438\u043f\u0440\u0435\u043c\u0459\u0435\u043d\u0430 \u043c\u0440\u043a\u043b\u0430 \u0442\u0430\u043c\u0430 \u0437\u0430 \u0432\u0435\u0447\u043d\u043e\u0441\u0442."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u041e \u043e\u0432\u0438\u043c\u0430 \u0458\u0435 \u043f\u0440\u043e\u0440\u043e\u043a\u043e\u0432\u0430\u043e \u0415\u043d\u043e\u0445, \u0441\u0435\u0434\u043c\u0438 \u043e\u0434 \u0410\u0434\u0430\u043c\u0430, \u0440\u0435\u043a\u0430\u0432\u0448\u0438: \u201e\u0415\u0432\u043e, \u0434\u043e\u043b\u0430\u0437\u0438 \u0413\u043e\u0441\u043f\u043e\u0434 \u0441\u0430 \u0445\u0438\u0459\u0430\u0434\u0430\u043c\u0430 \u0441\u0432\u043e\u0458\u0438\u0445 \u0441\u0432\u0435\u0442\u0438\u0445,"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0434\u0430 \u0441\u0443\u0434\u0438 \u0441\u0432\u0438\u043c\u0430 \u0438 \u0434\u0430 \u043e\u0441\u0443\u0434\u0438 \u0441\u0432\u0435 \u0431\u0435\u0437\u0431\u043e\u0436\u043d\u0438\u043a\u0435 \u0437\u0430 \u0441\u0432\u0430 \u0431\u0435\u0437\u0431\u043e\u0436\u043d\u0430 \u0434\u0435\u043b\u0430 \u043a\u043e\u0458\u0430 \u0441\u0443 \u0443\u0447\u0438\u043d\u0438\u043b\u0438, \u0438 \u0437\u0430 \u0441\u0432\u0435 \u0434\u0440\u0441\u043a\u0435 \u0440\u0435\u0447\u0438 \u043a\u043e\u0458\u0435 \u0441\u0443 \u0431\u0435\u0437\u0431\u043e\u0436\u043d\u0438 \u0433\u0440\u0435\u0448\u043d\u0438\u0446\u0438 \u0438\u0437\u0440\u0435\u043a\u043b\u0438 \u043f\u0440\u043e\u0442\u0438\u0432 \u045a\u0435\u0433\u0430.\u201c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0422\u0438 \u0459\u0443\u0434\u0438 \u0443\u0432\u0435\u043a \u0433\u0443\u043d\u0452\u0430\u0458\u0443 \u0438 \u0436\u0430\u043b\u0435 \u0441\u0435 \u043d\u0430 \u0441\u0443\u0434\u0431\u0438\u043d\u0443; \u043e\u043d\u0438 \u0436\u0438\u0432\u0435 \u043f\u043e \u0441\u043e\u043f\u0441\u0442\u0432\u0435\u043d\u0438\u043c \u0436\u0435\u0459\u0430\u043c\u0430, \u0440\u0430\u0437\u043c\u0435\u045b\u0443 \u0441\u0435 \u0440\u0435\u0447\u0438\u043c\u0430 \u0438 \u0438\u0437 \u043a\u043e\u0440\u0438\u0441\u0442\u0438 \u043b\u0430\u0441\u043a\u0430\u0458\u0443 \u0459\u0443\u0434\u0438\u043c\u0430."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0412\u043e\u0459\u0435\u043d\u0438 \u043c\u043e\u0458\u0438, \u0441\u0435\u0442\u0438\u0442\u0435 \u0441\u0435 \u0440\u0435\u0447\u0438 \u043a\u043e\u0458\u0435 \u0441\u0443 \u043f\u0440\u043e\u0440\u0435\u043a\u043b\u0438 \u0430\u043f\u043e\u0441\u0442\u043e\u043b\u0438 \u043d\u0430\u0448\u0435\u0433 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u041e\u043d\u0438 \u0441\u0443 \u0432\u0430\u043c \u0440\u0435\u043a\u043b\u0438 \u0434\u0430 \u045b\u0435 \u0441\u0435 \u0443 \u043f\u043e\u0441\u043b\u0435\u0434\u045a\u0430 \u0432\u0440\u0435\u043c\u0435\u043d\u0430 \u043f\u043e\u0458\u0430\u0432\u0438\u0442\u0438 \u043f\u043e\u0434\u0440\u0443\u0433\u0459\u0438\u0432\u0446\u0438 \u043a\u043e\u0458\u0438 \u045b\u0435 \u0436\u0438\u0432\u0435\u0442\u0438 \u043f\u043e \u0441\u0432\u043e\u0458\u0438\u043c \u0431\u0435\u0437\u0431\u043e\u0436\u043d\u0438\u043c \u0436\u0435\u0459\u0430\u043c\u0430."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0422\u043e \u0441\u0443 \u043e\u043d\u0438 \u0448\u0442\u043e \u043f\u0440\u0430\u0432\u0435 \u0440\u0430\u0437\u0434\u043e\u0440\u0435, \u0442\u0435\u043b\u0435\u0441\u043d\u0438\u043c \u0447\u0443\u043b\u0438\u043c\u0430 \u0432\u043e\u0452\u0435\u043d\u0438 \u0459\u0443\u0434\u0438 \u043a\u043e\u0458\u0438 \u043d\u0435\u043c\u0430\u0458\u0443 \u0414\u0443\u0445\u0430."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u041f\u043e\u0434\u0441\u0442\u0438\u0446\u0430\u0458 \u043d\u0430 \u043c\u0435\u0452\u0443\u0441\u043e\u0431\u043d\u0443 \u0459\u0443\u0431\u0430\u0432 - \u0410 \u0432\u0438, \u0432\u043e\u0459\u0435\u043d\u0438 \u043c\u043e\u0458\u0438, \u0438\u0437\u0433\u0440\u0430\u0452\u0443\u0458\u0442\u0435 \u0441\u0435 \u043d\u0430 \u0442\u0435\u043c\u0435\u0459\u0438\u043c\u0430 \u0432\u0430\u0448\u0435 \u043d\u0430\u0458\u0441\u0432\u0435\u0442\u0438\u0458\u0435 \u0432\u0435\u0440\u0435, \u043c\u043e\u043b\u0435\u045b\u0438 \u0441\u0435 \u0443 \u0414\u0443\u0445\u0443 \u0421\u0432\u0435\u0442\u043e\u043c."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u0414\u0440\u0436\u0438\u0442\u0435 \u0441\u0435 \u0411\u043e\u0436\u0438\u0458\u0435 \u0459\u0443\u0431\u0430\u0432\u0438 \u0434\u043e\u043a \u0447\u0435\u043a\u0430\u0442\u0435 \u043c\u0438\u043b\u043e\u0441\u0440\u0452\u0435 \u043d\u0430\u0448\u0435\u0433 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430, \u043a\u043e\u0458\u0435 \u0432\u043e\u0434\u0438 \u0443 \u0432\u0435\u0447\u043d\u0438 \u0436\u0438\u0432\u043e\u0442."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u041f\u0440\u0435\u043c\u0430 \u043e\u043d\u0438\u043c\u0430 \u043a\u043e\u0458\u0438 \u0441\u0435 \u043a\u043e\u043b\u0435\u0431\u0430\u0458\u0443 \u0431\u0443\u0434\u0438\u0442\u0435 \u043c\u0438\u043b\u043e\u0441\u0440\u0434\u043d\u0438."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u0414\u0440\u0443\u0433\u0435 \u0438\u0437\u0431\u0430\u0432\u0459\u0430\u0458\u0442\u0435 \u0438\u0441\u0442\u0440\u0436\u0443\u045b\u0438 \u0438\u0445 \u0438\u0437 \u043e\u0433\u045a\u0430. \u0411\u0443\u0434\u0438\u0442\u0435 \u043c\u0438\u043b\u043e\u0441\u0440\u0434\u043d\u0438 \u043f\u0440\u0435\u043c\u0430 \u0442\u0430\u043a\u0432\u0438\u043c\u0430 \u0443\u0437 \u043e\u043f\u0440\u0435\u0437, \u0430\u043b\u0438 \u043c\u0440\u0437\u0438\u0442\u0435 \u0438 \u0441\u0430\u043c\u0443 \u043e\u0434\u0435\u045b\u0443 \u043e\u043a\u0430\u0459\u0430\u043d\u0443 \u0442\u0435\u043b\u043e\u043c."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u0410 \u043e\u043d\u043e\u043c\u0435 \u043a\u043e\u0458\u0438 \u0458\u0435 \u0443 \u0441\u0442\u0430\u045a\u0443 \u0434\u0430 \u0432\u0430\u0441 \u0441\u0430\u0447\u0443\u0432\u0430 \u043e\u0434 \u043f\u0430\u0434\u0430 \u0438 \u0434\u043e\u0432\u0435\u0434\u0435 \u0432\u0430\u0441 \u0431\u0435\u0441\u043f\u0440\u0435\u043a\u043e\u0440\u043d\u0435 \u0438 \u0440\u0430\u0434\u043e\u0441\u043d\u0435 \u0443 \u0441\u0432\u043e\u0458\u0443 \u0441\u043b\u0430\u0432\u043d\u0443 \u043f\u0440\u0438\u0441\u0443\u0442\u043d\u043e\u0441\u0442,"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u0458\u0435\u0434\u0438\u043d\u043e\u043c\u0435 \u0411\u043e\u0433\u0443 \u0421\u043f\u0430\u0441\u0438\u0442\u0435\u0459\u0443 \u043d\u0430\u0448\u0435\u043c \u043f\u043e\u0441\u0440\u0435\u0434\u0441\u0442\u0432\u043e\u043c \u0418\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430, \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430\u0448\u0435\u0433, \u043f\u0440\u0438\u043f\u0430\u0434\u0430 \u0441\u043b\u0430\u0432\u0430, \u0432\u0435\u043b\u0438\u0447\u0430\u043d\u0441\u0442\u0432\u043e, \u0441\u0438\u043b\u0430 \u0438 \u0432\u043b\u0430\u0441\u0442 \u043e\u0434 \u043f\u0440\u043e\u0448\u043b\u043e\u0441\u0442\u0438, \u0441\u0430\u0434\u0430 \u0438 \u0437\u0430\u0443\u0432\u0435\u043a. \u0410\u043c\u0438\u043d."
                }
            }
        }
    }
