                "14": {
                    "verse_nr": "14",
                    "verse": "\u12cd\u1283 \u1263\u1215\u122d\u1295 \u12a5\u1295\u12f0\u121a\u12a8\u12f5\u1295\u1365 \u121d\u12f5\u122d \u12e8\u130c\u1273\u1295 \u12ad\u1265\u122d \u1260\u121b\u12c8\u1245 \u1275\u121e\u120b\u1208\u127d\u1293\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u1280\u134d\u1228\u1270 \u1225\u130b\u12cd\u1295 \u1208\u121b\u12e8\u1275 \u1263\u120d\u1295\u1300\u122b\u12cd\u1295 \u1208\u121a\u12eb\u1320\u1323\u1365 \u12ad\u1349\u1295\u121d \u1208\u121a\u1328\u121d\u122d\u1260\u1275\u1365 \u1208\u121a\u12eb\u1230\u12ad\u1228\u12cd\u121d \u12c8\u12ee\u1208\u1275!"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u1260\u12ad\u1265\u122d \u1348\u1295\u1273 \u12a5\u134d\u1228\u1275 \u121e\u120d\u1276\u1265\u1203\u120d\u1364 \u12a0\u1295\u1270 \u12f0\u130d\u121e \u1320\u1323\u1365 \u12a5\u1295\u12f3\u120d\u1270\u1308\u1228\u12d8\u121d \u1270\u1246\u1320\u122d\u1364 \u12e8\u130c\u1273 \u12e8\u1240\u1299 \u133d\u12cb \u1260\u12a0\u1295\u1270 \u120b\u12ed \u12ed\u1218\u1208\u1235\u1265\u1203\u120d\u1365 \u12a5\u134d\u1228\u1275\u121d \u1260\u12ad\u1265\u122d\u1205 \u120b\u12ed \u12ed\u1206\u1293\u120d\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u12a8\u1230\u12cd \u12f0\u121d\u1365 \u1260\u121d\u12f5\u122a\u1271\u1365 \u1260\u12a8\u1270\u121b\u12ed\u1271\u1293 \u1260\u12a5\u122d\u1237\u121d \u1260\u121a\u1296\u1229 \u1201\u1209 \u120b\u12ed \u12a8\u1270\u12f0\u1228\u1308\u12cd \u12d3\u1218\u133d \u12e8\u1270\u1290\u1223\u1364 \u1260\u120a\u1263\u1296\u1235 \u120b\u12ed \u12e8\u1270\u12f0\u1228\u1308\u12cd \u12d3\u1218\u133d \u12ed\u12a8\u12f5\u1295\u1203\u120d\u1364 \u12e8\u12a0\u122b\u12ca\u1275\u121d \u1325\u134b\u1275 \u12eb\u1235\u1348\u122b\u1278\u12cb\u120d\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u1220\u122a\u12cd \u12e8\u1240\u1228\u1338\u12cd\u1293 \u1240\u120d\u1326 \u12e8\u1270\u1220\u122b \u121d\u1235\u120d \u12e8\u12cd\u1238\u1275 \u12a0\u1235\u1270\u121b\u122a \u1290\u12cd\u1293\u1364 \u12f2\u12f3 \u1323\u12d6\u1275\u1295 \u1260\u1218\u1225\u122b\u1275 \u1220\u122a\u12cd \u1260\u1220\u122b\u12cd \u12ed\u1273\u1218\u1293\u120d\u1293\u1364 \u12e8\u1270\u1240\u1228\u1338 \u121d\u1235\u120d \u121d\u1295 \u12ed\u1320\u1245\u121b\u120d?"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12a5\u1295\u1328\u1271\u1295 \u201c\u1295\u1243\u201d \u12dd\u121d \u12eb\u1208\u12cd\u1295\u121d \u12f5\u1295\u130b\u12ed \u201c\u1270\u1290\u1223\u201d \u1208\u121a\u1208\u12cd \u12c8\u12ee\u1208\u1275! \u12ed\u1205 \u12eb\u1235\u1270\u121d\u122b\u120d\u1295? \u12a5\u1290\u1206\u1365 \u1260\u12c8\u122d\u1245\u1293 \u1260\u1265\u122d \u1270\u1208\u1265\u1326\u12a0\u120d\u1365 \u1260\u12cd\u1235\u1321\u121d \u121d\u1295\u121d \u12a5\u1235\u1275\u1295\u134b\u1235 \u12e8\u1208\u1260\u1275\u121d\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u130c\u1273 \u130d\u1295 \u1260\u1270\u1240\u12f0\u1230 \u1218\u1245\u12f0\u1231 \u12a0\u1208\u1364 \u121d\u12f5\u122a\u1277 \u1201\u1209 \u1260\u134a\u1271 \u12dd\u121d \u1275\u1260\u120d\u1362"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12e8\u1290\u1262\u12e9 \u12e8\u12d5\u1295\u1263\u1246\u121d \u1338\u120e\u1275 \u1260\u123a\u130d\u12ee\u1296\u1275\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u130c\u1273 \u1206\u12ed\u1365 \u12dd\u1293\u1205\u1295 \u1230\u121d\u127b\u1208\u1201\u1365 \u1260\u1225\u122b\u1205\u121d \u130c\u1273 \u1206\u12ed \u1348\u122b\u1201\u1365\n\u1260\u12d3\u1218\u1273\u1275 \u1218\u12ab\u12a8\u120d \u12a0\u12f5\u1230\u12cd\u1365\n\u1260\u12d3\u1218\u1273\u1275 \u1218\u12ab\u12a8\u120d \u12a0\u1235\u1273\u12cd\u1240\u12cd\u1365\n\u1260\u1241\u1323 \u130a\u12dc \u121d\u1215\u1228\u1275\u1295 \u12a0\u1235\u1265\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a8\u1274\u121b\u1295\u1365\n\u1245\u12f1\u1231\u121d \u12a8\u1353\u122b\u1295 \u1270\u122b\u122b \u1218\u1323\u1362\n(\u1234\u120b) \u12cd\u1260\u1271 \u1230\u121b\u12eb\u1275\u1295 \u1238\u134d\u1296\u12a0\u120d\u1365\n\u121d\u1235\u130b\u1293\u12cd\u121d \u121d\u12f5\u122d\u1295 \u121e\u120d\u1276\u12a0\u120d\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1340\u12f3\u1209\u121d \u12a5\u1295\u12f0 \u1265\u122d\u1203\u1295 \u1290\u12cd\u1365\n\u1328\u1228\u122d \u12a8\u12a5\u1301 \u12c8\u1325\u1276\u12a0\u120d\u1364\n\u1283\u12ed\u1209\u121d \u1260\u12da\u12eb \u1270\u1230\u12cd\u122e\u12a0\u120d\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1278\u1290\u1348\u122d \u1260\u134a\u1271 \u12ed\u1204\u12f3\u120d\u1365\n\u1218\u1245\u1220\u134d\u1275\u121d \u12a8\u12a5\u130d\u1229 \u12ed\u12c8\u1323\u120d\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u1246\u1218\u1365 \u121d\u12f5\u122d\u1295\u121d \u12a0\u1295\u1240\u1320\u1240\u1323\u1275\u1365\n\u1270\u1218\u1208\u12a8\u1270\u1365 \u1215\u12dd\u1266\u127d\u1295\u121d \u12a0\u1235\u12f0\u1290\u1308\u1320\u1365\n\u12e8\u12d8\u1208\u12d3\u1208\u121d \u1270\u122b\u122e\u127d \u1270\u1293\u1309\u1365\n\u12e8\u1325\u1295\u1275 \u12ae\u1228\u1265\u1276\u127d \u12a0\u130e\u1290\u1260\u1231\u1365\n\u12e8\u1325\u1295\u1275 \u1218\u1295\u1308\u12f6\u127d \u12e8\u12a5\u122d\u1231 \u1293\u1278\u12cd\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12e8\u12a2\u1275\u12ee\u1335\u12eb \u12f5\u1295\u12b3\u1296\u127d \u1260\u132d\u1295\u1240\u1275 \u120b\u12ed \u1206\u1290\u12cd \u12a0\u12e8\u1201\u1365\n\u12e8\u121a\u12f5\u12eb\u1295 \u121d\u12f5\u122d \u1218\u130b\u1228\u1303\u12ce\u127d \u1270\u1295\u1240\u1320\u1240\u1321\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u130c\u1273 \u1206\u12ed \u1295\u12f4\u1275\u1205 \u1260\u12c8\u1295\u12de\u127d \u120b\u12ed \u1290\u12cd\u1295?\n\u12c8\u12ed\u1235 \u1241\u1323\u1205 \u1260\u12c8\u1295\u12de\u127d \u120b\u12ed \u12c8\u12ed\u121d \u1218\u12d3\u1275\u1205 \u1260\u1263\u1215\u122d \u120b\u12ed \u1290\u12cd\u1295?\n\u1260\u1348\u1228\u1236\u127d\u1205\u1365 \u1260\u121b\u12f3\u1295 \u1230\u1228\u1308\u120e\u127d \u120b\u12ed \u1270\u1240\u121d\u1320\u1203\u120d\u1293\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1240\u1235\u1275\u1205\u1295 \u1308\u1208\u1325\u1205 \u1260\u1243\u120d\u1205 \u12a5\u1295\u12f0 \u121b\u120d\u1205\n\u1218\u1245\u1220\u134d\u1275\u1205\u1295 \u12a0\u12c8\u1323\u1205\u1364 \u1240\u1235\u1275\u1205\u1295\u121d \u1308\u1270\u122d\u1205\u1364\n\u121d\u12f5\u122d\u1295 \u1230\u1295\u1325\u1240\u1205 \u1348\u1233\u123e\u127d\u1295 \u12a0\u12c8\u1323\u1205\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u1270\u122b\u122e\u127d \u12a0\u1295\u1270\u1295 \u12a0\u12ed\u1270\u12cd \u1270\u12c8\u122b\u1329\u1365\n\u12e8\u12cd\u1283 \u12c8\u1300\u1265 \u12a0\u1208\u1348\u1365\n\u1325\u120d\u1241\u121d \u12f5\u121d\u1341\u1295 \u1230\u1320\u1365\n\u12a5\u1301\u1295\u121d \u12c8\u12f0 \u120b\u12ed \u12a0\u1290\u1233\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1260\u1240\u1235\u1276\u127d\u1205 \u1265\u122d\u1203\u1295\u1365\n\u1260\u1326\u122d\u1205 \u1218\u1265\u1228\u1245 \u1340\u12f3\u120d\u1365\n\u1340\u1210\u12ed\u1293 \u1328\u1228\u1243 \u1260\u1218\u1296\u122a\u12eb\u1278\u12cd \u1240\u1325 \u1265\u1208\u12cd \u1246\u1219\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u1260\u1241\u1323 \u121d\u12f5\u122d\u1295 \u1228\u1308\u1325\u1203\u1275\u1365\n\u12a0\u1215\u12db\u1265\u1295 \u1260\u1295\u12f4\u1275 \u12a0\u1204\u12f5\u1203\u1278\u12cd\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1215\u12dd\u1265\u1205\u1295 \u1208\u1218\u1273\u12f0\u130d\u1365\n\u12e8\u1240\u1263\u12b8\u12cd\u1295\u121d \u1208\u121b\u12f3\u1295 \u12c8\u1323\u1205\u1364\n\u12e8\u1283\u1322\u12a0\u1270\u129b\u12cd\u1295 \u1264\u1275 \u122b\u1235 \u1240\u1320\u1240\u1325\u1205\u1365\n\u12a8\u1218\u1220\u1228\u1271 \u12a5\u1235\u12a8 \u12a0\u1295\u1308\u1271 \u12f5\u1228\u1235 \u12a0\u122b\u1246\u1275\u1205\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u12f0\u1235\u1273\u1278\u12cd \u12e8\u1270\u12f0\u1260\u1241 \u127d\u130d\u1228\u129e\u127d\u1295 \u12a5\u1295\u12f0\u121a\u12cd\u1325 \u12e8\u1206\u1290\u12cd\u1295\u1365\n\u12a5\u129b\u1295 \u120a\u1260\u1275\u1291 \u12a5\u1295\u12f0 \u12d0\u12cd\u120e \u1290\u134b\u1235 \u12e8\u1218\u1321\u1275\u1295\u1364\n\u12e8\u1326\u1228\u129b\u12cd\u1295 \u122b\u1235 \u1260\u1308\u12db \u1326\u1229 \u12c8\u130b\u1205\u1364"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u1260\u1348\u1228\u1236\u127d\u1205 \u1260\u1263\u1215\u122d \u120b\u12ed \u1270\u122b\u1218\u12f5\u1205\u1365\n\u1265\u12d9 \u12cd\u1286\u127d\u1295\u121d \u1228\u1308\u1325\u1205\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u12a5\u1294 \u1230\u121d\u127b\u1208\u1201\u1365 \u12a0\u1295\u1300\u1274 \u122b\u12f0\u1265\u129d\u1365\n\u12a8\u12f5\u121d\u1341 \u12e8\u1270\u1290\u1223 \u12a8\u1295\u1348\u122e\u127c \u1270\u1295\u1240\u1320\u1240\u1321\u1364\n\u1218\u1260\u1235\u1260\u1235 \u12c8\u12f0 \u12a0\u1325\u1295\u1276\u127c \u12cd\u1235\u1325 \u1308\u1263\u1365\n\u1260\u12cd\u1235\u1324\u121d \u1270\u1295\u1240\u1320\u1240\u1325\u1201\u1365\n\u1260\u12a0\u1235\u1328\u1290\u1241\u1295 \u1215\u12dd\u1265 \u120b\u12ed \u12a5\u1235\u12aa\u1218\u1323 \u12f5\u1228\u1235\u1365\n\u12e8\u1218\u12a8\u122b\u1295 \u1240\u1295 \u12dd\u121d \u1265\u12ec \u12a5\u1320\u1265\u1243\u1208\u1201\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u12e8\u1260\u1208\u1235 \u12db\u134d \u1263\u1273\u1265\u1265\u121d\u1365\n\u1260\u12c8\u12ed\u1295 \u1270\u12ad\u120e\u127d \u120b\u12ed \u134d\u122c \u1263\u12ed\u1308\u129d\u1365\n\u12e8\u12c8\u12ed\u122b \u121d\u122d\u1275 \u1262\u124b\u1228\u1325\u1365\n\u12a5\u122d\u123e\u127d\u121d \u1218\u1265\u120d\u1295 \u1263\u12ed\u1230\u1321\u1365\n\u1260\u130e\u127d \u12a8\u1260\u1228\u1271 \u1262\u1320\u1349\u1365\n\u12a8\u1265\u1276\u127d\u121d \u1260\u130b\u1321 \u12cd\u1235\u1325 \u1263\u12ed\u1308\u1299\u1365"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u12a5\u1294 \u130d\u1295 \u1260\u130c\u1273 \u12f0\u1235 \u12ed\u1208\u129b\u120d\u1364\n\u1260\u1218\u12f5\u1283\u1292\u1274 \u12a0\u121d\u120b\u12ad \u1210\u1224\u1275 \u12a0\u12f0\u122d\u130b\u1208\u1201\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1283\u12ed\u120c \u1290\u12cd\u1364\n\u12a5\u130d\u122e\u127c\u1295 \u12a5\u1295\u12f0 \u12cb\u120b \u12a5\u130d\u122e\u127d \u12eb\u12f0\u122d\u130b\u120d\u1364\n\u1260\u12a8\u134d\u1273\u12ce\u127c\u121d \u120b\u12ed \u12eb\u1235\u1204\u12f0\u129b\u120d\u1362\n\u1208\u1218\u12d8\u121d\u122b\u1295 \u12a0\u1208\u1243\u1365\n\u1260\u1263\u1208 \u12a0\u12cd\u1273\u122d \u1218\u1223\u122a\u12eb\u12ce\u127c\u1362"
                }
            }
        }
    }
}