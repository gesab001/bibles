        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1235\u1208 \u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u12a5\u1235\u1228\u129b \u12a8\u1206\u1290\u12cd \u1333\u12cd\u120e\u1235\u1293 \u12a8\u12c8\u1295\u12f5\u1219 \u1322\u121e\u1274\u12ce\u1235\u1365 \u1208\u1270\u12c8\u12f0\u12f0\u12cd\u1293 \u12a0\u1265\u122e\u1295 \u1208\u121a\u1220\u122b \u1208\u134a\u120d\u121e\u1293\u1365"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1208\u12a5\u1285\u1273\u127d\u1295\u121d \u1208\u12a0\u134d\u1265\u12eb\u1365 \u12a8\u12a5\u129b\u121d \u130b\u122d \u12a0\u1265\u122e \u12c8\u1273\u12f0\u122d \u1208\u1206\u1290\u12cd \u1208\u12a0\u122d\u12ad\u1333\u1365 \u1260\u1264\u1275\u1205\u121d \u120b\u1208\u127d \u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1295\u1364"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a8\u12a0\u1263\u1273\u127d\u1295 \u12a8\u130c\u1273\u121d \u12a8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u1338\u130b\u1293 \u1230\u120b\u121d \u1208\u12a5\u1293\u1295\u1270 \u12ed\u1201\u1295\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1260\u1338\u120e\u1274 \u12a5\u12eb\u1230\u1265\u12a9\u1205 \u1201\u120d\u130a\u12dc \u12a0\u121d\u120b\u12ac\u1295 \u12a0\u1218\u1230\u130d\u1293\u1208\u1201\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1260\u130c\u1273 \u1260\u12a2\u12e8\u1231\u1235 \u12d8\u1295\u12f5\u1364 \u1260\u1245\u12f1\u1233\u1295\u121d \u1201\u1209 \u12d8\u1295\u12f5 \u1235\u120b\u1208\u12cd \u134d\u1245\u122d\u1205\u1293 \u12a5\u121d\u1290\u1275\u1205 \u12a5\u1230\u121b\u1208\u1201\u1293\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12e8\u12a5\u121d\u1290\u1275\u1205\u121d \u1285\u1265\u1228\u1275\u1365 \u1260\u12a5\u129b \u12d8\u1295\u12f5 \u12eb\u1208\u12cd\u1295 \u1260\u130e \u1290\u1308\u122d \u1201\u1209 \u1260\u121b\u12c8\u1245\u1365 \u1208\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u134d\u122c \u12a5\u1295\u12f2\u12eb\u1348\u122b \u12a5\u1208\u121d\u1293\u1208\u1201\u1364"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12e8\u1245\u12f1\u1233\u1295 \u120d\u1265 \u1260\u12a0\u1295\u1270 \u1225\u122b \u1235\u1208 \u12d0\u1228\u1348\u1365 \u12c8\u1295\u12f5\u121c \u1206\u12ed! \u1260\u134d\u1245\u122d\u1205 \u1265\u12d9 \u12f0\u1235\u1273\u1295\u1293 \u1218\u133d\u1293\u1293\u1275\u1295 \u12a0\u130d\u129d\u127c\u12a0\u1208\u1201\u1293\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1235\u1208\u12da\u1205 \u12e8\u121a\u1308\u1263\u12cd\u1295 \u12a0\u12dd\u1205 \u12d8\u1295\u12f5 \u1260\u12ad\u122d\u1235\u1276\u1235 \u121d\u1295\u121d \u12a5\u1295\u12b3 \u1265\u12d9 \u12f5\u134d\u1228\u1275 \u1262\u1296\u1228\u129d\u1365"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12ed\u120d\u1241\u1295\u121d \u12a5\u1295\u12f0\u12da\u1205 \u12e8\u1206\u1295\u1201 \u12a5\u1294 \u1333\u12cd\u120e\u1235 \u123d\u121b\u130d\u120c\u12cd\u1365 \u12a0\u1201\u1295\u121d \u12f0\u130d\u121e \u1235\u1208 \u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u12a5\u1235\u1228\u129b \u12e8\u1206\u1295\u1201\u1365 \u1235\u1208 \u134d\u1245\u122d \u12a5\u1208\u121d\u1293\u1208\u1201\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u1260\u12a5\u1235\u122b\u1274 \u1235\u1208 \u12c8\u1208\u12f5\u1201\u1275 \u1235\u1208 \u120d\u1304 \u1235\u1208 \u12a6\u1294\u1232\u121e\u1235 \u12a5\u1208\u121d\u1295\u1203\u1208\u1201\u1364"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1260\u134a\u1275 \u12a0\u120d\u1320\u1240\u1218\u1205\u121d \u1290\u1260\u122d\u1364 \u12a0\u1201\u1295 \u130d\u1295 \u1208\u12a5\u1294\u121d \u1208\u12a0\u1295\u1270\u121d \u1260\u1323\u121d \u12a5\u12e8\u1320\u1240\u1218\u1295 \u1290\u12cd\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u12a5\u122d\u1231\u1295 \u12a5\u120d\u12a8\u12cb\u1208\u1201\u1364 \u12a0\u1295\u1270\u121d \u120d\u1264 \u12a5\u1295\u12f0\u121d\u1275\u1240\u1260\u120d \u12a0\u12f5\u122d\u1308\u1205 \u1270\u1240\u1260\u1208\u12cd\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u12a5\u1294\u1235 \u1235\u1208 \u12c8\u1295\u130c\u120d \u1260\u1308\u1320\u1218\u129d \u12a5\u1235\u122b\u1275 \u12a0\u1295\u1270\u1295 \u12c8\u12ad\u120e \u12a5\u1295\u12f2\u12eb\u1308\u1208\u130d\u1208\u129d \u1208\u122b\u1234 \u120b\u1235\u1240\u1228\u12cd \u1260\u1348\u1245\u12f5\u12a9 \u1290\u1260\u122d\u1364"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u1290\u1308\u122d \u130d\u1295 \u1260\u130e\u1290\u1275\u1205 \u1260\u1348\u1243\u12f5\u1205 \u12a5\u1295\u1302 \u1260\u130d\u12f5 \u12a5\u1295\u12f3\u12ed\u1206\u1295\u1365 \u1233\u120d\u121b\u12a8\u122d\u1205 \u121d\u1295\u121d \u1290\u1308\u122d \u120b\u12f0\u122d\u130d \u12a0\u120d\u1348\u120d\u130d\u1201\u121d\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12f3\u130d\u1218\u129b \u1270\u1240\u1265\u1208\u1205 \u1208\u12d8\u1208\u12d3\u1208\u121d \u12a5\u1295\u12f5\u1275\u12ed\u12d8\u12cd \u12ed\u1206\u1293\u120d \u121d\u1293\u120d\u1263\u1275 \u1208\u130a\u12dc\u12cd \u12e8\u1270\u1208\u12e8\u1205\u1364"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u12a8\u12a5\u1295\u130d\u12f2\u1205 \u12c8\u12f2\u1205 \u12a5\u1295\u12f0 \u1263\u122d\u12eb \u12a0\u12ed\u1206\u1295\u121d\u1364 \u1290\u1308\u122d \u130d\u1295 \u1208\u12a5\u1294 \u1260\u1270\u1208\u12e8 \u12e8\u1270\u12c8\u12f0\u12f0 \u12c8\u1295\u12f5\u121d \u12a8\u1206\u1290\u1365 \u1208\u12a0\u1295\u1270\u121b \u12ed\u120d\u1245 \u1260\u1225\u130b\u12cd\u121d \u1206\u1290 \u1260\u130c\u1273 \u12d8\u1295\u12f5 \u12a8\u1263\u122d\u12eb \u12e8\u121a\u123b\u120d \u12e8\u1270\u12c8\u12f0\u12f0 \u12c8\u1295\u12f5\u121d \u12a5\u1295\u12f4\u1275 \u12a0\u12ed\u1206\u1295\u121d?"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u12a5\u1295\u130d\u12f2\u1205 \u12a5\u1295\u12f0 \u12c8\u1295\u12f5\u121d\u1205 \u12a5\u1235\u12a8 \u1246\u1320\u122d\u12a8\u129d \u12f5\u1228\u1235\u1365 \u12a5\u1295\u12f0 \u12a5\u1294 \u12a0\u12f5\u122d\u1308\u1205 \u1270\u1240\u1260\u1208\u12cd\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u1260\u12a0\u1295\u12f3\u127d \u1290\u1308\u122d \u1260\u12f5\u120e\u1205 \u12a5\u1295\u12f0\u1206\u1290 \u130d\u1295 \u12c8\u12ed\u121d \u1265\u12f5\u122d \u12eb\u1208\u1260\u1275 \u12a5\u1295\u12f0\u1206\u1290\u1365 \u12ed\u1205\u1295 \u1260\u12a5\u1294 \u120b\u12ed \u1241\u1320\u122d\u1364"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12a5\u1294 \u1333\u12cd\u120e\u1235 \u201c\u12a5\u1294 \u12a5\u1218\u120d\u1230\u12cb\u1208\u1201\u1364\u201d \u1265\u12ec \u1260\u12a5\u1304 \u12a5\u133d\u134b\u1208\u1201\u1364 \u12a8\u12da\u1205\u121d \u1260\u120b\u12ed \u1208\u12a5\u1294 \u12e8\u122b\u1235\u1205 \u12f0\u130d\u121e \u1265\u12f5\u122d \u12a5\u1295\u12f3\u1208\u1265\u1205 \u12a0\u120b\u1235\u1273\u12cd\u1235\u1205\u121d\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u12a0\u12ce\u1295\u1365 \u12c8\u1295\u12f5\u121c \u1206\u12ed! \u1260\u130c\u1273 \u12ed\u1205\u1295\u1295 \u1309\u12f3\u12ed \u1348\u133d\u121d\u120d\u129d\u1362 \u1260\u12ad\u122d\u1235\u1276\u1235 \u120d\u1264\u1295 \u12a0\u1233\u122d\u134d\u120d\u129d\u1362"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u12a8\u121d\u1320\u12ed\u1245\u1205 \u12ed\u120d\u1245 \u12a0\u1265\u120d\u1320\u1205 \u12a5\u1295\u12f0\u121d\u1273\u12f0\u122d\u130d \u1260\u121b\u12c8\u1245 \u12a5\u1295\u12f0\u121d\u1275\u1273\u12d8\u12dd\u121d \u1260\u1218\u1270\u121b\u1218\u1295 \u12a5\u133d\u134d\u120d\u1203\u1208\u1201\u1362"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u12f0\u130d\u121e \u12a8\u12da\u1205\u121d \u130b\u122d \u12e8\u121b\u1235\u1270\u1293\u1308\u1303 \u1264\u1275 \u12a0\u12d8\u130b\u1305\u120d\u129d\u1364 \u1260\u1338\u120e\u1273\u127d\u1201 \u1208\u12a5\u1293\u1295\u1270 \u12a5\u1295\u12f0\u121d\u1230\u1325 \u1270\u1235\u134b \u12a0\u12f0\u122d\u130b\u1208\u1201\u1293\u1362"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u1260\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u12a8\u12a5\u1294 \u130b\u122d \u12e8\u1273\u1230\u1228 \u12a4\u1333\u134d\u122b"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u12a0\u1265\u1228\u12cd\u129d\u121d \u12e8\u121a\u1220\u1229 \u121b\u122d\u1246\u1235\u1293 \u12a0\u122d\u1235\u1325\u122e\u12ae\u1235 \u12f4\u121b\u1235\u121d \u1209\u1243\u1235\u121d \u1230\u120b\u121d\u1273 \u12eb\u1240\u122d\u1261\u120d\u1203\u120d\u1362"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u12e8\u130c\u1273\u127d\u1295 \u12e8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u1338\u130b \u12a8\u1218\u1295\u1348\u1233\u127d\u1201 \u130b\u122d \u12ed\u1201\u1295\u1364 \u12a0\u121c\u1295\u1362"
