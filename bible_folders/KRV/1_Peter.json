                "14": {
                    "verse_nr": "14",
                    "verse": "\uadf8\ub7ec\ub098 \uc758\ub97c \uc704\ud558\uc5ec \uace0\ub09c\uc744 \ubc1b\uc73c\uba74 \ubcf5 \uc788\ub294 \uc790\ub2c8 \uc800\ud76c\uc758 \ub450\ub824\uc6cc\ud568\uc744 \ub450\ub824\uc6cc \ub9d0\uba70 \uc18c\ub3d9\uce58 \ub9d0\uace0"
