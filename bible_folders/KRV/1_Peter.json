                "15": {
                    "verse_nr": "15",
                    "verse": "\ub108\ud76c \ub9c8\uc74c\uc5d0 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc8fc\ub85c \uc0bc\uc544 \uac70\ub8e9\ud558\uac8c \ud558\uace0 \ub108\ud76c \uc18d\uc5d0 \uc788\ub294 \uc18c\ub9dd\uc5d0 \uad00\ud55c \uc774\uc720\ub97c \ubb3b\ub294 \uc790\uc5d0\uac8c\ub294 \ub300\ub2f5\ud560 \uac83\uc744 \ud56d\uc0c1 \uc608\ube44\ud558\ub418 \uc628\uc720\uc640 \ub450\ub824\uc6c0\uc73c\ub85c \ud558\uace0"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uc120\ud55c \uc591\uc2ec\uc744 \uac00\uc9c0\ub77c \uc774\ub294 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0 \uc788\ub294 \ub108\ud76c\uc758 \uc120\ud589\uc744 \uc695\ud558\ub294 \uc790\ub4e4\ub85c \uadf8 \ube44\ubc29\ud558\ub294 \uc77c\uc5d0 \ubd80\ub044\ub7ec\uc6c0\uc744 \ub2f9\ud558\uac8c \ud558\ub824 \ud568\uc774\ub77c"
