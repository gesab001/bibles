                "17": {
                    "verse_nr": "17",
                    "verse": "\uc120\uc744 \ud589\ud568\uc73c\ub85c \uace0\ub09c \ubc1b\ub294 \uac83\uc774 \ud558\ub098\ub2d8\uc758 \ub73b\uc77c\ucc10\ub300 \uc545\uc744 \ud589\ud568\uc73c\ub85c \uace0\ub09c \ubc1b\ub294 \uac83\ubcf4\ub2e4 \ub098\uc73c\ub2c8\ub77c"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c\ub3c4 \ud55c\ubc88 \uc8c4\ub97c \uc704\ud558\uc5ec \uc8fd\uc73c\uc0ac \uc758\uc778\uc73c\ub85c\uc11c \ubd88\uc758\ud55c \uc790\ub97c \ub300\uc2e0\ud558\uc168\uc73c\ub2c8 \uc774\ub294 \uc6b0\ub9ac\ub97c \ud558\ub098\ub2d8 \uc55e\uc73c\ub85c \uc778\ub3c4\ud558\ub824 \ud558\uc2ec\uc774\ub77c \uc721\uccb4\ub85c\ub294 \uc8fd\uc784\uc744 \ub2f9\ud558\uc2dc\uace0 \uc601\uc73c\ub85c\ub294 \uc0b4\ub9ac\uc2ec\uc744 \ubc1b\uc73c\uc168\uc73c\ub2c8"
