{
    "version": "KRV",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub97c \uc704\ud558\uc5ec \uac07\ud78c \uc790 \ub41c \ubc14\uc6b8\uacfc \ubc0f \ud615\uc81c \ub514\ubaa8\ub370\ub294 \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc1b\ub294 \uc790\uc694 \ub3d9\uc5ed\uc790\uc778 \ube4c\ub808\ubaac\uacfc"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\ubc0f \uc790\ub9e4 \uc555\ube44\uc544\uc640 \ubc0f \uc6b0\ub9ac\uc640 \ud568\uaed8 \uad70\uc0ac\ub41c \uc544\ud0b5\ubcf4\uc640 \ub124 \uc9d1\uc5d0 \uc788\ub294 \uad50\ud68c\uc5d0\uac8c \ud3b8\uc9c0\ud558\ub178\ub2c8"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\ud558\ub098\ub2d8 \uc6b0\ub9ac \uc544\ubc84\uc9c0\uc640 \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub85c \uc887\uc544 \uc740\ud61c\uc640 \ud3c9\uac15\uc774 \ub108\ud76c\uc5d0\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4!"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\ub0b4\uac00 \ud56d\uc0c1 \ub0b4 \ud558\ub098\ub2d8\uaed8 \uac10\uc0ac\ud558\uace0 \uae30\ub3c4\ud560 \ub54c\uc5d0 \ub108\ub97c \ub9d0\ud568\uc740"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\uc8fc \uc608\uc218\uc640 \ubc0f \ubaa8\ub4e0 \uc131\ub3c4\uc5d0 \ub300\ud55c \ub124 \uc0ac\ub791\uacfc \ubbff\uc74c\uc774 \uc788\uc74c\uc744 \ub4e4\uc74c\uc774\ub2c8"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\uc774\ub85c\uc368 \ub124 \ubbff\uc74c\uc758 \uad50\uc81c\uac00 \uc6b0\ub9ac \uac00\uc6b4\ub370 \uc788\ub294 \uc120\uc744 \uc54c\uac8c \ud558\uace0 \uadf8\ub9ac\uc2a4\ub3c4\uaed8 \ubbf8\uce58\ub3c4\ub85d \uc5ed\uc0ac\ud558\ub290\ub2c8\ub77c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\ud615\uc81c\uc5ec \uc131\ub3c4\ub4e4\uc758 \ub9c8\uc74c\uc774 \ub108\ub85c \ub9d0\ubbf8\uc554\uc544 \ud3c9\uc548\ud568\uc744 \uc5bb\uc5c8\uc73c\ub2c8 \ub0b4\uac00 \ub108\uc758 \uc0ac\ub791\uc73c\ub85c \ub9ce\uc740 \uae30\uc068\uacfc \uc704\ub85c\ub97c \uc5bb\uc5c8\ub178\ub77c"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\uc774\ub7ec\ubbc0\ub85c \ub0b4\uac00 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \ub9ce\uc740 \ub2f4\ub825\uc744 \uac00\uc9c0\uace0 \ub124\uac8c \ub9c8\ub545\ud55c \uc77c\ub85c \uba85\ud560 \uc218 \uc788\uc73c\ub098"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\uc0ac\ub791\uc744 \uc778\ud558\uc5ec \ub3c4\ub9ac\uc5b4 \uac04\uad6c\ud558\ub178\ub2c8 \ub098\uc774 \ub9ce\uc740 \ub098 \ubc14\uc6b8\uc740 \uc9c0\uae08 \ub610 \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc704\ud558\uc5ec \uac07\ud78c \uc790 \ub418\uc5b4"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\uac07\ud78c \uc911\uc5d0\uc11c \ub0b3\uc740 \uc544\ub4e4 \uc624\ub124\uc2dc\ubaa8\ub97c \uc704\ud558\uc5ec \ub124\uac8c \uac04\uad6c\ud558\ub178\ub77c"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\uc800\uac00 \uc804\uc5d0\ub294 \ub124\uac8c \ubb34\uc775\ud558\uc600\uc73c\ub098 \uc774\uc81c\ub294 \ub098\uc640 \ub124\uac8c \uc720\uc775\ud558\ubbc0\ub85c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\ub124\uac8c \uc800\ub97c \ub3cc\ub824\ubcf4\ub0b4\ub178\ub2c8 \uc800\ub294 \ub0b4 \uc2ec\ubcf5\uc774\ub77c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\uc800\ub97c \ub0b4\uac8c \uba38\ubb3c\ub7ec \ub450\uc5b4 \ub0b4 \ubcf5\uc74c\uc744 \uc704\ud558\uc5ec \uac07\ud78c \uc911\uc5d0\uc11c \ub124 \ub300\uc2e0 \ub098\ub97c \uc12c\uac8c\uac8c \ud558\uace0\uc790 \ud558\ub098"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\ub2e4\ub9cc \ub124 \uc2b9\ub099\uc774 \uc5c6\uc774\ub294 \ub0b4\uac00 \uc544\ubb34 \uac83\ub3c4 \ud558\uae30\ub97c \uc6d0\uce58 \uc544\ub2c8\ud558\ub178\ub2c8 \uc774\ub294 \ub108\uc758 \uc120\ud55c \uc77c\uc774 \uc5b5\uc9c0\uac19\uc774 \ub418\uc9c0 \uc544\ub2c8\ud558\uace0 \uc790\uc758\ub85c \ub418\uac8c \ud558\ub824 \ud568\uc774\ub85c\ub77c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\uc800\uac00 \uc7a0\uc2dc \ub5a0\ub098\uac8c \ub41c \uac83\uc740 \uc774\ub97c \uc778\ud558\uc5ec \uc800\ub97c \uc601\uc6d0\ud788 \ub450\uac8c \ud568\uc774\ub2c8"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\uc774 \ud6c4\ub85c\ub294 \uc885\uacfc \uac19\uc774 \uc544\ub2c8\ud558\uace0 \uc885\uc5d0\uc11c \ub6f0\uc5b4\ub098 \uace7 \uc0ac\ub791 \ubc1b\ub294 \ud615\uc81c\ub85c \ub458 \uc790\ub77c \ub0b4\uac8c \ud2b9\ubcc4\ud788 \uadf8\ub7ec\ud558\uac70\ub4e0 \ud558\ubb3c\uba70 \uc721\uc2e0\uacfc \uc8fc \uc548\uc5d0\uc11c \uc0c1\uad00\ub41c \ub124\uac8c\ub7b4"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \ub124\uac00 \ub098\ub97c \ub3d9\ubb34\ub85c \uc54c\uc9c4\ub300 \uc800\ub97c \uc601\uc811\ud558\uae30\ub97c \ub0b4\uac8c \ud558\ub4ef \ud558\uace0"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\uc800\uac00 \ub9cc\uc77c \ub124\uac8c \ubd88\uc758\ub97c \ud558\uc600\uac70\ub098 \ub124\uac8c \uc9c4 \uac83\uc774 \uc788\uac70\ub4e0 \uc774\uac83\uc744 \ub0b4\uac8c\ub85c \ud68c\uacc4\ud558\ub77c"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\ub098 \ubc14\uc6b8\uc774 \uce5c\ud544\ub85c \uc4f0\ub178\ub2c8 \ub0b4\uac00 \uac1a\uc73c\ub824\ub2c8\uc640 \ub108\ub294 \uc774 \uc678\uc5d0 \ub124 \uc790\uc2e0\uc73c\ub85c \ub0b4\uac8c \ube5a\uc9c4 \uac83\uc744 \ub0b4\uac00 \ub9d0\ud558\uc9c0 \uc544\ub2c8\ud558\ub178\ub77c"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\uc624, \ud615\uc81c\uc5ec! \ub098\ub85c \uc8fc \uc548\uc5d0\uc11c \ub108\ub97c \uc778\ud558\uc5ec \uae30\uc068\uc744 \uc5bb\uac8c \ud558\uace0 \ub0b4 \ub9c8\uc74c\uc774 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \ud3c9\uc548\ud558\uac8c \ud558\ub77c"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\ub098\ub294 \ub124\uac00 \uc21c\uc885\ud568\uc744 \ud655\uc2e0\ud558\ubbc0\ub85c \ub124\uac8c \uc37c\ub178\ub2c8 \ub124\uac00 \ub098\uc758 \ub9d0\ubcf4\ub2e4 \ub354 \ud589\ud560 \uc904\uc744 \uc544\ub178\ub77c"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\uc624\uc9c1 \ub108\ub294 \ub098\ub97c \uc704\ud558\uc5ec \ucc98\uc18c\ub97c \uc608\ube44\ud558\ub77c \ub108\ud76c \uae30\ub3c4\ub85c \ub0b4\uac00 \ub108\ud76c\uc5d0\uac8c \ub098\uc544\uac00\uac8c \ud558\uc5ec \uc8fc\uc2dc\uae30\ub97c \ubc14\ub77c\ub178\ub77c"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218 \uc548\uc5d0\uc11c \ub098\uc640 \ud568\uaed8 \uac07\ud78c \uc790 \uc5d0\ubc14\ube0c\ub77c\uc640"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\ub610\ud55c \ub098\uc758 \ub3d9\uc5ed\uc790 \ub9c8\uac00, \uc544\ub9ac\uc2a4\ub2e4\uace0, \ub370\ub9c8, \ub204\uac00\uac00 \ubb38\uc548\ud558\ub290\ub2c8\ub77c"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\uc6b0\ub9ac \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc740\ud61c\uac00 \ub108\ud76c \uc2ec\ub839\uacfc \ud568\uaed8 \ud560\uc9c0\uc5b4\ub2e4!"
                }
            }
        }
    }
}