        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u10d0\u10db\u10d8\u10e1 \u10e8\u10d4\u10db\u10d3\u10d4\u10d2 \u10db\u10dd\u10d5\u10d0\u10e4\u10d4\u10dc \u10e9\u10d4\u10db\u10e1 \u10e1\u10e3\u10da\u10e1 \u10e7\u10dd\u10d5\u10d4\u10da \u10ee\u10dd\u10e0\u10ea\u10d8\u10d4\u10da\u10d6\u10d4 \u10d3\u10d0 \u10ed\u10d0\u10d1\u10e3\u10d9\u10d4\u10d1\u10d8 \u10ee\u10d8\u10da\u10d5\u10d4\u10d1\u10e1 \u10d8\u10ee\u10d8\u10da\u10d0\u10d5\u10d4\u10dc."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u10e7\u10db\u10d4\u10d1\u10d6\u10d4\u10ea \u10d3\u10d0 \u10db\u10ee\u10d4\u10d5\u10da\u10d4\u10d1\u10d6\u10d4\u10ea \u10db\u10dd\u10d5\u10d0\u10e4\u10d4\u10dc \u10d8\u10db \u10d3\u10e6\u10d4\u10e1 \u10e9\u10d4\u10db\u10e1 \u10e1\u10e3\u10da\u10e1."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u10d2\u10d0\u10db\u10dd\u10d5\u10d0\u10e9\u10d4\u10dc \u10dc\u10d8\u10e8\u10dc\u10d4\u10d1\u10e1 \u10ea\u10d0\u10d6\u10d4 \u10d3\u10d0 \u10db\u10d8\u10ec\u10d0\u10d6\u10d4: \u10e1\u10d8\u10e1\u10ee\u10da\u10e1, \u10ea\u10d4\u10ea\u10ee\u10da\u10e1 \u10d3\u10d0 \u10d9\u10d5\u10d0\u10db\u10da\u10d8\u10e1 \u10e1\u10d5\u10d4\u10e2\u10d4\u10d1\u10e1."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u10db\u10d6\u10d4 \u10d3\u10d0\u10d1\u10dc\u10d4\u10da\u10d3\u10d4\u10d1\u10d0 \u10d3\u10d0 \u10db\u10d7\u10d5\u10d0\u10e0\u10d4 \u10d2\u10d0\u10e1\u10d8\u10e1\u10ee\u10da\u10d8\u10d0\u10dc\u10d3\u10d4\u10d1\u10d0, \u10e1\u10d0\u10dc\u10d0\u10db \u10d3\u10d0\u10d3\u10d2\u10d4\u10d1\u10dd\u10d3\u10d4\u10e1 \u10e3\u10e4\u10da\u10d8\u10e1 \u10d3\u10e6\u10d4, \u10d3\u10d8\u10d3\u10d8 \u10d3\u10d0 \u10e1\u10d0\u10e8\u10d8\u10dc\u10d4\u10da\u10d8."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u10e7\u10d5\u10d4\u10da\u10d0, \u10d5\u10d8\u10dc\u10ea \u10d9\u10d8 \u10d0\u10ee\u10e1\u10d4\u10dc\u10d4\u10d1\u10e1 \u10db\u10d0\u10e8\u10d8\u10dc \u10e3\u10e4\u10da\u10d8\u10e1 \u10e1\u10d0\u10ee\u10d4\u10da\u10e1, \u10d2\u10d0\u10d3\u10d0\u10e0\u10e9\u10d4\u10d1\u10d0, \u10d8\u10e5\u10dc\u10d4\u10d1\u10d0 \u10ee\u10e1\u10dc\u10d0 \u10db\u10d0\u10d7\u10d7\u10d5\u10d8\u10e1\u10d0\u10ea, \u10d5\u10d8\u10e1\u10d0\u10ea \u10e3\u10e4\u10d0\u10da\u10d8 \u10db\u10dd\u10e3\u10ee\u10db\u10dd\u10d1\u10e1, \u10e0\u10dd\u10d2\u10dd\u10e0\u10ea \u10dc\u10d0\u10d7\u10e5\u10d5\u10d0\u10db\u10d8 \u10d0\u10e5\u10d5\u10e1 \u10e3\u10e4\u10d0\u10da\u10e1."
                },
