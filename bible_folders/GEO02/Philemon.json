{
    "version": "GEO02",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u10de\u10d0\u10d5\u10da\u10d4, \u10e5\u10e0\u10d8\u10e1\u10e2\u10d4 \u10d8\u10d4\u10e1\u10dd\u10e1 \u10de\u10d0\u10e2\u10d8\u10db\u10d0\u10e0\u10d8, \u10d3\u10d0 \u10eb\u10db\u10d0 \u10e2\u10d8\u10db\u10dd\u10d7\u10d4, \u10e4\u10d8\u10da\u10d8\u10db\u10dd\u10dc\u10e1 \u2013 \u10e9\u10d5\u10d4\u10dc\u10e1 \u10e1\u10d0\u10e7\u10d5\u10d0\u10e0\u10d4\u10da\u10e1\u10d0 \u10d3\u10d0 \u10d7\u10d0\u10dc\u10d0\u10db\u10e8\u10e0\u10dd\u10db\u10d4\u10da\u10e1"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u10d3\u10d0 \u10d0\u10de\u10e4\u10d8\u10d0\u10e1, \u10e1\u10d0\u10e7\u10d5\u10d0\u10e0\u10d4\u10da \u10d3\u10d0\u10e1, \u10d3\u10d0 \u10d0\u10e0\u10e5\u10d8\u10de\u10dd\u10e1\u10e1, \u10e9\u10d5\u10d4\u10dc\u10e1 \u10d7\u10d0\u10dc\u10d0\u10db\u10d4\u10d1\u10e0\u10eb\u10dd\u10da\u10e1, \u10d3\u10d0 \u10e8\u10d4\u10dc\u10e1 \u10e1\u10d0\u10e8\u10d8\u10dc\u10d0\u10dd \u10d4\u10d9\u10da\u10d4\u10e1\u10d8\u10d0\u10e1"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u10db\u10d0\u10d3\u10da\u10d8 \u10d7\u10e5\u10d5\u10d4\u10dc \u10d3\u10d0 \u10db\u10e8\u10d5\u10d8\u10d3\u10dd\u10d1\u10d0 \u10e9\u10d5\u10d4\u10dc\u10d8 \u10db\u10d0\u10db\u10d0 \u10e6\u10db\u10d4\u10e0\u10d7\u10d8\u10e1\u10d0 \u10d3\u10d0 \u10e3\u10e4\u10d0\u10da \u10d8\u10d4\u10e1\u10dd \u10e5\u10e0\u10d8\u10e1\u10e2\u10d4\u10e1\u10d0\u10d2\u10d0\u10dc."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u10d5\u10db\u10d0\u10d3\u10da\u10dd\u10d1 \u10e9\u10d4\u10db\u10e1 \u10e6\u10db\u10d4\u10e0\u10d7\u10e1, \u10e7\u10dd\u10d5\u10d4\u10da\u10d7\u10d5\u10d8\u10e1 \u10d2\u10d8\u10ee\u10e1\u10d4\u10dc\u10d4\u10d1 \u10e9\u10d4\u10db\u10e1 \u10da\u10dd\u10ea\u10d5\u10d4\u10d1\u10e8\u10d8"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u10db\u10d4\u10e1\u10db\u10d8\u10e1 \u10e0\u10d0 \u10e8\u10d4\u10dc\u10d8 \u10e1\u10d8\u10e7\u10d5\u10d0\u10e0\u10e3\u10da\u10d8 \u10d3\u10d0 \u10e0\u10ec\u10db\u10d4\u10dc\u10d0, \u10e0\u10dd\u10db\u10d4\u10da\u10d8\u10ea \u10e8\u10d4\u10dc \u10d2\u10d0\u10e5\u10d5\u10e1 \u10e3\u10e4\u10d0\u10da \u10d8\u10d4\u10e1\u10dd\u10e1 \u10d3\u10d0 \u10e7\u10d5\u10d4\u10da\u10d0 \u10ec\u10db\u10d8\u10d3\u10d8\u10e1 \u10db\u10d8\u10db\u10d0\u10e0\u10d7"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u10e0\u10d0\u10d7\u10d0 \u10e8\u10d4\u10dc\u10d8 \u10d7\u10d0\u10dc\u10d0\u10d6\u10d8\u10d0\u10e0\u10d4\u10d1\u10d0 \u10e0\u10ec\u10db\u10d4\u10dc\u10d0\u10e8\u10d8 \u10e5\u10db\u10d4\u10d3\u10d8\u10d7\u10d8 \u10d8\u10e7\u10dd\u10e1 \u10e8\u10d4\u10ea\u10dc\u10dd\u10d1\u10d8\u10e1\u10d0\u10e1 \u10e7\u10dd\u10d5\u10d4\u10da\u10d8 \u10e1\u10d8\u10d9\u10d4\u10d7\u10d8\u10e1\u10d0, \u10e0\u10dd\u10db\u10d4\u10da\u10d8\u10ea \u10d7\u10e5\u10d5\u10d4\u10dc\u10e8\u10d8\u10d0, \u10e5\u10e0\u10d8\u10e1\u10e2\u10d4\u10e1\u10d0\u10d7\u10d5\u10d8\u10e1"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u10d5\u10d8\u10dc\u10d0\u10d8\u10d3\u10d0\u10dc \u10db\u10d4 \u10d3\u10d8\u10d3\u10d8 \u10e1\u10d8\u10ee\u10d0\u10e0\u10e3\u10da\u10d8 \u10d3\u10d0 \u10dc\u10e3\u10d2\u10d4\u10e8\u10d8 \u10db\u10e5\u10dd\u10dc\u10d3\u10d0 \u10e8\u10d4\u10dc\u10e1 \u10e1\u10d8\u10e7\u10d5\u10d0\u10e0\u10e3\u10da\u10e8\u10d8, \u10e0\u10d0\u10d3\u10d2\u10d0\u10dc \u10e8\u10d4\u10dc\u10d8\u10d7 \u10d2\u10d0\u10ee\u10d0\u10da\u10d8\u10e1\u10d3\u10dc\u10d4\u10dc \u10ec\u10db\u10d8\u10d3\u10d0\u10d7\u10d0 \u10d2\u10e3\u10da\u10d4\u10d1\u10d8, \u10eb\u10db\u10d0\u10dd."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u10d0\u10db\u10d8\u10e2\u10dd\u10db, \u10e0\u10d0\u10d9\u10d8 \u10e5\u10e0\u10d8\u10e1\u10e2\u10d4\u10e8\u10d8 \u10db\u10d0\u10e5\u10d5\u10e1 \u10d3\u10d8\u10d3\u10d8 \u10e1\u10d8\u10d7\u10d0\u10db\u10d0\u10db\u10d4, \u10e0\u10dd\u10db \u10d2\u10d8\u10d1\u10e0\u10eb\u10d0\u10dc\u10dd, \u10e0\u10d0\u10ea \u10ef\u10d4\u10e0 \u10d0\u10e0\u10e1"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u10e1\u10d8\u10e7\u10d5\u10d0\u10e0\u10e3\u10da\u10d8\u10d7 \u10d2\u10d7\u10ee\u10dd\u10d5 \u10e3\u10e4\u10e0\u10dd, \u10d0\u10e0\u10d0 \u10d5\u10d8\u10dc\u10db\u10d4 \u10e1\u10ee\u10d5\u10d0, \u10d0\u10e0\u10d0\u10db\u10d4\u10d3 \u10db\u10d4, \u10db\u10dd\u10ee\u10e3\u10ea\u10d4\u10d1\u10e3\u10da\u10d8 \u10de\u10d0\u10d5\u10da\u10d4, \u10d0\u10ee\u10da\u10d0 \u10d9\u10d8 \u10d0\u10d2\u10e0\u10d4\u10d7\u10d5\u10d4 \u10e5\u10e0\u10d8\u10e1\u10e2\u10d4 \u10d8\u10d4\u10e1\u10dd\u10e1\u10d7\u10d5\u10d8\u10e1 \u10de\u10d0\u10e2\u10d8\u10db\u10d0\u10e0\u10d8"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u10d2\u10d7\u10ee\u10dd\u10d5 \u10e9\u10d4\u10db\u10e1 \u10e8\u10d5\u10d8\u10da\u10d6\u10d4, \u10dd\u10dc\u10d8\u10e1\u10d8\u10db\u10dd\u10e1\u10d6\u10d4, \u10e0\u10dd\u10db\u10d4\u10da\u10d8\u10ea \u10d5\u10e8\u10d5\u10d8 \u10e9\u10d4\u10db\u10e1 \u10d1\u10dd\u10e0\u10d9\u10d8\u10da\u10d4\u10d1\u10e8\u10d8."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u10d8\u10d2\u10d8 \u10dd\u10d3\u10d4\u10e1\u10e6\u10d0\u10ea \u10d2\u10d0\u10db\u10dd\u10e3\u10e1\u10d0\u10d3\u10d4\u10d2\u10d0\u10e0\u10d8 \u10d8\u10e7\u10dd \u10e8\u10d4\u10dc\u10d7\u10d5\u10d8\u10e1, \u10d0\u10ee\u10da\u10d0 \u10d9\u10d8 \u10e8\u10d4\u10dc\u10d7\u10d5\u10d8\u10e1\u10d0\u10ea \u10d3\u10d0 \u10e9\u10d4\u10db\u10d7\u10d5\u10d8\u10e1\u10d0\u10ea \u10d2\u10d0\u10db\u10dd\u10e1\u10d0\u10d3\u10d4\u10d2\u10d8\u10d0, \u10e0\u10dd\u10db\u10d4\u10da\u10e1\u10d0\u10ea \u10d2\u10d8\u10d1\u10e0\u10e3\u10dc\u10d4\u10d1."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u10ee\u10dd\u10da\u10dd \u10e8\u10d4\u10dc \u10db\u10d8\u10d8\u10e6\u10d4 \u10d8\u10d2\u10d8, \u10e0\u10dd\u10d2\u10dd\u10e0\u10ea \u10e9\u10d4\u10db\u10d8 \u10d2\u10e3\u10da\u10d8."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u10db\u10d8\u10dc\u10d3\u10dd\u10d3\u10d0 \u10e9\u10d4\u10db\u10d7\u10d0\u10dc \u10d3\u10d0\u10db\u10d4\u10e7\u10d4\u10dc\u10d4\u10d1\u10d8\u10dc\u10d0 \u10d8\u10d2\u10d8, \u10e0\u10d0\u10d7\u10d0 \u10e8\u10d4\u10dc\u10e1 \u10db\u10d0\u10d2\u10d8\u10d5\u10e0\u10d0\u10d3 \u10d4\u10db\u10e1\u10d0\u10ee\u10e3\u10e0\u10d0 \u10e9\u10d4\u10db\u10d7\u10d5\u10d8\u10e1 \u10e1\u10d0\u10ee\u10d0\u10e0\u10d4\u10d1\u10d8\u10e1 \u10d1\u10dd\u10e0\u10d9\u10d8\u10da\u10d4\u10d1\u10e8\u10d8"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u10db\u10d0\u10d2\u10e0\u10d0\u10db \u10e8\u10d4\u10dc\u10d8 \u10d7\u10d0\u10dc\u10ee\u10db\u10dd\u10d1\u10d8\u10e1 \u10d2\u10d0\u10e0\u10d4\u10e8\u10d4 \u10d0\u10e0\u10d0\u10e4\u10e0\u10d8\u10e1 \u10d2\u10d0\u10d9\u10d4\u10d7\u10d4\u10d1\u10d0 \u10d0\u10e0 \u10d5\u10d8\u10dc\u10d3\u10dd\u10db\u10d4, \u10e0\u10d0\u10d7\u10d0 \u10e8\u10d4\u10dc\u10d8 \u10d9\u10d4\u10d7\u10d8\u10da\u10d8 \u10e1\u10d0\u10e5\u10db\u10d4 \u10dc\u10d0\u10eb\u10d0\u10da\u10d0\u10d3\u10d4\u10d5\u10d8 \u10d9\u10d8 \u10d0\u10e0 \u10e7\u10dd\u10e4\u10d8\u10da\u10d8\u10e7\u10dd, \u10d0\u10e0\u10d0\u10db\u10d4\u10d3 \u10dc\u10d4\u10d1\u10d0\u10e7\u10dd\u10e4\u10da\u10dd\u10d1\u10d8\u10d7\u10d8."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u10d5\u10d8\u10dc\u10d0\u10d8\u10d3\u10d0\u10dc, \u10d8\u10e5\u10dc\u10d4\u10d1 \u10d8\u10db\u10d8\u10e2\u10dd\u10db \u10d3\u10d0\u10d2\u10e8\u10dd\u10e0\u10d3\u10d0 \u10d3\u10e0\u10dd\u10d4\u10d1\u10d8\u10d7, \u10e0\u10dd\u10db \u10e1\u10d0\u10db\u10e3\u10d3\u10d0\u10db\u10dd\u10d3 \u10db\u10d8\u10d2\u10d4\u10e6\u10dd \u10d8\u10d2\u10d8"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u10e3\u10d9\u10d5\u10d4 \u10d0\u10e0\u10d0 \u10e0\u10dd\u10d2\u10dd\u10e0\u10ea \u10db\u10dd\u10dc\u10d0, \u10d0\u10e0\u10d0\u10db\u10d4\u10d3 \u10db\u10dd\u10dc\u10d0\u10d6\u10d4 \u10db\u10d4\u10e2\u10d8, \u10e1\u10d0\u10e7\u10d5\u10d0\u10e0\u10d4\u10da\u10d8 \u10eb\u10db\u10d0, \u10d2\u10d0\u10dc\u10e1\u10d0\u10d9\u10e3\u10d7\u10e0\u10d4\u10d1\u10d8\u10d7 \u10e9\u10d4\u10db\u10d7\u10d5\u10d8\u10e1 \u10d3\u10d0, \u10db\u10d8\u10d7 \u10e3\u10db\u10d4\u10e2\u10d4\u10e1, \u10e8\u10d4\u10dc\u10d7\u10d5\u10d8\u10e1 \u10ee\u10dd\u10e0\u10ea\u10d8\u10d4\u10da\u10d0\u10d3\u10d0\u10ea \u10d3\u10d0 \u10e3\u10e4\u10d0\u10da\u10e8\u10d8\u10ea."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u10db\u10d0\u10e8, \u10d7\u10e3 \u10d7\u10d0\u10dc\u10d0\u10d6\u10d8\u10d0\u10e0\u10d0\u10d3 \u10db\u10d8\u10d2\u10d0\u10e9\u10dc\u10d8\u10d5\u10d0\u10e0, \u10db\u10d8\u10d8\u10e6\u10d4 \u10d8\u10e1, \u10d5\u10d8\u10d7\u10dd\u10db\u10ea \u10db\u10d4 \u10d5\u10d8\u10e7\u10dd."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u10d7\u10e3 \u10e0\u10d0\u10db\u10d4 \u10d2\u10d0\u10ec\u10e7\u10d4\u10dc\u10d8\u10dc\u10d0 \u10d0\u10dc \u10db\u10d0\u10e0\u10d7\u10d4\u10d1\u10e1 \u10e0\u10d0\u10d8\u10db\u10d4, \u10db\u10d4 \u10db\u10dd\u10db\u10d9\u10d8\u10d7\u10ee\u10d4."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u10db\u10d4, \u10de\u10d0\u10d5\u10da\u10d4\u10db, \u10d3\u10d0\u10d5\u10ec\u10d4\u10e0\u10d4 \u10e9\u10d4\u10db\u10d8 \u10ee\u10d4\u10da\u10d8\u10d7. \u10db\u10d4 \u10d2\u10d0\u10d3\u10d0\u10d5\u10d8\u10ee\u10d3\u10d8, \u10e0\u10d0\u10d7\u10d0 \u10d0\u10e0 \u10d2\u10d8\u10d7\u10ee\u10e0\u10d0, \u10e0\u10dd\u10db \u10e8\u10d4\u10dc \u10d7\u10d0\u10d5\u10d0\u10d3 \u10db\u10dd\u10d5\u10d0\u10da\u10d4 \u10ee\u10d0\u10e0 \u10e9\u10d4\u10db\u10d8."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u10db\u10d0\u10e8, \u10d0\u10e1\u10d4, \u10eb\u10db\u10d0\u10dd, \u10d5\u10d8\u10ee\u10d0\u10e0\u10d4\u10d1\u10d3\u10d4 \u10e8\u10d4\u10dc\u10d2\u10d0\u10dc \u10e3\u10e4\u10d0\u10da\u10e8\u10d8. \u10d2\u10d0\u10d0\u10ee\u10d0\u10da\u10d8\u10e1\u10d4 \u10e9\u10d4\u10db\u10d8 \u10d2\u10e3\u10da\u10d8 \u10e5\u10e0\u10d8\u10e1\u10e2\u10d4\u10e8\u10d8."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u10e8\u10d4\u10dc\u10d8 \u10db\u10dd\u10e0\u10e9\u10d8\u10da\u10d4\u10d1\u10d8\u10e1 \u10e1\u10d0\u10e1\u10dd\u10d4\u10d1\u10d8\u10d7 \u10db\u10dd\u10d2\u10ec\u10d4\u10e0\u10d4 \u10d3\u10d0 \u10d5\u10d8\u10ea\u10d8, \u10e0\u10dd\u10db \u10d8\u10db\u10d0\u10d6\u10d4 \u10db\u10d4\u10e2\u10e1\u10d0\u10ea \u10d2\u10d0\u10d0\u10d9\u10d4\u10d7\u10d4\u10d1, \u10d5\u10d8\u10d3\u10e0\u10d4 \u10d5\u10d0\u10db\u10d1\u10dd\u10d1."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u10d0\u10db\u10d0\u10e1\u10d7\u10d0\u10dc \u10d2\u10d0\u10db\u10d8\u10db\u10d6\u10d0\u10d3\u10d4 \u10d1\u10d8\u10dc\u10d0\u10ea, \u10d5\u10d8\u10dc\u10d0\u10d8\u10d3\u10d0\u10dc \u10d8\u10db\u10d4\u10d3\u10d8 \u10db\u10d0\u10e5\u10d5\u10e1, \u10e0\u10dd\u10db \u10d7\u10e5\u10d5\u10d4\u10dc\u10d8 \u10da\u10dd\u10ea\u10d5\u10d4\u10d1\u10d8\u10e1 \u10db\u10d4\u10dd\u10ee\u10d4\u10d1\u10d8\u10d7 \u10e3\u10d9\u10d0\u10dc \u10d3\u10d0\u10d2\u10d8\u10d1\u10e0\u10e3\u10dc\u10d3\u10d4\u10d1\u10d8\u10d7."
                },
                "23": {
                    "verse_nr": "23",
