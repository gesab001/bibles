{
    "version": "NCA",
    "book_name": "3 John",
    "book_nr": "64",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u092e\u0947\u0902 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0915\u0947 \u0905\u0917\u0941\u0935\u093e, \u0905\u092a\u0928 \u092e\u092f\u093e\u0930\u0942 \u0938\u0902\u0917\u0935\u093e\u0930\u0940 \u0917\u092f\u0941\u0938 \u0932\u093e \u090f \u091a\u093f\u091f\u094d\u0920\u0940 \u0932\u093f\u0916\u0924 \u0939\u0902\u0935, \u091c\u090a\u0928 \u0932\u093e \u092e\u0947\u0902\u0939 \u0938\u091a\u094d\u200d\u091a\u093e \u092e\u092f\u093e \u0915\u0930\u0925\u0902\u0935\u0964"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0939\u0947 \u092e\u092f\u093e\u0930\u0942 \u0938\u0902\u0917\u0940, \u092e\u0947\u0902\u0939 \u092a\u0930\u093e\u0925\u0928\u093e \u0915\u0930\u0924 \u0939\u0902\u0935 \u0915\u093f \u0924\u0947\u0902\u0939 \u092d\u0932\u093e \u091a\u0902\u0917\u093e \u0930\u0939 \u0905\u090a \u0939\u0930 \u090f\u0915 \u092c\u093e\u0924 \u092e \u0924\u094b\u0930 \u092d\u0932\u0908 \u0939\u094b\u0935\u092f, \u091c\u0907\u0938\u0928\u0947 \u0915\u093f \u0924\u0947\u0902\u0939 \u0906\u0924\u092e\u093e \u092e \u0938\u0939\u0940 \u0905\u0938\u0964"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u092e\u0947\u0902\u0939 \u0906\u0928\u0902\u0926 \u0932\u0947 \u092d\u0930 \u0917\u0947\u0902\u0935, \u091c\u092c \u0915\u0941\u091b\u0942 \u092d\u093e\u0908\u092e\u0928 \u0906\u0908\u0928 \u0905\u090a \u090f \u0917\u0935\u093e\u0939\u0940 \u0926\u0940\u0928 \u0915\u093f \u0924\u0947\u0902\u0939 \u092a\u0930\u092e\u0947\u0938\u0930 \u0915\u0947 \u0938\u0924 \u092c\u091a\u0928 \u092e \u091a\u0932\u0924 \u0939\u0935\u0938\u0964 \u0913\u092e\u0928 \u092c\u0924\u093e\u0908\u0928 \u0915\u093f \u0924\u0947\u0902\u0939 \u0915\u0907\u0938\u0928\u0947 \u092a\u0930\u092e\u0947\u0938\u0930 \u0915\u0947 \u0938\u0924 \u092c\u091a\u0928 \u0915\u0947 \u092e\u0941\u0924\u093e\u092c\u093f\u0915 \u091c\u093f\u0928\u0917\u0940 \u092c\u093f\u0924\u093e\u0935\u0924 \u0939\u0938\u0964"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u090f\u0915\u0930 \u0932\u0947 \u092c\u0921\u093c\u0915\u0947 \u092e\u094b\u0930 \u0915\u0930\u093e \u0905\u090a \u0915\u094b\u0928\u094b \u0906\u0928\u0902\u0926 \u0915\u0947 \u092c\u093e\u0924 \u0928\u0907\u0902 \u0939\u094b \u0938\u0915\u092f \u0915\u093f \u092e\u0947\u0902\u0939 \u090f \u0938\u0941\u0928\u0902\u0935 \u0915\u093f \u092e\u094b\u0930 \u0932\u0907\u0915\u093e\u092e\u0928 \u092a\u0930\u092e\u0947\u0938\u0930 \u0915\u0947 \u0938\u0924 \u092c\u091a\u0928 \u0915\u0947 \u092e\u0941\u0924\u093e\u092c\u093f\u0915 \u091a\u0932\u0924 \u0939\u0935\u0902\u092f\u0964"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0939\u0947 \u092e\u092f\u093e\u0930\u0942 \u0938\u0902\u0917\u0940, \u0924\u0947\u0902\u0939 \u0913 \u0915\u093e\u092e \u092e \u092c\u093f\u0938\u0935\u093e\u0938 \u0932\u0907\u0915 \u0920\u0939\u093f\u0930\u0947 \u0939\u0938, \u091c\u090a\u0928 \u0932\u093e \u0924\u0947\u0902\u0939 \u092d\u093e\u0908\u092e\u0928 \u092c\u0930 \u0915\u0930\u0924 \u0939\u0935\u0938, \u0916\u093e\u0938 \u0915\u0930\u0915\u0947 \u091c\u092c \u0913\u092e\u0928 \u0924\u094b\u0930 \u092c\u0930 \u0905\u0928\u091c\u093e\u0928 \u0905\u0902\u092f\u0964"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0913\u092e\u0928 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0932\u093e \u0924\u094b\u0930 \u092e\u092f\u093e \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e \u092c\u0924\u093e\u092f \u0939\u0935\u0902\u092f\u0964 \u0924\u0947\u0902\u0939 \u0905\u0907\u0938\u0928\u0947 \u0915\u0930: \u0913\u092e\u0928 \u0915\u0947 \u0906\u0918\u0942 \u0915\u0947 \u092f\u093e\u0924\u0930\u093e \u0915\u0947 \u0905\u0907\u0938\u0928\u0947 \u092a\u0930\u092c\u0902\u0927 \u0915\u0930 \u0915\u093f \u092a\u0930\u092e\u0947\u0938\u0930 \u0939 \u0916\u0941\u0938 \u0939\u094b\u0935\u092f\u0964"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0915\u093e\u092c\u0930\u0915\u093f \u0913\u092e\u0928 \u092e\u0938\u0940\u0939 \u0915\u0947 \u0938\u0947\u0935\u093e \u092e \u0928\u093f\u0915\u0930\u0947 \u0939\u0935\u0902\u092f \u0905\u090a \u092e\u0938\u0940\u0939 \u090a\u092a\u0930 \u092c\u093f\u0938\u0935\u093e\u0938 \u0928\u0907\u0902 \u0915\u0930\u0907\u092f\u093e\u092e\u0928 \u0932\u0947 \u090f\u092e\u0928 \u0915\u0941\u091b\u0942 \u0928\u0907\u0902 \u0932\u0947\u0935\u0902\u092f\u0964"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u090f\u0915\u0930\u0938\u0947\u0924\u093f \u0939\u092e\u0928 \u0932\u093e \u0905\u0907\u0938\u0928\u0947 \u092e\u0928\u0916\u0947\u092e\u0928 \u0915\u0947 \u0938\u0947\u0935\u093e-\u0938\u0924\u0915\u093e\u0930 \u0915\u0930\u0928\u093e \u091a\u093e\u0939\u0940, \u0924\u093e\u0915\u093f \u0939\u092e\u0928 \u092a\u0930\u092e\u0947\u0938\u0930 \u0915\u0947 \u0938\u0924 \u092c\u091a\u0928 \u0916\u093e\u0924\u093f\u0930 \u090f\u0915 \u0938\u0902\u0917 \u0915\u093e\u092e \u0915\u0930 \u0938\u0915\u0928\u0964"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u092e\u0947\u0902\u0939 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0932\u093e \u0932\u093f\u0916\u0947 \u0939\u0935\u0902\u0935, \u092a\u0930 \u0926\u093f\u092f\u0941\u0924\u094d\u0930\u093f\u092b\u0947\u0938, \u091c\u090a\u0928 \u0939 \u0913\u092e\u0928 \u0915\u0947 \u092e\u0941\u0916\u093f\u092f\u093e \u092c\u0928\u0947 \u091a\u093e\u0939\u0924 \u0939\u0935\u092f, \u092e\u094b\u0930 \u092c\u093e\u0924 \u0932\u093e \u0928\u0907\u0902 \u092e\u093e\u0928\u092f\u0964"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u090f\u0915\u0930\u0938\u0947\u0924\u093f \u091c\u092c \u092e\u0947\u0902\u0939 \u0906\u0939\u0942\u0902, \u0924 \u0913 \u092c\u093e\u0924\u092e\u0928 \u0932\u093e \u092c\u0924\u093e\u0939\u0942\u0902 \u091c\u0947\u0932\u093e \u0913\u0939 \u0915\u0930\u0924 \u0939\u0935\u092f\u0964 \u0913\u0939 \u0939\u092e\u0930 \u092c\u0926\u0928\u093e\u092e\u0940 \u0915\u0930\u0924 \u0939\u0935\u092f\u0964 \u0913\u0932\u093e \u090f\u0915\u0930 \u0932\u0947 \u0938\u0902\u0924\u094b\u0938 \u0928\u0907\u0902 \u0939\u094b\u0908\u0938, \u0924 \u0913\u0939 \u092d\u093e\u0908\u092e\u0928 \u0932\u093e \u0917\u0930\u0939\u0928 \u0918\u0932\u094b \u0928\u0907\u0902 \u0915\u0930\u092f, \u0905\u090a \u091c\u090a\u0928 \u092e\u0928 \u092d\u093e\u0908\u092e\u0928 \u0932\u093e \u0917\u0930\u0939\u0928 \u0915\u0930\u0947 \u091a\u093e\u0939\u0925\u0947\u0902, \u0913\u0939 \u0913\u092e\u0928 \u0932\u093e \u092e\u0928\u093e \u0915\u0930\u0925\u0947 \u0905\u090a \u0913\u092e\u0928 \u0932\u093e \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0932\u0947 \u0928\u093f\u0915\u093e\u0930 \u0926\u0947\u0925\u0947\u0964"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0939\u0947 \u092e\u092f\u093e\u0930\u0942 \u0938\u0902\u0917\u0940, \u092c\u0941\u0930\u0908 \u0915\u0947 \u0928\u0907\u0902 \u092a\u0930 \u092d\u0932\u0908 \u0915\u0947 \u0928\u0915\u0932 \u0915\u0930\u0964 \u091c\u090a\u0928 \u0939 \u092d\u0932\u0908 \u0915\u0947 \u0915\u093e\u092e \u0915\u0930\u0925\u0947, \u0913\u0939 \u092a\u0930\u092e\u0947\u0938\u0930 \u0915\u0947 \u0905\u092f, \u0905\u090a \u091c\u090a\u0928 \u0939 \u092c\u0941\u0930\u0908 \u0915\u0947 \u0915\u093e\u092e \u0915\u0930\u0925\u0947, \u0913\u0939 \u092a\u0930\u092e\u0947\u0938\u0930 \u0932\u093e \u0928\u0907\u0902 \u091c\u093e\u0928\u092f\u0964"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0926\u0947\u092e\u0947\u0924\u093f\u0930\u093f\u092f\u0941\u0938 \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e \u091c\u092e\u094d\u092e\u094b \u091d\u0928 \u092c\u0928\u0947 \u092c\u093e\u0924 \u0915\u0939\u093f\u0925\u0947\u0902\u2009\u2013\u2009\u0905\u090a \u0924 \u0905\u090a \u0938\u0924 \u0939 \u0916\u0941\u0926 \u090f\u0939\u0940 \u0915\u0939\u093f\u0925\u0947\u0964 \u0939\u092e\u0928 \u0918\u0932\u094b \u0913\u0915\u0930 \u092c\u093e\u0930\u0947 \u092e \u092c\u0928\u0947 \u092c\u093e\u0924 \u0915\u0939\u093f\u0925\u0928, \u0905\u090a \u0924\u0947\u0902\u0939 \u091c\u093e\u0928\u0925\u0938 \u0915\u093f \u0939\u092e\u0930 \u0917\u0935\u093e\u0939\u0940 \u0939 \u0938\u0939\u0940 \u090f\u0964"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u092e\u094b\u0930 \u0915\u0930\u093e \u0924\u094b\u0932\u093e \u0932\u093f\u0916\u0947 \u092c\u0930 \u0915\u0924\u0915\u094b \u092c\u093e\u0924 \u0939\u0935\u092f, \u092a\u0930 \u092e\u0947\u0902\u0939 \u0915\u0932\u092e \u0905\u090a \u0938\u093f\u092f\u093e\u0939\u0940 \u0932\u0947 \u0932\u093f\u0916\u0947 \u0928\u0907\u0902 \u091a\u093e\u0939\u0924 \u0939\u0902\u0935\u0964"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u092e\u094b\u0932\u093e \u0906\u0938\u093e \u0939\u0935\u092f \u0915\u093f \u0924\u094b\u0930 \u0938\u0902\u0917 \u091c\u0932\u094d\u0926\u0940 \u092d\u0947\u0902\u091f \u0939\u094b\u0939\u0940 \u0905\u090a \u0924\u092c \u0939\u092e\u0928 \u090f\u0915 \u0938\u0902\u0917 \u092c\u0908\u0920\u0915\u0947 \u0917\u094b\u0920\u093f\u092f\u093e\u092c\u094b\u0964"
                }
            }
        }
    }
}