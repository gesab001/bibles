        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u062b\u0645 \u0646\u0633\u0627\u0644\u0643\u0645 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e\u0648\u0629 \u0645\u0646 \u062c\u0647\u0629 \u0645\u062c\u064a\u0621 \u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0648\u0627\u062c\u062a\u0645\u0627\u0639\u0646\u0627 \u0627\u0644\u064a\u0647\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0627\u0646 \u0644\u0627 \u062a\u062a\u0632\u0639\u0632\u0639\u0648\u0627 \u0633\u0631\u064a\u0639\u0627 \u0639\u0646 \u0630\u0647\u0646\u0643\u0645 \u0648\u0644\u0627 \u062a\u0631\u062a\u0627\u0639\u0648\u0627 \u0644\u0627 \u0628\u0631\u0648\u062d \u0648\u0644\u0627 \u0628\u0643\u0644\u0645\u0629 \u0648\u0644\u0627 \u0628\u0631\u0633\u0627\u0644\u0629 \u0643\u0627\u0646\u0647\u0627 \u0645\u0646\u0627 \u0627\u064a \u0627\u0646 \u064a\u0648\u0645 \u0627\u0644\u0645\u0633\u064a\u062d \u0642\u062f \u062d\u0636\u0631.\n"
                },
                "3": {
