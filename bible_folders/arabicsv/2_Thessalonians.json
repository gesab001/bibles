        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u0628\u0648\u0644\u0633 \u0648\u0633\u0644\u0648\u0627\u0646\u0633 \u0648\u062a\u064a\u0645\u0648\u062b\u0627\u0648\u0633 \u0627\u0644\u0649 \u0643\u0646\u064a\u0633\u0629 \u0627\u0644\u062a\u0633\u0627\u0644\u0648\u0646\u064a\u0643\u064a\u064a\u0646 \u0641\u064a \u0627\u0644\u0644\u0647 \u0627\u0628\u064a\u0646\u0627 \u0648\u0627\u0644\u0631\u0628 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0646\u0639\u0645\u0629 \u0644\u0643\u0645 \u0648\u0633\u0644\u0627\u0645 \u0645\u0646 \u0627\u0644\u0644\u0647 \u0627\u0628\u064a\u0646\u0627 \u0648\u0627\u0644\u0631\u0628 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u064a\u0646\u0628\u063a\u064a \u0644\u0646\u0627 \u0627\u0646 \u0646\u0634\u0643\u0631 \u0627\u0644\u0644\u0647 \u0643\u0644 \u062d\u064a\u0646 \u0645\u0646 \u062c\u0647\u062a\u0643\u0645 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e\u0648\u0629 \u0643\u0645\u0627 \u064a\u062d\u0642 \u0644\u0627\u0646 \u0627\u064a\u0645\u0627\u0646\u0643\u0645 \u064a\u0646\u0645\u0648 \u0643\u062b\u064a\u0631\u0627 \u0648\u0645\u062d\u0628\u0629 \u0643\u0644 \u0648\u0627\u062d\u062f \u0645\u0646\u0643\u0645 \u062c\u0645\u064a\u0639\u0627 \u0628\u0639\u0636\u0643\u0645 \u0644\u0628\u0639\u0636 \u062a\u0632\u062f\u0627\u062f\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u062d\u062a\u0649 \u0627\u0646\u0646\u0627 \u0646\u062d\u0646 \u0627\u0646\u0641\u0633\u0646\u0627 \u0646\u0641\u062a\u062e\u0631 \u0628\u0643\u0645 \u0641\u064a \u0643\u0644 \u0643\u0646\u0627\u0626\u0633 \u0627\u0644\u0644\u0647 \u0645\u0646 \u0627\u062c\u0644 \u0635\u0628\u0631\u0643\u0645 \u0648\u0627\u064a\u0645\u0627\u0646\u0643\u0645 \u0641\u064a \u062c\u0645\u064a\u0639 \u0627\u0636\u0637\u0647\u0627\u062f\u0627\u062a\u0643\u0645 \u0648\u0627\u0644\u0636\u064a\u0642\u0627\u062a \u0627\u0644\u062a\u064a \u062a\u062d\u062a\u0645\u0644\u0648\u0646\u0647\u0627\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u0628\u064a\u0646\u0629 \u0639\u0644\u0649 \u0642\u0636\u0627\u0621 \u0627\u0644\u0644\u0647 \u0627\u0644\u0639\u0627\u062f\u0644 \u0627\u0646\u0643\u0645 \u062a\u0624\u0647\u0644\u0648\u0646 \u0644\u0645\u0644\u0643\u0648\u062a \u0627\u0644\u0644\u0647 \u0627\u0644\u0630\u064a \u0644\u0627\u062c\u0644\u0647 \u062a\u062a\u0623\u0644\u0645\u0648\u0646 \u0627\u064a\u0636\u0627.\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u0627\u0630 \u0647\u0648 \u0639\u0627\u062f\u0644 \u0639\u0646\u062f \u0627\u0644\u0644\u0647 \u0627\u0646 \u0627\u0644\u0630\u064a\u0646 \u064a\u0636\u0627\u064a\u0642\u0648\u0646\u0643\u0645 \u064a\u062c\u0627\u0632\u064a\u0647\u0645 \u0636\u064a\u0642\u0627\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u0648\u0627\u064a\u0627\u0643\u0645 \u0627\u0644\u0630\u064a\u0646 \u062a\u062a\u0636\u0627\u064a\u0642\u0648\u0646 \u0631\u0627\u062d\u0629 \u0645\u0639\u0646\u0627 \u0639\u0646\u062f \u0627\u0633\u062a\u0639\u0644\u0627\u0646 \u0627\u0644\u0631\u0628 \u064a\u0633\u0648\u0639 \u0645\u0646 \u0627\u0644\u0633\u0645\u0627\u0621 \u0645\u0639 \u0645\u0644\u0627\u0626\u0643\u0629 \u0642\u0648\u062a\u0647\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u0641\u064a \u0646\u0627\u0631 \u0644\u0647\u064a\u0628 \u0645\u0639\u0637\u064a\u0627 \u0646\u0642\u0645\u0629 \u0644\u0644\u0630\u064a\u0646 \u0644\u0627 \u064a\u0639\u0631\u0641\u0648\u0646 \u0627\u0644\u0644\u0647 \u0648\u0627\u0644\u0630\u064a\u0646 \u0644\u0627 \u064a\u0637\u064a\u0639\u0648\u0646 \u0627\u0646\u062c\u064a\u0644 \u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u0627\u0644\u0630\u064a\u0646 \u0633\u064a\u0639\u0627\u0642\u0628\u0648\u0646 \u0628\u0647\u0644\u0627\u0643 \u0627\u0628\u062f\u064a \u0645\u0646 \u0648\u062c\u0647 \u0627\u0644\u0631\u0628 \u0648\u0645\u0646 \u0645\u062c\u062f \u0642\u0648\u062a\u0647\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u0645\u062a\u0649 \u062c\u0627\u0621 \u0644\u064a\u062a\u0645\u062c\u062f \u0641\u064a \u0642\u062f\u064a\u0633\u064a\u0647 \u0648\u064a\u062a\u0639\u062c\u0628 \u0645\u0646\u0647 \u0641\u064a \u062c\u0645\u064a\u0639 \u0627\u0644\u0645\u0624\u0645\u0646\u064a\u0646. \u0644\u0627\u0646 \u0634\u0647\u0627\u062f\u062a\u0646\u0627 \u0639\u0646\u062f\u0643\u0645 \u0635\u062f\u0642\u062a. \u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645.\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u0627\u0644\u0627\u0645\u0631 \u0627\u0644\u0630\u064a \u0644\u0627\u062c\u0644\u0647 \u0646\u0635\u0644\u0651\u064a \u0627\u064a\u0636\u0627 \u0643\u0644 \u062d\u064a\u0646 \u0645\u0646 \u062c\u0647\u062a\u0643\u0645 \u0627\u0646 \u064a\u0624\u0647\u0644\u0643\u0645 \u0627\u0644\u0647\u0646\u0627 \u0644\u0644\u062f\u0639\u0648\u0629 \u0648\u064a\u0643\u0645\u0644 \u0643\u0644 \u0645\u0633\u0631\u0629 \u0627\u0644\u0635\u0644\u0627\u062d \u0648\u0639\u0645\u0644 \u0627\u0644\u0627\u064a\u0645\u0627\u0646 \u0628\u0642\u0648\u0629\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u0644\u0643\u064a \u064a\u062a\u0645\u062c\u062f \u0627\u0633\u0645 \u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0641\u064a\u0643\u0645 \u0648\u0627\u0646\u062a\u0645 \u0641\u064a\u0647 \u0628\u0646\u0639\u0645\u0629 \u0627\u0644\u0647\u0646\u0627 \u0648\u0627\u0644\u0631\u0628 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d\n"
