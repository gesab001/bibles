                "1": {
                    "verse_nr": 1,
                    "verse": "\u0627\u062e\u064a\u0631\u0627 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e\u0648\u0629 \u0635\u0651\u0644\u0648\u0627 \u0644\u0627\u062c\u0644\u0646\u0627 \u0644\u0643\u064a \u062a\u062c\u0631\u064a \u0643\u0644\u0645\u0629 \u0627\u0644\u0631\u0628 \u0648\u062a\u062a\u0645\u062c\u062f \u0643\u0645\u0627 \u0639\u0646\u062f\u0643\u0645 \u0627\u064a\u0636\u0627.\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0648\u0644\u0643\u064a \u0646\u0646\u0642\u0630 \u0645\u0646 \u0627\u0644\u0646\u0627\u0633 \u0627\u0644\u0627\u0631\u062f\u064a\u0627\u0621 \u0627\u0644\u0627\u0634\u0631\u0627\u0631. \u0644\u0627\u0646 \u0627\u0644\u0627\u064a\u0645\u0627\u0646 \u0644\u064a\u0633 \u0644\u0644\u062c\u0645\u064a\u0639.\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u0627\u0645\u064a\u0646 \u0647\u0648 \u0627\u0644\u0631\u0628 \u0627\u0644\u0630\u064a \u0633\u064a\u062b\u0628\u062a\u0643\u0645 \u0648\u064a\u062d\u0641\u0638\u0643\u0645 \u0645\u0646 \u0627\u0644\u0634\u0631\u064a\u0631.\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u0648\u0646\u062b\u0642 \u0628\u0627\u0644\u0631\u0628 \u0645\u0646 \u062c\u0647\u062a\u0643\u0645 \u0627\u0646\u0643\u0645 \u062a\u0641\u0639\u0644\u0648\u0646 \u0645\u0627 \u0646\u0648\u0635\u064a\u0643\u0645 \u0628\u0647 \u0648\u0633\u062a\u0641\u0639\u0644\u0648\u0646 \u0627\u064a\u0636\u0627.\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u0648\u0627\u0644\u0631\u0628 \u064a\u0647\u062f\u064a \u0642\u0644\u0648\u0628\u0643\u0645 \u0627\u0644\u0649 \u0645\u062d\u0628\u0629 \u0627\u0644\u0644\u0647 \u0648\u0627\u0644\u0649 \u0635\u0628\u0631 \u0627\u0644\u0645\u0633\u064a\u062d\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u062b\u0645 \u0646\u0648\u0635\u064a\u0643\u0645 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e\u0648\u0629 \u0628\u0627\u0633\u0645 \u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0627\u0646 \u062a\u062a\u062c\u0646\u0628\u0648\u0627 \u0643\u0644 \u0627\u062e \u064a\u0633\u0644\u0643 \u0628\u0644\u0627 \u062a\u0631\u062a\u064a\u0628 \u0648\u0644\u064a\u0633 \u062d\u0633\u0628 \u0627\u0644\u062a\u0639\u0644\u064a\u0645 \u0627\u0644\u0630\u064a \u0627\u062e\u0630\u0647 \u0645\u0646\u0627.\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u0627\u0630 \u0627\u0646\u062a\u0645 \u062a\u0639\u0631\u0641\u0648\u0646 \u0643\u064a\u0641 \u064a\u062c\u0628 \u0627\u0646 \u064a\u062a\u0645\u062b\u0644 \u0628\u0646\u0627 \u0644\u0627\u0646\u0646\u0627 \u0644\u0645 \u0646\u0633\u0644\u0643 \u0628\u0644\u0627 \u062a\u0631\u062a\u064a\u0628 \u0628\u064a\u0646\u0643\u0645\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u0648\u0644\u0627 \u0627\u0643\u0644\u0646\u0627 \u062e\u0628\u0632\u0627 \u0645\u062c\u0627\u0646\u0627 \u0645\u0646 \u0627\u062d\u062f \u0628\u0644 \u0643\u0646\u0627 \u0646\u0634\u062a\u063a\u0644 \u0628\u062a\u0639\u0628 \u0648\u0643\u062f \u0644\u064a\u0644\u0627 \u0648\u0646\u0647\u0627\u0631\u0627 \u0644\u0643\u064a \u0644\u0627 \u0646\u062b\u0642\u0644 \u0639\u0644\u0649 \u0627\u062d\u062f \u0645\u0646\u0643\u0645.\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u0644\u064a\u0633 \u0627\u0646 \u0644\u0627 \u0633\u0644\u0637\u0627\u0646 \u0644\u0646\u0627 \u0628\u0644 \u0644\u0643\u064a \u0646\u0639\u0637\u064a\u0643\u0645 \u0627\u0646\u0641\u0633\u0646\u0627 \u0642\u062f\u0648\u0629 \u062d\u062a\u0649 \u062a\u062a\u0645\u062b\u0644\u0648\u0627 \u0628\u0646\u0627.\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u0641\u0627\u0646\u0646\u0627 \u0627\u064a\u0636\u0627 \u062d\u064a\u0646 \u0643\u0646\u0627 \u0639\u0646\u062f\u0643\u0645 \u0627\u0648\u0635\u064a\u0646\u0627\u0643\u0645 \u0628\u0647\u0630\u0627 \u0627\u0646\u0647 \u0627\u0646 \u0643\u0627\u0646 \u0627\u062d\u062f \u0644\u0627 \u064a\u0631\u064a\u062f \u0627\u0646 \u064a\u0634\u062a\u063a\u0644 \u0641\u0644\u0627 \u064a\u0627\u0643\u0644 \u0627\u064a\u0636\u0627.\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u0644\u0627\u0646\u0646\u0627 \u0646\u0633\u0645\u0639 \u0627\u0646 \u0642\u0648\u0645\u0627 \u064a\u0633\u0644\u0643\u0648\u0646 \u0628\u064a\u0646\u0643\u0645 \u0628\u0644\u0627 \u062a\u0631\u062a\u064a\u0628 \u0644\u0627 \u064a\u0634\u062a\u063a\u0644\u0648\u0646 \u0634\u064a\u0626\u0627 \u0628\u0644 \u0647\u0645 \u0641\u0636\u0648\u0644\u064a\u0648\u0646.\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u0641\u0645\u062b\u0644 \u0647\u0624\u0644\u0627\u0621 \u0646\u0648\u0635\u064a\u0647\u0645 \u0648\u0646\u0639\u0638\u0647\u0645 \u0628\u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0627\u0646 \u064a\u0634\u062a\u063a\u0644\u0648\u0627 \u0628\u0647\u062f\u0648\u0621 \u0648\u064a\u0623\u0643\u0644\u0648\u0627 \u062e\u0628\u0632 \u0627\u0646\u0641\u0633\u0647\u0645.\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u0627\u0645\u0627 \u0627\u0646\u062a\u0645 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e\u0648\u0629 \u0641\u0644\u0627 \u062a\u0641\u0634\u0644\u0648\u0627 \u0641\u064a \u0639\u0645\u0644 \u0627\u0644\u062e\u064a\u0631.\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u0648\u0627\u0646 \u0643\u0627\u0646 \u0627\u062d\u062f \u0644\u0627 \u064a\u0637\u064a\u0639 \u0643\u0644\u0627\u0645\u0646\u0627 \u0628\u0627\u0644\u0631\u0633\u0627\u0644\u0629 \u0641\u0633\u0645\u0648\u0627 \u0647\u0630\u0627 \u0648\u0644\u0627 \u062a\u062e\u0627\u0644\u0637\u0648\u0647 \u0644\u0643\u064a \u064a\u062e\u062c\u0644.\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u0648\u0644\u0643\u0646 \u0644\u0627 \u062a\u062d\u0633\u0628\u0648\u0647 \u0643\u0639\u062f\u0648 \u0628\u0644 \u0627\u0646\u0630\u0631\u0648\u0647 \u0643\u0623\u062e.\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u0648\u0631\u0628 \u0627\u0644\u0633\u0644\u0627\u0645 \u0646\u0641\u0633\u0647 \u064a\u0639\u0637\u064a\u0643\u0645 \u0627\u0644\u0633\u0644\u0627\u0645 \u062f\u0627\u0626\u0645\u0627 \u0645\u0646 \u0643\u0644 \u0648\u062c\u0647. \u0627\u0644\u0631\u0628 \u0645\u0639 \u062c\u0645\u064a\u0639\u0643\u0645\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u0627\u0644\u0633\u0644\u0627\u0645 \u0628\u064a\u062f\u064a \u0627\u0646\u0627 \u0628\u0648\u0644\u0633 \u0627\u0644\u0630\u064a \u0647\u0648 \u0639\u0644\u0627\u0645\u0629 \u0641\u064a \u0643\u0644 \u0631\u0633\u0627\u0644\u0629. \u0647\u0643\u0630\u0627 \u0627\u0646\u0627 \u0627\u0643\u062a\u0628.\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u0646\u0639\u0645\u0629 \u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0645\u0639 \u062c\u0645\u064a\u0639\u0643\u0645. \u0622\u0645\u064a\u0646\n"
                }
            }
        }
