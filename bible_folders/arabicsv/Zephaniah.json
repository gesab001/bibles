{
    "version": "arabicsv",
    "book_name": "Zephaniah",
    "book_nr": "36",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u0643\u0644\u0645\u0629 \u0627\u0644\u0631\u0628 \u0627\u0644\u062a\u064a \u0635\u0627\u0631\u062a \u0627\u0644\u0649 \u0635\u0641\u0646\u064a\u0627 \u0628\u0646 \u0643\u0648\u0634\u064a \u0628\u0646 \u062c\u062f\u0644\u064a\u0627 \u0628\u0646 \u0627\u0645\u0631\u064a\u0627 \u0628\u0646 \u062d\u0632\u0642\u064a\u0627 \u0641\u064a \u0627\u064a\u0627\u0645 \u064a\u0648\u0634\u064a\u0627 \u0628\u0646 \u0622\u0645\u0648\u0646 \u0645\u0644\u0643 \u064a\u0647\u0648\u0630\u0627.\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0646\u0632\u0639\u0627 \u0627\u0646\u0632\u0639 \u0627\u0644\u0643\u0644 \u0639\u0646 \u0648\u062c\u0647 \u0627\u0644\u0627\u0631\u0636 \u064a\u0642\u0648\u0644 \u0627\u0644\u0631\u0628.\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u0627\u0646\u0632\u0639 \u0627\u0644\u0627\u0646\u0633\u0627\u0646 \u0648\u0627\u0644\u062d\u064a\u0648\u0627\u0646. \u0627\u0646\u0632\u0639 \u0637\u064a\u0648\u0631 \u0627\u0644\u0633\u0645\u0627\u0621 \u0648\u0633\u0645\u0643 \u0627\u0644\u0628\u062d\u0631 \u0648\u0627\u0644\u0645\u0639\u0627\u062b\u0631 \u0645\u0639 \u0627\u0644\u0627\u0634\u0631\u0627\u0631 \u0648\u0627\u0642\u0637\u0639 \u0627\u0644\u0627\u0646\u0633\u0627\u0646 \u0639\u0646 \u0648\u062c\u0647 \u0627\u0644\u0627\u0631\u0636 \u064a\u0642\u0648\u0644 \u0627\u0644\u0631\u0628.\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u0648\u0627\u0645\u062f \u064a\u062f\u064a \u0639\u0644\u0649 \u064a\u0647\u0648\u0630\u0627 \u0648\u0639\u0644\u0649 \u0643\u0644 \u0633\u0643\u0627\u0646 \u0627\u0648\u0631\u0634\u0644\u064a\u0645 \u0648\u0627\u0642\u0637\u0639 \u0645\u0646 \u0647\u0630\u0627 \u0627\u0644\u0645\u0643\u0627\u0646 \u0628\u0642\u064a\u0629 \u0627\u0644\u0628\u0639\u0644 \u0627\u0633\u0645 \u0627\u0644\u0643\u0645\u0627\u0631\u064a\u0645 \u0645\u0639 \u0627\u0644\u0643\u0647\u0646\u0629\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u0648\u0627\u0644\u0633\u0627\u062c\u062f\u064a\u0646 \u0639\u0644\u0649 \u0627\u0644\u0633\u0637\u0648\u062d \u0644\u062c\u0646\u062f \u0627\u0644\u0633\u0645\u0627\u0621 \u0648\u0627\u0644\u0633\u0627\u062c\u062f\u064a\u0646 \u0627\u0644\u062d\u0627\u0644\u0641\u064a\u0646 \u0628\u0627\u0644\u0631\u0628 \u0648\u0627\u0644\u062d\u0627\u0644\u0641\u064a\u0646 \u0628\u0645\u0644\u0643\u0648\u0645\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u0648\u0627\u0644\u0645\u0631\u062a\u062f\u064a\u0646 \u0645\u0646 \u0648\u0631\u0627\u0621 \u0627\u0644\u0631\u0628 \u0648\u0627\u0644\u0630\u064a\u0646 \u0644\u0645 \u064a\u0637\u0644\u0628\u0648\u0627 \u0627\u0644\u0631\u0628 \u0648\u0644\u0627 \u0633\u0623\u0644\u0648\u0627 \u0639\u0646\u0647\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u0627\u0633\u0643\u062a \u0642\u062f\u0627\u0645 \u0627\u0644\u0633\u064a\u062f \u0627\u0644\u0631\u0628 \u0644\u0627\u0646 \u064a\u0648\u0645 \u0627\u0644\u0631\u0628 \u0642\u0631\u064a\u0628. \u0644\u0627\u0646 \u0627\u0644\u0631\u0628 \u0642\u062f \u0627\u0639\u062f\u0651 \u0630\u0628\u064a\u062d\u0629 \u0642\u062f\u0633 \u0645\u062f\u0639\u0648\u064a\u0647.\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u0648\u064a\u0643\u0648\u0646 \u0641\u064a \u064a\u0648\u0645 \u0630\u0628\u064a\u062d\u0629 \u0627\u0644\u0631\u0628 \u0627\u0646\u064a \u0627\u0639\u0627\u0642\u0628 \u0627\u0644\u0631\u0624\u0633\u0627\u0621 \u0648\u0628\u0646\u064a \u0627\u0644\u0645\u0644\u0643 \u0648\u062c\u0645\u064a\u0639 \u0627\u0644\u0644\u0627\u0628\u0633\u064a\u0646 \u0644\u0628\u0627\u0633\u0627 \u063a\u0631\u064a\u0628\u0627.\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u0648\u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645 \u0627\u0639\u0627\u0642\u0628 \u0643\u0644 \u0627\u0644\u0630\u064a\u0646 \u064a\u0642\u0641\u0632\u0648\u0646 \u0645\u0646 \u0641\u0648\u0642 \u0627\u0644\u0639\u062a\u0628\u0629 \u0627\u0644\u0630\u064a\u0646 \u064a\u0645\u0644\u0623\u0648\u0646 \u0628\u064a\u062a \u0633\u064a\u062f\u0647\u0645 \u0638\u0644\u0645\u0627 \u0648\u063a\u0634\u0651\u0627.\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u0648\u064a\u0643\u0648\u0646 \u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645 \u064a\u0642\u0648\u0644 \u0627\u0644\u0631\u0628 \u0635\u0648\u062a \u0635\u0631\u0627\u062e \u0645\u0646 \u0628\u0627\u0628 \u0627\u0644\u0633\u0645\u0643 \u0648\u0648\u0644\u0648\u0644\u0629 \u0645\u0646 \u0627\u0644\u0642\u0633\u0645 \u0627\u0644\u062b\u0627\u0646\u064a \u0648\u0643\u0633\u0631 \u0639\u0638\u064a\u0645 \u0645\u0646 \u0627\u0644\u0627\u0643\u0627\u0645.\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u0648\u0644\u0648\u0644\u0648\u0627 \u064a\u0627 \u0633\u0643\u0627\u0646 \u0645\u0643\u062a\u064a\u0634 \u0644\u0627\u0646 \u0643\u0644 \u0634\u0639\u0628 \u0643\u0646\u0639\u0627\u0646 \u0628\u0627\u062f. \u0627\u0646\u0642\u0637\u0639 \u0643\u0644 \u0627\u0644\u062d\u0627\u0645\u0644\u064a\u0646 \u0627\u0644\u0641\u0636\u0629.\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u0648\u064a\u0643\u0648\u0646 \u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u0648\u0642\u062a \u0627\u0646\u064a \u0627\u0641\u062a\u0634 \u0627\u0648\u0631\u0634\u0644\u064a\u0645 \u0628\u0627\u0644\u0633\u0651\u0631\u062c \u0648\u0627\u0639\u0627\u0642\u0628 \u0627\u0644\u0631\u062c\u0627\u0644 \u0627\u0644\u062c\u0627\u0645\u062f\u064a\u0646 \u0639\u0644\u0649 \u062f\u0631\u062f\u064a\u0647\u0645 \u0627\u0644\u0642\u0627\u0626\u0644\u064a\u0646 \u0641\u064a \u0642\u0644\u0648\u0628\u0647\u0645 \u0627\u0646 \u0627\u0644\u0631\u0628 \u0644\u0627 \u064a\u062d\u0633\u0646 \u0648\u0644\u0627 \u064a\u0633\u064a\u0621.\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u0641\u062a\u0643\u0648\u0646 \u062b\u0631\u0648\u062a\u0647\u0645 \u063a\u0646\u064a\u0645\u0629 \u0648\u0628\u064a\u0648\u062a\u0647\u0645 \u062e\u0631\u0627\u0628\u0627 \u0648\u064a\u0628\u0646\u0648\u0646 \u0628\u064a\u0648\u062a\u0627 \u0648\u0644\u0627 \u064a\u0633\u0643\u0646\u0648\u0646\u0647\u0627 \u0648\u064a\u063a\u0631\u0633\u0648\u0646 \u0643\u0631\u0648\u0645\u0627 \u0648\u0644\u0627 \u064a\u0634\u0631\u0628\u0648\u0646 \u062e\u0645\u0631\u0647\u0627\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u0642\u0631\u064a\u0628 \u064a\u0648\u0645 \u0627\u0644\u0631\u0628 \u0627\u0644\u0639\u0638\u064a\u0645 \u0642\u0631\u064a\u0628 \u0648\u0633\u0631\u064a\u0639 \u062c\u062f\u0627. \u0635\u0648\u062a \u064a\u0648\u0645 \u0627\u0644\u0631\u0628. \u064a\u0635\u0631\u062e \u062d\u064a\u0646\u0626\u0630 \u0627\u0644\u062c\u0628\u0627\u0631 \u0645\u0631\u0651\u0627.\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645 \u064a\u0648\u0645 \u0633\u062e\u0637 \u064a\u0648\u0645 \u0636\u064a\u0642 \u0648\u0634\u062f\u0651\u0629 \u064a\u0648\u0645 \u062e\u0631\u0627\u0628 \u0648\u062f\u0645\u0627\u0631 \u064a\u0648\u0645 \u0638\u0644\u0627\u0645 \u0648\u0642\u062a\u0627\u0645 \u064a\u0648\u0645 \u0633\u062d\u0627\u0628 \u0648\u0636\u0628\u0627\u0628\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u064a\u0648\u0645 \u0628\u0648\u0642 \u0648\u0647\u062a\u0627\u0641 \u0639\u0644\u0649 \u0627\u0644\u0645\u062f\u0646 \u0627\u0644\u0645\u062d\u0635\u0651\u0646\u0629 \u0648\u0639\u0644\u0649 \u0627\u0644\u0634\u0631\u0641 \u0627\u0644\u0631\u0641\u064a\u0639\u0629.\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u0648\u0627\u0636\u0627\u064a\u0642 \u0627\u0644\u0646\u0627\u0633 \u0641\u064a\u0645\u0634\u0648\u0646 \u0643\u0627\u0644\u0639\u0645\u064a \u0644\u0627\u0646\u0647\u0645 \u0627\u062e\u0637\u0623\u0648\u0627 \u0627\u0644\u0649 \u0627\u0644\u0631\u0628 \u0641\u064a\u0633\u0641\u062d \u062f\u0645\u0647\u0645 \u0643\u0627\u0644\u062a\u0631\u0627\u0628 \u0648\u0644\u062d\u0645\u0647\u0645 \u0643\u0627\u0644\u062c\u0644\u0651\u0629.\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u0644\u0627 \u0641\u0636\u062a\u0647\u0645 \u0648\u0644\u0627 \u0630\u0647\u0628\u0647\u0645 \u064a\u0633\u062a\u0637\u064a\u0639 \u0627\u0646\u0642\u0627\u0630\u0647\u0645 \u0641\u064a \u064a\u0648\u0645 \u063a\u0636\u0628 \u0627\u0644\u0631\u0628 \u0628\u0644 \u0628\u0646\u0627\u0631 \u063a\u064a\u0631\u062a\u0647 \u062a\u0624\u0643\u0644 \u0627\u0644\u0627\u0631\u0636 \u0643\u0644\u0647\u0627. \u0644\u0627\u0646\u0647 \u064a\u0635\u0646\u0639 \u0641\u0646\u0627\u0621 \u0628\u0627\u063a\u062a\u0627 \u0644\u0643\u0644 \u0633\u0643\u0627\u0646 \u0627\u0644\u0627\u0631\u0636\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u062a\u062c\u0645\u0651\u0639\u064a \u0648\u0627\u062c\u062a\u0645\u0639\u064a \u064a\u0627 \u0627\u064a\u062a\u0647\u0627 \u0627\u0644\u0627\u0645\u0629 \u063a\u064a\u0631 \u0627\u0644\u0645\u0633\u062a\u062d\u064a\u0629\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0642\u0628\u0644 \u0648\u0644\u0627\u062f\u0629 \u0627\u0644\u0642\u0636\u0627\u0621. \u0643\u0627\u0644\u0639\u0635\u0627\u0641\u0629 \u0639\u0628\u0631 \u0627\u0644\u064a\u0648\u0645. \u0642\u0628\u0644 \u0627\u0646 \u064a\u0627\u062a\u064a \u0639\u0644\u064a\u0643\u0645 \u062d\u0645\u0648 \u063a\u0636\u0628 \u0627\u0644\u0631\u0628 \u0642\u0628\u0644 \u0627\u0646 \u064a\u0623\u062a\u064a \u0639\u0644\u064a\u0643\u0645 \u064a\u0648\u0645 \u0633\u062e\u0637 \u0627\u0644\u0631\u0628.\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u0627\u0637\u0644\u0628\u0648\u0627 \u0627\u0644\u0631\u0628 \u064a\u0627 \u062c\u0645\u064a\u0639 \u0628\u0627\u0626\u0633\u064a \u0627\u0644\u0627\u0631\u0636 \u0627\u0644\u0630\u064a\u0646 \u0641\u0639\u0644\u0648\u0627 \u062d\u0643\u0645\u0647. \u0627\u0637\u0644\u0628\u0648\u0627 \u0627\u0644\u0628\u0631\u0651. \u0627\u0637\u0644\u0628\u0648\u0627 \u0627\u0644\u062a\u0648\u0627\u0636\u0639. \u0644\u0639\u0644\u0643\u0645 \u062a\u0633\u062a\u0631\u0648\u0646 \u0641\u064a \u064a\u0648\u0645 \u0633\u062e\u0637 \u0627\u0644\u0631\u0628\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u0644\u0627\u0646 \u063a\u0632\u0629 \u062a\u0643\u0648\u0646 \u0645\u062a\u0631\u0648\u0643\u0629 \u0648\u0627\u0634\u0642\u0644\u0648\u0646 \u0644\u0644\u062e\u0631\u0627\u0628. \u0627\u0634\u062f\u0648\u062f \u0639\u0646\u062f \u0627\u0644\u0638\u0647\u064a\u0631\u0629 \u064a\u0637\u0631\u062f\u0648\u0646\u0647\u0627 \u0648\u0639\u0642\u0631\u0648\u0646 \u062a\u0633\u062a\u0623\u0635\u0644.\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u0648\u064a\u0644 \u0644\u0633\u0643\u0627\u0646 \u0633\u0627\u062d\u0644 \u0627\u0644\u0628\u062d\u0631 \u0627\u0645\u0629 \u0627\u0644\u0643\u0631\u064a\u062a\u064a\u064a\u0646. \u0643\u0644\u0645\u0629 \u0627\u0644\u0631\u0628 \u0639\u0644\u064a\u0643\u0645. \u064a\u0627 \u0643\u0646\u0639\u0627\u0646 \u0627\u0631\u0636 \u0627\u0644\u0641\u0644\u0633\u0637\u064a\u0646\u064a\u064a\u0646 \u0627\u0646\u064a \u0627\u062e\u0631\u0628\u0643 \u0628\u0644\u0627 \u0633\u0627\u0643\u0646.\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u0648\u064a\u0643\u0648\u0646 \u0633\u0627\u062d\u0644 \u0627\u0644\u0628\u062d\u0631 \u0645\u0631\u0639\u0649 \u0628\u0622\u0628\u0627\u0631 \u0644\u0644\u0631\u0639\u0627\u0629 \u0648\u062d\u0638\u0627\u0626\u0631 \u0644\u0644\u063a\u0646\u0645.\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u0648\u064a\u0643\u0648\u0646 \u0627\u0644\u0633\u0627\u062d\u0644 \u0644\u0628\u0642\u064a\u0629 \u0628\u064a\u062a \u064a\u0647\u0648\u0630\u0627 \u0639\u0644\u064a\u0647 \u064a\u0631\u0639\u0648\u0646. \u0641\u064a \u0628\u064a\u0648\u062a \u0627\u0634\u0642\u0644\u0648\u0646 \u0639\u0646\u062f \u0627\u0644\u0645\u0633\u0627\u0621 \u064a\u0631\u0628\u0636\u0648\u0646 \u0644\u0627\u0646 \u0627\u0644\u0631\u0628 \u0627\u0644\u0647\u0647\u0645 \u064a\u062a\u0639\u0647\u0651\u062f\u0647\u0645 \u0648\u064a\u0631\u062f \u0633\u0628\u064a\u0647\u0645\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u0642\u062f \u0633\u0645\u0639\u062a \u062a\u0639\u064a\u064a\u0631 \u0645\u0648\u0622\u0628 \u0648\u062a\u062c\u0627\u062f\u064a\u0641 \u0628\u0646\u064a \u0639\u0645\u0651\u0648\u0646 \u0627\u0644\u062a\u064a \u0628\u0647\u0627 \u0639\u064a\u0651\u0631\u0648\u0627 \u0634\u0639\u0628\u064a \u0648\u062a\u0639\u0638\u0651\u0645\u0648\u0627 \u0639\u0644\u0649 \u062a\u062e\u0645\u0647\u0645.\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u0641\u0644\u0630\u0644\u0643 \u062d\u064a\u0651 \u0627\u0646\u0627 \u064a\u0642\u0648\u0644 \u0631\u0628 \u0627\u0644\u062c\u0646\u0648\u062f \u0627\u0644\u0647 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0627\u0646 \u0645\u0648\u0622\u0628 \u062a\u0643\u0648\u0646 \u0643\u0633\u062f\u0648\u0645 \u0648\u0628\u0646\u0648 \u0639\u0645\u0648\u0646 \u0643\u0639\u0645\u0648\u0631\u0629 \u0645\u0644\u0643 \u0627\u0644\u0642\u0631\u064a\u0635 \u0648\u062d\u0641\u0631\u0629 \u0645\u0644\u062d \u0648\u062e\u0631\u0627\u0628\u0627 \u0627\u0644\u0649 \u0627\u0644\u0627\u0628\u062f. \u062a\u0646\u0647\u0628\u0647\u0645 \u0628\u0642\u064a\u0629 \u0634\u0639\u0628\u064a \u0648\u0628\u0642\u064a\u0629 \u0627\u0645\u0651\u062a\u064a \u062a\u0645\u062a\u0644\u0643\u0647\u0645.\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u0647\u0630\u0627 \u0644\u0647\u0645 \u0639\u0648\u0636 \u062a\u0643\u0628\u0651\u0631\u0647\u0645 \u0644\u0627\u0646\u0647\u0645 \u0639\u064a\u0651\u0631\u0648\u0627 \u0648\u062a\u0639\u0638\u0645\u0648\u0627 \u0639\u0644\u0649 \u0634\u0639\u0628 \u0631\u0628 \u0627\u0644\u062c\u0646\u0648\u062f.\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u0627\u0644\u0631\u0628 \u0645\u062e\u064a\u0641 \u0627\u0644\u064a\u0647\u0645 \u0644\u0627\u0646\u0647 \u064a\u0647\u0632\u0644 \u062c\u0645\u064a\u0639 \u0622\u0644\u0647\u0629 \u0627\u0644\u0627\u0631\u0636 \u0641\u0633\u064a\u0633\u062c\u062f \u0644\u0647 \u0627\u0644\u0646\u0627\u0633 \u0643\u0644 \u0648\u0627\u062d\u062f \u0645\u0646 \u0645\u0643\u0627\u0646\u0647 \u0643\u0644 \u062c\u0632\u0627\u0626\u0631 \u0627\u0644\u0627\u0645\u0645\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u0648\u0627\u0646\u062a\u0645 \u064a\u0627 \u0627\u064a\u0647\u0627 \u0627\u0644\u0643\u0648\u0634\u064a\u0648\u0646. \u0642\u062a\u0644\u0649 \u0633\u064a\u0641\u064a \u0647\u0645.\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u0648\u064a\u0645\u062f \u064a\u062f\u0647 \u0639\u0644\u0649 \u0627\u0644\u0634\u0645\u0627\u0644 \u0648\u064a\u0628\u064a\u062f \u0627\u0634\u0648\u0631 \u0648\u064a\u062c\u0639\u0644 \u0646\u064a\u0646\u0648\u0649 \u062e\u0631\u0627\u0628\u0627 \u064a\u0627\u0628\u0633\u0629 \u0643\u0627\u0644\u0642\u0641\u0631.\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u0641\u062a\u0631\u0628\u0636 \u0641\u064a \u0648\u0633\u0637\u0647\u0627 \u0627\u0644\u0642\u0637\u0639\u0627\u0646 \u0643\u0644 \u0637\u0648\u0627\u0626\u0641 \u0627\u0644\u062d\u064a\u0648\u0627\u0646. \u0627\u0644\u0642\u0648\u0642 \u0627\u064a\u0636\u0627 \u0648\u0627\u0644\u0642\u0646\u0641\u0630 \u064a\u0623\u0648\u064a\u0627\u0646 \u0627\u0644\u0649 \u062a\u064a\u062c\u0627\u0646 \u0639\u0645\u062f\u0647\u0627. \u0635\u0648\u062a \u064a\u0646\u0639\u0628 \u0641\u064a \u0627\u0644\u0643\u0648\u0649. \u062e\u0631\u0627\u0628 \u0639\u0644\u0649 \u0627\u0644\u0627\u0639\u062a\u0627\u0628 \u0644\u0627\u0646\u0647 \u0642\u062f \u062a\u0639\u0631\u0651\u0649 \u0623\u0631\u0632\u064a\u0651\u0647\u0627.\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u0647\u0630\u0647 \u0647\u064a \u0627\u0644\u0645\u062f\u064a\u0646\u0629 \u0627\u0644\u0645\u0628\u062a\u0647\u062c\u0629 \u0627\u0644\u0633\u0627\u0643\u0646\u0629 \u0645\u0637\u0645\u0626\u0646\u0629 \u0627\u0644\u0642\u0627\u0626\u0644\u0629 \u0641\u064a \u0642\u0644\u0628\u0647\u0627 \u0627\u0646\u0627 \u0648\u0644\u064a\u0633 \u063a\u064a\u0631\u064a. \u0643\u064a\u0641 \u0635\u0627\u0631\u062a \u062e\u0631\u0627\u0628\u0627 \u0645\u0631\u0628\u0636\u0627 \u0644\u0644\u062d\u064a\u0648\u0627\u0646. \u0643\u0644 \u0639\u0627\u0628\u0631 \u0628\u0647\u0627 \u064a\u0635\u0641\u0631 \u0648\u064a\u0647\u0632\u0651 \u064a\u062f\u0647\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u0648\u064a\u0644 \u0644\u0644\u0645\u062a\u0645\u0631\u062f\u0629 \u0627\u0644\u0645\u0646\u062c\u0633\u0629 \u0627\u0644\u0645\u062f\u064a\u0646\u0629 \u0627\u0644\u062c\u0627\u0626\u0631\u0629.\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0644\u0645 \u062a\u0633\u0645\u0639 \u0627\u0644\u0635\u0648\u062a. \u0644\u0645 \u062a\u0642\u0628\u0644 \u0627\u0644\u062a\u0627\u062f\u064a\u0628. \u0644\u0645 \u062a\u062a\u0643\u0644 \u0639\u0644\u0649 \u0627\u0644\u0631\u0628. \u0644\u0645 \u062a\u062a\u0642\u0631\u0628 \u0627\u0644\u0649 \u0627\u0644\u0647\u0647\u0627.\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u0631\u0624\u0633\u0627\u0624\u0647\u0627 \u0641\u064a \u0648\u0633\u0637\u0647\u0627 \u0627\u0633\u0648\u062f \u0632\u0627\u0626\u0631\u0629. \u0642\u0636\u0627\u062a\u0647\u0627 \u0630\u0626\u0627\u0628 \u0645\u0633\u0627\u0621 \u0644\u0627 \u064a\u0628\u0642\u0648\u0646 \u0634\u064a\u0626\u0627 \u0627\u0644\u0649 \u0627\u0644\u0635\u0628\u0627\u062d.\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u0627\u0646\u0628\u064a\u0627\u0624\u0647\u0627 \u0645\u062a\u0641\u0627\u062e\u0631\u0648\u0646 \u0627\u0647\u0644 \u063a\u062f\u0631\u0627\u062a. \u0643\u0647\u0646\u062a\u0647\u0627 \u0646\u062c\u0651\u0633\u0648\u0627 \u0627\u0644\u0642\u062f\u0633 \u062e\u0627\u0644\u0641\u0648\u0627 \u0627\u0644\u0634\u0631\u064a\u0639\u0629.\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u0627\u0644\u0631\u0628 \u0639\u0627\u062f\u0644 \u0641\u064a \u0648\u0633\u0637\u0647\u0627 \u0644\u0627 \u064a\u0641\u0639\u0644 \u0638\u0644\u0645\u0627. \u063a\u062f\u0627\u0629 \u063a\u062f\u0627\u0629 \u064a\u0628\u0631\u0632 \u062d\u0643\u0645\u0647 \u0627\u0644\u0649 \u0627\u0644\u0646\u0648\u0631 \u0644\u0627 \u064a\u062a\u0639\u0630\u0651\u0631. \u0627\u0645\u0627 \u0627\u0644\u0638\u0627\u0644\u0645 \u0641\u0644\u0627 \u064a\u0639\u0631\u0641 \u0627\u0644\u062e\u0632\u064a.\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u0642\u0637\u0639\u062a \u0627\u0645\u0645\u0627 \u062e\u0631\u0651\u0628\u062a \u0634\u0631\u0641\u0627\u062a\u0647\u0645 \u0627\u0642\u0641\u0631\u062a \u0627\u0633\u0648\u0627\u0642\u0647\u0645 \u0628\u0644\u0627 \u0639\u0627\u0628\u0631. \u062f\u0645\u0631\u062a \u0645\u062f\u0646\u0647\u0645 \u0628\u0644\u0627 \u0627\u0646\u0633\u0627\u0646 \u0628\u063a\u064a\u0631 \u0633\u0627\u0643\u0646.\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u0641\u0642\u0644\u062a \u0627\u0646\u0643 \u0644\u062a\u062e\u0634\u064a\u0646\u0646\u064a \u062a\u0642\u0628\u0644\u064a\u0646 \u0627\u0644\u062a\u0623\u062f\u064a\u0628 \u0641\u0644\u0627 \u064a\u0646\u0642\u0637\u0639 \u0645\u0633\u0643\u0646\u0647\u0627 \u062d\u0633\u0628 \u0643\u0644 \u0645\u0627 \u0639\u064a\u0651\u0646\u062a\u0647 \u0639\u0644\u064a\u0647\u0627. \u0644\u0643\u0646 \u0628\u0643\u0651\u0631\u0648\u0627 \u0648\u0627\u0641\u0633\u062f\u0648\u0627 \u062c\u0645\u064a\u0639 \u0627\u0639\u0645\u0627\u0644\u0647\u0645\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u0644\u0630\u0644\u0643 \u0641\u0627\u0646\u062a\u0638\u0631\u0648\u0646\u064a \u064a\u0642\u0648\u0644 \u0627\u0644\u0631\u0628 \u0627\u0644\u0649 \u064a\u0648\u0645 \u0627\u0642\u0648\u0645 \u0627\u0644\u0649 \u0627\u0644\u0633\u0644\u0628 \u0644\u0627\u0646 \u062d\u0643\u0645\u064a \u0647\u0648 \u0628\u062c\u0645\u0639 \u0627\u0644\u0627\u0645\u0645 \u0648\u062d\u0634\u0631 \u0627\u0644\u0645\u0645\u0627\u0644\u0643 \u0644\u0627\u0635\u0628\u0651 \u0639\u0644\u064a\u0647\u0645 \u0633\u062e\u0637\u064a \u0643\u0644 \u062d\u0645\u0648 \u063a\u0636\u0628\u064a \u0644\u0627\u0646\u0647 \u0628\u0646\u0627\u0631 \u063a\u064a\u0631\u062a\u064a \u062a\u0624\u0643\u0644 \u0643\u0644 \u0627\u0644\u0627\u0631\u0636.\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u0644\u0627\u0646\u064a \u062d\u064a\u0646\u0626\u0630 \u0627\u062d\u0648\u0644 \u0627\u0644\u0634\u0639\u0648\u0628 \u0627\u0644\u0649 \u0634\u0641\u0629 \u0646\u0642\u064a\u0629 \u0644\u064a\u062f\u0639\u0648\u0627 \u0643\u0644\u0647\u0645 \u0628\u0627\u0633\u0645 \u0627\u0644\u0631\u0628 \u0644\u064a\u0639\u0628\u062f\u0648\u0647 \u0628\u0643\u062a\u0641 \u0648\u0627\u062d\u062f\u0629.\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u0645\u0646 \u0639\u0628\u0631 \u0627\u0646\u0647\u0627\u0631 \u0643\u0648\u0634 \u0627\u0644\u0645\u062a\u0636\u0631\u0639\u0648\u0646 \u0627\u0644\u064a\u0651 \u0645\u062a\u0628\u062f\u062f\u064a\u0651 \u064a\u0642\u062f\u0645\u0648\u0646 \u062a\u0642\u062f\u0645\u062a\u064a.\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645 \u0644\u0627 \u062a\u062e\u0632\u064a\u0646 \u0645\u0646 \u0643\u0644 \u0627\u0639\u0645\u0627\u0644\u0643 \u0627\u0644\u062a\u064a \u062a\u0639\u062f\u064a\u062a \u0628\u0647\u0627 \u0639\u0644\u064a\u0651. \u0644\u0627\u0646\u064a \u062d\u064a\u0646\u0626\u0630 \u0627\u0646\u0632\u0639 \u0645\u0646 \u0648\u0633\u0637\u0643 \u0645\u0628\u062a\u0647\u062c\u064a \u0643\u0628\u0631\u064a\u0627\u0626\u0643 \u0648\u0644\u0646 \u062a\u0639\u0648\u062f\u064a \u0628\u0639\u062f \u0627\u0644\u0649 \u0627\u0644\u062a\u0643\u0628\u0631 \u0641\u064a \u062c\u0628\u0644 \u0642\u062f\u0633\u064a.\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u0648\u0627\u0628\u0642\u064a \u0641\u064a \u0648\u0633\u0637\u0643 \u0634\u0639\u0628\u0627 \u0628\u0627\u0626\u0633\u0627 \u0648\u0645\u0633\u0643\u064a\u0646\u0627 \u0641\u064a\u062a\u0648\u0643\u0644\u0648\u0646 \u0639\u0644\u0649 \u0627\u0633\u0645 \u0627\u0644\u0631\u0628.\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u0628\u0642\u064a\u0629 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0644\u0627 \u064a\u0641\u0639\u0644\u0648\u0646 \u0627\u062b\u0645\u0627 \u0648\u0644\u0627 \u064a\u062a\u0643\u0644\u0645\u0648\u0646 \u0628\u0627\u0644\u0643\u0630\u0628 \u0648\u0644\u0627 \u064a\u0648\u062c\u062f \u0641\u064a \u0627\u0641\u0648\u0627\u0647\u0647\u0645 \u0644\u0633\u0627\u0646 \u063a\u0634 \u0644\u0627\u0646\u0647\u0645 \u064a\u0631\u0639\u0648\u0646 \u0648\u064a\u0631\u0628\u0636\u0648\u0646 \u0648\u0644\u0627 \u0645\u062e\u064a\u0641\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u062a\u0631\u0646\u0645\u064a \u064a\u0627 \u0627\u0628\u0646\u0629 \u0635\u0647\u064a\u0648\u0646 \u0627\u0647\u062a\u0641 \u064a\u0627 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0627\u0641\u0631\u062d\u064a \u0648\u0627\u0628\u062a\u0647\u062c\u064a \u0628\u0643\u0644 \u0642\u0644\u0628\u0643 \u064a\u0627 \u0627\u0628\u0646\u0629 \u0627\u0648\u0631\u0634\u0644\u064a\u0645.\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u0642\u062f \u0646\u0632\u0639 \u0627\u0644\u0631\u0628 \u0627\u0644\u0627\u0642\u0636\u064a\u0629 \u0639\u0644\u064a\u0643 \u0627\u0632\u0627\u0644 \u0639\u062f\u0648\u0643. \u0645\u0644\u0643 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0627\u0644\u0631\u0628 \u0641\u064a \u0648\u0633\u0637\u0643. \u0644\u0627 \u062a\u0646\u0638\u0631\u064a\u0646 \u0628\u0639\u062f \u0634\u0631\u0627.\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645 \u064a\u0642\u0627\u0644 \u0644\u0627\u0648\u0631\u0634\u0644\u064a\u0645 \u0644\u0627 \u062a\u062e\u0627\u0641\u064a \u064a\u0627 \u0635\u0647\u064a\u0648\u0646 \u0644\u0627 \u062a\u0631\u062a\u062e \u064a\u062f\u0627\u0643.\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u0627\u0644\u0631\u0628 \u0627\u0644\u0647\u0643 \u0641\u064a \u0648\u0633\u0637\u0643 \u062c\u0628\u0627\u0631. \u064a\u062e\u0644\u0651\u0635. \u064a\u0628\u062a\u0647\u062c \u0628\u0643 \u0641\u0631\u062d\u0627. \u064a\u0633\u0643\u062a \u0641\u064a \u0645\u062d\u0628\u062a\u0647. \u064a\u0628\u062a\u0647\u062c \u0628\u0643 \u0628\u062a\u0631\u0646\u0645.\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u0627\u062c\u0645\u0639 \u0627\u0644\u0645\u062d\u0632\u0648\u0646\u064a\u0646 \u0639\u0644\u0649 \u0627\u0644\u0645\u0648\u0633\u0645. \u0643\u0627\u0646\u0648\u0627 \u0645\u0646\u0643. \u062d\u0627\u0645\u0644\u064a\u0646 \u0639\u0644\u064a\u0647\u0627 \u0627\u0644\u0639\u0627\u0631.\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u0647\u0627\u0646\u0630\u0627 \u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u064a\u0648\u0645 \u0627\u0639\u0627\u0645\u0644 \u0643\u0644 \u0645\u0630\u0644\u0651\u0644\u064a\u0643 \u0648\u0627\u062e\u0644\u0635 \u0627\u0644\u0638\u0627\u0644\u0639\u0629 \u0648\u0627\u062c\u0645\u0639 \u0627\u0644\u0645\u0646\u0641\u064a\u0629 \u0648\u0627\u062c\u0639\u0644\u0647\u0645 \u062a\u0633\u0628\u064a\u062d\u0629 \u0648\u0627\u0633\u0645\u0627 \u0641\u064a \u0643\u0644 \u0627\u0631\u0636 \u062e\u0632\u064a\u0647\u0645\n"
                },
                "20": {
                    "verse_nr": 20,
