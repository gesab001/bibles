                "14": {
                    "verse_nr": "14",
                    "verse": "\u130c\u1273\u127d\u1295 \u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235 \u12a5\u1295\u12f3\u1218\u1208\u12a8\u1270\u129d \u12a8\u12da\u1205 \u12a0\u12ab\u120c \u1218\u1208\u12e8\u1274 \u1348\u1325\u1296 \u12a5\u1295\u12f2\u1206\u1295 \u12a0\u12cd\u1243\u1208\u1201\u1293\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12a8\u1218\u12cd\u1323\u1274\u121d \u1260\u128b\u120b \u12a5\u1290\u12da\u1205\u1295 \u1290\u1308\u122e\u127d \u12a5\u1295\u12f5\u1273\u1235\u1261 \u1260\u12e8\u130a\u12dc\u12cd \u1275\u127d\u1209 \u12d8\u1295\u12f5 \u12a5\u1270\u130b\u1208\u1201\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u12e8\u12a5\u122d\u1231\u1295 \u130d\u122d\u121b \u12a0\u12ed\u1270\u1295 \u12a5\u1295\u1302 \u1260\u1265\u120d\u1203\u1275 \u12e8\u1270\u1348\u1320\u1228\u12cd\u1295 \u1270\u1228\u1275 \u1233\u1295\u12a8\u1270\u120d \u12e8\u130c\u1273\u127d\u1295\u1295 \u12e8\u12a2\u12e8\u1231\u1235 \u12ad\u122d\u1235\u1276\u1235\u1295 \u1280\u12ed\u120d\u1293 \u1218\u121d\u1323\u1275 \u12a0\u1235\u1273\u12c8\u1245\u1293\u127d\u1201\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u12a8\u1308\u1293\u1293\u12cd \u12ad\u1265\u122d \u201c\u1260\u12a5\u122d\u1231 \u12f0\u1235 \u12e8\u121a\u1208\u129d \u12e8\u121d\u12c8\u12f0\u12cd \u120d\u1304 \u12ed\u1205 \u1290\u12cd\u1364\u201d \u12e8\u121a\u120d \u12eb \u12f5\u121d\u1345 \u1260\u1218\u1323\u1208\u1275 \u130a\u12dc \u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a0\u1265 \u12ad\u1265\u122d\u1295\u1293 \u121d\u1235\u130b\u1293\u1295 \u1270\u1240\u1265\u120e\u12a0\u120d\u1293\u1364"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u12a5\u129b\u121d \u1260\u1245\u12f1\u1231 \u1270\u122b\u122b \u12a8\u12a5\u122d\u1231 \u130b\u122d \u1233\u1208\u1295 \u12ed\u1205\u1295 \u12f5\u121d\u1345 \u12a8\u1230\u121b\u12ed \u1232\u12c8\u122d\u12f5 \u1230\u121b\u1295\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12a8\u12a5\u122d\u1231\u121d \u12ed\u120d\u1245 \u12a5\u1305\u130d \u12e8\u1338\u1293 \u12e8\u1275\u1295\u1262\u1275 \u1243\u120d \u12a0\u1208\u1295\u1364 \u121d\u12f5\u122d\u121d \u12a5\u1235\u12aa\u1320\u1263 \u12f5\u1228\u1235 \u12e8\u1295\u130b\u1275\u121d \u12ae\u12a8\u1265 \u1260\u120d\u1263\u127d\u1201 \u12a5\u1235\u12aa\u12c8\u1323 \u12f5\u1228\u1235\u1365 \u1230\u12cd \u1260\u1328\u1208\u121b \u1235\u134d\u122b \u12e8\u121a\u1260\u122b\u1295 \u1218\u1265\u122b\u1275 \u12a5\u1295\u12f0\u121a\u1320\u1290\u1240\u1245 \u12ed\u1205\u1295 \u1243\u120d \u12a5\u12e8\u1320\u1290\u1240\u1243\u127d\u1201 \u1218\u120d\u12ab\u121d \u1273\u12f0\u122d\u130b\u120b\u127d\u1201\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u12ed\u1205\u1295 \u1260\u1218\u1300\u1218\u122a\u12eb \u12a5\u12c8\u1241\u1364 \u1260\u1218\u133d\u1210\u134d \u12eb\u1208\u12cd\u1295 \u1275\u1295\u1262\u1275 \u1201\u1209 \u121b\u1295\u121d \u1208\u1308\u12db \u122b\u1231 \u120a\u1270\u1228\u1309\u121d \u12a0\u120d\u1270\u1348\u1240\u12f0\u121d\u1364"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u1275\u1295\u1262\u1275 \u12a8\u1276 \u1260\u1230\u12cd \u1348\u1243\u12f5 \u12a0\u120d\u1218\u1323\u121d\u1293\u1364 \u12f3\u1229 \u130d\u1295 \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1270\u120d\u12a8\u12cd \u1245\u12f1\u1233\u1295 \u1230\u12ce\u127d \u1260\u1218\u1295\u1348\u1235 \u1245\u12f1\u1235 \u1270\u1290\u12f5\u1270\u12cd \u1270\u1293\u1308\u1229\u1362"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1290\u1308\u122d \u130d\u1295 \u1210\u1230\u1270\u129e\u127d \u1290\u1262\u12eb\u1275 \u12f0\u130d\u121e \u1260\u1215\u12dd\u1261 \u1218\u12ab\u12a8\u120d \u1290\u1260\u1229\u1364 \u12a5\u1295\u12f2\u1201\u121d \u1260\u1218\u12ab\u12a8\u120b\u127d\u1201 \u12f0\u130d\u121e \u1210\u1230\u1270\u129e\u127d \u12a0\u1235\u1270\u121b\u122a\u12ce\u127d \u12ed\u1206\u1293\u1209\u1364 \u12a5\u1290\u122d\u1231\u121d \u12e8\u12cb\u1303\u1278\u12cd\u1295 \u130c\u1273 \u12a5\u1295\u12b3 \u12ad\u12f0\u12cd \u12e8\u121a\u1348\u1325\u1295\u1295 \u1325\u134b\u1275 \u1260\u122b\u1233\u1278\u12cd \u120b\u12ed \u12a5\u12e8\u1233\u1261 \u12e8\u121a\u12eb\u1320\u134b \u1291\u134b\u1244\u1295 \u12a0\u1239\u120d\u12a8\u12cd \u12eb\u1308\u1263\u1209\u1364"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1265\u12d9\u12ce\u127d\u121d \u1260\u1218\u12f3\u122b\u1273\u1278\u12cd \u12ed\u12a8\u1270\u1209\u12a0\u1278\u12cb\u120d\u1364 \u1260\u12a5\u1290\u122d\u1231\u121d \u1320\u1295\u1245 \u12e8\u12a5\u12cd\u1290\u1275 \u1218\u1295\u1308\u12f5 \u12ed\u1230\u12f0\u1263\u120d\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1308\u1295\u12d8\u1265\u1295\u121d \u1260\u1218\u1218\u1298\u1275 \u1260\u1270\u1348\u1320\u1228 \u1290\u1308\u122d \u12ed\u1228\u1261\u1263\u127d\u128b\u120d\u1364 \u134d\u122d\u12f3\u1278\u12cd\u121d \u12a8\u1325\u1295\u1275 \u1300\u121d\u122e \u12a0\u12ed\u12d8\u1308\u12ed\u121d\u1364 \u1325\u134b\u1273\u1278\u12cd\u121d \u12a0\u12eb\u1295\u1240\u120b\u134b\u121d\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1280\u1322\u12a0\u1275\u1295 \u120b\u12f0\u1228\u1309 \u1218\u120b\u12a5\u12ad\u1275 \u1233\u12ed\u122b\u122b\u120b\u1278\u12cd \u12c8\u12f0 \u1308\u1203\u1290\u121d \u1325\u120e \u1260\u1328\u1208\u121b \u1309\u12f5\u1313\u12f5 \u1208\u134d\u122d\u12f5 \u120a\u1320\u1260\u1241 \u12a0\u1233\u120d\u134e \u12a8\u1230\u1323\u1278\u12cd\u1365"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1208\u1240\u12f0\u1218\u12cd\u121d \u12d3\u1208\u121d \u1233\u12ed\u122b\u122b \u12a8\u120c\u120e\u127d \u1230\u1263\u1275 \u130b\u122d \u133d\u12f5\u1245\u1295 \u12e8\u121a\u1230\u1265\u12a8\u12cd\u1295 \u1296\u1285\u1295 \u12a0\u12f5\u1296 \u1260\u1280\u1322\u12a0\u1270\u129e\u127d \u12d3\u1208\u121d \u120b\u12ed \u12e8\u1325\u134b\u1275\u1295 \u12cd\u1203 \u12ab\u12c8\u1228\u12f0\u1365"
