    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12c8\u12f0 \u1263\u1271\u200b\u12a4\u120d \u120d\u1305 \u12c8\u12f0 \u12a2\u12ee\u200b\u12a4\u120d \u12e8\u1218\u200b\u1323\u12cd \u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1243\u120d \u12ed\u1205 \u1290\u12cd\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12a5\u1293\u200b\u1295\u1270 \u123d\u121b\u200b\u130d\u200b\u120c\u200b\u12ce\u127d! \u12ed\u1205\u1295 \u1235\u1219\u1364 \u1260\u121d\u200b\u12f5\u200b\u122d\u121d \u12e8\u121d\u200b\u1275\u200b\u1296\u1229 \u1201\u1209 \u12a0\u12f5\u200b\u121d\u1321\u1362 \u12ed\u1205 \u1260\u12d8\u200b\u1218\u200b\u1293\u200b\u127d\u1201 \u12c8\u12ed\u1235 \u1260\u12a0\u200b\u1263\u200b\u1276\u200b\u127b\u200b\u127d\u1201 \u12d8\u1218\u1295 \u1206\u1296 \u1290\u1260\u200b\u122d\u1295?"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12ed\u1205\u1295 \u1208\u120d\u200b\u1306\u200b\u127b\u200b\u127d\u1201 \u1295\u1308\u1229\u1364 \u120d\u1306\u200b\u127b\u200b\u127d\u200b\u1201\u121d \u1208\u120d\u200b\u1306\u200b\u127b\u200b\u1278\u12cd\u1365 \u120d\u1306\u200b\u127b\u200b\u1278\u200b\u12cd\u121d \u1208\u128b\u200b\u1208\u200b\u129b\u12cd \u1275\u12cd\u200b\u120d\u12f5 \u12ed\u1295\u200b\u1308\u1229\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a8\u1270\u200b\u121d\u127d \u12e8\u1240\u200b\u1228\u200b\u12cd\u1295 \u12a0\u1295\u200b\u1260\u1323 \u1260\u120b\u12cd\u1364 \u12a8\u12a0\u200b\u1295\u200b\u1260\u200b\u1323\u121d \u12e8\u1240\u200b\u1228\u200b\u12cd\u1295 \u12f0\u130e\u200b\u1265\u12eb \u1260\u120b\u12cd\u1364 \u12a8\u12f0\u200b\u130e\u200b\u1265\u200b\u12eb\u121d \u12e8\u1240\u200b\u1228\u200b\u12cd\u1295 \u12a9\u1265\u200b\u12a9\u1263 \u1260\u120b\u12cd\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12a5\u1293\u200b\u1295\u1270 \u1230\u12ab\u200b\u122b\u200b\u121e\u127d\u1365 \u1295\u1241\u1364 \u1208\u1218\u200b\u1235\u200b\u12a8\u200b\u122d\u121d \u12c8\u12ed\u200b\u1295\u1295 \u12e8\u121d\u200b\u1275\u200b\u1320\u1321 \u12a5\u1293\u200b\u1295\u1270 \u1201\u120b\u200b\u127d\u1201! \u1270\u12f5\u200b\u120b\u1293 \u12f0\u1235\u1273 \u12a8\u12a0\u200b\u134b\u200b\u127d\u1201 \u1320\u134d\u200b\u1275\u200b\u12cb\u200b\u120d\u1293 \u12a0\u120d\u200b\u1245\u1231\u1364 \u12a5\u12d8\u200b\u1291\u121d\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u124d\u1325\u200b\u122d\u121d \u12e8\u120c\u200b\u1208\u12cd \u1265\u122d\u1271 \u1215\u12dd\u1265 \u1260\u121d\u200b\u12f5\u122c \u120b\u12ed \u12c8\u1325\u200b\u1276\u200b\u12a0\u200b\u120d\u1293\u1364 \u1325\u122d\u200b\u1233\u200b\u1278\u12cd \u12a5\u1295\u12f0 \u12a0\u1295\u200b\u1260\u1233 \u1325\u122d\u1235\u1365 \u12ad\u1295\u200b\u12f3\u200b\u1278\u200b\u12cd\u121d \u12a5\u1295\u12f0 \u12a0\u1295\u200b\u1260\u1233 \u12f0\u1266\u120d \u12ad\u1295\u12f5 \u1290\u12cd\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12c8\u12ed\u200b\u1294\u1295 \u1263\u12f6 \u121d\u12f5\u122d \u12a0\u12f0\u200b\u1228\u200b\u1308\u12cd\u1364 \u1260\u1208\u200b\u1234\u200b\u1295\u121d \u1230\u1260\u200b\u1228\u12cd\u1364 \u1270\u1218\u200b\u1208\u200b\u12a8\u200b\u1270\u200b\u12cd\u121d\u1365 \u1323\u1208\u200b\u12cd\u121d\u1364 \u1245\u122d\u200b\u1295\u200b\u132b\u200b\u134e\u200b\u1279\u121d \u1290\u1321\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1235\u1208 \u120d\u1305\u200b\u1290\u1275 \u1263\u120d\u12cb \u121b\u1245 \u1208\u1265\u1233 \u12a5\u1295\u200b\u12f0\u200b\u121d\u200b\u1273\u200b\u1208\u200b\u1245\u1235 \u12f5\u1295\u200b\u130d\u120d \u12a0\u120d\u200b\u1245\u123a\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1218\u1225\u200b\u12cb\u200b\u12d5\u200b\u1271\u1293 \u12e8\u1218\u200b\u1320\u1321 \u124d\u122d\u200b\u1263\u1295 \u12a8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1264\u1275 \u1270\u12c8\u200b\u130d\u200b\u12f6\u200b\u12a0\u120d\u1364 \u1260\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1218\u1220\u200b\u12cd\u12eb \u12e8\u121d\u200b\u1273\u200b\u1308\u200b\u1208\u200b\u130d\u1209 \u12ab\u1205\u200b\u1293\u1271\u1365 \u12a0\u120d\u200b\u1245\u1231\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u122d\u200b\u123b\u12cd \u121d\u12f5\u1228 \u1260\u12f3 \u1206\u1296\u200b\u12a0\u120d\u1364 \u121d\u12f5\u200b\u122a\u200b\u1271\u121d \u1273\u120d\u200b\u1245\u1235\u1364 \u12a5\u1205\u1209 \u1320\u134d\u200b\u1276\u200b\u12a0\u200b\u120d\u1293\u1365 \u12c8\u12ed\u200b\u1291\u121d \u12f0\u122d\u200b\u1246\u200b\u12a0\u200b\u120d\u1293\u1365 \u12d8\u12ed\u200b\u1271\u121d \u1310\u12f5\u200b\u120e\u200b\u12a0\u200b\u120d\u1293\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1218\u12a8\u1229 \u12a8\u12a5\u200b\u122d\u200b\u123b\u200b\u1278\u12cd \u1320\u134d\u200b\u1276\u200b\u12a0\u200b\u120d\u1293 \u1308\u1260\u200b\u122c\u200b\u12ce\u127d \u1235\u1208 \u1235\u1295\u200b\u12f4\u200b\u12cd\u1293 \u1235\u1208 \u1308\u1265\u1231 \u12d0\u1348\u1229\u1364 \u12e8\u12c8\u200b\u12ed\u1295 \u12a0\u1275\u200b\u12ad\u200b\u120d\u200b\u1270\u200b\u129e\u200b\u127d\u121d \u1235\u1208 \u12c8\u12ed\u1291 \u12a0\u1208\u200b\u1240\u1231\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u12c8\u12ed\u1291 \u12f0\u122d\u200b\u1246\u200b\u12a0\u120d\u1364 \u1260\u1208\u200b\u1231\u121d \u1320\u134d\u200b\u1276\u200b\u12a0\u120d\u1364 \u122e\u121b\u200b\u1291\u1293 \u1270\u121d\u1229\u1365 \u12a5\u1295\u200b\u12ae\u200b\u12e9\u121d\u1365 \u12e8\u121d\u200b\u12f5\u200b\u122d\u121d \u12db\u134e\u127d \u1201\u1209 \u12f0\u122d\u200b\u1240\u200b\u12cb\u120d\u1364 \u12f0\u1235\u200b\u1273\u121d \u12a8\u1230\u12cd \u120d\u1306\u127d \u12d8\u1295\u12f5 \u122d\u1246\u200b\u12a0\u120d\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1218\u1225\u200b\u12cb\u200b\u12d5\u200b\u1271\u1293 \u12e8\u1218\u200b\u1320\u1321 \u124d\u122d\u200b\u1263\u1295 \u12a8\u12a0\u200b\u121d\u200b\u120b\u200b\u12ab\u200b\u127d\u1201 \u1264\u1275 \u1240\u122d\u200b\u1276\u200b\u12a0\u200b\u120d\u1293 \u12a5\u1293\u200b\u1295\u1270 \u12ab\u1205\u200b\u1293\u1275! \u121b\u1245 \u1273\u1325\u200b\u1243\u200b\u127d\u1201 \u12a0\u120d\u200b\u1245\u1231\u1364 \u12a5\u1293\u200b\u1295\u200b\u1270\u121d \u12e8\u1218\u200b\u1220\u200b\u12cd\u12eb \u12a0\u1308\u200b\u120d\u200b\u130b\u200b\u12ee\u127d! \u12cb\u12ed \u1260\u1209\u1364 \u12a5\u1293\u200b\u1295\u200b\u1270\u121d \u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u12a0\u1308\u200b\u120d\u200b\u130b\u200b\u12ee\u127d \u1291\u1293 \u1260\u121b\u1245 \u120b\u12ed \u1270\u1299\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u133e\u121d\u1295 \u1240\u12f5\u1231\u1364 \u121d\u1205\u200b\u120b\u200b\u1295\u121d \u12d0\u12cd\u1301\u1364 \u123d\u121b\u200b\u130d\u200b\u120c\u200b\u12ce\u200b\u1279\u200b\u1295\u1293 \u1260\u121d\u200b\u12f5\u122d \u12e8\u121a\u200b\u1296\u200b\u1229\u200b\u1275\u1295 \u1201\u1209 \u12c8\u12f0 \u12a0\u121d\u200b\u120b\u200b\u12ab\u200b\u127d\u1295 \u12c8\u12f0 \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1264\u1275 \u1230\u1265\u200b\u1235\u1261\u1364 \u12c8\u12f0 \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1260\u12a0\u200b\u1295\u200b\u12f5\u200b\u1290\u1275 \u1329\u12b9\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1240\u1295 \u1240\u122d\u200b\u1266\u200b\u12a0\u200b\u120d\u1293 \u1208\u1240\u1291 \u12c8\u12ee! \u12c8\u12ee! \u12a5\u122d\u200b\u1231\u121d \u1260\u1325\u200b\u134b\u1275 \u120b\u12ed \u12a5\u1295\u12f0 \u1325\u134b\u1275 \u12ed\u1218\u200b\u1323\u120d\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u12a8\u12d0\u200b\u12ed\u200b\u1296\u200b\u127b\u200b\u127d\u1201 \u134a\u1275 \u121d\u130d\u1265\u1365 \u12a8\u12a0\u200b\u121d\u200b\u120b\u200b\u12ab\u200b\u127d\u200b\u1201\u121d \u1264\u1275 \u12f0\u1235\u200b\u1273\u1293 \u1210\u1224\u1275 \u1320\u134d\u200b\u1276\u200b\u12a0\u120d\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u130a\u12f0\u200b\u122e\u127d \u12a8\u121d\u200b\u1233\u200b\u130b\u200b\u1278\u12cd \u1320\u1349\u1364 \u130e\u1270\u200b\u122b\u200b\u12ce\u127d \u1263\u12f6 \u1206\u1291\u1364 \u12d0\u12cd\u200b\u12f5\u200b\u121b\u200b\u12ce\u200b\u1279\u121d \u1348\u122b\u200b\u1228\u1231\u1364 \u12a5\u1205\u1209 \u12f0\u122d\u200b\u1246\u200b\u12a0\u200b\u120d\u1293\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u12a5\u1295\u200b\u130d\u200b\u12f2\u1205 \u1208\u122b\u200b\u1233\u200b\u127d\u1295 \u121d\u1295 \u12a5\u1295\u200b\u1230\u200b\u1265\u200b\u1235\u1265? \u12e8\u12a5\u200b\u1295\u200b\u1235\u200b\u1233\u1275 \u1320\u1263\u200b\u1242\u200b\u12ce\u127d \u12a0\u1208\u200b\u1240\u1231\u1364 \u121b\u1230\u200b\u121b\u200b\u122a\u12eb \u12e8\u120b\u200b\u1278\u200b\u12cd\u200b\u121d\u1293\u1365 \u12e8\u1260\u200b\u130e\u127d \u1218\u1295\u200b\u130b\u200b\u12ce\u200b\u127d\u121d \u1320\u134d\u200b\u1270\u200b\u12cb\u200b\u120d\u1293\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12a0\u1264\u1271! \u12a5\u1233\u1275 \u12e8\u121d\u200b\u12f5\u1228 \u1260\u12f3\u200b\u12cd\u1295 \u12cd\u1260\u1275 \u12a0\u1325\u200b\u134d\u200b\u1276\u200b\u12a0\u200b\u120d\u1293\u1365 \u1290\u1260\u200b\u120d\u200b\u1263\u200b\u1209\u121d \u12e8\u12f1\u200b\u1229\u1295 \u12db\u134d \u1201\u1209 \u12a0\u1243\u200b\u1325\u200b\u120e\u200b\u12a0\u200b\u120d\u1293 \u12c8\u12f0 \u12a0\u1295\u1270 \u12a5\u132e\u200b\u12bb\u200b\u1208\u1201\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u1348\u1233\u1239 \u12cd\u1283 \u12f0\u122d\u200b\u1246\u200b\u12a0\u200b\u120d\u1293\u1365 \u12a5\u1233\u200b\u1271\u121d \u12e8\u121d\u200b\u12f5\u1228 \u1260\u12f3\u200b\u12cd\u1295 \u12cd\u1260\u1275 \u1260\u120d\u200b\u1276\u200b\u12a0\u200b\u120d\u1293 \u12e8\u121d\u200b\u12f5\u122d \u12a0\u122b\u200b\u12ca\u1275 \u12c8\u12f0 \u12a0\u1295\u1270 \u12a0\u1295\u200b\u130b\u200b\u1320\u1321\u1362"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u133d\u200b\u12ee\u1295 \u1218\u1208\u200b\u12a8\u200b\u1275\u1295 \u1295\u1349\u1364 \u1260\u1245\u200b\u12f1\u200b\u1231\u121d \u1270\u122b\u200b\u122b\u12ec \u120b\u12ed \u12d0\u12cb\u1305 \u1295\u1308\u1229\u1364 \u1260\u121d\u200b\u12f5\u200b\u122d\u121d \u12e8\u121a\u200b\u1296\u1229 \u1201\u1209 \u12ed\u12f0\u200b\u1295\u200b\u130d\u1321\u1364 \u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1240\u1295 \u1218\u1325\u200b\u1276\u200b\u12a0\u200b\u120d\u1293\u1365 \u12a5\u122d\u200b\u1231\u121d \u1240\u122d\u200b\u1266\u200b\u12a0\u200b\u120d\u1293\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12e8\u1328\u200b\u1208\u200b\u121b\u1293 \u12e8\u1290\u200b\u134b\u1235 \u1240\u1295\u1365 \u12e8\u12f0\u200b\u1218\u200b\u1293\u1293 \u12e8\u1309\u121d \u1240\u1295 \u1290\u12cd\u1364 \u1273\u120b\u200b\u1245\u1293 \u1265\u122d\u1271 \u1215\u12dd\u1265 \u1260\u1270\u200b\u122b\u200b\u122e\u127d \u120b\u12ed \u12a5\u1295\u12f0 \u12c8\u1308\u200b\u130d\u1273 \u1270\u12d8\u200b\u122d\u200b\u130d\u200b\u1276\u200b\u12a0\u120d\u1364 \u12a8\u12d8\u200b\u1208\u200b\u12d3\u200b\u1208\u200b\u121d\u121d \u1300\u121d\u122e \u12a5\u1295\u12f0 \u12a5\u122d\u1231 \u12eb\u1208 \u12a0\u120d\u200b\u1290\u200b\u1260\u200b\u1228\u121d\u1364 \u12a8\u12a5\u200b\u122d\u200b\u1231\u121d \u1260\u128b\u120b \u12a5\u1235\u12a8 \u120d\u1305 \u120d\u1305 \u12f5\u1228\u1235 \u12a5\u1295\u12f0 \u12a5\u122d\u1231 \u12eb\u1208 \u12a0\u12ed\u200b\u1206\u200b\u1295\u121d\u1362"
                },
                "3": {
