    "book_nr": "37",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u1295\u1309\u1221 \u1260\u12f3\u122d\u12ee\u1235 \u1260\u1201\u1208\u1270\u129b\u12cd \u12d3\u1218\u1275 \u1260\u1235\u12f5\u1235\u1270\u129b\u12cd \u12c8\u122d \u12a8\u12c8\u1229\u121d \u1260\u1218\u1300\u1218\u122a\u12eb\u12cd \u1240\u1295 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1243\u120d \u1260\u1290\u1262\u12e9 \u1260\u1210\u130c \u12a5\u1305 \u12c8\u12f0 \u12ed\u1201\u12f3 \u12a0\u1208\u1243 \u12c8\u12f0 \u1230\u120b\u1275\u12eb\u120d \u120d\u1305 \u12c8\u12f0 \u12d8\u1229\u1263\u1264\u120d\u1365 \u12c8\u12f0 \u1273\u120b\u1241\u121d \u12ab\u1205\u1295 \u12c8\u12f0 \u12a2\u12ee\u1234\u12f4\u1245 \u120d\u1305 \u12c8\u12f0 \u12a2\u12eb\u1231 \u12a5\u1295\u12f2\u1205 \u1232\u120d \u1218\u1323\u1366"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1366 \u12ed\u1205 \u1215\u12dd\u1265\u1366 \u12d8\u1218\u1291 \u12a0\u120d\u12f0\u1228\u1230\u121d\u1365 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1264\u1275 \u12e8\u121a\u1220\u122b\u1260\u1275 \u12d8\u1218\u1295 \u12a0\u120d\u12f0\u1228\u1230\u121d \u12ed\u120b\u120d \u1265\u120e \u1270\u1293\u1308\u1228\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u1243\u120d \u1260\u1290\u1262\u12e9 \u1260\u1210\u130c \u12a5\u1305 \u12a5\u1295\u12f2\u1205 \u1232\u120d \u1218\u1323\u1366"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u1260\u12cd\u1291 \u12ed\u1205 \u1264\u1275 \u1348\u122d\u1236 \u1233\u1208 \u12a5\u1293\u1295\u1270 \u122b\u1233\u127d\u1201 \u1260\u1270\u1238\u1208\u1219 \u1264\u1276\u127b\u127d\u1201 \u1208\u1218\u1296\u122d \u130a\u12dc\u12cd \u1290\u12cd\u1295?"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12a0\u1201\u1295\u121d \u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u1295\u12f2\u1205 \u12ed\u120b\u120d\u1366 \u120d\u1263\u127d\u1201\u1295 \u1260\u1218\u1295\u1308\u12f3\u127d\u1201 \u120b\u12ed \u12a0\u12f5\u122d\u1309\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u1265\u12d9 \u12d8\u122b\u127d\u1201\u1365 \u1325\u1242\u1275\u121d \u12a0\u1308\u1263\u127d\u1201\u1363 \u1260\u120b\u127d\u1201\u1365 \u1290\u1308\u122d \u130d\u1295 \u12a0\u120d\u1320\u1308\u1263\u127d\u1201\u121d\u1363 \u1320\u1323\u127d\u1201\u1365 \u1290\u1308\u122d \u130d\u1295 \u12a0\u120d\u1228\u12ab\u127d\u1201\u121d\u1363 \u1208\u1260\u1233\u127d\u1201\u1365 \u1290\u1308\u122d \u130d\u1295 \u12a0\u120d\u121e\u1243\u127d\u1201\u121d\u1363 \u12f0\u1218\u12c8\u12d9\u1295 \u12e8\u1270\u1240\u1260\u1208 \u1230\u12cd \u1260\u1240\u12f3\u12f3 \u12a8\u1228\u1322\u1275 \u12eb\u12f0\u122d\u1308\u12cd \u12d8\u1295\u12f5 \u12f0\u1218\u12c8\u12d9\u1295 \u1270\u1240\u1260\u1208\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u1295\u12f2\u1205 \u12ed\u120b\u120d\u1366 \u120d\u1263\u127d\u1201\u1295 \u1260\u1218\u1295\u1308\u12f3\u127d\u1201 \u120b\u12ed \u12a0\u12f5\u122d\u1309\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u12c8\u12f0 \u1270\u122b\u122b\u12cd \u12cd\u1321\u1365 \u12a5\u1295\u1328\u1275\u1295\u121d \u12a0\u121d\u1321\u1365 \u1264\u1271\u1295\u121d \u1225\u1229\u1363 \u12a5\u1294\u121d \u1260\u12a5\u122d\u1231 \u12f0\u1235 \u12ed\u1208\u129b\u120d\u1365 \u12a5\u1294\u121d \u12a5\u1218\u1230\u1308\u1293\u1208\u1201\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a5\u1293\u1295\u1270 \u1265\u12d9 \u1290\u1308\u122d\u1295 \u1270\u1235\u134b \u12a0\u12f0\u1228\u130b\u127d\u1201\u1365 \u12a5\u1290\u1206\u121d\u1365 \u1325\u1242\u1275 \u1206\u1290\u1363 \u12c8\u12f0 \u1264\u1275\u121d \u1263\u1308\u1263\u127d\u1201\u1275 \u130a\u12dc \u12a5\u134d \u12a0\u120d\u1201\u1260\u1275\u1362 \u12ed\u1205 \u1235\u1208 \u121d\u1295\u12f5\u122d \u1290\u12cd? \u12ed\u120b\u120d \u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362 \u12a5\u1293\u1295\u1270 \u1201\u1209 \u12c8\u12f0 \u12a5\u12e8\u1264\u1273\u127d\u1201 \u12a5\u12e8\u122e\u1323\u127d\u1201 \u12e8\u12a5\u1294 \u1264\u1275 \u1348\u122d\u1236 \u1235\u1208 \u1270\u1240\u1218\u1320 \u1290\u12cd\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u1235\u1208\u12da\u1205 \u1230\u121b\u12eb\u1275 \u1260\u120b\u12eb\u127d\u1201 \u1320\u120d\u1295 \u12a8\u120d\u12ad\u1208\u12cb\u120d\u1365 \u121d\u12f5\u122a\u1271\u121d \u134d\u122c\u12cb\u1295 \u12a8\u120d\u12ad\u120b\u1208\u127d\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u12a5\u1294\u121d \u1260\u121d\u12f5\u122d\u1293 \u1260\u1270\u122b\u122e\u127d\u1365 \u1260\u12a5\u1205\u120d\u1293 \u1260\u12c8\u12ed\u1295\u121d\u1365 \u1260\u12d8\u12ed\u1275\u1293 \u121d\u12f5\u122d\u121d \u1260\u121d\u1273\u1260\u1245\u1208\u12cd \u120b\u12ed\u1365 \u1260\u1230\u12ce\u127d\u1293 \u1260\u12a5\u1295\u1235\u1236\u127d\u121d \u120b\u12ed\u1365 \u12a5\u1305\u121d \u1260\u121a\u12f0\u12ad\u121d\u1260\u1275 \u1201\u1209 \u120b\u12ed \u12f5\u122d\u1245\u1295 \u1320\u122d\u127b\u1208\u1201\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u12e8\u1230\u120b\u1275\u12eb\u120d\u121d \u120d\u1305 \u12d8\u1229\u1263\u1264\u120d\u1365 \u1273\u120b\u1241\u121d \u12ab\u1205\u1295 \u12e8\u12a2\u12ee\u1234\u12f4\u1245 \u120d\u1305 \u12a2\u12eb\u1231\u1365 \u12e8\u1240\u1229\u1275\u121d \u1215\u12dd\u1265 \u1201\u1209 \u12e8\u12a0\u121d\u120b\u12ab\u1278\u12cd\u1295 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u12f5\u121d\u1345 \u12e8\u1290\u1262\u12e9\u1295\u121d \u12e8\u1210\u130c\u1295 \u1243\u120d\u1365 \u12a0\u121d\u120b\u12ab\u1278\u12cd \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u1295\u12f0 \u120b\u12a8\u12cd \u1201\u1209 \u1230\u1219\u1363 \u1215\u12dd\u1261\u121d \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u134a\u1275 \u1348\u1229\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u1218\u120d\u12a5\u12ad\u1270\u129b \u1210\u130c \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1218\u120d\u12a5\u12ad\u1275 \u1215\u12dd\u1261\u1295\u1366 \u12a5\u1294 \u12a8\u12a5\u1293\u1295\u1270 \u130b\u122d \u1290\u129d\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1265\u120e \u1270\u1293\u1308\u1228\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u12e8\u12ed\u1201\u12f3\u1295 \u12a0\u1208\u1243 \u12e8\u1230\u120b\u1275\u12eb\u120d\u1295 \u120d\u1305 \u12e8\u12d8\u1229\u1263\u1264\u120d\u1295 \u1218\u1295\u1348\u1235\u1365 \u12e8\u1273\u120b\u1241\u1295\u121d \u12ab\u1205\u1295 \u12e8\u12a2\u12ee\u1234\u12f4\u1245\u1295 \u120d\u1305 \u12e8\u12a2\u12eb\u1231\u1295 \u1218\u1295\u1348\u1235\u1365 \u12e8\u1240\u1229\u1275\u1295\u121d \u1215\u12dd\u1265 \u1201\u1209 \u1218\u1295\u1348\u1235 \u12a0\u1235\u1290\u1223\u1363 \u1260\u1295\u1309\u1221\u121d \u1260\u12f3\u122d\u12ee\u1235 \u1260\u1201\u1208\u1270\u129b\u12cd \u12d3\u1218\u1275 \u1260\u1235\u12f5\u1235\u1270\u129b\u12cd \u12c8\u122d \u12a8\u12c8\u1229\u121d \u1260\u1200\u12eb \u12a0\u122b\u1270\u129b\u12cd \u1240\u1295 \u1218\u1321\u1365 \u12e8\u12a0\u121d\u120b\u12ab\u1278\u12cd\u1295\u121d \u12e8\u1220\u122b\u12ca\u1275\u1295 \u130c\u1273 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u1264\u1275 \u1220\u1229\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u12e8\u12ed\u1201\u12f3\u1295 \u12a0\u1208\u1243 \u12e8\u1230\u120b\u1275\u12eb\u120d\u1295 \u120d\u1305 \u12e8\u12d8\u1229\u1263\u1264\u120d\u1295 \u1218\u1295\u1348\u1235\u1365 \u12e8\u1273\u120b\u1241\u1295\u121d \u12ab\u1205\u1295 \u12e8\u12a2\u12ee\u1234\u12f4\u1245\u1295 \u120d\u1305 \u12e8\u12a2\u12eb\u1231\u1295 \u1218\u1295\u1348\u1235\u1365 \u12e8\u1240\u1229\u1275\u1295\u121d \u1215\u12dd\u1265 \u1201\u1209 \u1218\u1295\u1348\u1235 \u12a0\u1235\u1290\u1223\u1363 \u1260\u1295\u1309\u1221\u121d \u1260\u12f3\u122d\u12ee\u1235 \u1260\u1201\u1208\u1270\u129b\u12cd \u12d3\u1218\u1275 \u1260\u1235\u12f5\u1235\u1270\u129b\u12cd \u12c8\u122d \u12a8\u12c8\u1229\u121d \u1260\u1200\u12eb \u12a0\u122b\u1270\u129b\u12cd \u1240\u1295 \u1218\u1321\u1365 \u12e8\u12a0\u121d\u120b\u12ab\u1278\u12cd\u1295\u121d \u12e8\u1220\u122b\u12ca\u1275\u1295 \u130c\u1273 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u1264\u1275 \u1220\u1229\u1362"
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u1230\u1263\u1270\u129b\u12cd \u12c8\u122d \u12a8\u12c8\u1229\u121d \u1260\u1200\u12eb \u12a0\u1295\u12f0\u129b\u12cd \u1240\u1295 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1243\u120d \u1260\u1290\u1262\u12e9 \u1260\u1210\u130c \u12a5\u1305 \u12a5\u1295\u12f2\u1205 \u1232\u120d \u1218\u1323\u1366"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1208\u12ed\u1201\u12f3 \u12a0\u1208\u1243 \u1208\u1230\u120b\u1275\u12eb\u120d \u120d\u1305 \u1208\u12d8\u1229\u1263\u1264\u120d\u1365 \u1208\u1273\u120b\u1241\u121d \u12ab\u1205\u1295 \u1208\u12a2\u12ee\u1234\u12f4\u1245 \u120d\u1305 \u1208\u12a2\u12eb\u1231\u1365 \u1208\u1240\u1229\u1275\u121d \u1215\u12dd\u1265 \u12a5\u1295\u12f2\u1205 \u1235\u1275\u120d \u1270\u1293\u1308\u122d\u1366"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1260\u1240\u12f5\u121e \u12ad\u1265\u1229 \u1233\u1208 \u12ed\u1205\u1295 \u1264\u1275 \u12eb\u12e8 \u1260\u12a5\u1293\u1295\u1270 \u1218\u12ab\u12a8\u120d \u12e8\u1240\u1228 \u121b\u1295 \u1290\u12cd? \u12db\u122c\u1235 \u12a5\u1295\u12f4\u1275 \u1206\u1296 \u12a0\u12eb\u127d\u1201\u1275? \u1260\u12d3\u12ed\u1293\u127d\u1201 \u12a5\u1295\u12f0 \u121d\u1293\u121d\u1295 \u12a0\u12ed\u12f0\u1208\u121d\u1295?"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a0\u1201\u1295 \u130d\u1295\u1365 \u12d8\u1229\u1263\u1264\u120d \u1206\u12ed\u1365 \u1260\u122d\u1273\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1363 \u1273\u120b\u1241\u121d \u12ab\u1205\u1295 \u12e8\u12a2\u12ee\u1234\u12f4\u1245 \u120d\u1305 \u12a2\u12eb\u1231 \u1206\u12ed\u1365 \u1260\u122d\u1273\u1365 \u12a5\u1293\u1295\u1270\u121d \u12e8\u12a0\u1308\u1229 \u1215\u12dd\u1265 \u1206\u12ed\u1365 \u1260\u122d\u1271\u1293 \u1225\u1229\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1363 \u12a5\u1294 \u12a8\u12a5\u1293\u1295\u1270 \u130b\u122d \u1290\u129d\u1293\u1365 \u12ed\u120b\u120d \u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1218\u1295\u1348\u1234\u121d \u1260\u1218\u12ab\u12a8\u120b\u127d\u1201 \u12ed\u1296\u122b\u120d\u1293 \u12a0\u1275\u134d\u1229\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u1295\u12f2\u1205 \u12ed\u120b\u120d\u1293\u1366 \u1308\u1293 \u12a0\u1295\u12f5 \u130a\u12dc \u1260\u1245\u122d\u1265 \u12d8\u1218\u1295 \u12a5\u1294 \u1230\u121b\u12eb\u1275\u1295\u1293 \u121d\u12f5\u122d\u1295 \u1263\u1215\u122d\u1295\u1293 \u12e8\u1265\u1235\u1295\u121d \u12a0\u1293\u12cd\u1323\u1208\u1201\u1363"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12a0\u1215\u12db\u1265\u1295 \u1201\u1209 \u12a0\u1293\u12cd\u1323\u1208\u1201\u1365 \u1260\u12a0\u1215\u12db\u1265 \u1201\u1209 \u12e8\u1270\u1218\u1228\u1320\u12cd\u121d \u12d5\u1243 \u12ed\u1218\u1323\u120d\u1363 \u12ed\u1205\u1295\u121d \u1264\u1275 \u1260\u12ad\u1265\u122d \u12a5\u121e\u120b\u12cb\u1208\u1201\u1365 \u12ed\u120b\u120d \u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1265\u1229 \u12e8\u12a5\u1294 \u1290\u12cd\u1365 \u12c8\u122d\u1241\u121d \u12e8\u12a5\u1294 \u1290\u12cd\u1365 \u12ed\u120b\u120d \u12e8\u1220\u122b\u12ca\u1275 \u130c\u1273 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362"
                },
                "9": {
