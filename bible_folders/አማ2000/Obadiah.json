        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12e8\u12a0\u200b\u1265\u200b\u12f5\u12e9 \u122b\u12a5\u12ed\u1362\n\u130c\u1273 \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1235\u1208 \u12a4\u12f6\u200b\u121d\u200b\u12eb\u1235 \u12a5\u1295\u200b\u12f2\u1205 \u12ed\u120b\u120d\u1366 \u12a8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u12d8\u1295\u12f5 \u1218\u1235\u200b\u121b\u200b\u1275\u1295 \u1230\u121b\u1201\u1364 \u12a8\u1263\u200b\u1262\u1295 \u12c8\u12f0 \u12a0\u1215\u200b\u12db\u1265 \u120d\u12ae\u1365 \u201c\u1270\u1290\u1221\u1364 \u1260\u120b\u200b\u12ed\u200b\u12cb\u121d \u12a5\u1295\u200b\u1290\u200b\u1223\u1293 \u12a5\u1295\u200b\u12cd\u200b\u130b\u1275\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12a5\u1290\u1206 \u1260\u12a0\u200b\u1215\u200b\u12db\u1265 \u12d8\u1295\u12f5 \u1273\u1293\u123d \u12a0\u12f5\u200b\u122d\u200b\u130c\u200b\u1203\u200b\u1208\u1201\u1364 \u12a0\u1295\u1270 \u12a5\u1305\u130d \u1270\u1295\u200b\u1240\u200b\u1203\u120d\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1260\u1270\u200b\u1230\u200b\u1290\u200b\u1320\u200b\u1240\u121d \u12d3\u1208\u1275 \u12cd\u1235\u1325 \u12a5\u1295\u200b\u12f0\u200b\u121a\u200b\u1296\u122d- \u121b\u12f0\u200b\u122a\u200b\u12eb\u200b\u12cd\u200b\u1295\u121d \u12a8\u134d \u12a8\u134d \u12a5\u1295\u200b\u12f0\u200b\u121a\u200b\u12eb\u200b\u12f0\u200b\u122d\u130d\u1365 \u1260\u120d\u200b\u1261\u121d\u1366 \u12c8\u12f0 \u121d\u12f5\u122d \u12e8\u121a\u200b\u12eb\u200b\u12c8\u200b\u122d\u200b\u12f0\u129d \u121b\u1295 \u1290\u12cd? \u12a5\u1295\u200b\u12f0\u200b\u121a\u120d\u1365 \u12e8\u120d\u200b\u1265\u1205 \u1275\u12d5\u200b\u1262\u1275 \u12a5\u1305\u130d \u12a0\u12b5\u200b\u122d\u200b\u1276\u200b\u1203\u120d\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u1295\u12f0 \u1295\u1235\u122d \u1218\u1325\u200b\u1240\u1205 \u1265\u1275\u200b\u12c8\u1323\u1365 \u1264\u1275\u200b\u1205\u121d \u1260\u12a8\u200b\u12cb\u200b\u12ad\u200b\u1265\u1275 \u1218\u12ab\u200b\u12a8\u120d \u1262\u1206\u1295\u1365 \u12a8\u12da\u12eb \u12a0\u12c8\u200b\u122d\u200b\u12f5\u200b\u1203\u200b\u1208\u1201\u1365\u201d \u12ed\u120b\u120d \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u201c\u120c\u1266\u127d \u1262\u1218\u200b\u1321\u200b\u1265\u1205\u1365 \u12c8\u12ed\u121d \u12c8\u1295\u200b\u1260\u200b\u12f4\u200b\u12ce\u127d \u1260\u120c\u200b\u120a\u1275 \u1262\u1218\u1321\u1365 \u12e8\u121a\u200b\u1260\u200b\u1243\u200b\u1278\u200b\u12cd\u1295 \u12e8\u121a\u200b\u1230\u200b\u122d\u1241 \u12a0\u12ed\u200b\u12f0\u200b\u1209\u200b\u121d\u1295? \u12c8\u12ed\u200b\u1295\u121d \u12e8\u121a\u200b\u1248\u200b\u122d\u1321 \u12c8\u12f0 \u12a0\u1295\u1270 \u1262\u1218\u1321 \u1243\u122d\u200b\u121a\u12eb \u12a0\u12eb\u200b\u1235\u200b\u1240\u200b\u1229\u200b\u121d\u1295?"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12d4\u1233\u200b\u12cd\u1295 \u12a5\u1295\u200b\u12f4\u1275 \u1218\u1228\u200b\u1218\u200b\u1229\u1275! \u12e8\u1238\u200b\u1238\u200b\u1308\u200b\u12cd\u200b\u1295\u121d \u12a5\u1295\u200b\u12f4\u1275 \u12c8\u1230\u200b\u12f1\u200b\u1260\u1275!"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12e8\u1270\u200b\u121b\u200b\u121b\u200b\u120d\u200b\u1203\u200b\u1278\u12cd \u1230\u12ce\u127d \u1201\u1209 \u12c8\u12f0 \u12f3\u122d\u200b\u127b\u1205 \u1230\u12f0\u200b\u12f1\u1205\u1364 \u12e8\u1270\u200b\u1235\u200b\u121b\u200b\u1219\u200b\u1205\u121d \u1230\u12ce\u127d \u1270\u1290\u200b\u1221\u200b\u1265\u1205\u1364 \u12a0\u1238\u200b\u1290\u200b\u1349\u200b\u1205\u121d\u1364 \u1260\u1260\u200b\u1273\u200b\u127d\u200b\u1205\u121d \u12a0\u123d\u200b\u12ad\u120b \u12d8\u1228\u200b\u1309\u200b\u1265\u1205\u1364 \u12a5\u1290\u200b\u122d\u200b\u1231\u121d \u121b\u1235\u200b\u1270\u200b\u12cb\u120d \u12e8\u120b\u200b\u1278\u200b\u12cd\u121d\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1260\u12da\u12eb \u1240\u1295 \u12a8\u12a4\u200b\u12f6\u200b\u121d\u200b\u12eb\u1235 \u1325\u1260\u200b\u1260\u200b\u129e\u200b\u127d\u1295\u1365 \u12a8\u12d4\u200b\u1233\u12cd \u1270\u122b\u200b\u122b\u121d \u121b\u1235\u200b\u1270\u200b\u12cb\u200b\u120d\u1295 \u12a0\u1320\u200b\u134b\u200b\u1208\u1201 \u12ed\u120b\u120d \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a8\u12d4\u200b\u1233\u12cd \u1270\u122b\u122b \u1230\u12ce\u127d \u1201\u1209 \u12ed\u1320\u1349 \u12d8\u1295\u12f5 \u12e8\u1274\u200b\u121b\u1295 \u1230\u120d\u200b\u1348\u200b\u129e\u200b\u127d\u1205 \u12ed\u12f0\u200b\u1290\u200b\u130d\u200b\u1323\u1209\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u201c\u1260\u12c8\u200b\u1295\u200b\u12f5\u200b\u121d\u1205 \u1260\u12eb\u200b\u12d5\u200b\u1246\u1265 \u120b\u12ed \u1235\u1208 \u1270\u12f0\u200b\u1228\u1308 \u1280\u1322\u200b\u12a0\u200b\u1275\u1293 \u130d\u12f5\u12eb \u1280\u134d\u200b\u1228\u1275 \u12ed\u12a8\u200b\u12f5\u200b\u1295\u200b\u1203\u120d\u1364 \u1208\u12d8\u200b\u1208\u200b\u12d3\u200b\u1208\u200b\u121d\u121d \u1275\u1320\u200b\u134b\u200b\u1208\u1205\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1260\u134a\u1271 \u12a0\u1295\u200b\u133b\u122d \u1260\u1246\u200b\u121d\u1205 \u1240\u1295\u1365 \u12a0\u1215\u200b\u12db\u1265 \u132d\u134d\u200b\u122b\u200b\u12cd\u1295 \u1260\u121b\u200b\u1228\u200b\u12a9\u200b\u1260\u1275\u1365 \u12a5\u1295\u200b\u130d\u200b\u12f6\u200b\u127d\u121d \u1260\u1260\u1229 \u1260\u1308\u200b\u1261\u200b\u1260\u1275\u1365 \u1260\u12a2\u200b\u12e8\u200b\u1229\u200b\u1233\u200b\u120c\u200b\u121d\u121d \u12d5\u1323 \u1260\u1270\u200b\u1323\u200b\u1323\u200b\u1209\u200b\u1260\u1275 \u1240\u1295 \u12a0\u1295\u1270 \u12f0\u130d\u121e \u12a8\u12a5\u200b\u1290\u200b\u122d\u1231 \u12a5\u1295\u12f0 \u12a0\u1295\u12f1 \u1290\u1260\u200b\u122d\u1205\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u1260\u1320\u200b\u120b\u1275 \u1240\u1295 \u12c8\u1295\u200b\u12f5\u200b\u121d\u200b\u1205\u1295 \u12dd\u1245 \u12a0\u12f5\u200b\u122d\u200b\u1308\u1205 \u1275\u1218\u200b\u1208\u200b\u12a8\u200b\u1270\u12cd \u12d8\u1295\u12f5\u1365 \u1260\u1325\u200b\u134b\u200b\u1273\u200b\u1278\u200b\u12cd\u121d \u1240\u1295 \u1260\u12ed\u200b\u1201\u12f3 \u120d\u1306\u127d \u120b\u12ed \u12f0\u1235 \u12ed\u120d\u1205 \u12d8\u1295\u12f5\u1365 \u1260\u132d\u200b\u1295\u200b\u1240\u200b\u1273\u200b\u1278\u200b\u12cd\u121d \u1240\u1295 \u1260\u1275\u200b\u12d5\u200b\u1262\u1275 \u1275\u1293\u200b\u1308\u122d \u12d8\u1295\u12f5 \u1263\u120d\u200b\u1270\u200b\u1308\u200b\u1263\u1205 \u1290\u1260\u122d\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1260\u127d\u200b\u130d\u200b\u122b\u200b\u1278\u200b\u12cd\u121d \u1240\u1295 \u1260\u1215\u200b\u12dd\u1264 \u1260\u122d \u1275\u1308\u1263 \u12d8\u1295\u12f5\u1365 \u1260\u1325\u200b\u134b\u200b\u1273\u200b\u1278\u200b\u12cd\u121d \u1240\u1295 \u1309\u1263\u200b\u12a4\u200b\u12eb\u200b\u1278\u200b\u12cd\u1295 \u1275\u1218\u200b\u1208\u200b\u12a8\u1275 \u12d8\u1295\u12f5\u1365 \u1260\u12a5\u200b\u120d\u200b\u1242\u200b\u1273\u200b\u1278\u200b\u12cd\u121d \u1240\u1295 \u1260\u1220\u200b\u122b\u200b\u12ca\u200b\u1273\u200b\u1278\u12cd \u120b\u12ed \u12a0\u12f0\u130b \u1275\u1325\u120d \u12d8\u1295\u12f5 \u12a0\u12ed\u200b\u1308\u200b\u1263\u200b\u1205\u121d \u1290\u1260\u122d\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u12e8\u1238\u200b\u1239\u200b\u1275\u200b\u1295\u121d \u1208\u121b\u200b\u1325\u200b\u134b\u1275 \u1260\u1218\u200b\u1270\u200b\u120b\u200b\u1208\u200b\u134a\u12eb \u1218\u1295\u200b\u1308\u200b\u12f3\u200b\u1278\u12cd \u1275\u1246\u121d \u12d8\u1295\u12f5\u1365 \u12f5\u120d \u1260\u1270\u200b\u1290\u200b\u1221\u200b\u1260\u200b\u1275\u121d \u1240\u1295 \u12a8\u12a5\u200b\u122d\u1231 \u12eb\u1218\u200b\u1208\u200b\u1321\u200b\u1275\u1295 \u1208\u121b\u200b\u1235\u200b\u1328\u200b\u1290\u1245 \u1275\u12a8\u200b\u1265\u200b\u1263\u200b\u1278\u12cd \u12d8\u1295\u12f5 \u1263\u120d\u200b\u1270\u200b\u1308\u200b\u1263\u1205 \u1290\u1260\u122d\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u201c\u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1240\u1295 \u1260\u12a0\u200b\u1215\u200b\u12db\u1265 \u1201\u1209 \u120b\u12ed \u1240\u122d\u200b\u1266\u200b\u12a0\u200b\u120d\u1293\u1365 \u12a0\u1295\u1270 \u12a5\u1295\u12f0 \u12a0\u12f0\u200b\u1228\u200b\u130d\u200b\u12b8\u12cd \u12a5\u1295\u200b\u12f2\u1201 \u12ed\u12f0\u200b\u1228\u200b\u130d\u200b\u1265\u200b\u1203\u120d\u1364 \u134d\u12f3\u200b\u1205\u121d \u1260\u122b\u200b\u1235\u1205 \u120b\u12ed \u12ed\u1218\u200b\u1208\u200b\u1233\u120d\u1362"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u1260\u1245\u200b\u12f1\u1231 \u1270\u122b\u200b\u122b\u12ec \u120b\u12ed \u12a5\u1295\u12f0 \u1320\u1323\u1205 \u12a5\u1295\u200b\u12f2\u1201 \u12a0\u1215\u200b\u12db\u1265 \u1201\u1209 \u12c8\u12ed\u200b\u1295\u1295 \u12ed\u1320\u200b\u1323\u1209\u1364 \u12a0\u12ce\u1295\u1365 \u12ed\u1320\u200b\u1323\u1209\u1365 \u12ed\u1328\u200b\u120d\u200b\u1321\u200b\u121b\u120d\u1364 \u12a5\u1295\u200b\u12f3\u200b\u120d\u200b\u1290\u200b\u1260\u200b\u1229\u121d \u12ed\u1206\u200b\u1293\u1209\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u201c\u1290\u1308\u122d \u130d\u1295 \u1260\u133d\u200b\u12ee\u1295 \u1270\u122b\u122b \u120b\u12ed \u1218\u12f5\u200b\u1280\u200b\u1292\u1275 \u12ed\u1206\u200b\u1293\u120d\u1364 \u12a5\u122d\u200b\u1231\u121d \u1245\u12f1\u1235 \u12ed\u1206\u200b\u1293\u120d\u1364 \u12e8\u12eb\u200b\u12d5\u200b\u1246\u200b\u1265\u121d \u1264\u1275 \u1230\u12ce\u127d \u12e8\u12c8\u200b\u1228\u200b\u1231\u200b\u12a0\u200b\u1278\u200b\u12cd\u1295 \u12ed\u12c8\u200b\u122d\u200b\u1233\u1209\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1270\u1293\u200b\u130d\u200b\u122e\u200b\u12a0\u200b\u120d\u1293 \u12e8\u12eb\u200b\u12d5\u200b\u1246\u1265 \u1264\u1275 \u12a5\u1233\u1275\u1365 \u12e8\u12ee\u200b\u1234\u200b\u134d\u121d \u1264\u1275 \u1290\u1260\u200b\u120d\u200b\u1263\u120d\u1365 \u12e8\u12d4\u200b\u1233\u200b\u12cd\u121d \u1264\u1275 \u1308\u1208\u1263 \u12ed\u1206\u200b\u1293\u1209\u1364 \u12a5\u1290\u200b\u122d\u200b\u1231\u200b\u1295\u121d \u12eb\u1243\u200b\u1325\u200b\u1209\u200b\u12a0\u200b\u1278\u200b\u12cb\u120d\u1364 \u12ed\u1260\u200b\u1209\u200b\u12a0\u200b\u1278\u200b\u12cd\u200b\u121b\u120d\u1364 \u12a8\u12d4\u200b\u1233\u200b\u12cd\u121d \u1264\u1275 \u1245\u122c\u1273 \u12a0\u12ed\u200b\u1296\u200b\u122d\u121d\u1362"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u12e8\u12f0\u200b\u1261\u200b\u1265\u121d \u1230\u12ce\u127d \u12e8\u12d4\u200b\u1233\u200b\u12cd\u1295 \u1270\u122b\u122b\u1365 \u12e8\u1246\u200b\u120b\u200b\u12cd\u121d \u1230\u12ce\u127d \u134d\u120d\u200b\u1235\u200b\u1325\u200b\u12a4\u200b\u121b\u200b\u12cd\u200b\u12eb\u200b\u1295\u1295 \u12ed\u12c8\u200b\u122d\u200b\u1233\u1209\u1364 \u12e8\u12a4\u200b\u134d\u200b\u122c\u200b\u121d\u200b\u1295\u121d \u1270\u122b\u122b\u1365 \u12e8\u1230\u200b\u121b\u200b\u122d\u200b\u12eb\u1295\u1365 \u12e8\u1265\u200b\u1295\u200b\u12eb\u200b\u121d\u200b\u1295\u1293 \u12e8\u1308\u200b\u1208\u200b\u12d3\u200b\u12f5\u1295 \u1200\u1308\u122d \u12ed\u12c8\u200b\u122d\u200b\u1233\u1209\u1362"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u12ed\u1205\u121d \u12e8\u12a5\u200b\u1235\u200b\u122b\u200b\u12a4\u120d \u120d\u1306\u127d \u12e8\u132d\u200b\u134d\u200b\u122b\u200b\u1278\u12cd \u121d\u122d\u12ae \u12e8\u12a8\u200b\u1290\u200b\u12d3\u200b\u1295\u1295 \u1235\u134d\u122b \u1201\u1209 \u12a5\u1235\u12a8 \u1230\u122b\u200b\u1335\u1273 \u12f5\u1228\u1235 \u12ed\u12c8\u200b\u122d\u200b\u1233\u120d\u1364 \u12e8\u12a2\u200b\u12e8\u200b\u1229\u200b\u1233\u200b\u120c\u121d \u121d\u122d\u200b\u12ae\u200b\u129e\u127d \u12a5\u1235\u12a8 \u12a4\u134d\u200b\u122b\u1273 \u12f5\u1228\u1235 \u12e8\u12f0\u200b\u1261\u200b\u1265\u1295 \u12a8\u1270\u200b\u121e\u127d \u12ed\u12c8\u200b\u122d\u200b\u1233\u1209\u1362"
                },
