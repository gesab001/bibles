            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1243\u120d \u12c8\u12f0 \u12a0\u121b\u1274 \u120d\u1305 \u12c8\u12f0 \u12ee\u1293\u1235 \u12a5\u1295\u200b\u12f2\u1205 \u1232\u120d \u1218\u1323\u1366"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201c\u1270\u1290\u200b\u1225\u200b\u1270\u1205 \u12c8\u12f0 \u1273\u120b\u200b\u1242\u1271 \u12a8\u1270\u121b \u12c8\u12f0 \u1290\u1290\u12cc \u1202\u12f5\u1364 \u12e8\u12ad\u200b\u134b\u200b\u1273\u200b\u1278\u200b\u12cd\u121d \u1329\u12b8\u1275 \u12c8\u12f0 \u134a\u1274 \u12c8\u1325\u200b\u1276\u200b\u12a0\u200b\u120d\u1293 \u1208\u12a5\u200b\u1290\u200b\u122d\u1231 \u1235\u1260\u12ad\u1362\u201d"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12ee\u1293\u1235 \u130d\u1295 \u12a8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u134a\u1275 \u12c8\u12f0 \u1270\u122d\u200b\u1234\u1235 \u12ed\u1238\u123d \u12d8\u1295\u12f5 \u1270\u1290\u1223\u1364 \u12c8\u12f0 \u12a2\u12ee\u200b\u1334\u121d \u12c8\u1228\u12f0\u1364 \u12c8\u12f0 \u1270\u122d\u200b\u1234\u200b\u1235\u121d \u12e8\u121d\u200b\u1273\u200b\u120d\u134d \u1218\u122d\u200b\u12a8\u1265 \u12a0\u1308\u1298\u1364 \u12a8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u134a\u1275 \u12b0\u1265\u200b\u120d\u120e \u12a8\u12a5\u200b\u1290\u200b\u122d\u1231 \u130b\u122d \u12c8\u12f0 \u1270\u122d\u200b\u1234\u1235 \u12ed\u1204\u12f5 \u12d8\u1295\u12f5 \u12cb\u130b \u1230\u1325\u1276 \u12c8\u12f0 \u12a5\u122d\u200b\u1235\u12cb \u1308\u1263\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1260\u1263\u200b\u1215\u1229 \u120b\u12ed \u1273\u120b\u1245 \u1290\u134b\u200b\u1235\u1295 \u12a0\u1218\u1323\u1364 \u1260\u1263\u200b\u1215\u200b\u1229\u121d \u120b\u12ed \u1273\u120b\u1245 \u121b\u12d5\u200b\u1260\u120d \u1206\u1290\u1364 \u1218\u122d\u200b\u12a8\u200b\u1262\u200b\u1271\u121d \u120d\u1275\u200b\u1230\u200b\u1260\u122d \u1240\u1228\u200b\u1260\u127d\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1218\u122d\u200b\u12a8\u200b\u1260\u200b\u129e\u200b\u1279\u121d \u1348\u1229\u1364 \u12a5\u12eb\u200b\u1295\u200b\u12f3\u200b\u1295\u200b\u12f1\u121d \u12c8\u12f0 \u12a0\u121d\u200b\u120b\u12a9 \u132e\u12b8\u1364 \u1218\u122d\u200b\u12a8\u200b\u1262\u200b\u1271\u121d \u12a5\u1295\u200b\u12f5\u200b\u1275\u200b\u1240\u200b\u120d\u200b\u120d\u200b\u120b\u200b\u1278\u12cd \u1260\u12cd\u200b\u1235\u200b\u1325\u12cb \u12e8\u1290\u200b\u1260\u200b\u1228\u200b\u12cd\u1295 \u12d5\u1243 \u12c8\u12f0 \u1263\u1215\u122d \u1323\u1209\u1275\u1364 \u12ee\u1293\u1235 \u130d\u1295 \u12c8\u12f0 \u1218\u122d\u200b\u12a8\u1261 \u12cd\u1235\u200b\u1320\u200b\u129b\u12cd \u12ad\u134d\u120d \u12c8\u122d\u12f6 \u1290\u1260\u122d\u1365 \u1270\u129d\u200b\u1276\u121d \u12eb\u1295\u200b\u12b3\u200b\u122d\u134d \u1290\u1260\u122d\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12e8\u1218\u200b\u122d\u200b\u12a8\u200b\u1261\u121d \u12a0\u1208\u1243 \u12c8\u12f0 \u12a5\u122d\u1231 \u1240\u122d\u1266\u1365 \u201c\u121d\u1290\u12cd \u1270\u129d\u200b\u1270\u200b\u1203\u120d? \u12a5\u1295\u200b\u12f3\u200b\u1295\u200b\u1320\u134b \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u12eb\u12f5\u200b\u1290\u1295 \u12d8\u1295\u12f5 \u1270\u1290\u200b\u1225\u200b\u1270\u1205 \u12a0\u121d\u200b\u120b\u200b\u12ad\u200b\u1205\u1295 \u1325\u122b\u201d \u12a0\u1208\u12cd\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12a5\u122d\u1235 \u1260\u122d\u200b\u1233\u200b\u1278\u200b\u12cd\u121d\u1365 \u201c\u12ed\u1205 \u12ad\u1349 \u1290\u1308\u122d \u1260\u121b\u1295 \u121d\u12ad\u200b\u1295\u200b\u12eb\u1275 \u12a5\u1295\u12f0 \u12a0\u1308\u200b\u1298\u1295 \u12a5\u1293\u200b\u12cd\u1245 \u12d8\u1295\u12f5 \u1291\u1364 \u12d5\u1323 \u12a5\u1295\u200b\u1323\u200b\u1323\u120d\u201d \u1270\u1263\u200b\u1263\u1209\u1362 \u12d5\u1323\u121d \u1270\u1323\u200b\u1323\u1209\u1364 \u12d5\u1323\u200b\u12cd\u121d \u1260\u12ee\u200b\u1293\u1235 \u120b\u12ed \u12c8\u12f0\u1240\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u12e8\u12da\u200b\u12eb\u1295 \u130a\u12dc\u121d\u1365 \u201c\u12ed\u1205 \u12ad\u1349 \u1290\u1308\u122d \u1260\u121b\u1295 \u121d\u12ad\u200b\u1295\u200b\u12eb\u1275 \u12a5\u1295\u200b\u12f3\u200b\u1308\u200b\u1298\u1295 \u12a5\u1263\u200b\u12ad\u1205 \u1295\u1308\u200b\u1228\u1295\u1364 \u1225\u122b\u1205 \u121d\u1295\u200b\u12f5\u1295 \u1290\u12cd? \u12a8\u12c8\u200b\u12f4\u200b\u1275\u1235 \u1218\u1323\u1205? \u12c8\u12f4\u200b\u1275\u1235 \u1275\u1204\u200b\u12f3\u200b\u1208\u1205? \u1200\u1308\u200b\u122d\u1205 \u12e8\u1275 \u1290\u12cd? \u12c8\u1308\u200b\u1295\u200b\u1205\u1235 \u121d\u1295\u200b\u12f5\u1295 \u1290\u12cd?\u201d \u12a0\u1209\u1275\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a5\u122d\u200b\u1231\u121d\u1365 \u201c\u12a5\u1294 \u12d5\u1265\u200b\u122b\u12ca \u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1263\u122a\u12eb \u1290\u129d\u1364 \u1263\u1215\u200b\u1229\u200b\u1295\u1293 \u12e8\u1265\u200b\u1231\u1295 \u12e8\u1348\u200b\u1320\u200b\u1228\u200b\u12cd\u1295 \u12e8\u1230\u200b\u121b\u200b\u12ed\u1295 \u12a0\u121d\u200b\u120b\u12ad \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u1295 \u12a0\u1218\u200b\u120d\u200b\u12ab\u200b\u1208\u1201\u201d \u12a0\u120b\u200b\u1278\u12cd\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u1290\u200b\u12da\u200b\u12eb\u121d \u1230\u12ce\u127d \u12a8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u134a\u1275 \u12a5\u1295\u12f0 \u12b0\u1260\u200b\u1208\u1208 \u12a5\u122d\u1231 \u1235\u1208 \u1290\u1308\u200b\u122b\u200b\u1278\u12cd \u12d0\u12cd\u200b\u1240\u200b\u12cb\u200b\u120d\u1293 \u12a5\u1305\u130d \u1348\u122d\u200b\u1270\u12cd\u1365 \u201c\u12ed\u1205 \u12eb\u12f0\u200b\u1228\u200b\u130d\u200b\u12b8\u12cd \u121d\u1295\u200b\u12f5\u1295 \u1290\u12cd?\u201d \u12a0\u1209\u1275\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1263\u1215\u1229 \u12ed\u1273\u200b\u12c8\u12ad \u1290\u1260\u200b\u122d\u1293\u1365 \u1273\u120b\u1245 \u121b\u12d5\u200b\u1260\u200b\u120d\u121d \u1270\u1290\u200b\u1225\u1276 \u1290\u1260\u200b\u122d\u1293\u1365 \u201c\u1263\u1215\u1229 \u1338\u1325 \u12ed\u120d\u200b\u120d\u1295 \u12d8\u1295\u12f5 \u12a5\u1295\u200b\u130d\u200b\u12f2\u1205 \u121d\u1295 \u12a5\u1293\u200b\u12f5\u200b\u122d\u200b\u130d\u1205?\u201d \u12a0\u1209\u1275\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u12a5\u122d\u200b\u1231\u121d\u1365 \u201c\u12ed\u1205 \u1273\u120b\u1245 \u121b\u12d5\u200b\u1260\u120d \u1260\u12a5\u1294 \u121d\u12ad\u200b\u1295\u200b\u12eb\u1275 \u12a5\u1295\u12f0 \u12a0\u1308\u200b\u129b\u200b\u127d\u1201 \u12d0\u12cd\u200b\u1243\u200b\u1208\u200b\u1201\u1293 \u12a0\u1295\u200b\u1225\u200b\u1273\u200b\u127d\u1201 \u12c8\u12f0 \u1263\u1215\u1229 \u1323\u1209\u129d\u1364 \u1263\u1215\u200b\u1229\u121d \u133d\u1325 \u12ed\u120d\u200b\u120b\u200b\u127d\u200b\u128b\u120d\u201d \u12a0\u120b\u200b\u1278\u12cd\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1230\u12ce\u1279 \u130d\u1295 \u12c8\u12f0 \u121d\u12f5\u1229 \u120a\u1218\u200b\u1208\u1231 \u12a0\u1325\u200b\u1265\u200b\u1240\u12cd \u1240\u12d8\u1349\u1364 \u12f3\u1229 \u130d\u1295 \u1263\u1215\u1229 \u12a5\u1305\u130d \u12a0\u1265\u200b\u12dd\u1276 \u12ed\u1293\u200b\u12c8\u200b\u1325\u200b\u1263\u200b\u1278\u12cd \u1290\u1260\u200b\u122d\u1293 \u12a0\u120d\u200b\u127b\u200b\u1209\u121d\u1362"
