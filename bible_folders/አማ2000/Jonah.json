                "7": {
                    "verse_nr": "7",
                    "verse": "\u12a5\u1290\u1206 \u122b\u1234 \u12c8\u12f0 \u1273\u127d \u12c8\u12f0 \u1270\u122b\u200b\u122e\u127d \u1218\u1220\u200b\u1228\u1275 \u12c8\u1228\u12f0\u1364\n\u12a8\u1325\u200b\u1295\u1275 \u1300\u121d\u122e \u1218\u12c8\u200b\u122d\u200b\u12c8\u200b\u122a\u200b\u12eb\u200b\u12ce\u200b\u127d\u12cb \u12c8\u12f0 \u1270\u12d8\u1309 \u121d\u12f5\u122d \u12c8\u1228\u200b\u12f5\u1201\u1364\n\u12a0\u1264\u1271 \u1348\u1323\u200b\u122a\u12ec! \u1215\u12ed\u200b\u12c8\u1274 \u1325\u134b\u1275 \u1233\u12eb\u200b\u1308\u200b\u129b\u1275 \u12c8\u12f3\u200b\u1295\u1270 \u1275\u12cd\u1323\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1290\u134d\u1234 \u1260\u12db\u200b\u1208\u200b\u127d\u200b\u1265\u129d \u130a\u12dc \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u1295 \u12a0\u1230\u200b\u1265\u200b\u1201\u1275\u1364\n\u1338\u120e\u200b\u1274\u121d \u12c8\u12f0 \u12a0\u1295\u1270 \u12c8\u12f0 \u1245\u12f1\u1235 \u1218\u1245\u200b\u12f0\u200b\u1235\u1205 \u1275\u130d\u1263\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a8\u1295\u200b\u1271\u200b\u1290\u200b\u1275\u200b\u1295\u1293 \u1210\u1230\u200b\u1275\u1295 \u12e8\u121a\u200b\u1320\u200b\u1265\u1241\u1364\n\u12ed\u1245\u200b\u122d\u200b\u1273\u200b\u1278\u200b\u12cd\u1295 \u1275\u1270\u200b\u12cb\u120d\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u1294 \u130d\u1295 \u12a8\u121d\u200b\u1235\u200b\u130b\u200b\u1293\u1293 \u12a8\u1291\u200b\u12db\u12dc \u1243\u120d \u130b\u122d \u12a5\u1220\u200b\u12cb\u200b\u120d\u200b\u1203\u200b\u1208\u1201\u1364\n\u12e8\u12f5\u200b\u1285\u200b\u1290\u1274 \u12a0\u121d\u200b\u120b\u12ad \u1206\u12ed! \u12e8\u1270\u200b\u1233\u200b\u120d\u200b\u1201\u200b\u1275\u1295 \u1208\u12a0\u200b\u1295\u1270 \u12a5\u12a8\u200b\u134d\u200b\u120b\u200b\u1208\u1201\u1362\u201d"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12e8\u12a5\u200b\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1243\u120d \u1201\u1208\u200b\u1270\u129b \u12c8\u12f0 \u12ee\u1293\u1235 \u12a5\u1295\u200b\u12f2\u1205 \u1232\u120d \u1218\u1323\u1366"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201c\u1270\u1290\u200b\u1225\u200b\u1270\u1205 \u12c8\u12f0\u200b\u12da\u200b\u12eb\u127d \u12c8\u12f0 \u1273\u120b\u200b\u1242\u1271 \u12a8\u1270\u121b \u12c8\u12f0 \u1290\u1290\u12cc \u1202\u12f5\u1364 \u12e8\u1290\u200b\u1308\u200b\u122d\u200b\u1201\u200b\u1205\u200b\u1295\u121d \u12e8\u1218\u200b\u1300\u200b\u1218\u200b\u122a\u200b\u12eb\u200b\u12cd\u1295 \u1235\u1265\u200b\u12a8\u1275 \u1235\u1260\u200b\u12ad\u200b\u120b\u1275\u201d \u12a0\u1208\u12cd\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12ee\u1293\u200b\u1235\u121d \u1270\u1290\u200b\u1225\u1276 \u12a5\u1295\u12f0 \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1243\u120d \u12c8\u12f0 \u1290\u1290\u12cc \u1204\u12f0\u1364 \u1290\u1290\u200b\u12cc\u121d \u1273\u120b\u1245 \u12a8\u1270\u121b \u1290\u1260\u200b\u1228\u127d\u1364 \u12e8\u1245\u200b\u1325\u200b\u122d\u200b\u12cb\u121d \u12d9\u122a\u12eb \u1260\u12a5\u200b\u130d\u122d \u12e8\u1226\u200b\u1235\u1275 \u1240\u1295 \u1218\u1295\u200b\u1308\u12f5 \u12eb\u1205\u120d \u1290\u1260\u1228\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12ee\u1293\u200b\u1235\u121d \u12e8\u12a0\u200b\u1295\u12f5 \u1240\u1295 \u1218\u1295\u200b\u1308\u12f5 \u12eb\u1205\u120d \u12c8\u12f0 \u12a8\u1270\u200b\u121b\u200b\u12ea\u1271 \u12cd\u1235\u1325 \u120a\u1308\u1263 \u1300\u1218\u1228\u1364 \u132e\u12be\u121d\u1365 \u201c\u1260\u1226\u200b\u1235\u1275 \u1240\u1295 \u12cd\u1235\u1325 \u1290\u1290\u12cc \u1275\u1308\u200b\u1208\u200b\u1260\u200b\u1323\u200b\u1208\u127d \u12a0\u1208\u1362\u201d"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12e8\u1290\u200b\u1290\u12cc \u1230\u12ce\u200b\u127d\u121d \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u1295 \u12a0\u1218\u1291\u1364 \u1208\u133e\u121d \u12d0\u12cb\u1305 \u1290\u1308\u1229\u1364 \u12a8\u1273\u200b\u120b\u200b\u1241\u121d \u1300\u121d\u122e \u12a5\u1235\u12a8 \u1273\u1293\u1239 \u12f5\u1228\u1235 \u121b\u1245 \u1208\u1260\u1231\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12c8\u122c\u200b\u12cd\u121d \u12c8\u12f0 \u1290\u1290\u12cc \u1295\u1309\u1225 \u12f0\u1228\u1230\u1364 \u12a5\u122d\u200b\u1231\u121d \u12a8\u12d9\u200b\u134b\u1291 \u1270\u1290\u200b\u1225\u1276 \u1218\u1310\u200b\u1293\u200b\u1338\u200b\u134a\u200b\u12eb\u200b\u12cd\u1295 \u12a0\u12c8\u200b\u1208\u1240\u1364 \u121b\u1245\u121d \u1208\u1260\u1230\u1364 \u1260\u12a0\u200b\u1218\u200b\u12f5\u121d \u120b\u12ed \u1270\u1240\u200b\u1218\u1320\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12d0\u12cb\u200b\u1305\u121d \u12a0\u1235\u200b\u1290\u200b\u1308\u1228\u1364 \u1260\u1290\u200b\u1290\u200b\u12cc\u121d \u12cd\u1235\u1325 \u12e8\u1295\u200b\u1309\u200b\u1221\u200b\u1295\u1293 \u12e8\u1218\u200b\u12b3\u200b\u1295\u200b\u1295\u200b\u1271\u1295 \u1275\u12a5\u200b\u12db\u12dd \u12a0\u1233\u200b\u12c8\u1300\u1364 \u12a5\u1295\u200b\u12f2\u200b\u1205\u121d \u12a0\u1208\u1365 \u201c\u1230\u12ce\u200b\u127d\u1293 \u12a5\u1295\u200b\u1235\u200b\u1236\u127d\u1365 \u120b\u121e\u200b\u127d\u1293 \u1260\u130e\u127d \u12a0\u1295\u200b\u12f3\u127d \u12a0\u12ed\u200b\u1245\u200b\u1218\u1231\u1364 \u12a0\u12ed\u200b\u1230\u200b\u121b\u200b\u1229\u121d\u1364 \u12cd\u1283\u200b\u1295\u121d \u12a0\u12ed\u200b\u1320\u1321\u1364"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1230\u12ce\u200b\u127d\u1293 \u12a5\u1295\u200b\u1235\u200b\u1236\u200b\u127d\u121d \u121b\u1245 \u12ed\u120d\u200b\u1260\u1231\u1364 \u12c8\u12f0 \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1260\u12a0\u200b\u1295\u200b\u12f5\u200b\u1290\u1275 \u12ed\u1329\u12b9\u1364 \u1230\u12ce\u200b\u127d\u121d \u1201\u1209 \u12a8\u12ad\u1349 \u1218\u1295\u200b\u1308\u200b\u12f3\u200b\u1278\u200b\u12cd\u1293 \u1260\u12a5\u200b\u1303\u200b\u1278\u12cd \u12a8\u12a0\u200b\u1208\u12cd \u130d\u134d \u12ed\u1218\u200b\u1208\u1231\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a5\u129b \u12a5\u1295\u12f3\u1295\u1320\u134b \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12ed\u121d\u122b\u1295\u1365 \u12a8\u133d\u1291 \u1241\u1323\u12cd\u121d \u12ed\u1218\u1208\u1235 \u12a5\u1295\u12f0\u1206\u1290 \u121b\u1295 \u12eb\u12cd\u1243\u120d?\u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u12a8\u12ad\u1349 \u1218\u1295\u1308\u12f3\u1278\u12cd \u12a5\u1295\u12f0\u1270\u1218\u1208\u1231 \u1225\u122b\u1278\u12cd\u1295 \u12a0\u12e8\u1364 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u12eb\u12f0\u122d\u130d\u1263\u1278\u12cd \u12d8\u1295\u12f5 \u12a8\u1270\u1293\u1308\u1228\u12cd \u12ad\u1349 \u1290\u1308\u122d \u1270\u1218\u1208\u1230\u1364 \u12a0\u120b\u12f0\u1228\u1308\u12cd\u121d\u121d\u1362"
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12ee\u1293\u200b\u1235\u121d \u1348\u133d\u121e \u12a0\u12d8\u1290\u1364 \u1270\u12a8\u200b\u12d8\u121d\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12c8\u12f0 \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u1338\u1208\u12e8\u1364 \u12a5\u1295\u200b\u12f2\u200b\u1205\u121d \u12a0\u1208\u1365 \u201c\u12a0\u1264\u1271\u1365 \u1260\u1200\u200b\u1308\u122c \u1233\u1208\u1201 \u12e8\u1270\u200b\u1293\u200b\u1308\u200b\u122d\u200b\u1201\u1275 \u12ed\u1205 \u12a0\u120d\u200b\u1290\u200b\u1260\u200b\u1228\u200b\u121d\u1295? \u12a0\u1295\u1270 \u1278\u122d\u1293 \u12ed\u1245\u122d \u1263\u12ed\u1365 \u1273\u130b\u200b\u123d\u121d\u1365 \u121d\u1215\u200b\u1228\u200b\u1275\u200b\u1205\u121d \u12e8\u1260\u12db\u1365 \u12a8\u12ad\u200b\u1349\u12cd \u1290\u1308\u200b\u122d\u121d \u12e8\u121d\u200b\u1275\u200b\u1218\u200b\u1208\u1235 \u12a0\u121d\u200b\u120b\u12ad \u12a5\u1295\u12f0 \u1206\u1295\u1205 \u12d0\u12cd\u1244 \u1290\u1260\u200b\u122d\u1293 \u1235\u1208\u200b\u12da\u1205 \u12c8\u12f0 \u1270\u122d\u200b\u1234\u1235 \u1208\u1218\u200b\u12b0\u200b\u1265\u200b\u1208\u120d \u1348\u1325\u129c \u1290\u1260\u122d\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12a0\u1201\u200b\u1295\u121d \u12a0\u1264\u1271! \u12a8\u1215\u200b\u12ed\u200b\u12c8\u1275 \u121e\u1275 \u12ed\u123b\u200b\u1208\u200b\u129b\u200b\u120d\u1293 \u12a5\u1263\u200b\u12ad\u1205! \u1290\u134d\u200b\u1234\u1295 \u12a8\u12a5\u1294 \u12cd\u1230\u12f5\u1362\u201d"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u12ee\u1293\u200b\u1235\u1295\u1365 \u201c\u1348\u133d\u200b\u1218\u1205 \u1273\u12dd\u200b\u1293\u200b\u1208\u200b\u1205\u1295?\u201d \u12a0\u1208\u12cd\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12ee\u1293\u200b\u1235\u121d \u12a8\u12a8\u200b\u1270\u200b\u121b\u200b\u12ea\u1271 \u12c8\u1323\u1364 \u1260\u12a8\u200b\u1270\u200b\u121b\u200b\u12ea\u200b\u1271\u121d \u134a\u1275 \u1208\u134a\u1275 \u1270\u1240\u200b\u1218\u1320\u1364 \u1260\u12a8\u200b\u1270\u200b\u121b\u200b\u12ea\u200b\u1271\u121d \u12e8\u121a\u200b\u1206\u200b\u1290\u200b\u12cd\u1295 \u12a5\u1235\u200b\u12aa\u200b\u12eb\u12ed \u12f5\u1228\u1235 \u1260\u12da\u12eb \u1208\u122b\u1231 \u12f3\u1235 \u1220\u122d\u1276 \u12a8\u1325\u200b\u120b\u12cd \u1260\u1273\u127d \u1270\u1240\u200b\u1218\u1320\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u12a0\u121d\u200b\u120b\u200b\u12ad\u121d \u1245\u120d\u1295 \u12a0\u12d8\u12d8\u1364 \u12a8\u132d\u200b\u1295\u200b\u1240\u200b\u1271\u121d \u1273\u12f5\u200b\u1290\u12cd \u12d8\u1295\u12f5 \u1260\u122b\u1231 \u120b\u12ed \u1325\u120b \u12a5\u1295\u200b\u12f5\u200b\u1275\u200b\u1206\u1295 \u1260\u12ee\u200b\u1293\u1235 \u122b\u1235 \u120b\u12ed \u12a8\u134d \u12a8\u134d \u12a0\u12f0\u200b\u1228\u200b\u130b\u1275\u1364 \u12ee\u1293\u200b\u1235\u121d \u1235\u1208 \u1245\u120a\u1271 \u12a5\u1305\u130d \u12f0\u1235 \u12a0\u1208\u12cd\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u1260\u1290\u200b\u130b\u12cd \u130d\u1295 \u12c8\u1308\u130d \u1263\u1208 \u130a\u12dc \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1275\u120d\u1295 \u12a0\u12d8\u12d8\u1364 \u12a5\u122d\u200b\u1235\u200b\u12cb\u121d \u1245\u120a\u200b\u1271\u1295 \u1218\u1273\u200b\u127b\u1275\u1364 \u12f0\u1228\u200b\u1240\u200b\u127d\u121d\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1340\u1210\u200b\u12ed\u121d \u1260\u12c8\u200b\u1323\u127d \u130a\u12dc \u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u122d \u1275\u12a9\u1235 \u12e8\u121d\u200b\u1225\u200b\u122b\u1245 \u1290\u134b\u200b\u1235\u1295 \u12a0\u12d8\u12d8\u1364 \u12ee\u1293\u200b\u1235\u200b\u1295\u121d \u12a5\u1235\u200b\u12aa\u200b\u12dd\u120d \u12f5\u1228\u1235 \u1340\u1210\u12ed \u122b\u1231\u1295 \u1218\u1273\u12cd\u1364 \u1208\u122b\u200b\u1231\u121d \u1270\u1235\u134b \u1248\u122d\u1326\u1365 \u201c\u12a8\u1215\u200b\u12ed\u200b\u12c8\u1275 \u121e\u1275 \u12ed\u123b\u200b\u1208\u200b\u129b\u120d\u201d \u12a0\u1208\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a5\u130d\u200b\u12da\u200b\u12a0\u200b\u1265\u200b\u1214\u200b\u122d\u121d \u12ee\u1293\u200b\u1235\u1295\u1365 \u201c\u1260\u12cd\u1291 \u1235\u1208\u200b\u12da\u127d \u1245\u120d \u1273\u12dd\u200b\u1293\u200b\u1208\u200b\u1205\u1295?\u201d \u12a0\u1208\u12cd\u1362 \u12a5\u122d\u200b\u1231\u121d\u1365 \u201c\u12a5\u1235\u200b\u12ad\u200b\u121e\u1275 \u12f5\u1228\u1235 \u12a5\u1305\u130d \u12a0\u12dd\u200b\u129b\u200b\u1208\u1201\u201d \u12a0\u1208\u1362"
                },
