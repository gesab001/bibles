                "15": {
                    "verse_nr": "15",
                    "verse": "\u12ee\u1293\u200b\u1235\u200b\u1295\u121d \u12c8\u1235\u200b\u12f0\u12cd \u12c8\u12f0 \u1263\u1215\u1229 \u1323\u1209\u1275\u1364 \u1263\u1215\u200b\u1229\u121d \u12a8\u1218\u200b\u1293\u200b\u12c8\u1321 \u1338\u1325 \u12a0\u1208\u1362"
