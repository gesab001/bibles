            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u12a5\u1295\u12f2\u1205\u121d \u1206\u1290\u1365 \u1218\u1233\u134d\u1295\u1275 \u12ed\u1348\u122d\u12f1 \u1260\u1290\u1260\u1228 \u130a\u12dc \u1260\u12a0\u1308\u1229 \u120b\u12ed \u122b\u1265 \u1206\u1290\u1362 \u12a0\u1295\u12f5 \u1230\u12cd\u121d \u12a8\u121a\u1235\u1271\u1293 \u12a8\u1201\u1208\u1271 \u120d\u1306\u1279 \u130b\u122d \u1260\u121e\u12d3\u1265 \u121d\u12f5\u122d \u120a\u1240\u1218\u1325 \u12a8\u1264\u1270 \u120d\u1214\u121d \u12ed\u1201\u12f3 \u1270\u1290\u1225\u1276 \u1204\u12f0\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12e8\u1230\u12cd\u12ee\u12cd\u121d \u1235\u121d \u12a0\u1264\u121c\u120c\u12ad\u1365 \u12e8\u121a\u1235\u1271\u121d \u1235\u121d \u1291\u1283\u121a\u1295\u1365 \u12e8\u1201\u1208\u1271\u121d \u120d\u1306\u127d \u1235\u121d \u1218\u1210\u120e\u1295\u1293 \u12ac\u120c\u12ce\u1295 \u1290\u1260\u1228\u1365 \u12e8\u1264\u1270 \u120d\u1214\u121d \u12ed\u1201\u12f3\u121d \u12e8\u12a4\u134d\u122b\u1273 \u1230\u12ce\u127d \u1290\u1260\u1229\u1362 \u12c8\u12f0 \u121e\u12d3\u1265\u121d \u121d\u12f5\u122d \u1218\u1321 \u1260\u12da\u12eb\u121d \u1270\u1240\u1218\u1321\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u12e8\u1291\u1283\u121a\u1295\u121d \u1263\u120d \u12a0\u1264\u121c\u120c\u12ad \u121e\u1270\u1363 \u12a5\u122d\u1235\u12cb\u1293 \u1201\u1208\u1271 \u120d\u1306\u127d\u12cb \u1240\u1229\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u1290\u122d\u1231\u121d \u12a8\u121e\u12d3\u1263\u12cd\u12eb\u1295 \u1234\u1276\u127d \u121a\u1235\u1275 \u12a0\u1308\u1261\u1363 \u12e8\u12a0\u1295\u12f2\u1271 \u1235\u121d \u12d6\u122d\u134b \u12e8\u1201\u1208\u1270\u129b\u12ed\u1271\u121d \u1235\u121d \u1229\u1275 \u1290\u1260\u1228\u1362 \u1260\u12da\u12eb\u121d \u12a0\u1225\u122d \u12d3\u1218\u1275 \u12eb\u1205\u120d \u1270\u1240\u1218\u1321\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1218\u1210\u120e\u1295\u1293 \u12ac\u120c\u12ce\u1295\u121d \u1201\u1208\u1271 \u121e\u1271\u1363 \u1234\u1272\u1271\u121d \u12a8\u1201\u1208\u1271 \u120d\u1306\u127d\u12cb\u1293 \u12a8\u1263\u120d\u12cb \u1270\u1208\u12ed\u1273 \u1240\u1228\u127d\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12a5\u122d\u1235\u12cb\u121d \u1260\u121e\u12d3\u1265 \u121d\u12f5\u122d \u1233\u1208\u127d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1215\u12dd\u1261\u1295 \u12a5\u1295\u12f0 \u1310\u1260\u1298 \u12a5\u1295\u1300\u122b\u121d \u12a5\u1295\u12f0 \u1230\u1323\u1278\u12cd \u1235\u1208 \u1230\u121b\u127d\u1365 \u12a8\u121e\u12d3\u1265 \u121d\u12f5\u122d \u120d\u1275\u1218\u1208\u1235 \u12a8\u1201\u1208\u1271 \u121d\u122b\u1276\u127d\u12cb \u130b\u122d \u1270\u1290\u1223\u127d\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12a5\u122d\u1235\u12cb\u121d \u12a8\u1201\u1208\u1271 \u121d\u122b\u1276\u127d\u12cb \u130b\u122d \u12a8\u1270\u1240\u1218\u1320\u127d\u1260\u1275 \u1235\u134d\u122b \u12c8\u1323\u127d\u1363 \u12c8\u12f0 \u12ed\u1201\u12f3\u121d \u121d\u12f5\u122d \u120a\u1218\u1208\u1231 \u1260\u1218\u1295\u1308\u12f5 \u1204\u12f1\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1291\u1283\u121a\u1295\u121d \u121d\u122b\u1276\u127d\u12cb\u1295\u1366 \u1202\u12f1\u1365 \u12c8\u12f0 \u12a5\u1293\u1276\u127b\u127d\u1201\u121d \u1264\u1275 \u1270\u1218\u1208\u1231\u1363 \u1260\u12a5\u1294\u1293 \u1260\u121e\u1271\u1275 \u12a5\u1295\u12f3\u12f0\u1228\u130b\u127d\u1201\u1365 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1278\u122d\u1290\u1275 \u12eb\u12f5\u122d\u130d\u120b\u127d\u1201\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1260\u12e8\u1263\u120b\u127d\u1201 \u1264\u1275 \u12d5\u1228\u134d\u1275 \u12ed\u1235\u1323\u127d\u1201 \u12a0\u1208\u127b\u1278\u12cd\u1362 \u1233\u1218\u127b\u1278\u12cd\u121d\u1363 \u12f5\u121d\u1343\u1278\u12cd\u1295\u121d \u12a8\u134d \u12a0\u12f5\u122d\u1308\u12cd \u12a0\u1208\u1240\u1231\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a5\u1290\u122d\u1231\u121d\u1366 \u12a8\u12a0\u1295\u127a \u130b\u122d \u12c8\u12f0 \u1215\u12dd\u1265\u123d \u12a5\u1295\u1218\u1208\u1233\u1208\u1295 \u12a0\u1209\u12a0\u1275\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u1291\u1283\u121a\u1295\u121d \u12a0\u1208\u127d\u1366 \u120d\u1306\u127c \u1206\u12ed\u1365 \u1270\u1218\u1208\u1231\u1363 \u1208\u121d\u1295 \u12a8\u12a5\u1294 \u130b\u122d \u1275\u1204\u12f3\u120b\u127d\u1201? \u1263\u120e\u127b\u127d\u1201 \u12e8\u121a\u1206\u1291 \u120d\u1306\u127d \u1260\u1206\u12f4 \u12a0\u1209\u129d\u1295?"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u120d\u1306\u127c \u1206\u12ed\u1365 \u1270\u1218\u1208\u1231\u1363 \u1263\u120d \u1208\u121b\u130d\u1263\u1275 \u12a0\u122d\u1305\u127b\u1208\u1201\u1293 \u1202\u12f1\u1363 \u1270\u1235\u134b \u12a0\u1208\u129d \u1265\u120d\u1365 \u12db\u122c \u120c\u120a\u1275\u1235 \u12a5\u1295\u12b3 \u1263\u120d \u1263\u1308\u1263\u1365 \u12c8\u1295\u12f6\u127d \u120d\u1306\u127d\u121d \u1265\u12c8\u120d\u12f5\u1365"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u12a5\u1290\u122d\u1231 \u12a5\u1235\u12aa\u12eb\u12f5\u1309 \u12f5\u1228\u1235 \u1275\u1246\u12eb\u120b\u127d\u1201\u1295? \u1235\u1208 \u12a5\u1290\u122d\u1231\u1235 \u1263\u120d \u121b\u130d\u1263\u1275 \u1275\u1270\u12cb\u120b\u127d\u1201\u1295? \u120d\u1306\u127c \u1206\u12ed\u1365 \u12a5\u1295\u12f2\u1205 \u12a0\u12ed\u12f0\u1208\u121d\u1363 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a5\u1305 \u1260\u12a5\u1294 \u12c8\u1325\u1276\u12a0\u120d\u1293 \u12a8\u12a5\u1293\u1295\u1270 \u12e8\u1270\u1290\u1223 \u12a5\u1305\u130d \u1270\u1218\u122d\u122c\u12a0\u1208\u1201\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u12f5\u121d\u1343\u1278\u12cd\u1295\u121d \u12a8\u134d \u12a0\u12f5\u122d\u1308\u12cd \u12a5\u1295\u12f0 \u1308\u1293 \u12a0\u1208\u1240\u1231\u1363 \u12d6\u122d\u134b\u121d \u12a0\u121b\u1275\u12cb\u1295 \u1233\u1218\u127d\u1363 \u1229\u1275 \u130d\u1295 \u1270\u1320\u130b\u127b\u1275\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u1291\u1283\u121a\u1295\u121d\u1366 \u12a5\u1290\u1206\u1365 \u1263\u120d\u1295\u1300\u122b\u123d \u12c8\u12f0 \u1215\u12dd\u1265\u12cb\u1293 \u12c8\u12f0 \u12a0\u121b\u120d\u12ad\u1275\u12cb \u1270\u1218\u120d\u1233\u1208\u127d\u1363 \u12a0\u1295\u127a\u121d \u12f0\u130d\u121e \u12a8\u1263\u120d\u1295\u1300\u122b\u123d \u130b\u122d \u1270\u1218\u1208\u123d \u12a0\u1208\u127b\u1275\u1362"
