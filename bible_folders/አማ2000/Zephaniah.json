        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u12ed\u1201\u12f3 \u1295\u1309\u1225 \u1260\u12a0\u121e\u133d \u120d\u1305 \u1260\u12a2\u12ee\u1235\u12eb\u1235 \u12d8\u1218\u1295 \u12c8\u12f0 \u1215\u12dd\u1245\u12eb\u1235 \u120d\u1305 \u12c8\u12f0 \u12a0\u121b\u122d\u12eb \u120d\u1305 \u12c8\u12f0 \u130e\u12f6\u120d\u12eb\u1235 \u120d\u1305 \u12c8\u12f0 \u12b5\u1232 \u120d\u1305 \u12c8\u12f0 \u1236\u134e\u1295\u12eb\u1235 \u12e8\u1218\u1323 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1243\u120d \u12ed\u1205 \u1290\u12cd\u1362"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1290\u1308\u122d\u1295 \u1201\u1209 \u12a8\u121d\u12f5\u122d \u134a\u1275 \u1348\u133d\u121c \u12a0\u1320\u134b\u1208\u1201\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1230\u12cd\u1295\u1293 \u12a5\u1295\u1235\u1233\u1295 \u12a0\u1320\u134b\u1208\u1201\u1363 \u12e8\u1230\u121b\u12ed\u1295 \u12c8\u134e\u127d\u1293 \u12e8\u1263\u1215\u122d\u1295 \u12d3\u1223\u12ce\u127d \u121b\u1230\u1293\u12a8\u12eb\u1295\u121d \u12a8\u1283\u1322\u12a0\u1270\u129e\u127d \u130b\u122d \u12a0\u1320\u134b\u1208\u1201\u1363 \u1230\u12cd\u1295\u121d \u12a8\u121d\u12f5\u122d \u134a\u1275 \u12a5\u1248\u122d\u1323\u1208\u1201\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u1304\u1295\u121d \u1260\u12ed\u1201\u12f3 \u120b\u12ed \u1260\u12a2\u12e8\u1229\u1233\u120c\u121d\u121d \u1260\u121a\u1296\u1229\u1275 \u1201\u1209 \u120b\u12ed \u12a5\u12d8\u1228\u130b\u1208\u1201\u1363 \u12a8\u12da\u1205\u121d \u1235\u134d\u122b \u12e8\u1260\u12a3\u120d\u1295 \u1245\u122c\u1273\u1293 \u12e8\u1323\u12d6\u1273\u1271\u1295 \u12ab\u1205\u1293\u1275 \u1235\u121d \u12a0\u1320\u134b\u1208\u1201\u1363"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u1260\u1230\u1308\u1290\u1275\u121d \u120b\u12ed \u1208\u1230\u121b\u12ed \u1220\u122b\u12ca\u1275 \u12e8\u121a\u1230\u130d\u12f1\u1275\u1295\u1365 \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1293 \u1260\u1295\u1309\u1223\u1278\u12cd \u1260\u121a\u120d\u12ae\u121d \u121d\u1208\u12cd \u12e8\u121a\u1230\u130d\u12f1\u1275\u1295\u1365"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295\u121d \u12a8\u1218\u12a8\u1270\u120d \u12e8\u1270\u1218\u1208\u1231\u1275\u1295\u1365 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295\u121d \u12eb\u120d\u1348\u1208\u1309\u1275\u1295\u1293 \u12eb\u120d\u1320\u12e8\u1241\u1275\u1295 \u12a0\u1320\u134b\u1208\u1201\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1240\u1295 \u1240\u122d\u1266\u12a0\u120d\u1293\u1365 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1218\u1225\u12cb\u12d5\u1275\u1295 \u12a0\u12d8\u130b\u1305\u1276\u12a0\u120d\u1293\u1365 \u12e8\u1320\u122b\u1278\u12cd\u1295\u121d \u1240\u12f5\u1236\u12a0\u120d\u1293 \u1260\u130c\u1273 \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u134a\u1275 \u12dd\u121d \u1260\u1209\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u1218\u1225\u12cb\u12d5\u1275 \u1240\u1295 \u12a0\u1208\u1246\u127d\u1295\u1293 \u12e8\u1295\u1309\u1225\u1295 \u120d\u1306\u127d \u12a5\u1295\u130d\u12f3 \u120d\u1265\u1235 \u12e8\u121a\u1208\u1265\u1231\u1275\u1295\u121d \u1201\u1209 \u12a5\u1240\u1323\u1208\u1201\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1260\u12da\u12eb\u121d \u1240\u1295 \u1260\u1218\u12f5\u1228\u12a9 \u120b\u12ed \u12e8\u121a\u12d8\u120d\u1209\u1275\u1295\u1365 \u12e8\u130c\u1273\u1278\u12cd\u1295 \u1264\u1275 \u12d3\u1218\u1343\u1295\u1293 \u123d\u1295\u1308\u120b\u1295 \u12e8\u121a\u121e\u1209\u1275\u1295 \u12a5\u1240\u1323\u1208\u1201\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u1260\u12da\u12eb \u1240\u1295\u1365 \u12ed\u120b\u120d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1365 \u12a8\u12d3\u1223\u12cd \u1260\u122d \u12e8\u1329\u12b8\u1275 \u12f5\u121d\u1345\u1365 \u12a8\u12a8\u1270\u121b\u12cd\u121d \u1260\u1201\u1208\u1270\u129b\u12cd \u12ad\u134d\u120d \u12cd\u12ab\u1273\u1365 \u12a8\u12ae\u1228\u1265\u1276\u1279\u121d \u1273\u120b\u1245 \u123d\u1265\u122d \u12ed\u1206\u1293\u120d\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u12a5\u1293\u1295\u1270 \u1260\u1218\u12ad\u1274\u123d \u12e8\u121d\u1275\u1296\u1229 \u1206\u12ed\u1365 \u12e8\u12a8\u1290\u12d3\u1295 \u1215\u12dd\u1265 \u1201\u1209 \u1320\u134d\u1270\u12cb\u120d\u1293\u1365 \u1265\u122d\u121d \u12e8\u1270\u1238\u12a8\u1219 \u1201\u1209 \u1270\u1248\u122d\u1320\u12cb\u120d\u1293 \u12a0\u120d\u1245\u1231\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u1260\u12da\u12eb\u121d \u12d8\u1218\u1295 \u12a2\u12e8\u1229\u1233\u120c\u121d\u1295 \u1260\u1218\u1265\u122b\u1275 \u12a5\u1218\u1228\u121d\u122b\u1208\u1201\u1363 \u1260\u12a0\u1270\u120b\u1278\u12cd\u121d \u120b\u12ed \u12e8\u121a\u1240\u1218\u1321\u1275\u1295\u1365 \u1260\u120d\u1263\u1278\u12cd\u121d\u1366 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1218\u120d\u12ab\u121d\u1295 \u12a0\u12eb\u12f0\u122d\u130d\u121d\u1365 \u12ad\u1349\u121d \u12a0\u12eb\u12f0\u122d\u130d\u121d \u12e8\u121a\u1209\u1275\u1295 \u1230\u12ce\u127d \u12a5\u1240\u1323\u1208\u1201\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u1265\u120d\u1325\u130d\u1293\u1278\u12cd\u121d \u1208\u121d\u122d\u12ae \u12ed\u1206\u1293\u120d\u1365 \u1264\u1276\u127b\u1278\u12cd\u121d \u12ed\u1348\u122d\u1233\u1209\u1363 \u1264\u1276\u127d\u1295\u121d \u12ed\u1220\u122b\u1209\u1365 \u1290\u1308\u122d \u130d\u1295 \u12a0\u12ed\u1240\u1218\u1321\u1263\u1278\u12cd\u121d\u1363 \u12c8\u12ed\u1295\u1295\u121d \u12ed\u1270\u12ad\u120b\u1209\u1365 \u12e8\u12c8\u12ed\u1295 \u1320\u1301\u1295 \u130d\u1295 \u12a0\u12ed\u1320\u1321\u121d\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u1273\u120b\u1241 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1240\u1295 \u1240\u122d\u1266\u12a0\u120d\u1363 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1240\u1295 \u12f5\u121d\u1345 \u1240\u122d\u1266\u12a0\u120d \u12a5\u1305\u130d\u121d \u1348\u1325\u1296\u12a0\u120d\u1363 \u1283\u12eb\u1209\u121d \u1260\u12da\u12eb \u1260\u1218\u122b\u122b \u120d\u1245\u1236 \u12ed\u132e\u12bb\u120d\u1362"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u12eb \u1240\u1295 \u12e8\u1218\u12d3\u1275 \u1240\u1295 \u12e8\u1218\u12a8\u122b\u1293 \u12e8\u132d\u1295\u1240\u1275 \u1240\u1295\u1365 \u12e8\u1218\u134d\u1228\u1235\u1293 \u12e8\u1218\u1325\u134b\u1275 \u1240\u1295\u1365 \u12e8\u1328\u1208\u121b\u1293 \u12e8\u132d\u130b\u130d \u1240\u1295\u1365 \u12e8\u12f0\u1218\u1293\u1293 \u12e8\u12f5\u1245\u12f5\u1245 \u1328\u1208\u121b \u1240\u1295\u1365"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u1260\u1270\u1218\u1238\u1309 \u12a8\u1270\u121e\u127d\u1293 \u1260\u1228\u12d8\u1219 \u130d\u1295\u1266\u127d \u120b\u12ed \u12e8\u1218\u1208\u12a8\u1275\u1293 \u12e8\u1230\u120d\u134d \u1329\u12b8\u1275 \u1240\u1295 \u1290\u12cd\u1362"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u120b\u12ed \u1283\u1322\u12a0\u1275 \u1220\u122d\u1270\u12cb\u120d\u1293 \u12a5\u1295\u12f0 \u12d5\u12cd\u122d \u12a5\u1235\u12aa\u1204\u12f1 \u12f5\u1228\u1235 \u1230\u12ce\u127d\u1295 \u12a0\u1235\u1328\u1295\u1243\u1208\u1201\u1363 \u12f0\u121b\u1278\u12cd\u121d \u12a5\u1295\u12f0 \u1275\u1262\u12eb\u1365 \u1225\u130b\u1278\u12cd\u121d \u12a5\u1295\u12f0 \u1315\u12f5\u134d \u12ed\u1348\u1235\u1233\u120d\u1362"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u121d \u124d\u1323 \u1240\u1295 \u1265\u122b\u1278\u12cd\u1293 \u12c8\u122d\u1243\u1278\u12cd \u120a\u12eb\u12f5\u1293\u1278\u12cd \u12a0\u12ed\u127d\u120d\u121d\u1363 \u12a5\u122d\u1231\u121d \u1260\u121d\u12f5\u122d \u12e8\u121a\u1296\u1229\u1275\u1295 \u1201\u1209 \u1348\u1325\u1296 \u12ed\u1328\u122d\u1233\u1278\u12cb\u120d\u1293 \u121d\u12f5\u122d \u1201\u1209 \u1260\u1245\u1295\u12d3\u1271 \u12a5\u1233\u1275 \u1275\u1260\u120b\u1208\u127d\u1362"
                }
