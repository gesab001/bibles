                "20": {
                    "verse_nr": "20",
                    "verse": "\u0632\u0631\u0648\u0628\u0627\u0628\u0644 \u0626\u06d5\u0646\u06af\u0648\u0633\u062a\u06cc\u0644\u06d5\u06cc \u06cc\u06d5\u0632\u062f\u0627\u0646\u06d5 - \u062f\u0648\u0648\u0628\u0627\u0631\u06d5 \u0641\u06d5\u0631\u0645\u0627\u06cc\u0634\u062a\u06cc \u06cc\u06d5\u0632\u062f\u0627\u0646 \u0628\u06c6 \u062d\u06d5\u06af\u06d5\u06cc \u0647\u0627\u062a\u06d5\u0648\u06d5\u060c \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u0628\u06cc\u0633\u062a \u0648 \u0686\u0648\u0627\u0631\u06cc \u0645\u0627\u0646\u06af\u06cc \u0646\u06c6\u060c \u0641\u06d5\u0631\u0645\u0648\u0648\u06cc:"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u00ab\u0642\u0633\u06d5 \u0644\u06d5\u06af\u06d5\u06b5 \u0632\u0631\u0648\u0628\u0627\u0628\u0644\u06cc \u0641\u06d5\u0631\u0645\u0627\u0646\u0695\u06d5\u0648\u0627\u06cc \u06cc\u06d5\u0647\u0648\u062f\u0627 \u0628\u06a9\u06d5 \u0648 \u0628\u06b5\u06ce: \u201d\u0645\u0646 \u0626\u0627\u0633\u0645\u0627\u0646 \u0648 \u0632\u06d5\u0648\u06cc \u062f\u06d5\u0647\u06d5\u0698\u06ce\u0646\u0645."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u062a\u06d5\u062e\u062a\u06cc \u067e\u0627\u0634\u0627\u06cc\u06d5\u062a\u06cc\u06cc\u06d5\u06a9\u0627\u0646 \u0648\u06d5\u0631\u062f\u06d5\u06af\u06ce\u0695\u0645 \u0648 \u0647\u06ce\u0632\u06cc \u067e\u0627\u0634\u0627\u06cc\u06d5\u062a\u06cc \u0646\u06d5\u062a\u06d5\u0648\u06d5\u06a9\u0627\u0646 \u0644\u06d5\u0646\u0627\u0648\u062f\u06d5\u0628\u06d5\u0645. \u06af\u0627\u0644\u06cc\u0633\u06a9\u06d5 \u0648 \u0633\u0648\u0627\u0631\u06d5\u06a9\u06d5\u06cc \u0648\u06d5\u0631\u062f\u06d5\u06af\u06ce\u0695\u0645\u060c \u0626\u06d5\u0633\u067e \u0648 \u0633\u0648\u0627\u0631\u06d5\u06a9\u0627\u0646\u06cc\u0627\u0646 \u062f\u06d5\u06a9\u0648\u0698\u0631\u06ce\u0646\u060c \u0647\u06d5\u0631\u06cc\u06d5\u06a9\u06d5 \u0628\u06d5 \u0634\u0645\u0634\u06ce\u0631\u06cc \u0628\u0631\u0627\u06a9\u06d5\u06cc.\u201c"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u00ab\u06cc\u06d5\u0632\u062f\u0627\u0646\u06cc \u0633\u0648\u067e\u0627\u0633\u0627\u0644\u0627\u0631 \u062f\u06d5\u0641\u06d5\u0631\u0645\u0648\u06ce\u062a: \u201d\u0626\u06d5\u06cc \u0632\u0631\u0648\u0628\u0627\u0628\u0644\u06cc \u06a9\u0648\u0695\u06cc \u0634\u06d5\u0626\u06d5\u0644\u062a\u06cc\u06ce\u0644\u06cc \u0628\u06d5\u0646\u062f\u06d5\u06cc \u0645\u0646\u060c \u0644\u06d5\u0648 \u0695\u06c6\u0698\u06d5\u062f\u0627 \u062f\u06d5\u062a\u0628\u06d5\u0645 \u0648 \u0648\u06d5\u06a9 \u0626\u06d5\u0646\u06af\u0648\u0633\u062a\u06cc\u0644\u06d5\u062a \u0644\u06ce \u062f\u06d5\u06a9\u06d5\u0645 \u0628\u06c6 \u062e\u06c6\u0645\u060c \u0686\u0648\u0646\u06a9\u06d5 \u062a\u06c6\u0645 \u0647\u06d5\u06b5\u0628\u0698\u0627\u0631\u062f.\u201c \u0626\u06d5\u0648\u06d5 \u0641\u06d5\u0631\u0645\u0627\u06cc\u0634\u062a\u06cc \u06cc\u06d5\u0632\u062f\u0627\u0646\u06cc \u0633\u0648\u067e\u0627\u0633\u0627\u0644\u0627\u0631\u06d5.\u00bb"
                }
            }
        }
    }
}