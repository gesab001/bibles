    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0628\u06cc\u0646\u06cc\u0646\u06d5\u06a9\u06d5\u06cc \u0639\u06c6\u0628\u06d5\u062f\u06cc\u0627 - \u0628\u06cc\u0646\u06cc\u0646\u06d5\u06a9\u06d5\u06cc \u0639\u06c6\u0628\u06d5\u062f\u06cc\u0627. \u06cc\u06d5\u0632\u062f\u0627\u0646\u06cc \u0628\u0627\u06b5\u0627\u062f\u06d5\u0633\u062a \u0633\u06d5\u0628\u0627\u0631\u06d5\u062a \u0628\u06d5 \u0626\u06d5\u062f\u06c6\u0645 \u0626\u06d5\u0645\u06d5 \u062f\u06d5\u0641\u06d5\u0631\u0645\u0648\u06ce\u062a\u060c \u0647\u06d5\u0648\u0627\u06b5\u06ce\u06a9\u0645\u0627\u0646 \u0644\u06d5\u0644\u0627\u06cc\u06d5\u0646 \u06cc\u06d5\u0632\u062f\u0627\u0646\u06d5\u0648\u06d5 \u0628\u06cc\u0633\u062a\u060c \u0646\u06ce\u0631\u062f\u0631\u0627\u0648\u06ce\u06a9 \u0628\u06c6 \u0646\u06d5\u062a\u06d5\u0648\u06d5\u06a9\u0627\u0646 \u0646\u06ce\u0631\u062f\u0631\u0627\u0648\u06d5 \u062a\u0627\u06a9\u0648 \u067e\u06ce\u06cc\u0627\u0646 \u0628\u06b5\u06ce\u062a: \u00ab\u0647\u06d5\u0633\u062a\u0646! \u0647\u06d5\u0633\u062a\u0646 \u0628\u06c6 \u062c\u06d5\u0646\u06af.\u00bb"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u00ab\u0626\u06ce\u0633\u062a\u0627 \u0644\u06d5\u0646\u06ce\u0648 \u06af\u06d5\u0644\u0627\u0646\u062f\u0627 \u0628\u0686\u0648\u0648\u06a9\u062a \u062f\u06d5\u06a9\u06d5\u0645\u06d5\u0648\u06d5\u060c \u062a\u06c6 \u0632\u06c6\u0631 \u0695\u06cc\u0633\u0648\u0627 \u062f\u06d5\u0628\u06cc\u062a."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0641\u06cc\u0632\u06cc \u062f\u06b5\u062a \u0647\u06d5\u06b5\u06cc\u062e\u06d5\u06b5\u06d5\u062a\u0627\u0646\u062f\u06cc\u060c \u0626\u06d5\u06cc \u0646\u06cc\u0634\u062a\u06d5\u062c\u06ce\u06cc \u0646\u06ce\u0648 \u06a9\u06d5\u0644\u06ce\u0646\u06cc \u062a\u0627\u0634\u06d5\u0628\u06d5\u0631\u062f\u060c \u0626\u06d5\u0648\u06d5\u06cc \u0646\u0634\u06cc\u0646\u06af\u06d5\u06a9\u06d5\u062a \u0644\u06d5 \u0628\u06d5\u0631\u0632\u0627\u06cc\u06cc\u062f\u0627\u06cc\u06d5\u060c \u0626\u06d5\u0648\u06d5\u06cc \u0644\u06d5 \u062f\u06b5\u06cc \u062e\u06c6\u062a\u062f\u0627 \u062f\u06d5\u06b5\u06ce\u06cc\u062a: \u201d\u06a9\u06ce \u062f\u0627\u0645\u062f\u06d5\u06af\u0631\u06ce\u062a\u06d5 \u0633\u06d5\u0631 \u0632\u06d5\u0648\u06cc\u061f\u201c"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0626\u06d5\u06af\u06d5\u0631 \u0648\u06d5\u06a9 \u0647\u06d5\u06b5\u06c6 \u0628\u06d5\u0631\u0632 \u0628\u06cc\u062a\u06d5\u0648\u06d5 \u0648 \u0626\u06d5\u06af\u06d5\u0631 \u0647\u06ce\u0644\u0627\u0646\u06d5\u062a \u0644\u06d5\u0646\u0627\u0648 \u0626\u06d5\u0633\u062a\u06ce\u0631\u06d5\u06a9\u0627\u0646 \u062f\u0627\u0628\u0646\u06ce\u06cc\u062a\u060c \u0644\u06d5\u0648\u06ce\u0648\u06d5 \u062f\u06d5\u062a\u0647\u06ce\u0646\u0645\u06d5 \u062e\u0648\u0627\u0631\u06d5\u0648\u06d5.\u00bb \u0626\u06d5\u0648\u06d5 \u0641\u06d5\u0631\u0645\u0627\u06cc\u0634\u062a\u06cc \u06cc\u06d5\u0632\u062f\u0627\u0646\u06d5."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u00ab\u0626\u06d5\u06af\u06d5\u0631 \u062f\u0632 \u06cc\u0627\u0646 \u0686\u06d5\u062a\u06d5\u06cc \u0634\u06d5\u0648\u0627\u0646\u062a \u0628\u06c6 \u0628\u06ce\u062a\u060c \u0626\u0627\u06cc\u0627 \u062a\u06d5\u0646\u0647\u0627 \u0626\u06d5\u0648\u06d5\u06cc \u067e\u06ce\u0648\u06cc\u0633\u062a\u06cc\u0627\u0646 \u0628\u06ce\u062a \u0646\u0627\u06cc\u062f\u0632\u0646\u061f \u0626\u06d5\u06af\u06d5\u0631 \u0695\u06d5\u0632\u0628\u0695\u062a \u0628\u06c6 \u0628\u06ce\u062a \u0626\u0627\u06cc\u0627 \u0647\u06d5\u0646\u062f\u06ce\u06a9 \u062a\u0631\u06ce \u0628\u06d5\u062c\u06ce\u0646\u0627\u0647\u06ce\u06b5\u0646\u061f \u0626\u0627\u06cc \u0686\u06c6\u0646 \u0628\u06d5 \u062a\u06d5\u0648\u0627\u0648\u06cc \u0628\u0695\u0627\u06cc\u062a\u06d5\u0648\u06d5!"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0626\u0627\u06cc \u0639\u06cc\u0633\u06c6 \u0686\u06c6\u0646 \u062f\u06d5\u067e\u0634\u06a9\u06ce\u0646\u0631\u06ce\u062a! \u06af\u06d5\u0646\u062c\u06cc\u0646\u06d5 \u0634\u0627\u0631\u062f\u0631\u0627\u0648\u06d5\u06a9\u0627\u0646\u06cc \u062a\u0627\u06b5\u0627\u0646 \u062f\u06d5\u06a9\u0631\u06ce\u0646!"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0647\u06d5\u0645\u0648\u0648 \u0647\u0627\u0648\u067e\u06d5\u06cc\u0645\u0627\u0646\u06d5\u06a9\u0627\u0646\u062a \u0647\u06d5\u062a\u0627 \u0633\u0646\u0648\u0648\u0631 \u0695\u0627\u0648\u06cc\u0627\u0646 \u0646\u0627\u06cc\u060c \u0647\u06d5\u06b5\u06cc\u0627\u0646\u062e\u06d5\u06b5\u06d5\u062a\u0627\u0646\u062f\u06cc \u0648 \u0628\u06d5\u0633\u06d5\u0631\u062a\u062f\u0627 \u0632\u0627\u06b5\u0628\u0648\u0648\u0646\u060c \u0626\u06d5\u0648\u0627\u0646\u06d5\u06cc \u0646\u0645\u06d5\u06a9\u06cc \u062a\u06c6\u06cc\u0627\u0646 \u06a9\u0631\u062f\u0628\u0648\u0648 \u062f\u0627\u0648\u062a \u0628\u06c6 \u062f\u06d5\u0646\u06ce\u0646\u06d5\u0648\u06d5\u060c \u0628\u06d5\u06b5\u0627\u0645 \u062a\u06c6 \u062a\u06ce\u0646\u0627\u06af\u06d5\u06cc\u062a.\u00bb"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u06cc\u06d5\u0632\u062f\u0627\u0646 \u062f\u06d5\u0641\u06d5\u0631\u0645\u0648\u06ce\u062a: \u00ab\u0626\u0627\u06cc\u0627 \u0644\u06d5\u0648 \u0695\u06c6\u0698\u06d5\u062f\u0627\u060c \u062f\u0627\u0646\u0627\u06cc\u0627\u0646 \u0644\u06d5 \u0626\u06d5\u062f\u06c6\u0645 \u0644\u06d5\u0646\u0627\u0648 \u0646\u0627\u0628\u06d5\u0645\u060c \u062a\u06ce\u06af\u06d5\u06cc\u0634\u062a\u0648\u0648\u0627\u0646\u06cc\u0634 \u0644\u06d5 \u0686\u06cc\u0627\u06cc \u0639\u06cc\u0633\u06c6\u061f"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0626\u06d5\u06cc \u062a\u06ce\u0645\u0627\u0646\u060c \u067e\u0627\u06b5\u06d5\u0648\u0627\u0646\u06d5\u06a9\u0627\u0646\u062a \u062f\u06d5\u062a\u06c6\u0642\u0646\u060c \u0647\u06d5\u0645\u0648\u0648 \u0626\u06d5\u0648\u0627\u0646\u06d5\u06cc \u0644\u06d5 \u0686\u06cc\u0627\u06cc \u0639\u06cc\u0633\u06c6\u0646 \u0628\u06d5 \u06a9\u0648\u0634\u062a\u0646 \u0644\u06d5\u0646\u0627\u0648\u062f\u06d5\u0686\u0646."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0644\u06d5\u0628\u06d5\u0631 \u0633\u062a\u06d5\u0645\u06cc \u062a\u06c6 \u0644\u06d5 \u06cc\u0627\u0642\u0648\u0628\u06cc \u0628\u0631\u0627\u062a\u060c \u0695\u06cc\u0633\u0648\u0627\u06cc\u06cc \u062f\u0627\u062a\u062f\u06d5\u067e\u06c6\u0634\u06ce\u062a\u060c \u0628\u06c6 \u0647\u06d5\u062a\u0627\u0647\u06d5\u062a\u0627\u06cc\u06d5 \u0644\u06d5\u0646\u0627\u0648\u062f\u06d5\u0686\u06cc\u062a."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0626\u06d5\u0648 \u0695\u06c6\u0698\u06d5\u06cc \u062a\u06d5\u0645\u0627\u0634\u0627\u062a \u062f\u06d5\u06a9\u0631\u062f \u06a9\u0627\u062a\u06ce\u06a9 \u0628\u06cc\u0627\u0646\u06cc\u06cc\u06d5\u06a9\u0627\u0646 \u0633\u0627\u0645\u0627\u0646\u06d5\u06a9\u06d5\u06cc\u0627\u0646 \u062f\u0627\u06af\u06cc\u0631 \u06a9\u0631\u062f \u0648 \u0646\u0627\u0645\u06c6\u06a9\u0627\u0646 \u0647\u0627\u062a\u0646\u06d5 \u0646\u0627\u0648 \u062f\u06d5\u0631\u0648\u0627\u0632\u06d5\u06a9\u0627\u0646\u06cc \u0648 \u0644\u06d5\u0633\u06d5\u0631 \u0626\u06c6\u0631\u0634\u06d5\u0644\u06cc\u0645 \u062a\u06cc\u0631\u0648\u067e\u0634\u06a9\u06cc\u0627\u0646 \u06a9\u0631\u062f\u060c \u0647\u06d5\u0631\u0648\u06d5\u0647\u0627 \u062a\u06c6\u0634 \u0648\u06d5\u06a9 \u06cc\u06d5\u06a9\u06ce\u06a9 \u0628\u0648\u0648\u06cc \u0644\u06d5\u0648\u0627\u0646."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0628\u06d5\u06b5\u0627\u0645 \u0646\u0627\u0628\u06ce\u062a \u0628\u06d5 \u0695\u06c6\u0698\u06cc \u06a9\u0627\u0631\u06d5\u0633\u0627\u062a\u06cc \u0628\u0631\u0627\u06a9\u06d5\u062a \u062f\u06b5\u062e\u06c6\u0634 \u0628\u06cc\u062a\u060c \u06cc\u0627\u0646 \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u0644\u06d5\u0646\u0627\u0648\u0686\u0648\u0648\u0646\u06cc \u0646\u06d5\u0648\u06d5\u06cc \u06cc\u06d5\u0647\u0648\u062f\u0627 \u0634\u0627\u062f\u0645\u0627\u0646 \u0628\u06cc\u062a\u060c \u06cc\u0627\u0646 \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u062a\u06d5\u0646\u06af\u0627\u0646\u06d5\u06cc\u0627\u0646\u062f\u0627 \u0634\u0627\u0646\u0627\u0632\u06cc \u0628\u06a9\u06d5\u06cc\u062a."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0646\u0627\u0628\u06ce\u062a \u0628\u0686\u06cc\u062a\u06d5 \u0646\u0627\u0648 \u062f\u06d5\u0631\u0648\u0627\u0632\u06d5\u06cc \u06af\u06d5\u0644\u06d5\u06a9\u06d5\u0645 \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u06a9\u0627\u0631\u06d5\u0633\u0627\u062a\u06cc\u0627\u0646\u060c \u06cc\u0627\u0646 \u0628\u06d5 \u0646\u0627\u062e\u06c6\u0634\u06cc\u06cc\u06d5\u06a9\u06d5\u06cc\u0627\u0646 \u062f\u06b5\u062e\u06c6\u0634 \u0628\u06cc\u062a \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u06a9\u0627\u0631\u06d5\u0633\u0627\u062a\u06cc\u0627\u0646\u060c \u06cc\u0627\u0646 \u062f\u06d5\u0633\u062a\u062a \u0628\u06c6 \u0633\u0627\u0645\u0627\u0646\u06d5\u06a9\u06d5\u06cc\u0627\u0646 \u062f\u0631\u06ce\u0698 \u0628\u06a9\u06d5\u06cc\u062a \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u06a9\u0627\u0631\u06d5\u0633\u0627\u062a\u06cc\u0627\u0646."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0646\u0627\u0628\u06ce\u062a \u0644\u06d5\u0633\u06d5\u0631 \u062f\u0648\u0648\u0695\u06cc\u0627\u0646\u06d5\u06a9\u0627\u0646 \u0628\u0648\u06d5\u0633\u062a\u06cc\u062a \u0628\u06c6 \u06a9\u0648\u0634\u062a\u0646\u06cc \u062f\u06d5\u0631\u0628\u0627\u0632\u0628\u0648\u0648\u06d5\u06a9\u0627\u0646\u06cc\u0627\u0646\u060c \u0646\u0627\u0628\u06ce\u062a \u0647\u06d5\u06b5\u0627\u062a\u0648\u0648\u06d5\u06a9\u0627\u0646\u06cc\u0627\u0646 \u0695\u0627\u062f\u06d5\u0633\u062a \u0628\u06a9\u06d5\u06cc\u062a \u0644\u06d5 \u0695\u06c6\u0698\u06cc \u062a\u06d5\u0646\u06af\u0627\u0646\u06d5\u06cc\u0627\u0646\u062f\u0627."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u00ab\u0695\u06c6\u0698\u06cc \u06cc\u06d5\u0632\u062f\u0627\u0646 \u0628\u06c6 \u0647\u06d5\u0645\u0648\u0648 \u06af\u06d5\u0644\u0627\u0646 \u0646\u0632\u06cc\u06a9\u06d5. \u0647\u06d5\u0631\u0648\u06d5\u06a9 \u0626\u06d5\u0648\u06d5\u06cc \u06a9\u0631\u062f\u062a \u0626\u0627\u0648\u0627\u062a \u067e\u06ce \u062f\u06d5\u06a9\u0631\u06ce\u062a\u06d5\u0648\u06d5\u060c \u06a9\u0631\u062f\u06d5\u0648\u06d5\u06a9\u0627\u0646\u062a \u0628\u06d5\u0633\u06d5\u0631 \u062e\u06c6\u062a \u062f\u06ce\u062a\u06d5\u0648\u06d5."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0647\u06d5\u0631\u0648\u06d5\u06a9 \u0644\u06d5\u0633\u06d5\u0631 \u06a9\u06ce\u0648\u06cc \u067e\u06cc\u0631\u06c6\u0632\u0645 \u062e\u0648\u0627\u0631\u062f\u062a\u0627\u0646\u06d5\u0648\u06d5\u060c \u0647\u06d5\u0645\u0648\u0648 \u06af\u06d5\u0644\u0627\u0646 \u0628\u06d5\u0631\u062f\u06d5\u0648\u0627\u0645 \u062c\u0627\u0645\u06cc \u0633\u0632\u0627 \u062f\u06d5\u062e\u06c6\u0646\u06d5\u0648\u06d5\u061b \u062f\u06d5\u062e\u06c6\u0646\u06d5\u0648\u06d5 \u0648 \u062f\u06d5\u062e\u06c6\u0646\u06d5\u0648\u06d5 \u0648 \u0648\u06d5\u06a9 \u0626\u06d5\u0648\u06d5\u06cc\u0627\u0646 \u0644\u06ce\u062f\u06ce\u062a \u06a9\u06d5 \u0647\u06d5\u0631\u06af\u06cc\u0632 \u0646\u06d5\u0628\u0648\u0648\u0628\u0646."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0628\u06d5\u06b5\u0627\u0645 \u062f\u06d5\u0631\u0628\u0627\u0632\u0628\u0648\u0648\u0646 \u0644\u06d5\u0633\u06d5\u0631 \u06a9\u06ce\u0648\u06cc \u0633\u06cc\u06cc\u06c6\u0646 \u062f\u06d5\u0628\u06ce\u062a\u060c \u067e\u06cc\u0631\u06c6\u0632 \u062f\u06d5\u0628\u06ce\u062a\u060c \u0628\u0646\u06d5\u0645\u0627\u06b5\u06d5\u06cc \u06cc\u0627\u0642\u0648\u0628\u06cc\u0634 \u0628\u06d5\u0634\u06d5 \u0645\u06cc\u0631\u0627\u062a\u06cc \u062e\u06c6\u06cc\u0627\u0646 \u0648\u06d5\u0631\u062f\u06d5\u06af\u0631\u0646."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0628\u0646\u06d5\u0645\u0627\u06b5\u06d5\u06cc \u06cc\u0627\u0642\u0648\u0628 \u062f\u06d5\u0628\u06ce\u062a\u06d5 \u0626\u0627\u06af\u0631 \u0648 \u0628\u0646\u06d5\u0645\u0627\u06b5\u06d5\u06cc \u06cc\u0648\u0633\u0641 \u062f\u06d5\u0628\u06ce\u062a\u06d5 \u06af\u0695\u061b \u0628\u0646\u06d5\u0645\u0627\u06b5\u06d5\u06cc \u0639\u06cc\u0633\u06c6\u0634 \u062f\u06d5\u0628\u06ce\u062a\u06d5 \u067e\u0648\u0648\u0634\u060c \u062c\u0627 \u06af\u0695\u06cc\u0627\u0646 \u062a\u06ce\u0628\u06d5\u0631\u062f\u06d5\u062f\u06d5\u0646 \u0648 \u062f\u06d5\u06cc\u0627\u0646\u062e\u06c6\u0646. \u0628\u0646\u06d5\u0645\u0627\u06b5\u06d5\u06cc \u0639\u06cc\u0633\u06c6 \u06a9\u06d5\u0633\u06cc\u0627\u0646 \u0644\u06ce \u062f\u06d5\u0631\u0628\u0627\u0632 \u0646\u0627\u0628\u06ce\u062a.\u00bb \u0626\u06d5\u0645\u06d5 \u0641\u06d5\u0631\u0645\u0627\u06cc\u0634\u062a\u06cc \u06cc\u06d5\u0632\u062f\u0627\u0646\u06d5."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u062e\u06d5\u06b5\u06a9\u06cc \u0646\u06d5\u0642\u06d5\u0628 \u0686\u06cc\u0627\u06cc \u0639\u06cc\u0633\u06c6 \u062f\u0627\u06af\u06cc\u0631 \u062f\u06d5\u06a9\u06d5\u0646\u060c \u062e\u06d5\u06b5\u06a9\u06cc \u0632\u0648\u0648\u0631\u06af\u06d5\u06a9\u0627\u0646\u06cc\u0634 \u062e\u0627\u06a9\u06cc \u0641\u06d5\u0644\u06d5\u0633\u062a\u06cc\u06cc\u06d5\u06a9\u0627\u0646. \u062f\u06d5\u0628\u0646\u06d5 \u062e\u0627\u0648\u06d5\u0646\u06cc \u06a9\u06ce\u06b5\u06af\u06d5\u06a9\u0627\u0646\u06cc \u0626\u06d5\u0641\u0631\u0627\u06cc\u0645 \u0648 \u0633\u0627\u0645\u06cc\u0631\u06d5\u060c \u0628\u0646\u06cc\u0627\u0645\u06cc\u0646\u06cc\u0634 \u062f\u06d5\u0628\u06ce\u062a\u06d5 \u062e\u0627\u0648\u06d5\u0646\u06cc \u06af\u0644\u0639\u0627\u062f."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0626\u06d5\u0648 \u06a9\u06c6\u0645\u06d5\u06b5\u06d5\u06cc \u0695\u0627\u067e\u06ce\u0686\u06a9\u0631\u0627\u0648\u0627\u0646\u06cc \u0626\u06cc\u0633\u0631\u0627\u0626\u06cc\u0644 \u06a9\u06d5 \u0644\u06d5 \u06a9\u06d5\u0646\u0639\u0627\u0646\u0646 \u0647\u06d5\u062a\u0627 \u0633\u06d5\u0631\u06d5\u0641\u06d5\u0646\u062f \u062f\u06d5\u0633\u062a \u0628\u06d5\u0633\u06d5\u0631 \u062e\u0627\u06a9\u06d5\u06a9\u06d5\u062f\u0627 \u062f\u06d5\u06af\u0631\u0646\u061b \u0695\u0627\u067e\u06ce\u0686\u06a9\u0631\u0627\u0648\u0627\u0646\u06cc \u0626\u06c6\u0631\u0634\u06d5\u0644\u06cc\u0645\u06cc\u0634 \u06a9\u06d5 \u0644\u06d5 \u0633\u0641\u0627\u0631\u06d5\u062f\u0646 \u062f\u06d5\u0633\u062a \u0628\u06d5\u0633\u06d5\u0631 \u0634\u0627\u0631\u06c6\u0686\u06a9\u06d5\u06a9\u0627\u0646\u06cc \u0646\u06d5\u0642\u06d5\u0628\u062f\u0627 \u062f\u06d5\u06af\u0631\u0646."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u0695\u0632\u06af\u0627\u0631\u06a9\u06d5\u0631\u0627\u0646\u06cc \u062e\u0627\u06a9\u06d5\u06a9\u06d5\u0634 \u0633\u06d5\u0631\u062f\u06d5\u06a9\u06d5\u0648\u0646\u06d5 \u0633\u06d5\u0631 \u06a9\u06ce\u0648\u06cc \u0633\u06cc\u06cc\u06c6\u0646 \u0628\u06c6 \u0641\u06d5\u0631\u0645\u0627\u0646\u0695\u06d5\u0648\u0627\u06cc\u06d5\u062a\u06cc \u0644\u06d5\u0633\u06d5\u0631 \u0686\u06cc\u0627\u06cc \u0639\u06cc\u0633\u06c6. \u067e\u0627\u0634\u0627\u06cc\u06d5\u062a\u06cc\u0634 \u0628\u06c6 \u06cc\u06d5\u0632\u062f\u0627\u0646 \u062f\u06d5\u0628\u06ce\u062a."
                }
            }
        }
    }
