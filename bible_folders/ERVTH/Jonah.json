                "8": {
                    "verse_nr": "8",
                    "verse": "\u0e41\u0e15\u0e48\u0e43\u0e2b\u0e49\u0e17\u0e38\u0e01\u0e04\u0e19\u0e23\u0e27\u0e21\u0e17\u0e31\u0e49\u0e07\u0e2a\u0e31\u0e15\u0e27\u0e4c \u0e43\u0e2a\u0e48\u0e40\u0e2a\u0e37\u0e49\u0e2d\u0e1c\u0e49\u0e32\u0e01\u0e23\u0e30\u0e2a\u0e2d\u0e1a \u0e43\u0e2b\u0e49\u0e17\u0e38\u0e01\u0e04\u0e19\u0e2d\u0e18\u0e34\u0e29\u0e10\u0e32\u0e19\u0e15\u0e48\u0e2d\u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e2a\u0e38\u0e14\u0e01\u0e33\u0e25\u0e31\u0e07 \u0e41\u0e15\u0e48\u0e25\u0e30\u0e04\u0e19\u0e08\u0e30\u0e15\u0e49\u0e2d\u0e07\u0e40\u0e25\u0e34\u0e01\u0e17\u0e33\u0e0a\u0e31\u0e48\u0e27 \u0e41\u0e25\u0e30\u0e40\u0e25\u0e34\u0e01\u0e17\u0e33\u0e17\u0e32\u0e23\u0e38\u0e13\u0e42\u0e2b\u0e14\u0e23\u0e49\u0e32\u0e22"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0e16\u0e49\u0e32\u0e17\u0e33\u0e2d\u0e22\u0e48\u0e32\u0e07\u0e19\u0e35\u0e49 \u0e44\u0e21\u0e48\u0e41\u0e19\u0e48 \u0e1a\u0e32\u0e07\u0e17\u0e35\u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e2d\u0e32\u0e08\u0e08\u0e30\u0e40\u0e1b\u0e25\u0e35\u0e48\u0e22\u0e19\u0e43\u0e08 \u0e41\u0e25\u0e30\u0e2b\u0e31\u0e19\u0e08\u0e32\u0e01\u0e04\u0e27\u0e32\u0e21\u0e42\u0e01\u0e23\u0e18\u0e02\u0e2d\u0e07\u0e1e\u0e23\u0e30\u0e2d\u0e07\u0e04\u0e4c\u0e17\u0e35\u0e48\u0e01\u0e33\u0e25\u0e31\u0e07\u0e40\u0e1c\u0e32\u0e1c\u0e25\u0e32\u0e0d\u0e2d\u0e22\u0e39\u0e48\u0e15\u0e48\u0e2d\u0e40\u0e23\u0e32\u0e01\u0e47\u0e44\u0e14\u0e49 \u0e40\u0e1e\u0e37\u0e48\u0e2d\u0e1e\u0e27\u0e01\u0e40\u0e23\u0e32\u0e08\u0e30\u0e44\u0e14\u0e49\u0e44\u0e21\u0e48\u0e15\u0e49\u0e2d\u0e07\u0e15\u0e32\u0e22\u201d"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e40\u0e2b\u0e47\u0e19\u0e2a\u0e34\u0e48\u0e07\u0e17\u0e35\u0e48\u0e1e\u0e27\u0e01\u0e40\u0e02\u0e32\u0e17\u0e33 \u0e1e\u0e23\u0e30\u0e2d\u0e07\u0e04\u0e4c\u0e40\u0e2b\u0e47\u0e19\u0e27\u0e48\u0e32\u0e1e\u0e27\u0e01\u0e40\u0e02\u0e32\u0e44\u0e14\u0e49\u0e2b\u0e31\u0e19\u0e44\u0e1b\u0e08\u0e32\u0e01\u0e17\u0e32\u0e07\u0e0a\u0e31\u0e48\u0e27\u0e02\u0e2d\u0e07\u0e40\u0e02\u0e32 \u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e08\u0e36\u0e07\u0e40\u0e1b\u0e25\u0e35\u0e48\u0e22\u0e19\u0e43\u0e08 \u0e44\u0e21\u0e48\u0e25\u0e07\u0e42\u0e17\u0e29\u0e1e\u0e27\u0e01\u0e40\u0e02\u0e32\u0e2d\u0e22\u0e48\u0e32\u0e07\u0e17\u0e35\u0e48\u0e1e\u0e23\u0e30\u0e2d\u0e07\u0e04\u0e4c\u0e1a\u0e2d\u0e01\u0e44\u0e27\u0e49\u0e27\u0e48\u0e32\u0e08\u0e30\u0e17\u0e33"
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c\u0e42\u0e01\u0e23\u0e18\u0e17\u0e35\u0e48\u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e43\u0e08\u0e14\u0e35 - \u0e41\u0e15\u0e48\u0e2a\u0e34\u0e48\u0e07\u0e19\u0e35\u0e49\u0e17\u0e33\u0e43\u0e2b\u0e49\u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c\u0e44\u0e21\u0e48\u0e1e\u0e2d\u0e43\u0e08\u0e21\u0e32\u0e01 \u0e40\u0e02\u0e32\u0e42\u0e01\u0e23\u0e18\u0e21\u0e32\u0e01"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0e40\u0e02\u0e32\u0e08\u0e36\u0e07\u0e2d\u0e18\u0e34\u0e29\u0e10\u0e32\u0e19\u0e15\u0e48\u0e2d\u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c\u0e41\u0e25\u0e30\u0e1e\u0e39\u0e14\u0e27\u0e48\u0e32 \u201c\u0e02\u0e49\u0e32\u0e41\u0e15\u0e48\u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c \u0e19\u0e36\u0e01\u0e41\u0e25\u0e49\u0e27\u0e27\u0e48\u0e32\u0e21\u0e31\u0e19\u0e08\u0e30\u0e15\u0e49\u0e2d\u0e07\u0e40\u0e1b\u0e47\u0e19\u0e2d\u0e22\u0e48\u0e32\u0e07\u0e19\u0e35\u0e49\u0e41\u0e19\u0e48\u0e46\u0e15\u0e31\u0e49\u0e07\u0e41\u0e15\u0e48\u0e2d\u0e22\u0e39\u0e48\u0e17\u0e35\u0e48\u0e1a\u0e49\u0e32\u0e19\u0e41\u0e25\u0e49\u0e27 \u0e19\u0e31\u0e48\u0e19\u0e40\u0e1b\u0e47\u0e19\u0e40\u0e2b\u0e15\u0e38\u0e17\u0e35\u0e48\u0e02\u0e49\u0e32\u0e1e\u0e40\u0e08\u0e49\u0e32\u0e16\u0e36\u0e07\u0e1e\u0e22\u0e32\u0e22\u0e32\u0e21\u0e2b\u0e19\u0e35\u0e44\u0e1b\u0e40\u0e21\u0e37\u0e2d\u0e07\u0e17\u0e32\u0e23\u0e4c\u0e0a\u0e34\u0e0a \u0e40\u0e1e\u0e23\u0e32\u0e30\u0e02\u0e49\u0e32\u0e1e\u0e40\u0e08\u0e49\u0e32\u0e23\u0e39\u0e49\u0e27\u0e48\u0e32 \u0e1e\u0e23\u0e30\u0e2d\u0e07\u0e04\u0e4c\u0e21\u0e35\u0e40\u0e21\u0e15\u0e15\u0e32\u0e1b\u0e23\u0e32\u0e19\u0e35 \u0e1e\u0e23\u0e30\u0e2d\u0e07\u0e04\u0e4c\u0e2d\u0e14\u0e17\u0e19\u0e19\u0e32\u0e19 \u0e1e\u0e23\u0e30\u0e2d\u0e07\u0e04\u0e4c\u0e21\u0e35\u0e04\u0e27\u0e32\u0e21\u0e23\u0e31\u0e01\u0e17\u0e35\u0e48\u0e22\u0e34\u0e48\u0e07\u0e43\u0e2b\u0e0d\u0e48 \u0e41\u0e25\u0e30\u0e1e\u0e23\u0e49\u0e2d\u0e21\u0e40\u0e2a\u0e21\u0e2d\u0e17\u0e35\u0e48\u0e08\u0e30\u0e40\u0e1b\u0e25\u0e35\u0e48\u0e22\u0e19\u0e43\u0e08\u0e44\u0e21\u0e48\u0e25\u0e07\u0e42\u0e17\u0e29"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0e14\u0e31\u0e07\u0e19\u0e31\u0e49\u0e19 \u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c\u0e40\u0e08\u0e49\u0e32\u0e02\u0e49\u0e32 \u0e06\u0e48\u0e32\u0e02\u0e49\u0e32\u0e1e\u0e40\u0e08\u0e49\u0e32\u0e40\u0e25\u0e22 \u0e40\u0e1e\u0e23\u0e32\u0e30\u0e02\u0e49\u0e32\u0e1e\u0e40\u0e08\u0e49\u0e32\u0e15\u0e32\u0e22\u0e0b\u0e30\u0e14\u0e35\u0e01\u0e27\u0e48\u0e32\u201d"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0e41\u0e25\u0e49\u0e27\u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c\u0e1e\u0e39\u0e14\u0e27\u0e48\u0e32 \u201c\u0e17\u0e35\u0e48\u0e40\u0e08\u0e49\u0e32\u0e42\u0e01\u0e23\u0e18\u0e2d\u0e22\u0e48\u0e32\u0e07\u0e19\u0e35\u0e49\u0e04\u0e34\u0e14\u0e27\u0e48\u0e32\u0e16\u0e39\u0e01\u0e41\u0e25\u0e49\u0e27\u0e2b\u0e23\u0e37\u0e2d\u201d"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0e41\u0e25\u0e49\u0e27\u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c\u0e01\u0e47\u0e2d\u0e2d\u0e01\u0e44\u0e1b\u0e19\u0e2d\u0e01\u0e40\u0e21\u0e37\u0e2d\u0e07 \u0e40\u0e02\u0e32\u0e44\u0e1b\u0e19\u0e31\u0e48\u0e07\u0e2d\u0e22\u0e39\u0e48\u0e17\u0e32\u0e07\u0e17\u0e34\u0e28\u0e15\u0e30\u0e27\u0e31\u0e19\u0e2d\u0e2d\u0e01\u0e02\u0e2d\u0e07\u0e40\u0e21\u0e37\u0e2d\u0e07 \u0e17\u0e33\u0e40\u0e1e\u0e34\u0e07\u0e17\u0e35\u0e48\u0e1e\u0e31\u0e01\u0e02\u0e36\u0e49\u0e19\u0e21\u0e32\u0e2b\u0e25\u0e31\u0e07\u0e2b\u0e19\u0e36\u0e48\u0e07\u0e41\u0e25\u0e30\u0e19\u0e31\u0e48\u0e07\u0e2b\u0e25\u0e1a\u0e41\u0e14\u0e14\u0e2d\u0e22\u0e39\u0e48\u0e43\u0e19\u0e19\u0e31\u0e49\u0e19 \u0e40\u0e1d\u0e49\u0e32\u0e14\u0e39\u0e27\u0e48\u0e32\u0e08\u0e30\u0e40\u0e01\u0e34\u0e14\u0e2d\u0e30\u0e44\u0e23\u0e02\u0e36\u0e49\u0e19\u0e01\u0e31\u0e1a\u0e40\u0e21\u0e37\u0e2d\u0e07"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0e41\u0e25\u0e49\u0e27\u0e1e\u0e23\u0e30\u0e22\u0e32\u0e2b\u0e4c\u0e40\u0e27\u0e2b\u0e4c\u0e01\u0e47\u0e2a\u0e31\u0e48\u0e07\u0e43\u0e2b\u0e49\u0e15\u0e49\u0e19\u0e25\u0e30\u0e2b\u0e38\u0e48\u0e07\u0e15\u0e49\u0e19\u0e2b\u0e19\u0e36\u0e48\u0e07\u0e07\u0e2d\u0e01\u0e02\u0e36\u0e49\u0e19\u0e21\u0e32\u0e2d\u0e22\u0e39\u0e48\u0e40\u0e2b\u0e19\u0e37\u0e2d\u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c \u0e40\u0e1e\u0e37\u0e48\u0e2d\u0e40\u0e1b\u0e47\u0e19\u0e23\u0e48\u0e21\u0e40\u0e07\u0e32\u0e1a\u0e31\u0e07\u0e2b\u0e31\u0e27\u0e43\u0e2b\u0e49\u0e01\u0e31\u0e1a\u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c \u0e41\u0e25\u0e30\u0e0a\u0e48\u0e27\u0e22\u0e14\u0e31\u0e1a\u0e23\u0e49\u0e2d\u0e19\u0e04\u0e25\u0e32\u0e22\u0e17\u0e38\u0e01\u0e02\u0e4c \u0e43\u0e2b\u0e49\u0e01\u0e31\u0e1a\u0e40\u0e02\u0e32 \u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c\u0e14\u0e35\u0e43\u0e08\u0e21\u0e32\u0e01\u0e17\u0e35\u0e48\u0e21\u0e35\u0e15\u0e49\u0e19\u0e25\u0e30\u0e2b\u0e38\u0e48\u0e07\u0e19\u0e35\u0e49"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0e40\u0e0a\u0e49\u0e32\u0e27\u0e31\u0e19\u0e15\u0e48\u0e2d\u0e21\u0e32 \u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e01\u0e47\u0e2a\u0e31\u0e48\u0e07\u0e43\u0e2b\u0e49\u0e2b\u0e19\u0e2d\u0e19\u0e15\u0e31\u0e27\u0e2b\u0e19\u0e36\u0e48\u0e07\u0e21\u0e32\u0e01\u0e31\u0e14\u0e01\u0e34\u0e19\u0e15\u0e49\u0e19\u0e25\u0e30\u0e2b\u0e38\u0e48\u0e07\u0e19\u0e31\u0e49\u0e19 \u0e17\u0e33\u0e43\u0e2b\u0e49\u0e21\u0e31\u0e19\u0e40\u0e2b\u0e35\u0e48\u0e22\u0e27\u0e41\u0e2b\u0e49\u0e07\u0e15\u0e32\u0e22\u0e44\u0e1b"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0e15\u0e48\u0e2d\u0e21\u0e32 \u0e40\u0e21\u0e37\u0e48\u0e2d\u0e14\u0e27\u0e07\u0e2d\u0e32\u0e17\u0e34\u0e15\u0e22\u0e4c\u0e25\u0e2d\u0e22\u0e2a\u0e39\u0e07\u0e02\u0e36\u0e49\u0e19\u0e1a\u0e19\u0e17\u0e49\u0e2d\u0e07\u0e1f\u0e49\u0e32 \u0e1e\u0e23\u0e30\u0e40\u0e08\u0e49\u0e32\u0e01\u0e47\u0e2a\u0e31\u0e48\u0e07\u0e43\u0e2b\u0e49\u0e25\u0e21\u0e23\u0e49\u0e2d\u0e19\u0e1e\u0e31\u0e14\u0e21\u0e32\u0e08\u0e32\u0e01\u0e17\u0e32\u0e07\u0e17\u0e34\u0e28\u0e15\u0e30\u0e27\u0e31\u0e19\u0e2d\u0e2d\u0e01 \u0e41\u0e25\u0e30\u0e41\u0e2a\u0e07\u0e2d\u0e32\u0e17\u0e34\u0e15\u0e22\u0e4c\u0e01\u0e47\u0e41\u0e1c\u0e14\u0e01\u0e25\u0e49\u0e32\u0e25\u0e07\u0e1a\u0e19\u0e2b\u0e31\u0e27\u0e02\u0e2d\u0e07\u0e42\u0e22\u0e19\u0e32\u0e2b\u0e4c \u0e17\u0e33\u0e43\u0e2b\u0e49\u0e40\u0e02\u0e32\u0e2d\u0e48\u0e2d\u0e19\u0e40\u0e1e\u0e25\u0e35\u0e22\u0e2b\u0e21\u0e14\u0e41\u0e23\u0e07\u0e44\u0e1b \u0e40\u0e02\u0e32\u0e23\u0e48\u0e33\u0e23\u0e49\u0e2d\u0e07\u0e2d\u0e22\u0e32\u0e01\u0e15\u0e32\u0e22 \u0e40\u0e02\u0e32\u0e1e\u0e39\u0e14\u0e27\u0e48\u0e32 \u201c\u0e43\u0e2b\u0e49\u0e02\u0e49\u0e32\u0e1e\u0e40\u0e08\u0e49\u0e32\u0e15\u0e32\u0e22\u0e0b\u0e30\u0e14\u0e35\u0e01\u0e27\u0e48\u0e32\u201d"
                },
