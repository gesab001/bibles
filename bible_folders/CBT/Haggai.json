    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0412\u044a\u0437\u0441\u0442\u0430\u043d\u043e\u0432\u044f\u0432\u0430\u043d\u0435 \u043d\u0430 \u0419\u0435\u0440\u0443\u0441\u0430\u043b\u0438\u043c\u0441\u043a\u0438\u044f \u0445\u0440\u0430\u043c - \u041f\u0440\u0435\u0437 \u0432\u0442\u043e\u0440\u0430\u0442\u0430 \u0433\u043e\u0434\u0438\u043d\u0430 \u043e\u0442 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u0442\u043e \u043d\u0430 \u0414\u0430\u0440\u0438\u0439, \u0432 \u043f\u044a\u0440\u0432\u0438\u044f \u0434\u0435\u043d \u043e\u0442 \u0448\u0435\u0441\u0442\u0438\u044f \u043c\u0435\u0441\u0435\u0446, \u0431\u0435\u0448\u0435 \u0432\u044a\u0437\u0432\u0435\u0441\u0442\u0435\u043d\u043e \u0441\u043b\u043e\u0432\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0447\u0440\u0435\u0437 \u043f\u0440\u043e\u0440\u043e\u043a \u0410\u0433\u0435\u0439 \u043a\u044a\u043c \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0441\u0438\u043d \u043d\u0430 \u0421\u0430\u043b\u0430\u0442\u0438\u0438\u043b, \u0443\u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b \u043d\u0430 \u042e\u0434\u0435\u044f, \u0438 \u043a\u044a\u043c \u043f\u044a\u0440\u0432\u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u043a\u0430 \u0418\u0438\u0441\u0443\u0441, \u0441\u0438\u043d \u043d\u0430 \u0419\u043e\u0441\u0435\u0434\u0435\u043a."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201e\u0422\u0430\u043a\u0430 \u0433\u043e\u0432\u043e\u0440\u0438 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u2018\u0422\u043e\u0437\u0438 \u043d\u0430\u0440\u043e\u0434 \u043a\u0430\u0437\u0432\u0430: \u00ab\u041e\u0449\u0435 \u043d\u0435 \u0435 \u0434\u043e\u0448\u043b\u043e \u0432\u0440\u0435\u043c\u0435\u0442\u043e, \u043d\u0435 \u0435 \u0432\u0440\u0435\u043c\u0435 \u0434\u0430 \u0441\u0435 \u043f\u043e\u0441\u0442\u0440\u043e\u0438 \u0434\u043e\u043c \u0437\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430\u00bb\u2019.\u201c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0422\u043e\u0433\u0430\u0432\u0430 \u0431\u0435\u0448\u0435 \u0432\u044a\u0437\u0432\u0435\u0441\u0442\u0435\u043d\u043e \u0441\u043b\u043e\u0432\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0447\u0440\u0435\u0437 \u043f\u0440\u043e\u0440\u043e\u043a \u0410\u0433\u0435\u0439:"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u201e\u0410 \u0437\u0430 \u0441\u0430\u043c\u0438\u0442\u0435 \u0432\u0430\u0441 \u0432\u0440\u0435\u043c\u0435 \u043b\u0438 \u0435 \u0434\u0430 \u0436\u0438\u0432\u0435\u0435\u0442\u0435 \u0432 \u0441\u0432\u043e\u0438\u0442\u0435, \u0441\u043d\u0430\u0431\u0434\u0435\u043d\u0438 \u0441 \u043f\u043e\u043a\u0440\u0438\u0432\u0438 \u0438 \u0443\u043a\u0440\u0430\u0441\u0430 \u043a\u044a\u0449\u0438, \u043a\u043e\u0433\u0430\u0442\u043e \u0442\u043e\u0437\u0438 \u0434\u043e\u043c \u0435 \u0432 \u0440\u0430\u0437\u0432\u0430\u043b\u0438\u043d\u0438?\u201c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0417\u0430\u0442\u043e\u0432\u0430 \u0441\u0435\u0433\u0430 \u0442\u0430\u043a\u0430 \u0433\u043e\u0432\u043e\u0440\u0438 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u201e\u041f\u043e\u043c\u0438\u0441\u043b\u0435\u0442\u0435 \u043a\u0430\u043a \u0449\u0435 \u043f\u043e\u0441\u0442\u044a\u043f\u0438\u0442\u0435."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0412\u0438\u0435 \u043f\u043e\u0441\u044f\u0445\u0442\u0435 \u0431\u043e\u0433\u0430\u0442\u043e, \u043d\u043e \u0441\u044a\u0431\u0440\u0430\u0445\u0442\u0435 \u043c\u0430\u043b\u043a\u043e, \u044f\u0434\u0435\u0442\u0435, \u0431\u0435\u0437 \u0434\u0430 \u0441\u0435 \u043d\u0430\u0441\u0438\u0449\u0430\u0442\u0435, \u043f\u0438\u0435\u0442\u0435, \u0431\u0435\u0437 \u0434\u0430 \u0443\u0442\u043e\u043b\u044f\u0432\u0430\u0442\u0435 \u0436\u0430\u0436\u0434\u0430\u0442\u0430 \u0441\u0438, \u043e\u0431\u043b\u0438\u0447\u0430\u0442\u0435 \u0441\u0435, \u0431\u0435\u0437 \u0434\u0430 \u0432\u0438 \u0435 \u0442\u043e\u043f\u043b\u043e; \u043d\u0430\u0434\u043d\u0438\u0447\u0430\u0440, \u043a\u043e\u0439\u0442\u043e \u0437\u0430\u0440\u0430\u0431\u043e\u0442\u0432\u0430 \u0437\u0430\u043f\u043b\u0430\u0442\u0430, \u043f\u043e\u0441\u0442\u0430\u0432\u044f \u044f \u0432 \u043f\u0440\u043e\u0431\u0438\u0442\u0430 \u043a\u0435\u0441\u0438\u044f.\u201c"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0422\u0430\u043a\u0430 \u0433\u043e\u0432\u043e\u0440\u0438 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u201e\u041f\u043e\u043c\u0438\u0441\u043b\u0435\u0442\u0435 \u043a\u0430\u043a \u0449\u0435 \u043f\u043e\u0441\u0442\u044a\u043f\u0438\u0442\u0435."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0412\u044a\u0437\u043b\u0435\u0437\u0442\u0435 \u043d\u0430 \u043f\u043b\u0430\u043d\u0438\u043d\u0430\u0442\u0430, \u043f\u043e\u0434\u0433\u043e\u0442\u0432\u0435\u0442\u0435 \u0434\u044a\u0440\u0432\u0435\u0442\u0430 \u0438 \u043f\u043e\u0441\u0442\u0440\u043e\u0439\u0442\u0435 \u0445\u0440\u0430\u043c\u0430 \u0442\u0430\u043a\u0430, \u0447\u0435 \u0410\u0437 \u0434\u0430 \u0431\u043b\u0430\u0433\u043e\u0432\u043e\u043b\u044f \u043a\u044a\u043c \u043d\u0435\u0433\u043e \u0438 \u0434\u0430 \u0441\u0435 \u043f\u0440\u043e\u0441\u043b\u0430\u0432\u044f."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u041e\u0447\u0430\u043a\u0432\u0430\u0445\u0442\u0435 \u043c\u043d\u043e\u0433\u043e, \u0430 \u0441\u0435 \u043f\u043e\u043b\u0443\u0447\u0438 \u043c\u0430\u043b\u043a\u043e, \u0438 \u0442\u043e\u0432\u0430, \u043a\u043e\u0435\u0442\u043e \u0434\u043e\u043d\u0435\u0441\u0435\u0442\u0435 \u0432 \u043a\u044a\u0449\u0438, \u0442\u043e\u0432\u0430 \u0410\u0437 \u0449\u0435 \u0440\u0430\u0437\u043f\u0438\u043b\u0435\u044f. \u0417\u0430 \u043a\u0430\u043a\u0432\u043e? \u2013 \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u0437\u0430\u0442\u043e\u0432\u0430, \u0447\u0435 \u041c\u043e\u044f\u0442 \u0434\u043e\u043c \u0435 \u0432 \u0440\u0430\u0437\u0432\u0430\u043b\u0438\u043d\u0438, \u0434\u043e\u043a\u0430\u0442\u043e \u0432\u0441\u0435\u043a\u0438 \u043e\u0442 \u0432\u0430\u0441 \u0441\u0435 \u0433\u0440\u0438\u0436\u0438 \u0437\u0430 \u043a\u044a\u0449\u0430\u0442\u0430 \u0441\u0438."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0415\u0442\u043e \u043f\u043e\u0440\u0430\u0434\u0438 \u0442\u043e\u0432\u0430 \u043d\u0435\u0431\u0435\u0442\u043e \u043e\u0442\u043a\u0430\u0437\u0432\u0430 \u0434\u0430 \u043f\u0440\u0430\u0449\u0430 \u0434\u044a\u0436\u0434 \u0438 \u0437\u0435\u043c\u044f\u0442\u0430 \u043d\u0435 \u0435 \u043f\u043b\u043e\u0434\u043e\u0440\u043e\u0434\u043d\u0430."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0410\u0437 \u0438\u0437\u043f\u0440\u0430\u0442\u0438\u0445 \u0441\u0443\u0448\u0430 \u0432\u044a\u0440\u0445\u0443 \u0437\u0435\u043c\u044f, \u0432\u044a\u0440\u0445\u0443 \u043f\u043b\u0430\u043d\u0438\u043d\u0438, \u0432\u044a\u0440\u0445\u0443 \u0436\u0438\u0442\u043e, \u0432\u044a\u0440\u0445\u0443 \u0433\u0440\u043e\u0437\u0434\u043e\u0432 \u0441\u043e\u043a, \u0432\u044a\u0440\u0445\u0443 \u043c\u0430\u0441\u043b\u0438\u043d\u043e\u0432\u043e \u043c\u0430\u0441\u043b\u043e \u0438 \u0432\u044a\u0440\u0445\u0443 \u0432\u0441\u0438\u0447\u043a\u043e, \u043a\u043e\u0435\u0442\u043e \u0440\u0430\u0436\u0434\u0430 \u0437\u0435\u043c\u044f\u0442\u0430, \u0438 \u0432\u044a\u0440\u0445\u0443 \u0445\u043e\u0440\u0430 \u0438 \u0434\u043e\u0431\u0438\u0442\u044a\u043a, \u0438 \u0432\u044a\u0440\u0445\u0443 \u043f\u0440\u0438\u0434\u043e\u0431\u0438\u0442\u043e\u0442\u043e \u0441 \u0440\u044a\u0447\u0435\u043d \u0442\u0440\u0443\u0434.\u201c"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0422\u043e\u0433\u0430\u0432\u0430 \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0441\u0438\u043d \u043d\u0430 \u0421\u0430\u043b\u0430\u0442\u0438\u0438\u043b, \u0438 \u0418\u0438\u0441\u0443\u0441, \u0441\u0438\u043d \u043d\u0430 \u0419\u043e\u0441\u0435\u0434\u0435\u043a, \u0438 \u0446\u0435\u043b\u0438\u044f\u0442 \u043e\u0446\u0435\u043b\u044f\u043b \u043d\u0430\u0440\u043e\u0434 \u043f\u043e\u0441\u043b\u0443\u0448\u0430\u0445\u0430 \u0433\u043b\u0430\u0441\u0430 \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430, \u0441\u0432\u043e\u044f \u0411\u043e\u0433, \u0430 \u0438\u043c\u0435\u043d\u043d\u043e \u0434\u0443\u043c\u0438\u0442\u0435 \u043d\u0430 \u043f\u0440\u043e\u0440\u043e\u043a \u0410\u0433\u0435\u0439, \u043a\u0430\u0442\u043e \u043f\u0440\u0430\u0442\u0435\u043d\u0438\u043a \u043d\u0430 \u0442\u0435\u0445\u043d\u0438\u044f \u0413\u043e\u0441\u043f\u043e\u0434 \u0411\u043e\u0433, \u0438 \u043d\u0430\u0440\u043e\u0434\u044a\u0442 \u0441\u0435 \u043f\u043e\u0431\u043e\u044f \u043e\u0442 \u0413\u043e\u0441\u043f\u043e\u0434\u0430."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0422\u043e\u0433\u0430\u0432\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u043d\u0438\u044f\u0442 \u0432\u0435\u0441\u0442\u0438\u0442\u0435\u043b \u0410\u0433\u0435\u0439 \u0432\u044a\u0437\u0432\u0435\u0441\u0442\u0438 \u043d\u0430 \u043d\u0430\u0440\u043e\u0434\u0430 \u043f\u043e \u0432\u044a\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430: \u201e\u0410\u0437 \u0441\u044a\u043c \u0441 \u0432\u0430\u0441!\u201c, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0418 \u0413\u043e\u0441\u043f\u043e\u0434 \u0432\u044a\u0437\u0431\u0443\u0434\u0438 \u0434\u0443\u0445\u0430 \u043d\u0430 \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0441\u0438\u043d \u043d\u0430 \u0421\u0430\u043b\u0430\u0442\u0438\u0438\u043b, \u0443\u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b \u043d\u0430 \u042e\u0434\u0435\u044f, \u0434\u0443\u0445\u0430 \u043d\u0430 \u043f\u044a\u0440\u0432\u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u043a\u0430 \u0418\u0438\u0441\u0443\u0441, \u0441\u0438\u043d \u043d\u0430 \u0419\u043e\u0441\u0435\u0434\u0435\u043a, \u0438 \u0434\u0443\u0445\u0430 \u043d\u0430 \u043e\u0446\u0435\u043b\u0435\u043b\u0438\u044f \u043d\u0430\u0440\u043e\u0434 \u2013 \u0438 \u0442\u0435 \u0434\u043e\u0439\u0434\u043e\u0445\u0430 \u0438 \u043f\u043e\u0447\u043d\u0430\u0445\u0430 \u0434\u0430 \u0440\u0430\u0431\u043e\u0442\u044f\u0442 \u0432 \u0434\u043e\u043c\u0430 \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435, \u0441\u0432\u043e\u044f \u0411\u043e\u0433,"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0432 \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442 \u0438 \u0447\u0435\u0442\u0432\u044a\u0440\u0442\u0438\u044f \u0434\u0435\u043d \u043d\u0430 \u0448\u0435\u0441\u0442\u0438\u044f \u043c\u0435\u0441\u0435\u0446, \u043f\u0440\u0435\u0437 \u0432\u0442\u043e\u0440\u0430\u0442\u0430 \u0433\u043e\u0434\u0438\u043d\u0430 \u043e\u0442 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u0442\u043e \u043d\u0430 \u0446\u0430\u0440 \u0414\u0430\u0440\u0438\u0439."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0412\u0435\u043b\u0438\u043a\u043e\u043b\u0435\u043f\u0438\u0435\u0442\u043e \u043d\u0430 \u0445\u0440\u0430\u043c\u0430 - \u041d\u0430 \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442 \u0438 \u043f\u044a\u0440\u0432\u0438\u044f \u0434\u0435\u043d \u043e\u0442 \u0441\u0435\u0434\u043c\u0438\u044f \u043c\u0435\u0441\u0435\u0446 \u0431\u0435\u0448\u0435 \u0432\u044a\u0437\u0432\u0435\u0441\u0442\u0435\u043d\u043e \u0441\u043b\u043e\u0432\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0447\u0440\u0435\u0437 \u043f\u0440\u043e\u0440\u043e\u043a \u0410\u0433\u0435\u0439:"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u201e\u0413\u043e\u0432\u043e\u0440\u0438 \u043d\u0430 \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0441\u0438\u043d \u043d\u0430 \u0421\u0430\u043b\u0430\u0442\u0438\u0438\u043b, \u0443\u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b \u043d\u0430 \u042e\u0434\u0435\u044f, \u0438 \u043d\u0430 \u043f\u044a\u0440\u0432\u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u043a\u0430 \u0418\u0438\u0441\u0443\u0441, \u0441\u0438\u043d \u043d\u0430 \u0419\u043e\u0441\u0435\u0434\u0435\u043a, \u0438 \u043d\u0430 \u043e\u0446\u0435\u043b\u0435\u043b\u0438\u044f \u043d\u0430\u0440\u043e\u0434 \u0441\u043b\u0435\u0434\u043d\u043e\u0442\u043e:"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u2018\u041c\u0435\u0436\u0434\u0443 \u0432\u0430\u0441 \u043e\u0441\u0442\u0430\u043d\u0430\u043b \u043b\u0438 \u0435 \u0442\u0430\u043a\u044a\u0432, \u043a\u043e\u0439\u0442\u043e \u0435 \u0432\u0438\u0434\u044f\u043b \u0442\u043e\u0437\u0438 \u0445\u0440\u0430\u043c \u0432 \u043f\u0440\u0435\u0434\u0438\u0448\u043d\u0430\u0442\u0430 \u043c\u0443 \u0441\u043b\u0430\u0432\u0430? \u0410 \u0441\u0435\u0433\u0430 \u043a\u0430\u043a\u044a\u0432 \u0433\u043e \u0432\u0438\u0436\u0434\u0430\u0442\u0435? \u041d\u0435 \u0435 \u043b\u0438 \u0442\u043e\u0439 \u0432\u044a\u0432 \u0432\u0430\u0448\u0438\u0442\u0435 \u043e\u0447\u0438, \u043a\u0430\u0442\u043e \u0434\u0430 \u0435 \u043d\u0438\u0449\u043e?"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u041d\u043e \u0442\u0438 \u0441\u0435 \u043e\u0431\u043e\u0434\u0440\u0438 \u0441\u0435\u0433\u0430, \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430, \u043e\u0431\u043e\u0434\u0440\u0438 \u0441\u0435 \u0442\u0438, \u043f\u044a\u0440\u0432\u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u043a \u0418\u0438\u0441\u0443\u0441, \u0441\u0438\u043d \u043d\u0430 \u0419\u043e\u0441\u0435\u0434\u0435\u043a! \u041e\u0431\u043e\u0434\u0440\u0435\u0442\u0435 \u0441\u0435 \u0432\u0441\u0438\u0447\u043a\u0438 \u043e\u0431\u0438\u0442\u0430\u0442\u0435\u043b\u0438 \u043d\u0430 \u0442\u0430\u0437\u0438 \u0437\u0435\u043c\u044f, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435, \u0438 \u0441\u0435 \u0437\u0430\u0445\u0432\u0430\u043d\u0435\u0442\u0435 \u0437\u0430 \u0440\u0430\u0431\u043e\u0442\u0430, \u0437\u0430\u0449\u043e\u0442\u043e \u0410\u0437 \u0441\u044a\u043c \u0441 \u0432\u0430\u0441, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0421\u044a\u044e\u0437\u044a\u0442, \u043a\u043e\u0439\u0442\u043e \u0410\u0437 \u0441\u043a\u043b\u044e\u0447\u0438\u0445 \u0441 \u0432\u0430\u0441, \u043a\u043e\u0433\u0430\u0442\u043e \u0438\u0437\u043b\u044f\u0437\u043e\u0445\u0442\u0435 \u043e\u0442 \u0415\u0433\u0438\u043f\u0435\u0442, \u0438 \u041c\u043e\u044f\u0442 \u0414\u0443\u0445 \u0441\u0430 \u0441 \u0432\u0430\u0441. \u041d\u0435 \u0431\u043e\u0439\u0442\u0435 \u0441\u0435!\u2019"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0417\u0430\u0449\u043e\u0442\u043e \u0442\u0430\u043a\u0430 \u0433\u043e\u0432\u043e\u0440\u0438 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u2018\u041e\u0449\u0435 \u043c\u0430\u043b\u043a\u043e \u0432\u0440\u0435\u043c\u0435 \u0438\u043c\u0430 \u2013 \u0442\u043e\u0433\u0430\u0432\u0430 \u0449\u0435 \u0440\u0430\u0437\u0442\u044a\u0440\u0441\u044f \u043d\u0435\u0431\u0435 \u0438 \u0437\u0435\u043c\u044f, \u043c\u043e\u0440\u0435 \u0438 \u0441\u0443\u0448\u0430;"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0449\u0435 \u0440\u0430\u0437\u0442\u044a\u0440\u0441\u044f \u0432\u0441\u0438\u0447\u043a\u0438 \u043d\u0430\u0440\u043e\u0434\u0438 \u2013 \u0438 \u0449\u0435 \u0434\u043e\u0439\u0434\u0435 \u0422\u043e\u0437\u0438, \u041a\u043e\u0439\u0442\u043e \u0435 \u0436\u0435\u043b\u0430\u043d \u043e\u0442 \u0432\u0441\u0438\u0447\u043a\u0438 \u043d\u0430\u0440\u043e\u0434\u0438; \u0449\u0435 \u0438\u0437\u043f\u044a\u043b\u043d\u044f \u0442\u043e\u0437\u0438 \u0414\u043e\u043c \u0441\u044a\u0441 \u0441\u043b\u0430\u0432\u0430, \u043a\u0430\u0437\u0432\u0430 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u041c\u043e\u0435 \u0435 \u0432\u0441\u0438\u0447\u043a\u043e\u0442\u043e \u0441\u0440\u0435\u0431\u0440\u043e, \u041c\u043e\u0435 \u0435 \u0432\u0441\u0438\u0447\u043a\u043e\u0442\u043e \u0437\u043b\u0430\u0442\u043e, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0412\u0435\u043b\u0438\u043a\u043e\u043b\u0435\u043f\u0438\u0435\u0442\u043e \u043d\u0430 \u0442\u043e\u0437\u0438 \u043d\u043e\u0432 \u0445\u0440\u0430\u043c \u0449\u0435 \u0431\u044a\u0434\u0435 \u043f\u043e-\u0433\u043e\u043b\u044f\u043c\u043e, \u043e\u0442\u043a\u043e\u043b\u043a\u043e\u0442\u043e \u043d\u0430 \u043f\u0440\u0435\u0434\u0438\u0448\u043d\u0438\u044f, \u043a\u0430\u0437\u0432\u0430 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u0442\u043e\u0432\u0430 \u043c\u044f\u0441\u0442\u043e \u0410\u0437 \u0449\u0435 \u043e\u0431\u0434\u0430\u0440\u044f \u0441 \u0431\u043b\u0430\u0433\u043e\u043f\u043e\u043b\u0443\u0447\u0438\u0435\u2019, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435.\u201c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u041f\u0438\u0442\u0430\u043d\u0435 \u043a\u044a\u043c \u0441\u0432\u0435\u0449\u0435\u043d\u0438\u0446\u0438\u0442\u0435 - \u0412 \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442 \u0438 \u0447\u0435\u0442\u0432\u044a\u0440\u0442\u0438\u044f \u0434\u0435\u043d \u043e\u0442 \u0434\u0435\u0432\u0435\u0442\u0438\u044f \u043c\u0435\u0441\u0435\u0446, \u043f\u0440\u0435\u0437 \u0432\u0442\u043e\u0440\u0430\u0442\u0430 \u0433\u043e\u0434\u0438\u043d\u0430 \u043e\u0442 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u0442\u043e \u043d\u0430 \u0414\u0430\u0440\u0438\u0439, \u0431\u0435\u0448\u0435 \u0432\u044a\u0437\u0432\u0435\u0441\u0442\u0435\u043d\u043e \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0447\u0440\u0435\u0437 \u043f\u0440\u043e\u0440\u043e\u043a \u0410\u0433\u0435\u0439:"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u201e\u0422\u0430\u043a\u0430 \u0433\u043e\u0432\u043e\u0440\u0438 \u0413\u043e\u0441\u043f\u043e\u0434 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435: \u2018\u041f\u043e\u043f\u0438\u0442\u0430\u0439 \u0441\u0435\u0433\u0430 \u0441\u0432\u0435\u0449\u0435\u043d\u0438\u0446\u0438\u0442\u0435 \u0437\u0430 \u043d\u0430\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u043f\u0440\u0438 \u0441\u043b\u0435\u0434\u043d\u043e\u0442\u043e:"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0430\u043a\u043e \u043d\u044f\u043a\u043e\u0439 \u043d\u043e\u0441\u0438 \u043e\u0441\u0432\u0435\u0442\u0435\u043d\u043e \u0436\u0435\u0440\u0442\u0432\u0435\u043d\u043e \u043c\u0435\u0441\u043e \u0432 \u043f\u043e\u043b\u0430\u0442\u0430 \u043d\u0430 \u0434\u0440\u0435\u0445\u0430\u0442\u0430 \u0441\u0438 \u0438 \u0441 \u043f\u043e\u043b\u0430 \u0434\u043e\u043f\u0440\u0435 \u0445\u043b\u044f\u0431 \u0438\u043b\u0438 \u043d\u0435\u0449\u043e \u0432\u0430\u0440\u0435\u043d\u043e, \u0438\u043b\u0438 \u0432\u0438\u043d\u043e, \u0438\u043b\u0438 \u0435\u043b\u0435\u0439, \u0438\u043b\u0438 \u043a\u0430\u043a\u0432\u043e\u0442\u043e \u0438 \u0434\u0430 \u0431\u0438\u043b\u043e \u044f\u0441\u0442\u0438\u0435, \u0449\u0435 \u0441\u0442\u0430\u043d\u0435 \u043b\u0438 \u0442\u043e\u0432\u0430 \u0441\u0432\u0435\u0449\u0435\u043d\u043e\u2019?\u201c \u0421\u0432\u0435\u0449\u0435\u043d\u0438\u0446\u0438\u0442\u0435 \u043e\u0442\u0433\u043e\u0432\u043e\u0440\u0438\u0445\u0430: \u201e\u041d\u0435.\u201c"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0422\u043e\u0433\u0430\u0432\u0430 \u0410\u0433\u0435\u0439 \u043a\u0430\u0437\u0430: \u201e\u0410\u043a\u043e \u0441\u0435 \u0434\u043e\u043f\u0440\u0435 \u0434\u043e \u0432\u0441\u0438\u0447\u043a\u043e \u0442\u043e\u0432\u0430 \u043d\u044f\u043a\u043e\u0439, \u043a\u043e\u0439\u0442\u043e \u0441\u0435 \u0435 \u043e\u0441\u043a\u0432\u0435\u0440\u043d\u0438\u043b \u043e\u0442 \u0434\u043e\u043f\u0438\u0440\u0430\u043d\u0435 \u0434\u043e \u043c\u044a\u0440\u0442\u0432\u0435\u0446, \u0449\u0435 \u0441\u0442\u0430\u043d\u0435 \u043b\u0438 \u0442\u043e\u0432\u0430 \u043d\u0435\u0447\u0438\u0441\u0442\u043e?\u201c \u0421\u0432\u0435\u0449\u0435\u043d\u0438\u0446\u0438\u0442\u0435 \u043e\u0442\u0433\u043e\u0432\u043e\u0440\u0438\u0445\u0430: \u201e\u0429\u0435 \u0441\u0442\u0430\u043d\u0435 \u043d\u0435\u0447\u0438\u0441\u0442\u043e.\u201c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0422\u043e\u0433\u0430\u0432\u0430 \u0410\u0433\u0435\u0439 \u043e\u0442\u0433\u043e\u0432\u043e\u0440\u0438: \u201e\u0422\u0430\u043a\u044a\u0432 \u0435 \u0442\u043e\u0437\u0438 \u043d\u0430\u0440\u043e\u0434, \u0442\u0430\u043a\u043e\u0432\u0430 \u0435 \u0442\u043e\u0432\u0430 \u043f\u043b\u0435\u043c\u0435 \u043f\u0440\u0435\u0434 \u041c\u0435\u043d\u0435, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430, \u0438 \u0442\u0430\u043a\u043e\u0432\u0430 \u0435 \u0432\u0441\u0438\u0447\u043a\u043e, \u043a\u043e\u0435\u0442\u043e \u0432\u044a\u0440\u0448\u0430\u0442! \u041a\u0430\u043a\u0432\u0438\u0442\u043e \u0438 \u0436\u0435\u0440\u0442\u0432\u0438 \u0434\u0430 \u043f\u0440\u0438\u043d\u0430\u0441\u044f\u0442 \u0442\u0430\u043c, \u0432\u0441\u0438\u0447\u043a\u043e \u0435 \u043d\u0435\u0447\u0438\u0441\u0442\u043e.\u201c"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u041e\u0431\u0435\u0449\u0430\u043d\u0438\u0435 \u0437\u0430 \u043f\u043b\u043e\u0434\u043e\u0440\u043e\u0434\u0438\u0435 \u0438 \u0431\u043b\u0430\u0433\u043e\u0441\u043b\u043e\u0432\u0435\u043d\u0438\u0435 \u0437\u0430 \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b - \u201e\u0410 \u0441\u0435\u0433\u0430 \u043d\u0430\u0441\u043e\u0447\u0435\u0442\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435\u0442\u043e \u0441\u0438 \u043a\u044a\u043c \u043c\u0438\u043d\u0430\u043b\u043e\u0442\u043e \u0434\u043e \u0434\u043d\u0435\u0441, \u043f\u0440\u0435\u0434\u0438 \u0434\u0430 \u0435 \u043f\u043e\u043b\u043e\u0436\u0435\u043d \u043a\u0430\u043c\u044a\u043a \u0432\u044a\u0440\u0445\u0443 \u043a\u0430\u043c\u044a\u043a \u0432 \u0413\u043e\u0441\u043f\u043e\u0434\u043d\u0438\u044f \u0445\u0440\u0430\u043c."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0421\u043b\u0443\u0447\u0432\u0430\u043b\u043e \u0441\u0435 \u0435 \u0434\u0430 \u0438\u0434\u0432\u0430\u0442 \u043f\u0440\u0438 \u0436\u0438\u0442\u0435\u043d \u043a\u0443\u043f\u0435\u043d, \u043a\u043e\u0439\u0442\u043e \u043c\u043e\u0436\u0435 \u0434\u0430 \u0438\u043c\u0430 \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442 \u0441\u043d\u043e\u043f\u0430, \u0430 \u0441\u0435 \u043e\u043a\u0430\u0437\u0432\u0430\u0445\u0430 \u0441\u0430\u043c\u043e \u0434\u0435\u0441\u0435\u0442, \u0438\u0434\u0432\u0430\u0445\u0430 \u043f\u0440\u0438 \u0433\u0440\u043e\u0437\u0434\u043e\u0432\u0438\u044f \u043b\u0438\u043d, \u0437\u0430 \u0434\u0430 \u043d\u0430\u0433\u0440\u0435\u0431\u0430\u0442 \u043f\u0435\u0442\u0434\u0435\u0441\u0435\u0442 \u043c\u0435\u0440\u0438 \u043e\u0442 \u043d\u0435\u0433\u043e, \u0430 \u0441\u0435 \u043e\u043a\u0430\u0437\u0432\u0430\u0445\u0430 \u0441\u0430\u043c\u043e \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u041d\u0430\u043a\u0430\u0437\u0432\u0430\u0445 \u0432\u0438 \u0441\u044a\u0441 \u0441\u0443\u0445 \u0432\u044f\u0442\u044a\u0440, \u0441 \u043f\u043e\u0440\u0430\u0437\u0438 \u043f\u043e \u0436\u0438\u0442\u0430\u0442\u0430 \u0438 \u0441 \u0433\u0440\u0430\u0434\u0443\u0448\u043a\u0430 \u043f\u043e \u0432\u0441\u0438\u0447\u043a\u043e, \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u0434\u0435\u043d\u043e \u043e\u0442 \u0432\u0430\u0441, \u043d\u043e \u0432\u0438\u0435 \u043d\u0435 \u0441\u0435 \u043e\u0431\u044a\u0440\u043d\u0430\u0445\u0442\u0435 \u043a\u044a\u043c \u041c\u0435\u043d\u0435, \u043a\u0430\u0437\u0432\u0430 \u0413\u043e\u0441\u043f\u043e\u0434."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u041d\u0430\u0441\u043e\u0447\u0435\u0442\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435\u0442\u043e \u0441\u0438 \u043a\u044a\u043c \u043c\u0438\u043d\u0430\u043b\u043e\u0442\u043e \u0434\u043e \u0434\u043d\u0435\u0441, \u043e\u0442 \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442 \u0438 \u0447\u0435\u0442\u0432\u044a\u0440\u0442\u0438\u044f \u0434\u0435\u043d \u043d\u0430 \u0434\u0435\u0432\u0435\u0442\u0438\u044f \u043c\u0435\u0441\u0435\u0446, \u043e\u0442 \u0434\u0435\u043d\u044f, \u043a\u043e\u0433\u0430\u0442\u043e \u0431\u044f\u0445\u0430 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438 \u043e\u0441\u043d\u043e\u0432\u0438\u0442\u0435 \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u043d\u0438\u044f \u0445\u0440\u0430\u043c, \u043d\u0430\u0441\u043e\u0447\u0435\u0442\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435\u0442\u043e \u0441\u0438:"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u043d\u0438\u043c\u0430 \u0437\u044a\u0440\u043d\u043e\u0442\u043e \u0437\u0430 \u043f\u043e\u0441\u0435\u0432 \u0435 \u043e\u0449\u0435 \u0432 \u0436\u0438\u0442\u043d\u0438\u0446\u0438\u0442\u0435? \u0414\u043e\u0441\u0435\u0433\u0430 \u043d\u0435 \u0441\u0430 \u0440\u043e\u0434\u0438\u043b\u0438 \u043f\u043b\u043e\u0434 \u043d\u0438\u0442\u043e \u043b\u043e\u0437\u0430, \u043d\u0438\u0442\u043e \u0441\u043c\u043e\u043a\u0438\u043d\u044f, \u043d\u0438\u0442\u043e \u043d\u0430\u0440, \u043d\u0438\u0442\u043e \u043c\u0430\u0441\u043b\u0438\u043d\u0430. \u041d\u043e \u043e\u0442 \u0442\u043e\u0437\u0438 \u0434\u0435\u043d \u043d\u0430\u0442\u0430\u0442\u044a\u043a \u0410\u0437 \u0449\u0435 \u0433\u0438 \u043e\u0431\u0434\u0430\u0440\u044f \u0441 \u0431\u043b\u0430\u0433\u043e\u0441\u043b\u043e\u0432\u0435\u043d\u0438\u0435.\u201c"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0421\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0431\u0435\u0448\u0435 \u0432\u044a\u0437\u0432\u0435\u0441\u0442\u0435\u043d\u043e \u0437\u0430 \u0432\u0442\u043e\u0440\u0438 \u043f\u044a\u0442 \u043a\u044a\u043c \u0410\u0433\u0435\u0439 \u0432 \u0434\u0432\u0430\u0434\u0435\u0441\u0435\u0442 \u0438 \u0447\u0435\u0442\u0432\u044a\u0440\u0442\u0438\u044f \u0434\u0435\u043d \u043e\u0442 \u043c\u0435\u0441\u0435\u0446\u0430 \u0441 \u0434\u0443\u043c\u0438\u0442\u0435:"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u201e\u041e\u0431\u044f\u0432\u0438 \u043d\u0430 \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0443\u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b \u043d\u0430 \u042e\u0434\u0435\u044f: \u2018\u0410\u0437 \u0449\u0435 \u0440\u0430\u0437\u0442\u044a\u0440\u0441\u044f \u043d\u0435\u0431\u0435 \u0438 \u0437\u0435\u043c\u044f;"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u0449\u0435 \u0441\u044a\u0431\u043e\u0440\u044f \u0446\u0430\u0440\u0441\u043a\u0438\u0442\u0435 \u043f\u0440\u0435\u0441\u0442\u043e\u043b\u0438 \u0438 \u0449\u0435 \u043f\u043e\u0440\u0430\u0437\u044f \u043c\u043e\u0449\u0442\u0430 \u043d\u0430 \u0446\u0430\u0440\u0441\u0442\u0432\u0430\u0442\u0430 \u043d\u0430 \u043d\u0430\u0440\u043e\u0434\u0438\u0442\u0435, \u0449\u0435 \u043f\u0440\u0435\u043e\u0431\u044a\u0440\u043d\u0430 \u043a\u043e\u043b\u0435\u0441\u043d\u0438\u0446\u0438 \u0438 \u0441\u0435\u0434\u044f\u0449\u0438\u0442\u0435 \u043d\u0430 \u0442\u044f\u0445, \u0438 \u043a\u043e\u043d\u0435\u0442\u0435 \u0438 \u043a\u043e\u043d\u043d\u0438\u0446\u0438\u0442\u0435 \u0438\u043c \u0449\u0435 \u0431\u044a\u0434\u0430\u0442 \u043f\u043e\u0432\u0430\u043b\u0435\u043d\u0438, \u0432\u0441\u0435\u043a\u0438 \u043e\u0442 \u043c\u0435\u0447\u0430 \u043d\u0430 \u0431\u0440\u0430\u0442 \u0441\u0438."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u0412 \u043e\u043d\u0437\u0438 \u0434\u0435\u043d, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435, \u0449\u0435 \u0432\u0437\u0435\u043c\u0430 \u0442\u0435\u0431\u0435, \u0417\u043e\u0440\u043e\u0432\u0430\u0432\u0435\u043b, \u0441\u0438\u043d \u043d\u0430 \u0421\u0430\u043b\u0430\u0442\u0438\u0438\u043b, \u0442\u0435\u0431\u0435, \u043a\u043e\u0439\u0442\u043e \u0441\u0438 \u041c\u043e\u0439 \u0441\u043b\u0443\u0436\u0438\u0442\u0435\u043b, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430, \u0438 \u0449\u0435 \u0442\u0435 \u043f\u0440\u0435\u0432\u044a\u0440\u043d\u0430 \u0432 \u043f\u0440\u044a\u0441\u0442\u0435\u043d \u0441 \u043f\u0435\u0447\u0430\u0442, \u0437\u0430\u0449\u043e\u0442\u043e \u0442\u0435 \u0438\u0437\u0431\u0440\u0430\u0445\u2019, \u0433\u043b\u0430\u0441\u0438 \u0441\u043b\u043e\u0432\u043e\u0442\u043e \u043d\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0441\u0438\u043b\u0438\u0442\u0435.\u201c"
                }
            }
        }
    }
}