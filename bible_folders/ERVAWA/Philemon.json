{
    "version": "ERVAWA",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0908\u0938\u0942 \u092e\u0938\u0940\u0939 \u0915 \u092c\u0930\u0947 \u092c\u0928\u094d\u0926\u0940 \u092c\u0928\u093e \u092a\u094c\u0932\u0941\u0938 \u0905\u0909\u0930 \u0939\u092e\u093e\u0930 \u092d\u093e\u0908 \u0924\u0940\u092e\u0941\u0925\u093f\u092f\u0941\u0938 \u0915\u0907\u0901\u0924\u0940 \u0938\u0903 \u0939\u092e\u093e\u0930 \u092a\u094d\u0930\u093f\u092f \u0926\u094b\u0938\u094d\u0924 \u0905\u0909\u0930 \u0938\u0939\u0915\u0930\u094d\u092e\u0940 \u092b\u093f\u0932\u0947\u092e\u094b\u0928,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0939\u092e\u093e\u0930 \u092c\u0939\u093f\u0928 \u0905\u092b\u092b\u093f\u092f\u093e, \u0939\u092e\u093e\u0930 \u0938\u093e\u0925\u0940 \u0938\u0948\u0928\u093f\u0915 \u0905\u0930\u094d\u0916\u093f\u092a\u094d\u092a\u0941\u0938 \u0905\u0909\u0930 \u0924\u094b\u0939\u0930\u0947 \u0918\u0930\u0947 \u092a\u0930 \u090f\u0915\u091f\u094d\u0920\u093e \u0939\u094b\u0907\u0935\u093e\u0932\u0940 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0915\u0903"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0909 \u0939\u092e\u093e\u0930 \u092a\u0930\u092e\u092a\u093f\u0924\u093e \u092a\u0930\u092e\u0947\u0938\u094d\u0938\u0930 \u0905\u0909\u0930 \u092a\u0930\u094d\u092d\u0942 \u0908\u0938\u0942 \u092e\u0938\u0940\u0939 \u0915\u0907\u0901\u0924\u0940 \u0938 \u0924\u094b\u0939\u0947 \u092a\u091a\u0928 \u0915 \u0905\u0928\u0941\u0917\u094d\u0930\u0939 \u0905\u0909\u0930 \u0938\u093e\u0928\u094d\u0924\u093f \u092e\u093f\u0932\u0907\u0964"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u092b\u093f\u0932\u0947\u092e\u094b\u0928 \u0915 \u092a\u093f\u0930\u0947\u092e \u0905\u0909\u0930 \u092c\u093f\u0938\u0935\u093e\u0938 - \u0906\u092a\u0928 \u092a\u0930\u093e\u0925\u0928\u093e \u092e\u0901 \u0924\u094b\u0939\u093e\u0930 \u0909\u0932\u094d\u0932\u0947\u0916 \u0915\u0930\u0924 \u092d\u090f \u092e\u0907\u0901 \u0939\u092e\u0947\u0938\u093e \u0905\u092a\u0928\u0947 \u092a\u0930\u092e\u0947\u0938\u094d\u0938\u0930 \u0915 \u0927\u0928\u094d\u092f\u092c\u093e\u0926 \u0915\u0930\u0924 \u0939\u0909\u0901\u0964"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0915\u093e\u0939\u0947\u0915\u093f \u092e\u0907\u0901 \u0938\u0902\u0924 \u091c\u0928\u0928 \u092c\u0930\u0947 \u0924\u094b\u0939\u0930\u0947 \u0913\u0907\u0945 \u092a\u093f\u0930\u0947\u092e \u0905\u0909\u0930 \u092c\u093f\u0938\u0935\u093e\u0938 \u0915 \u091a\u0930\u094d\u091a\u093e \u0938\u0941\u0928\u0924 \u0939\u0909\u0901, \u091c\u094b \u092a\u0930\u094d\u092d\u0942 \u0908\u0938\u0942 \u0905\u0909\u0930 \u0938\u092e\u0938\u094d\u0924 \u092a\u0935\u093f\u0924\u094d\u0924\u0930 \u0932\u094b\u0917\u0928 \u0915 \u092a\u094d\u0930\u0924\u093f \u0905\u0939\u0907\u0964"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u092e\u094b\u0930 \u092a\u0930\u093e\u0925\u0928\u093e \u092c\u093e \u0915\u093f \u0924\u094b\u0939\u0930\u0947 \u092c\u093f\u0938\u0935\u093e\u0938 \u0938 \u092a\u0907\u0926\u093e \u0909\u0926\u093e\u0930 \u0938\u0939\u092d\u093e\u0917\u093f\u0924\u093e \u0932\u094b\u0917\u0928 \u0915 \u092e\u093e\u0930\u094d\u0917 \u0926\u0930\u0938\u0928 \u0915\u0930\u0907\u0964 \u091c\u0947\u0939\u0938\u0947 \u0913\u0928\u094d\u0939\u0947 \u0938\u092d\u0928 \u0905\u091a\u094d\u091b\u0940 \u091a\u0940\u091c\u0928 \u0915 \u0917\u093f\u092f\u093e\u0928 \u0939\u094b\u0907 \u091c\u093e\u0907 \u091c\u0909\u0928 \u092e\u0938\u0940\u0939 \u0915 \u0909\u0926\u094d\u0926\u0947\u0938\u094d\u0938 \u0915 \u0906\u0917\u0947 \u092c\u0922\u093c\u093e\u0935\u0907 \u092e\u0901 \u0939\u092e\u0930\u0947 \u092c\u0940\u091a \u0918\u091f\u0928 \u0939\u094b\u0924 \u0930\u0939\u093f\u0928 \u092c\u093e\u091f\u093f\u0928\u0964"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0939\u0947 \u092d\u093e\u0908, \u0924\u094b\u0939\u0930 \u0915\u094b\u0938\u093f\u0938\u0928 \u0938 \u0938\u0902\u0924 \u091c\u0928\u0928 \u0915 \u0939\u0940\u0930\u0926\u0907 \u0939\u0930\u093e-\u092d\u0930\u093e \u0939\u094b\u0907 \u0917 \u0939\u092f\u0947\u0928, \u0907\u0939\u0940 \u092c\u0930\u0947 \u0924\u094b\u0939\u0930\u0947 \u092a\u093f\u0930\u0947\u092e \u0938 \u092e\u094b\u0915\u093e \u092c\u0939\u0941\u0924 \u0906\u0928\u0928\u094d\u0926 \u0905\u0909\u0930 \u092a\u094d\u0930\u094b\u0924\u094d\u0938\u093e\u0939\u0928 \u092e\u093f\u0932\u093e \u092c\u093e\u0964"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0909\u0928\u0947\u0938\u093f\u092e\u0941\u0938 \u0915 \u092d\u093e\u0908 \u0938\u094d\u0935\u0940\u0915\u093e\u0930\u093e - \u0907\u0939\u0940 \u092c\u0930\u0947 \u092e\u0907\u0901 \u092c\u093f\u0938\u0935\u093e\u0938 \u0915\u0930\u0924 \u0939\u0909\u0901 \u0915\u093f \u092e\u0938\u0940\u0939 \u092e\u0901 \u092e\u094b\u0915\u093e \u0924\u094b\u0939\u0930\u0947 \u0915\u0930\u094d\u0924\u094d\u0924\u0935\u094d\u092f\u0928 \u0915 \u092c\u093e\u0930\u0947 \u092e\u0901 \u0939\u0941\u0915\u0941\u092e \u0926\u0947\u0907 \u0915 \u0905\u0927\u093f\u0915\u093e\u0930 \u092c\u093e"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u092e\u0941\u0932\u093e \u092a\u093f\u0930\u0947\u092e \u0915 \u0906\u0927\u093e\u0930 \u092a\u0930 \u092e\u0907\u0901 \u0924\u094b\u0939\u0938\u0947 \u0928\u093f\u0935\u0947\u0926\u0928 \u0915\u0930\u092c \u0939\u0940 \u0920\u0940\u0915 \u0938\u092e\u091d\u0924 \u0939\u0909\u0901\u0964 \u092e\u0907\u0901 \u092a\u094c\u0932\u0941\u0938 \u091c\u0909\u0928 \u0905\u092c \u092c\u0942\u0922\u093c\u093e\u0907 \u0939\u094b\u0907 \u091a\u0932\u093e \u0939\u0909\u0901 \u0905\u0909\u0930 \u092e\u0938\u0940\u0939 \u0908\u0938\u0942 \u0915 \u092c\u0930\u0947 \u0905\u092c \u092d\u0940 \u092c\u0902\u0926\u0940 \u092c\u0928\u093e \u0939\u0941\u0906 \u0939\u0909\u0901\u0964 \u0907\u0939\u0940 \u0909\u091a\u093f\u0924 \u0905\u0939\u0907 \u0915\u093f \u0924\u094b\u0939\u0938\u0947 \u0938\u092c\u0928 \u0938 \u0906\u0917\u094d\u0930\u0939 \u0915\u0930\u0909\u0901\u0964"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u092e\u0907\u0901 \u0909 \u0909\u0928\u0947\u0938\u093f\u092e\u0941\u0938 \u0915 \u092c\u093e\u0930\u0947 \u092e\u0901 \u0928\u093f\u0935\u0947\u0926\u0928 \u0915\u0930\u0924 \u0939\u0909\u0901, \u0915\u093f \u091c\u0909\u0928 \u091c\u092c \u0924\u092c \u092e\u094b\u0930 \u0927\u0930\u092e \u092a\u0941\u0924\u094d\u0930 \u092c\u0928\u093e \u0930\u0939\u093e \u091c\u0947\u0915\u0930 \u092e\u0907\u0901 \u092c\u0902\u0926\u0940 \u0917\u0943\u0939 \u092e\u0901 \u0930\u0939\u0947\u0909\u0901\u0964"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u090f\u0915 \u0938\u092e\u0907 \u0930\u0939\u093e \u091c\u092c \u0909 \u0924\u094b\u0939\u0930\u0947 \u0915\u0909\u0928\u0909 \u0915\u093e\u092e \u0915 \u0928\u093e\u0939\u0940\u0902 \u0930\u0939\u093e, \u092e\u0941\u0932\u093e \u0905\u092c \u0928 \u0915\u0947\u0935\u0932 \u0924\u094b\u0939\u0930\u0947 \u092c\u0930\u0947 \u092c\u0932\u094d\u0915\u093f \u092e\u094b\u0930\u0947 \u092c\u0930\u0947 \u092d\u0940 \u0909 \u092c\u0939\u0941\u0924 \u0915\u093e\u092e \u0915 \u0905\u0939\u0907\u0964"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u092e\u0907\u0901 \u0913\u0915\u093e \u092b\u093f\u0928 \u0938 \u0924\u094b\u0939\u0930\u0947 \u0932\u0917\u0947 \u092d\u0947\u091c\u0924 \u0939\u0909\u0901(\u092c\u0932\u094d\u0915\u093f \u092e\u094b\u0915\u093e \u0924\u0909 \u0915\u0939\u0907 \u091a\u093e\u0939\u0940 \u0905\u092a\u0928\u0947 \u0939\u093f\u0930\u0926\u0907 \u0915 \u0939\u0940 \u0924\u094b\u0939\u0930\u0947 \u0932\u0917\u0947 \u092d\u0947\u091c\u0924 \u0939\u0909\u0901)"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u092e\u0907\u0901 \u0913\u0915\u093e \u0907\u0939\u093e\u0901 \u0905\u092a\u0928\u0947 \u0932\u0917\u0947 \u0939\u0940 \u0930\u0916\u0907 \u091a\u093e\u0939\u0924 \u0930\u0939\u0947\u0909\u0901, \u0924\u093e\u0915\u093f \u0938\u0941\u0938\u092e\u093e\u091a\u093e\u0930 \u0915 \u092c\u0930\u0947 \u092e\u094b\u0939 \u092c\u0928\u094d\u0926\u0940 \u0915 \u0909 \u0924\u094b\u0939\u0930\u0940 \u0924\u0930\u092b \u0938 \u0938\u0947\u0935\u093e \u0915\u0907 \u0938\u0915\u0907\u0964"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u092e\u0941\u0932\u093e \u0924\u094b\u0939\u0930\u0940 \u0906\u091c\u094d\u091e\u093e \u0915 \u092c\u093f\u0928\u093e \u092e\u0907\u0901 \u0915\u091b\u0942 \u092d\u0940 \u0915\u0930\u0907 \u0928\u093e\u0939\u0940 \u091a\u093e\u0939\u093f\u0924 \u0924\u093e\u0915\u093f \u0924\u094b\u0939\u093e\u0930 \u0907 \u092d\u0932\u093e\u0908 \u0926\u092c\u093e\u0935 \u0938 \u0928\u093e\u0939\u0940\u0902 \u092c\u0932\u094d\u0915\u093f \u0938\u094d\u0935\u0947\u091a\u094d\u091b\u093e \u0938 \u0939\u0940 \u0939\u094b\u0907\u0964"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0939\u094b\u0907 \u0938\u0915\u0924 \u0939 \u0915\u093f \u0913\u0915\u093e \u0925\u094b\u0921\u093c\u0947 \u0938\u092e\u0907 \u0915 \u092c\u0930\u0947 \u0924\u094b\u0939\u0938\u0947 \u0926\u0942\u0930 \u0930\u0939\u0907 \u0915 \u0915\u093e\u0930\u0923 \u0907\u0939\u0907 \u0939\u094b\u0907 \u0915\u093f \u0924\u0942 \u0913\u0915\u093e \u092b\u093f\u0928 \u0939\u092e\u0947\u0938\u093e \u092c\u0930\u0947 \u092a\u093e\u0907 \u0932\u094d\u092f\u093e\u0964"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0926\u093e\u0938 \u0915 \u0930\u0942\u092a\u0947 \u092e\u0901 \u0928\u093e\u0939\u0940\u0902, \u092c\u0932\u094d\u0915\u093f \u0926\u093e\u0938 \u0938 \u091c\u093f\u092f\u093e\u0926\u093e \u090f\u0915 \u092a\u094d\u0930\u093f\u092f \u092c\u0928\u094d\u0927\u0941 \u0915 \u0930\u0942\u092a\u0947 \u092e\u0901 \u092e\u0907\u0901 \u0913\u0938\u0947 \u092c\u0939\u0941\u0924 \u092a\u093f\u0930\u0947\u092e \u0915\u0930\u0924 \u0939\u0909\u0901 \u0915\u093f\u0928\u094d\u0924\u0941 \u0924\u0942 \u0913\u0915\u093e \u0905\u0909\u0930 \u091c\u093f\u092f\u093e\u0926\u093e \u092a\u093f\u0930\u0947\u092e \u0915\u0930\u092c\u094d\u092f\u093e\u0964 \u0915\u0947\u0935\u0932 \u090f\u0915 \u092e\u0928\u0941\u0937\u094d\u092f \u0915 \u0930\u0942\u092a \u092e\u0901 \u0939\u0940 \u0928\u093e\u0939\u0940\u0902 \u092c\u0932\u094d\u0915\u093f \u092a\u0930\u094d\u092d\u0942 \u092e\u0901 \u0938\u094d\u0925\u093f\u0924 \u090f\u0915 \u092c\u0928\u094d\u0927\u0942 \u0915 \u0930\u0942\u092a\u0947 \u092e\u0901 \u092d\u0940\u0964"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0924\u0909 \u092f\u0926\u093f \u0924\u0941\u092e \u092e\u094b\u0915\u093e \u0906\u092a\u0928 \u0926\u094b\u0938\u094d\u0924 \u0915 \u0930\u0942\u092a \u092e\u0901 \u0938\u094d\u0935\u0940\u0915\u093e\u0930 \u0915\u0930\u0924 \u0939, \u0924\u092c \u0909\u0928\u0947\u0938\u093f\u092e\u0941\u0938 \u0915 \u0935\u093e\u092a\u0938 \u0938\u094d\u0935\u0940\u0915\u093e\u0930 \u0915\u0907 \u0932\u094d\u092f\u093e\u0964 \u0913\u0915\u0930 \u0910\u0938\u093e \u0938\u0941\u0906\u0917\u0924 \u0915\u0930\u093e, \u091c\u0948\u0938\u093e \u0915\u093f \u0924\u0941\u092e \u092e\u094b\u0930 \u0938\u0941\u0906\u0917\u0924 \u0915\u093f\u0939\u093e \u0939\u0964"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0905\u0909\u0930 \u0905\u0917\u0930 \u0909 \u0924\u094b\u0939\u093e\u0930 \u0915\u0909\u0928\u094b \u092a\u094d\u0930\u0915\u093e\u0930 \u0938 \u0939\u093e\u0928\u093f \u092a\u0939\u0941\u0901\u091a\u093e\u090f\u0938 \u0905\u0925\u0935\u093e \u0915\u0909\u0928\u0947 \u0935\u0938\u094d\u0924\u0941 \u092c\u0930\u0947 \u0924\u094b\u0939\u093e\u0930 \u0915\u0930\u094d\u091c\u0940\u0926\u093e\u0930 \u0905\u0939\u0907\u0964 \u0924\u0909 \u0913\u0915\u093e \u0939\u092e\u0930\u0947 \u0916\u093e\u0924\u093e \u092e\u0901 \u0932\u093f\u0916 \u0926\u094d\u092f\u093e\u0964"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u092e\u0907\u0901 \u092a\u094c\u0932\u0941\u0938 \u0916\u0941\u0926 \u0906\u092a\u0928 \u0926\u0938\u094d\u0924\u0916\u0924 \u0938 \u0907\u0939\u0907 \u0932\u093f\u0916\u0924 \u0939\u0909\u0901\u0964 \u0913\u0915\u0930 \u092d\u0930\u092a\u093e\u0908 \u0924\u094b\u0939\u0915\u093e \u092e\u0907\u0901 \u0915\u0930\u092c\u0907\u0964 \u092e\u094b\u0915\u093e \u0907 \u092c\u0924\u093e\u0935\u0907 \u0915 \u091c\u0930\u0942\u0930\u0924 \u0928\u093e\u0939\u0940\u0902 \u092c\u093e \u0915\u093f \u0924\u094b\u0930 \u0938\u092e\u094d\u092a\u0942\u0930\u094d\u0923 \u091c\u0940\u0935\u0928 \u0939\u0940 \u092e\u094b\u0930 \u090b\u0923\u0940 \u092c\u093e\u0964"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0939\u093e\u0901 \u092d\u093e\u0908 \u092e\u094b\u0915\u093e \u092a\u0930\u094d\u092d\u0942 \u092e\u0901 \u0924\u094b\u0939\u0938\u0947 \u0907 \u0932\u093e\u092d \u092a\u0939\u0941\u0902\u091a\u0907 \u0915\u093f \u092e\u0938\u0940\u0939 \u092e\u0901 \u092e\u094b\u0930 \u0939\u093f\u0930\u0926\u092f \u0939\u0930\u093e \u092d\u0930\u093e \u0939\u094b\u0907 \u091c\u093e\u0907\u0964"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u0924\u094b\u0939 \u092a\u0907 \u092c\u093f\u0938\u0935\u093e\u0938 \u0930\u0916\u0924 \u0907 \u091a\u093f\u0920\u094d\u091f\u0940 \u092e\u0907\u0901 \u0924\u094b\u0939\u0915\u093e \u0932\u093f\u0916\u0924 \u0939\u0909\u0901\u0964 \u092e\u0907\u0901 \u091c\u093e\u0928\u0924 \u0939\u0909\u0901 \u0915\u093f \u0924\u094b\u0939\u0938\u0947 \u092e\u0907\u0901 \u091c\u0947\u0924\u0928\u093e \u0915\u0939\u0924 \u0939\u0909\u0901, \u0924\u0942 \u0913\u0938\u0947 \u0915\u0939\u0942\u0901 \u091c\u093f\u092f\u093e\u0926\u093e \u0915\u0930\u092c\u094d\u092f\u093e\u0964"
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u0938\u093e\u0925 \u0939\u0940 \u092e\u0901 \u092e\u094b\u0930\u0947 \u092c\u0930\u0947 \u0928\u093f\u0935\u093e\u0938 \u0915\u093e \u092d\u0940 \u092a\u094d\u0930\u092c\u0928\u094d\u0927 \u0915\u0930\u093e, \u0915\u093e\u0939\u0947\u0915\u093f \u092e\u094b\u0930 \u0906\u0938\u093e \u0905\u0939\u0907 \u0915\u093f \u0924\u094b\u0939\u093e\u0930 \u0938\u092c \u092a\u0930\u093e\u0925\u0928\u093e \u0915 \u092a\u0930\u0925\u0928\u093e\u0935\u0928 \u0926\u094d\u0935\u093e\u0930\u093e \u092e\u0907\u0901 \u0924\u094b\u0939\u0915\u093e \u0938\u094c\u0902\u092a \u0926\u093f\u092f\u093e \u0928\u093e\u0909\u0901\u0964"
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u0905\u0928\u094d\u0924\u093f\u092e \u0909\u092a\u0939\u093e\u0930 - \u092e\u0938\u0940\u0939 \u0908\u0938\u0942 \u092e\u0901 \u0930\u0939\u0908 \u0915 \u092e\u094b\u0930 \u0938\u093e\u0925\u0940 \u092c\u0928\u094d\u0926\u0940 \u0907\u092a\u092b\u094d\u0930\u093e\u0938 \u0915 \u0924\u094b\u0939\u0947 \u0938\u092c\u0928 \u0915 \u0928\u092e\u0938\u094d\u0915\u093e\u0930\u0964"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u092e\u094b\u0930 \u0938\u093e\u0925\u0940 \u0915\u093e\u0930\u094d\u092f\u0915\u0930\u094d\u0924\u093e \u092e\u0930\u0915\u0941\u0938, \u0905\u0930\u093f\u0938\u094d\u0924\u0930\u094d\u0916\u0941\u0938, \u0926\u0947\u092e\u093e\u0938 \u0905\u0909\u0930 \u0932\u0942\u0915\u093e \u0915 \u0924\u094b\u0939\u0947 \u0928\u092e\u0938\u094d\u0915\u093e\u0930 \u092a\u0939\u0941\u0901\u091a\u0907\u0964"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u092a\u0930\u094d\u092d\u0942 \u0908\u0938\u0942 \u092e\u0938\u0940\u0939 \u0915 \u0905\u0928\u0941\u0917\u094d\u0930\u0939 \u0924\u094b\u0939\u0930\u0947 \u0906\u0924\u093f\u092e\u093e \u0915 \u0938\u093e\u0925 \u0930\u0939\u0907\u0964"
                }
            }
        }
    }
