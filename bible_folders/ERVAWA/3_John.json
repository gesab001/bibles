{
    "version": "ERVAWA",
    "book_name": "3 John",
    "book_nr": "64",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u092e\u0907\u0901 \u092c\u0941\u091c\u0941\u0930\u094d\u0917 \u0915\u0907\u0901\u0924\u0940 \u0938: \u092a\u093f\u0906\u0930\u0947 \u092c\u0928\u094d\u0927\u0941, \u0917\u092f\u0941\u0938 \u0915 \u0928\u093e\u0909\u0901 \u091c\u0947\u0938\u0947 \u092e\u0907\u0901 \u0938\u0924\u094d\u092f \u092e\u0901 \u0938\u0939\u092d\u093e\u0917\u0940 \u0915 \u0930\u0942\u092a \u092e\u0901 \u092a\u093f\u0930\u0947\u092e \u0930\u0916\u0924 \u0939\u0909\u0901\u0964"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u092e\u094b\u0930 \u092a\u093f\u0906\u0930\u0947 \u092c\u0928\u094d\u0927\u0941, \u092e\u0907\u0901 \u092a\u0930\u093e\u0925\u0928\u093e \u0915\u0930\u0924 \u0905\u0939\u0909\u0901 \u0915\u093f \u0924\u0942 \u091c\u0907\u0938\u0947\u0928 \u0906\u0927\u094d\u092f\u093e\u0924\u094d\u092e\u093f\u0915 \u0930\u0942\u092a \u0938 \u0909\u0928\u094d\u0928\u0924\u093f \u0915\u0930\u0924 \u0905\u0939\u093e, \u0935\u0907\u0938\u0947\u0928 \u0924\u0942 \u0938\u092c \u092c\u093e\u0924\u0928 \u092e\u0901 \u0909\u0928\u094d\u0928\u0924\u093f \u0915\u0930\u0924 \u0930\u0939\u093e \u0905\u0909\u0930 \u0938\u094d\u0935\u0938\u094d\u0925 \u0930\u0939\u093e\u0964"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u091c\u092c \u0915\u091b\u0942\u00a0\u092d\u093e\u0907\u092f\u0928 \u0906\u0907\u0915\u0947 \u092e\u094b\u0915\u093e \u0924\u094b\u0939\u0930\u0940 \u0938\u091a\u094d\u091a\u093e\u0908 \u092e\u0901 \u092c\u093f\u0938\u0935\u093e\u0938 \u0915 \u092c\u093e\u0935\u0924 \u092c\u0924\u093e\u090f\u0928 \u0924\u0909 \u092e\u0907\u0901 \u092c\u0939\u0941\u0924 \u0916\u0941\u0938 \u092d\u090f\u0909\u0901\u0964 \u0909 \u092a\u091a\u0947 \u092e\u094b\u0915\u093e \u092c\u0924\u093e\u090f\u0928\u0947 \u0915\u093f \u0924\u0942 \u092a\u091a\u0947 \u0915\u0907\u0938\u0947 \u0938\u091a\u094d\u091a\u093e\u0908 \u0915 \u0930\u093e\u0938\u094d\u0924\u093e \u092a \u091a\u0932\u0924 \u0905\u0939\u093e\u0964"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u092e\u094b\u0930\u0947 \u092c\u0930\u0947 \u090f\u0939\u0938\u0947 \u092c\u0922\u093c\u0915\u0930 \u0905\u0909\u0930 \u0906\u0928\u0928\u094d\u0926 \u0915\u0940 \u092c\u093e\u0924 \u0928\u093e\u0939\u0940\u0902 \u092c\u093e\u091f\u0907 \u0915\u093f \u0907 \u0938\u0941\u0928\u0909\u0901 \u0915\u093f \u092e\u094b\u0930 \u092c\u091a\u0935\u0928 \u0938\u091a\u094d\u091a\u093e\u0908 \u0915 \u0930\u093e\u0938\u0924\u093e \u092a \u091a\u0932\u0924 \u0905\u0939\u0907\u0901\u0964"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u092e\u094b\u0930\u093e \u092a\u093f\u092f\u093e\u0930\u0947 \u092c\u0928\u094d\u0927\u0941, \u0924\u0942 \u092e\u094b\u0930\u0947 \u092d\u093e\u0907\u092f\u0928 \u092c\u0930\u0947 \u091c\u0909\u0928 \u0915\u0930\u0924 \u0930\u0939\u0947 \u0905\u0939\u093e, \u0913\u0915\u093e \u092c\u093f\u0938\u0935\u093e\u0938\u092f\u094b\u0917\u094d\u092f\u0924\u093e \u0938 \u092a\u0942\u0930\u093e \u0915\u0930\u0924 \u0905\u0939\u093e \u0905\u0909\u0930 \u0935\u093f\u0938\u0947\u0938\u0915\u0930 \u091c\u092c \u0909 \u092a\u091a\u0947 \u092a\u0930\u0926\u0947\u0938\u0940 \u0905\u0939\u0907\u0901\u0964"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u092e\u0941\u0932\u093e \u091c\u0909\u0928 \u092a\u093f\u0930\u0947\u092e \u0924\u0942 \u0913\u0928\u0915\u0947 \u090a\u092a\u0930 \u0926\u0930\u0938\u093e\u090f \u0905\u0939\u093e, \u0913\u0915\u0930\u0940 \u0938\u093e\u091a\u094d\u091b\u0940 \u0909 \u092a\u091a\u0947 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0915 \u0938\u093e\u092e\u0928\u0947 \u0926\u093f\u0939\u0947\u0928\u0964 \u0913\u0928\u0915\u0947 \u092f\u093e\u0924\u094d\u0930\u093e \u091c\u093e\u0930\u0940 \u0930\u0916\u0907 \u092c\u0930\u0947 \u0913\u0928\u0947\u0915 \u0935\u0907\u0938\u0947\u0928 \u0938\u0939\u093e\u092f\u0924\u093e \u0915\u0930\u0924 \u0930\u0939\u093e, \u091c\u0907\u0938\u0947\u0928 \u092a\u0930\u092e\u0947\u0938\u094d\u0938\u0930 \u092c\u0924\u093e\u090f \u0905\u0939\u0907\u0964"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u092e\u0938\u0940\u0939 \u0915 \u0938\u0947\u0935\u093e \u092c\u0930\u0947 \u0909 \u092a\u091a\u0947 \u092f\u093e\u0924\u094d\u0930\u093e \u092a \u0928\u093f\u0915\u0932 \u092a\u0921\u093c\u093e \u0905\u0939\u0907\u0901 \u0905\u0909\u0930 \u0909 \u092a\u091a\u0947 \u0905\u092c\u093f\u0938\u0935\u093e\u0938\u0940 \u0932\u094b\u0917\u0928 \u0938 \u0915\u0909\u0928\u0909 \u092e\u0926\u0926 \u0928\u093e\u0939\u0940\u0902 \u0932\u093f\u090f \u0905\u0939\u0907\u0901\u0964"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0907\u0939\u0940 \u092c\u0930\u0947 \u0939\u092e \u092a\u091a\u0947 \u0915 \u0905\u0907\u0938\u0947 \u092e\u0928\u0907\u092f\u0928 \u0915 \u0938\u0939\u093e\u092f\u0924\u093e \u0915\u0930\u0907 \u091a\u093e\u0939\u0940, \u091c\u0907\u0938\u0947\u0928 \u0915\u093f \u0939\u092e\u0939\u0942\u0901 \u0938\u091a\u094d\u091a\u093e\u0908 \u092c\u0930\u0947 \u0938\u0939\u0915\u0930\u094d\u092e\u0940 \u0939\u094b\u0907 \u0938\u0915\u0940\u0964"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u090f\u0915 \u0920\u0941 \u091a\u093f\u0920\u094d\u0920\u0940 \u092e\u0907\u0901 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0915 \u0932\u093f\u0916\u0947 \u0930\u0939\u0947\u0909\u0901 \u092e\u0941\u0932\u093e \u0926\u093f\u092f\u0941\u0924\u094d\u0930\u093f\u092b\u0947\u0938 \u091c\u0909\u0928 \u0913\u0928\u0915\u0930 \u092a\u094d\u0930\u092e\u0941\u0916 \u092c\u0928\u0907 \u0915 \u0932\u093e\u0932\u0938\u093e \u0930\u0916\u0924 \u0939\u0964 \u0909 \u092e\u094b\u0930\u0947 \u0938\u092c\u0915 \u092c\u0924\u093e\u0908 \u092c\u093e\u0924\u0928 \u0915 \u0928 \u092e\u093e\u0928\u0940\u0964"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0907\u0939\u0907 \u0915\u093e\u0930\u0923 \u0905\u0939\u0907 \u091c\u0926\u093f \u092e\u0907\u0901 \u0906\u0935\u0909\u0901 \u0924\u0909 \u0913\u0915\u0930 \u0913\u0928 \u0915\u0930\u092e\u0928 \u0915\u093f \u091c\u0909\u0928 \u0909 \u0915\u0930\u0924 \u0939, \u092f\u093e\u0926 \u0926\u093f\u092f\u093e\u0909\u092c\u0964 \u0935\u0939 \u0905\u0928\u0941\u091a\u093f\u0924 \u0930\u0942\u092a \u0938 \u092e\u094b\u0930\u0947 \u0916\u093f\u0932\u093e\u092b \u092c\u0941\u0930\u0940 \u092c\u0941\u0930\u0940 \u092c\u093e\u0924 \u0915\u0939\u093f\u0915\u0947 \u0926\u094b\u0916 \u0932\u0917\u093e\u0935\u0924 \u0939; \u0905\u0909\u0930 \u0907\u0924\u0928\u0947 \u090d\u0924\u0928\u0947 \u0938 \u0939\u0940 \u0909 \u0938\u0902\u0924\u0941\u0938\u094d\u091f \u0928\u093e\u0930\u094d\u0939\u0940 \u0905\u0939\u0907, \u0909 \u0929 \u0924\u0909 \u0916\u0941\u0926 \u092d\u093e\u0907\u092f\u0928 \u0915 \u0938\u094d\u0935\u093e\u0917\u0924 \u0915\u0930\u0924 \u0939 \u0905\u0909\u0930 \u091c\u0909\u0928 \u0938\u094d\u0935\u093e\u0917\u0924 \u0915\u0930\u0907 \u091a\u093e\u0939\u0924 \u0939 \u0913\u0915\u093e \u092e\u0928\u093e \u0915\u0930\u0924 \u0939 \u0905\u0909\u0930 \u0915\u0932\u0940\u0938\u093f\u092f\u093e \u0938 \u0928\u093f\u0915\u093e\u0930 \u0926\u0947\u0924 \u0939\u092f\u0964"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u092a\u093f\u0906\u0930\u0947 \u092c\u0928\u094d\u0927\u0941, \u092c\u0941\u0930\u0947 \u0909\u0926\u093e\u0939\u0930\u0923 \u0915 \u0928\u093e\u0939\u0940\u0902 \u0935\u0930\u0928 \u092d\u0932\u093e\u0908 \u0915 \u0905\u0928\u0941\u0915\u0930\u0923 \u0915\u0930\u093e\u0964 \u091c\u0909\u0928 \u092e\u0928\u0908 \u092d\u0932\u093e\u0908 \u0915\u0930\u0924 \u0939, \u0909 \u092a\u0930\u092e\u0947\u0938\u094d\u0938\u0930 \u0915 \u0905\u0939\u0907! \u0909 \u091c\u0909\u0928 \u092c\u0941\u0930\u093e\u0908 \u0915\u0930\u0924 \u0939 \u0909 \u092a\u0930\u092e\u0947\u0938\u094d\u0938\u0930 \u0915 \u0928\u093e\u0939\u0940\u0902 \u0926\u0947\u0916\u0947\u0938\u0964"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0926\u093f\u092e\u0947\u0924\u094d\u0930\u093f\u092f\u0941\u0938 \u0915 \u092c\u093e\u0935\u0924 \u0938\u092c \u0905\u091a\u094d\u091b\u0940 \u092c\u093e\u0924 \u0915\u0939\u0924 \u0905\u0939\u0907\u0901\u0964 \u0939\u093f\u0906\u0901 \u0924\u0915 \u0915\u093f \u0916\u0941\u0926\u0907 \u0938\u091a\u094d\u091a\u093e\u0908 \u0913\u0915\u0930\u0947 \u092c\u093e\u0930\u0947 \u092e\u0901 \u0915\u0939\u0924 \u0939\u0964 \u0939\u092e\u0939\u0942\u0901 \u0913\u0928\u0947\u0915 \u092c\u093e\u0935\u0924 \u0905\u091a\u094d\u091b\u0940 \u092c\u093e\u0924 \u0915\u0939\u093f\u0924 \u0939\u0964 \u0905\u0909\u0930 \u0924\u0942 \u091c\u093e\u0928\u0924 \u0905\u0939\u093e \u0915\u093f \u091c\u0909\u0928 \u0939\u092e \u0915\u0939\u093f\u0924 \u0905\u0939\u0940 \u0907 \u0938\u091a \u092c\u093e\u091f\u0907\u0964"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0924\u094b\u0939\u0915\u093e \u0938\u092c\u0928 \u0915 \u0932\u093f\u0916\u0907 \u0915 \u0935\u093e\u0938\u094d\u0924\u0947 \u092e\u094b\u0930\u0947 \u092a\u093e\u0938 \u092c\u0939\u0941\u0924 \u092c\u093e\u0924\u0928 \u0905\u0939\u0907\u0901, \u092e\u0941\u0932\u093e \u092e\u0907\u0901 \u0915\u0932\u092e \u0905\u0909\u0930 \u0938\u093f\u092f\u093e\u0939\u0940 \u0938 \u0913\u0915\u093e \u0938\u092c \u0932\u093f\u0916\u0907 \u0928\u093e\u0939\u0940\u0902 \u091a\u093e\u0939\u093f\u0924\u0964"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u092e\u0941\u0932\u093e \u092e\u094b\u0915\u093e \u0924\u0909 \u0907 \u0906\u0938\u093e \u0905\u0939\u0907 \u0915\u093f \u092e\u0907\u0901 \u091c\u0932\u094d\u0926\u0940 \u0924\u094b\u0939\u0938\u0947 \u092e\u093f\u0932\u092c\u0907\u0964 \u0924\u0909 \u0939\u092e \u0906\u092a\u0941\u0938 \u092e\u0901 \u092c\u093e\u0924 \u0915\u0930\u092c\u0964"
                }
            }
        }
    }
}