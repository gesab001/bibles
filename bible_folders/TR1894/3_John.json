        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u03bf \u03c0\u03c1\u03b5\u03c3\u03b2\u03c5\u03c4\u03b5\u03c1\u03bf\u03c2 \u03b3\u03b1\u03b9\u03c9 \u03c4\u03c9 \u03b1\u03b3\u03b1\u03c0\u03b7\u03c4\u03c9 \u03bf\u03bd \u03b5\u03b3\u03c9 \u03b1\u03b3\u03b1\u03c0\u03c9 \u03b5\u03bd \u03b1\u03bb\u03b7\u03b8\u03b5\u03b9\u03b1"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u03b1\u03b3\u03b1\u03c0\u03b7\u03c4\u03b5 \u03c0\u03b5\u03c1\u03b9 \u03c0\u03b1\u03bd\u03c4\u03c9\u03bd \u03b5\u03c5\u03c7\u03bf\u03bc\u03b1\u03b9 \u03c3\u03b5 \u03b5\u03c5\u03bf\u03b4\u03bf\u03c5\u03c3\u03b8\u03b1\u03b9 \u03ba\u03b1\u03b9 \u03c5\u03b3\u03b9\u03b1\u03b9\u03bd\u03b5\u03b9\u03bd \u03ba\u03b1\u03b8\u03c9\u03c2 \u03b5\u03c5\u03bf\u03b4\u03bf\u03c5\u03c4\u03b1\u03b9 \u03c3\u03bf\u03c5 \u03b7 \u03c8\u03c5\u03c7\u03b7"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u03b5\u03c7\u03b1\u03c1\u03b7\u03bd \u03b3\u03b1\u03c1 \u03bb\u03b9\u03b1\u03bd \u03b5\u03c1\u03c7\u03bf\u03bc\u03b5\u03bd\u03c9\u03bd \u03b1\u03b4\u03b5\u03bb\u03c6\u03c9\u03bd \u03ba\u03b1\u03b9 \u03bc\u03b1\u03c1\u03c4\u03c5\u03c1\u03bf\u03c5\u03bd\u03c4\u03c9\u03bd \u03c3\u03bf\u03c5 \u03c4\u03b7 \u03b1\u03bb\u03b7\u03b8\u03b5\u03b9\u03b1 \u03ba\u03b1\u03b8\u03c9\u03c2 \u03c3\u03c5 \u03b5\u03bd \u03b1\u03bb\u03b7\u03b8\u03b5\u03b9\u03b1 \u03c0\u03b5\u03c1\u03b9\u03c0\u03b1\u03c4\u03b5\u03b9\u03c2"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u03bc\u03b5\u03b9\u03b6\u03bf\u03c4\u03b5\u03c1\u03b1\u03bd \u03c4\u03bf\u03c5\u03c4\u03c9\u03bd \u03bf\u03c5\u03ba \u03b5\u03c7\u03c9 \u03c7\u03b1\u03c1\u03b1\u03bd \u03b9\u03bd\u03b1 \u03b1\u03ba\u03bf\u03c5\u03c9 \u03c4\u03b1 \u03b5\u03bc\u03b1 \u03c4\u03b5\u03ba\u03bd\u03b1 \u03b5\u03bd \u03b1\u03bb\u03b7\u03b8\u03b5\u03b9\u03b1 \u03c0\u03b5\u03c1\u03b9\u03c0\u03b1\u03c4\u03bf\u03c5\u03bd\u03c4\u03b1"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u03b1\u03b3\u03b1\u03c0\u03b7\u03c4\u03b5 \u03c0\u03b9\u03c3\u03c4\u03bf\u03bd \u03c0\u03bf\u03b9\u03b5\u03b9\u03c2 \u03bf \u03b5\u03b1\u03bd \u03b5\u03c1\u03b3\u03b1\u03c3\u03b7 \u03b5\u03b9\u03c2 \u03c4\u03bf\u03c5\u03c2 \u03b1\u03b4\u03b5\u03bb\u03c6\u03bf\u03c5\u03c2 \u03ba\u03b1\u03b9 \u03b5\u03b9\u03c2 \u03c4\u03bf\u03c5\u03c2 \u03be\u03b5\u03bd\u03bf\u03c5\u03c2"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u03bf\u03b9 \u03b5\u03bc\u03b1\u03c1\u03c4\u03c5\u03c1\u03b7\u03c3\u03b1\u03bd \u03c3\u03bf\u03c5 \u03c4\u03b7 \u03b1\u03b3\u03b1\u03c0\u03b7 \u03b5\u03bd\u03c9\u03c0\u03b9\u03bf\u03bd \u03b5\u03ba\u03ba\u03bb\u03b7\u03c3\u03b9\u03b1\u03c2 \u03bf\u03c5\u03c2 \u03ba\u03b1\u03bb\u03c9\u03c2 \u03c0\u03bf\u03b9\u03b7\u03c3\u03b5\u03b9\u03c2 \u03c0\u03c1\u03bf\u03c0\u03b5\u03bc\u03c8\u03b1\u03c2 \u03b1\u03be\u03b9\u03c9\u03c2 \u03c4\u03bf\u03c5 \u03b8\u03b5\u03bf\u03c5"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u03c5\u03c0\u03b5\u03c1 \u03b3\u03b1\u03c1 \u03c4\u03bf\u03c5 \u03bf\u03bd\u03bf\u03bc\u03b1\u03c4\u03bf\u03c2 \u03b1\u03c5\u03c4\u03bf\u03c5 \u03b5\u03be\u03b7\u03bb\u03b8\u03bf\u03bd \u03bc\u03b7\u03b4\u03b5\u03bd \u03bb\u03b1\u03bc\u03b2\u03b1\u03bd\u03bf\u03bd\u03c4\u03b5\u03c2 \u03b1\u03c0\u03bf \u03c4\u03c9\u03bd \u03b5\u03b8\u03bd\u03c9\u03bd"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u03b7\u03bc\u03b5\u03b9\u03c2 \u03bf\u03c5\u03bd \u03bf\u03c6\u03b5\u03b9\u03bb\u03bf\u03bc\u03b5\u03bd \u03b1\u03c0\u03bf\u03bb\u03b1\u03bc\u03b2\u03b1\u03bd\u03b5\u03b9\u03bd \u03c4\u03bf\u03c5\u03c2 \u03c4\u03bf\u03b9\u03bf\u03c5\u03c4\u03bf\u03c5\u03c2 \u03b9\u03bd\u03b1 \u03c3\u03c5\u03bd\u03b5\u03c1\u03b3\u03bf\u03b9 \u03b3\u03b9\u03bd\u03c9\u03bc\u03b5\u03b8\u03b1 \u03c4\u03b7 \u03b1\u03bb\u03b7\u03b8\u03b5\u03b9\u03b1"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u03b5\u03b3\u03c1\u03b1\u03c8\u03b1 \u03c4\u03b7 \u03b5\u03ba\u03ba\u03bb\u03b7\u03c3\u03b9\u03b1 \u03b1\u03bb\u03bb \u03bf \u03c6\u03b9\u03bb\u03bf\u03c0\u03c1\u03c9\u03c4\u03b5\u03c5\u03c9\u03bd \u03b1\u03c5\u03c4\u03c9\u03bd \u03b4\u03b9\u03bf\u03c4\u03c1\u03b5\u03c6\u03b7\u03c2 \u03bf\u03c5\u03ba \u03b5\u03c0\u03b9\u03b4\u03b5\u03c7\u03b5\u03c4\u03b1\u03b9 \u03b7\u03bc\u03b1\u03c2"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u03b4\u03b9\u03b1 \u03c4\u03bf\u03c5\u03c4\u03bf \u03b5\u03b1\u03bd \u03b5\u03bb\u03b8\u03c9 \u03c5\u03c0\u03bf\u03bc\u03bd\u03b7\u03c3\u03c9 \u03b1\u03c5\u03c4\u03bf\u03c5 \u03c4\u03b1 \u03b5\u03c1\u03b3\u03b1 \u03b1 \u03c0\u03bf\u03b9\u03b5\u03b9 \u03bb\u03bf\u03b3\u03bf\u03b9\u03c2 \u03c0\u03bf\u03bd\u03b7\u03c1\u03bf\u03b9\u03c2 \u03c6\u03bb\u03c5\u03b1\u03c1\u03c9\u03bd \u03b7\u03bc\u03b1\u03c2 \u03ba\u03b1\u03b9 \u03bc\u03b7 \u03b1\u03c1\u03ba\u03bf\u03c5\u03bc\u03b5\u03bd\u03bf\u03c2 \u03b5\u03c0\u03b9 \u03c4\u03bf\u03c5\u03c4\u03bf\u03b9\u03c2 \u03bf\u03c5\u03c4\u03b5 \u03b1\u03c5\u03c4\u03bf\u03c2 \u03b5\u03c0\u03b9\u03b4\u03b5\u03c7\u03b5\u03c4\u03b1\u03b9 \u03c4\u03bf\u03c5\u03c2 \u03b1\u03b4\u03b5\u03bb\u03c6\u03bf\u03c5\u03c2 \u03ba\u03b1\u03b9 \u03c4\u03bf\u03c5\u03c2 \u03b2\u03bf\u03c5\u03bb\u03bf\u03bc\u03b5\u03bd\u03bf\u03c5\u03c2 \u03ba\u03c9\u03bb\u03c5\u03b5\u03b9 \u03ba\u03b1\u03b9 \u03b5\u03ba \u03c4\u03b7\u03c2 \u03b5\u03ba\u03ba\u03bb\u03b7\u03c3\u03b9\u03b1\u03c2 \u03b5\u03ba\u03b2\u03b1\u03bb\u03bb\u03b5\u03b9"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u03b1\u03b3\u03b1\u03c0\u03b7\u03c4\u03b5 \u03bc\u03b7 \u03bc\u03b9\u03bc\u03bf\u03c5 \u03c4\u03bf \u03ba\u03b1\u03ba\u03bf\u03bd \u03b1\u03bb\u03bb\u03b1 \u03c4\u03bf \u03b1\u03b3\u03b1\u03b8\u03bf\u03bd \u03bf \u03b1\u03b3\u03b1\u03b8\u03bf\u03c0\u03bf\u03b9\u03c9\u03bd \u03b5\u03ba \u03c4\u03bf\u03c5 \u03b8\u03b5\u03bf\u03c5 \u03b5\u03c3\u03c4\u03b9\u03bd \u03bf \u03b4\u03b5 \u03ba\u03b1\u03ba\u03bf\u03c0\u03bf\u03b9\u03c9\u03bd \u03bf\u03c5\u03c7 \u03b5\u03c9\u03c1\u03b1\u03ba\u03b5\u03bd \u03c4\u03bf\u03bd \u03b8\u03b5\u03bf\u03bd"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u03b4\u03b7\u03bc\u03b7\u03c4\u03c1\u03b9\u03c9 \u03bc\u03b5\u03bc\u03b1\u03c1\u03c4\u03c5\u03c1\u03b7\u03c4\u03b1\u03b9 \u03c5\u03c0\u03bf \u03c0\u03b1\u03bd\u03c4\u03c9\u03bd \u03ba\u03b1\u03b9 \u03c5\u03c0 \u03b1\u03c5\u03c4\u03b7\u03c2 \u03c4\u03b7\u03c2 \u03b1\u03bb\u03b7\u03b8\u03b5\u03b9\u03b1\u03c2 \u03ba\u03b1\u03b9 \u03b7\u03bc\u03b5\u03b9\u03c2 \u03b4\u03b5 \u03bc\u03b1\u03c1\u03c4\u03c5\u03c1\u03bf\u03c5\u03bc\u03b5\u03bd \u03ba\u03b1\u03b9 \u03bf\u03b9\u03b4\u03b1\u03c4\u03b5 \u03bf\u03c4\u03b9 \u03b7 \u03bc\u03b1\u03c1\u03c4\u03c5\u03c1\u03b9\u03b1 \u03b7\u03bc\u03c9\u03bd \u03b1\u03bb\u03b7\u03b8\u03b7\u03c2 \u03b5\u03c3\u03c4\u03b9\u03bd"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u03c0\u03bf\u03bb\u03bb\u03b1 \u03b5\u03b9\u03c7\u03bf\u03bd \u03b3\u03c1\u03b1\u03c6\u03b5\u03b9\u03bd \u03b1\u03bb\u03bb \u03bf\u03c5 \u03b8\u03b5\u03bb\u03c9 \u03b4\u03b9\u03b1 \u03bc\u03b5\u03bb\u03b1\u03bd\u03bf\u03c2 \u03ba\u03b1\u03b9 \u03ba\u03b1\u03bb\u03b1\u03bc\u03bf\u03c5 \u03c3\u03bf\u03b9 \u03b3\u03c1\u03b1\u03c8\u03b1\u03b9"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u03b5\u03bb\u03c0\u03b9\u03b6\u03c9 \u03b4\u03b5 \u03b5\u03c5\u03b8\u03b5\u03c9\u03c2 \u03b9\u03b4\u03b5\u03b9\u03bd \u03c3\u03b5 \u03ba\u03b1\u03b9 \u03c3\u03c4\u03bf\u03bc\u03b1 \u03c0\u03c1\u03bf\u03c2 \u03c3\u03c4\u03bf\u03bc\u03b1 \u03bb\u03b1\u03bb\u03b7\u03c3\u03bf\u03bc\u03b5\u03bd \u03b5\u03b9\u03c1\u03b7\u03bd\u03b7 \u03c3\u03bf\u03b9 \u03b1\u03c3\u03c0\u03b1\u03b6\u03bf\u03bd\u03c4\u03b1\u03b9 \u03c3\u03b5 \u03bf\u03b9 \u03c6\u03b9\u03bb\u03bf\u03b9 \u03b1\u03c3\u03c0\u03b1\u03b6\u03bf\u03c5 \u03c4\u03bf\u03c5\u03c2 \u03c6\u03b9\u03bb\u03bf\u03c5\u03c2 \u03ba\u03b1\u03c4 \u03bf\u03bd\u03bf\u03bc\u03b1"
                }
            }
        }
    }
}