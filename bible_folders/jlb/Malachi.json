                "4": {
                    "verse_nr": "4",
                    "verse": "\u305d\u306e\u6642\u3001\u4e3b\u306f\u307e\u305f\u4ee5\u524d\u306e\u3088\u3046\u306b\u3001 \u30e6\u30c0\u3068\u30a8\u30eb\u30b5\u30ec\u30e0\u306e\u4eba\u3005\u304c\u643a\u3048\u3066\u6765\u308b\u3055\u3055\u3052\u7269\u3092 \u559c\u3093\u3067\u53d7\u3051\u308b\u3002"
                },
                "5": {
                    "verse_nr": "5",
