        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u062e\u062f\u0627\u0648\u0646\u062f\u060c \u0622\u064a\u0646\u062f\u0647\u0654 \u0633\u0631\u0632\u0645\u064a\u0646 \u0627\u062f\u0648\u0645 \u0631\u0627 \u062f\u0631 \u0631\u0624\u064a\u0627\u064a\u06cc \u0628\u0647 \u0639\u0648\u0628\u062f\u064a\u0627 \u0646\u0634\u0627\u0646 \u062f\u0627\u062f.\n\u0627\u0632 \u062c\u0627\u0646\u0628 \u062e\u062f\u0627\u0648\u0646\u062f \u062e\u0628\u0631 \u0631\u0633\u064a\u062f\u0647 \u0643\u0647 \u0642\u0627\u0635\u062f\u06cc \u0628\u0627 \u0627\u064a\u0646 \u067e\u064a\u0627\u0645 \u0646\u0632\u062f \u0642\u0648\u0645\u0647\u0627 \u0641\u0631\u0633\u062a\u0627\u062f\u0647 \u0634\u062f\u0647 \u0627\u0633\u062a: \u00ab\u0622\u0645\u0627\u062f\u0647 \u0634\u0648\u064a\u062f \u062a\u0627 \u0628\u0647 \u062c\u0646\u06af \u0627\u062f\u0648\u0645 \u0628\u0631\u0648\u064a\u0645.\u00bb"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u0641\u0631\u0645\u0627\u064a\u062f: \u00ab\u0627\u06cc \u0627\u062f\u0648\u0645\u060c \u062a\u0648 \u0631\u0627 \u062f\u0631 \u0645\u064a\u0627\u0646 \u0642\u0648\u0645\u0647\u0627 \u062e\u0648\u0627\u0631 \u0648 \u0636\u0639\u064a\u0641 \u0645\u06cc\u200c\u0633\u0627\u0632\u0645."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u00ab\u0627\u0632 \u0627\u064a\u0646\u0643\u0647 \u0628\u0631 \u0635\u062e\u0631\u0647\u200c\u0647\u0627\u06cc \u0628\u0644\u0646\u062f \u0633\u0627\u0643\u0646 \u0647\u0633\u062a\u06cc \u0628\u0647 \u062e\u0648\u062f \u0645\u06cc\u200c\u0628\u0627\u0644\u06cc \u0648 \u0628\u0627 \u063a\u0631\u0648\u0631 \u0645\u06cc\u200c\u06af\u0648\u064a\u06cc: \u201d\u0643\u064a\u0633\u062a \u0643\u0647 \u062f\u0633\u062a\u0634 \u062f\u0631 \u0627\u064a\u0646 \u0628\u0644\u0646\u062f\u064a\u0647\u0627 \u0628\u0647 \u0645\u0646 \u0628\u0631\u0633\u062f!\u201c \u062e\u0648\u062f \u0631\u0627 \u06af\u0648\u0644 \u0646\u0632\u0646!"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0627\u06af\u0631 \u0647\u0645\u0686\u0648\u0646 \u0639\u0642\u0627\u0628 \u0628\u0647 \u0627\u0648\u062c \u0622\u0633\u0645\u0627\u0646\u0647\u0627 \u0628\u0631\u0648\u06cc \u0648 \u0622\u0634\u064a\u0627\u0646\u0647\u0654 \u062e\u0648\u062f \u0631\u0627 \u0628\u064a\u0646 \u0633\u062a\u0627\u0631\u06af\u0627\u0646 \u0628\u0631 \u067e\u0627 \u062f\u0627\u0631\u06cc\u060c \u062a\u0648 \u0631\u0627 \u0627\u0632 \u0622\u0646\u062c\u0627 \u0628\u0647 \u0632\u0645\u064a\u0646 \u0645\u06cc\u200c\u0622\u0648\u0631\u0645."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u00ab\u0627\u06af\u0631 \u062f\u0632\u062f\u0647\u0627 \u0634\u0628\u0627\u0646\u06af\u0627\u0647 \u0622\u0645\u062f\u0647 \u062a\u0648 \u0631\u0627 \u063a\u0627\u0631\u062a \u0645\u06cc\u200c\u0643\u0631\u062f\u0646\u062f \u0628\u0647 \u0645\u0631\u0627\u062a\u0628 \u0628\u0631\u0627\u06cc \u062a\u0648 \u0628\u0647\u062a\u0631 \u0645\u06cc\u200c\u0628\u0648\u062f\u060c \u0632\u064a\u0631\u0627 \u0647\u0645\u0647 \u0686\u064a\u0632 \u0631\u0627 \u0646\u0645\u06cc\u200c\u0628\u0631\u062f\u0646\u062f! \u064a\u0627 \u0627\u06af\u0631 \u0627\u0646\u06af\u0648\u0631\u0686\u064a\u0646\u0627\u0646 \u0628\u0647 \u0633\u0631\u0627\u063a \u062a\u0648 \u0645\u06cc\u200c\u0622\u0645\u062f\u0646\u062f \u067e\u0633 \u0627\u0632 \u0686\u064a\u062f\u0646 \u0627\u0646\u06af\u0648\u0631 \u062e\u0648\u0634\u0647\u200c\u0627\u06cc \u0686\u0646\u062f \u0628\u0627\u0642\u06cc \u0645\u06cc\u200c\u06af\u0630\u0627\u0634\u062a\u0646\u062f!"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0648\u0644\u06cc \u0648\u0642\u062a\u06cc \u0628\u0647 \u0686\u0646\u06af \u062f\u0634\u0645\u0646 \u0628\u064a\u0641\u062a\u06cc \u062a\u0645\u0627\u0645 \u062b\u0631\u0648\u062a \u062a\u0648 \u0628\u0647 \u064a\u063a\u0645\u0627 \u062e\u0648\u0627\u0647\u062f \u0631\u0641\u062a \u0648 \u0686\u064a\u0632\u06cc \u0628\u0631\u0627\u06cc \u062a\u0648 \u0628\u0627\u0642\u06cc \u0646\u062e\u0648\u0627\u0647\u062f \u0645\u0627\u0646\u062f."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u00ab\u062a\u0645\u0627\u0645 \u0647\u0645\u200c\u067e\u064a\u0645\u0627\u0646\u0627\u0646\u062a \u062f\u0634\u0645\u0646 \u062a\u0648 \u0645\u06cc\u200c\u0634\u0648\u0646\u062f \u0648 \u062f\u0633\u062a \u0628\u0647 \u062f\u0633\u062a \u0647\u0645 \u062f\u0627\u062f\u0647\u060c \u062a\u0648 \u0631\u0627 \u0627\u0632 \u0633\u0631\u0632\u0645\u064a\u0646\u062a \u0628\u064a\u0631\u0648\u0646 \u0645\u06cc\u200c\u0631\u0627\u0646\u0646\u062f. \u062f\u0648\u0633\u062a\u0627\u0646 \u0645\u0648\u0631\u062f \u0627\u0639\u062a\u0645\u0627\u062f\u062a\u060c \u0628\u0631\u0627\u064a\u062a \u062f\u0627\u0645 \u0645\u06cc\u200c\u06af\u0630\u0627\u0631\u0646\u062f \u0648 \u062a\u0648 \u0627\u0632 \u0622\u0646 \u0622\u06af\u0627\u0647 \u0646\u062e\u0648\u0627\u0647\u06cc \u0634\u062f.\u00bb"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u0641\u0631\u0645\u0627\u064a\u062f: \u00ab\u062f\u0631 \u0622\u0646 \u0631\u0648\u0632 \u062f\u0631 \u0633\u0631\u0627\u0633\u0631 \u0627\u062f\u0648\u0645 \u062d\u062a\u06cc \u064a\u06a9 \u0634\u062e\u0635 \u062f\u0627\u0646\u0627 \u0628\u0627\u0642\u06cc \u0646\u062e\u0648\u0627\u0647\u062f \u0645\u0627\u0646\u062f! \u0632\u064a\u0631\u0627 \u0645\u0646 \u0647\u0645\u0647\u0654 \u062f\u0627\u0646\u0627\u064a\u0627\u0646 \u0627\u062f\u0648\u0645 \u0631\u0627 \u0627\u0632 \u062d\u0645\u0627\u0642\u062a \u067e\u0631 \u0645\u06cc\u200c\u0633\u0627\u0632\u0645."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u062f\u0644\u064a\u0631\u062a\u0631\u064a\u0646 \u0633\u0631\u0628\u0627\u0632\u0627\u0646 \u062a\u064a\u0645\u0627\u0646\u060c \u0647\u0631\u0627\u0633\u0627\u0646 \u0648 \u062f\u0631\u0645\u0627\u0646\u062f\u0647 \u0634\u062f\u0647 \u0646\u062e\u0648\u0627\u0647\u0646\u062f \u062a\u0648\u0627\u0646\u0633\u062a \u0627\u0632 \u0643\u0634\u062a\u0627\u0631 \u062c\u0644\u0648\u06af\u064a\u0631\u06cc \u0643\u0646\u0646\u062f."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u00ab\u0628\u0647 \u0633\u0628\u0628 \u0638\u0644\u0645\u06cc \u0643\u0647 \u0628\u0647 \u0628\u0631\u0627\u062f\u0631 \u062e\u0648\u062f \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0643\u0631\u062f\u06cc \u0631\u0633\u0648\u0627 \u0648 \u0628\u0631\u0627\u06cc \u0647\u0645\u064a\u0634\u0647 \u0631\u064a\u0634\u0647\u200c\u0643\u0646 \u062e\u0648\u0627\u0647\u06cc \u0634\u062f\u061b"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0632\u064a\u0631\u0627 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0631\u0627 \u0628\u0647 \u0647\u0646\u06af\u0627\u0645 \u0633\u062e\u062a\u06cc \u0648 \u0627\u062d\u062a\u064a\u0627\u062c\u0634 \u062a\u0631\u06a9 \u0643\u0631\u062f\u06cc. \u0648\u0642\u062a\u06cc \u0643\u0647 \u0645\u0647\u0627\u062c\u0645\u0627\u0646\u060c \u062b\u0631\u0648\u062a \u0627\u0648 \u0631\u0627 \u063a\u0627\u0631\u062a \u0645\u06cc\u200c\u0643\u0631\u062f\u0646\u062f \u0648 \u0628\u0631 \u0627\u0648\u0631\u0634\u0644\u064a\u0645 \u0642\u0631\u0639\u0647 \u0627\u0646\u062f\u0627\u062e\u062a\u0647\u060c \u0622\u0646 \u0631\u0627 \u0645\u064a\u0627\u0646 \u062e\u0648\u062f \u062a\u0642\u0633\u064a\u0645 \u0645\u06cc\u200c\u0646\u0645\u0648\u062f\u0646\u062f\u060c \u062a\u0648 \u0643\u0646\u0627\u0631 \u0627\u064a\u0633\u062a\u0627\u062f\u0647\u060c \u0646\u062e\u0648\u0627\u0633\u062a\u06cc \u0647\u064a\u0686 \u0643\u0645\u0643\u06cc \u0628\u0647 \u0627\u0648 \u0628\u0643\u0646\u06cc \u0648 \u0645\u0627\u0646\u0646\u062f \u064a\u0643\u06cc \u0627\u0632 \u062f\u0634\u0645\u0646\u0627\u0646\u0634 \u0639\u0645\u0644 \u0646\u0645\u0648\u062f\u06cc."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u00ab\u062a\u0648 \u0646\u0645\u06cc\u200c\u0628\u0627\u064a\u0633\u062a \u0627\u064a\u0646 \u0643\u0627\u0631 \u0631\u0627 \u0645\u06cc\u200c\u0643\u0631\u062f\u06cc. \u0648\u0642\u062a\u06cc \u0643\u0647 \u0627\u0648 \u0631\u0627 \u0628\u0647 \u0633\u0631\u0632\u0645\u064a\u0646\u0647\u0627\u06cc \u0628\u064a\u06af\u0627\u0646\u0647 \u0645\u06cc\u200c\u0628\u0631\u062f\u0646\u062f \u062a\u0648 \u0646\u0645\u06cc\u200c\u0628\u0627\u064a\u0633\u062a \u0645\u06cc\u200c\u0646\u0634\u0633\u062a\u06cc \u0648 \u0627\u0648 \u0631\u0627 \u062a\u0645\u0627\u0634\u0627 \u0645\u06cc\u200c\u0643\u0631\u062f\u06cc. \u062f\u0631 \u0631\u0648\u0632 \u0645\u0635\u064a\u0628\u062a\u0634 \u0646\u0645\u06cc\u200c\u0628\u0627\u064a\u0633\u062a \u0634\u0627\u062f\u06cc \u0645\u06cc\u200c\u0643\u0631\u062f\u06cc \u0648 \u0632\u0645\u0627\u0646\u06cc \u0643\u0647 \u062f\u0631 \u0633\u062e\u062a\u06cc \u0628\u0648\u062f \u0646\u0645\u06cc\u200c\u0628\u0627\u064a\u0633\u062a \u0628\u0647 \u0627\u0648 \u0645\u06cc\u200c\u062e\u0646\u062f\u064a\u062f\u06cc."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0631\u0648\u0632\u06cc \u0643\u0647 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u06af\u0631\u0641\u062a\u0627\u0631 \u0627\u064a\u0646 \u0645\u0635\u064a\u0628\u062a \u0648 \u0628\u0644\u0627 \u0634\u062f\u0647 \u0628\u0648\u062f\u060c \u062a\u0648 \u0646\u064a\u0632 \u0628\u0647 \u0627\u0648 \u0628\u062f\u06cc \u0631\u0633\u0627\u0646\u062f\u06cc \u0648 \u0631\u0641\u062a\u0647\u060c \u063a\u0627\u0631\u062a\u0634 \u0643\u0631\u062f\u06cc."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0628\u0631 \u0633\u0631 \u0686\u0647\u0627\u0631\u0631\u0627\u0647\u200c\u0647\u0627 \u0627\u064a\u0633\u062a\u0627\u062f\u06cc \u0648 \u0643\u0633\u0627\u0646\u06cc \u0631\u0627 \u0643\u0647 \u0633\u0639\u06cc \u0645\u06cc\u200c\u0643\u0631\u062f\u0646\u062f \u0641\u0631\u0627\u0631 \u0643\u0646\u0646\u062f \u0643\u0634\u062a\u06cc. \u062f\u0631 \u0622\u0646 \u0632\u0645\u0627\u0646 \u0648\u062d\u0634\u062a \u0648 \u067e\u0631\u064a\u0634\u0627\u0646\u06cc\u060c \u0628\u0627\u0632\u0645\u0627\u0646\u062f\u06af\u0627\u0646 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0631\u0627 \u062f\u0633\u062a\u06af\u064a\u0631 \u0646\u0645\u0648\u062f\u0647\u060c \u062a\u062d\u0648\u064a\u0644 \u062f\u0634\u0645\u0646 \u062f\u0627\u062f\u06cc."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u00ab\u0645\u0646\u060c \u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0632\u0648\u062f\u06cc \u0627\u0632 \u062a\u0645\u0627\u0645 \u0642\u0648\u0645\u0647\u0627 \u0627\u0646\u062a\u0642\u0627\u0645 \u062e\u0648\u0627\u0647\u0645 \u0643\u0634\u064a\u062f. \u0627\u06cc \u0627\u062f\u0648\u0645\u060c \u0647\u0645\u0627\u0646\u0637\u0648\u0631 \u0643\u0647 \u0628\u0627 \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0631\u0641\u062a\u0627\u0631 \u0643\u0631\u062f\u06cc\u060c \u0628\u0627 \u062a\u0648 \u0646\u064a\u0632 \u0628\u0647 \u0647\u0645\u0627\u0646\u06af\u0648\u0646\u0647 \u0631\u0641\u062a\u0627\u0631 \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \u0647\u0631 \u0686\u0647 \u0643\u0631\u062f\u06cc \u0628\u0631 \u0633\u0631 \u062e\u0648\u062f\u062a \u062e\u0648\u0627\u0647\u062f \u0622\u0645\u062f."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0628\u0631 \u0628\u0627\u0644\u0627\u06cc \u0643\u0648\u0647 \u0645\u0642\u062f\u0633 \u0645\u0646\u060c \u062c\u0627\u0645 \u0645\u0643\u0627\u0641\u0627\u062a\u0645 \u0631\u0627 \u0646\u0648\u0634\u064a\u062f\u06cc\u060c \u0642\u0648\u0645\u0647\u0627\u06cc \u062f\u064a\u06af\u0631 \u0646\u064a\u0632 \u0622\u0646 \u0631\u0627 \u062e\u0648\u0627\u0647\u0646\u062f \u0646\u0648\u0634\u064a\u062f. \u0622\u0631\u06cc\u060c \u0622\u0646\u0647\u0627 \u062e\u0648\u0627\u0647\u0646\u062f \u0646\u0648\u0634\u064a\u062f \u0648 \u0627\u0632 \u0628\u064a\u0646 \u062e\u0648\u0627\u0647\u0646\u062f \u0631\u0641\u062a \u0648 \u0627\u062b\u0631\u06cc \u0627\u0632 \u0622\u0646\u0647\u0627 \u0628\u0627\u0642\u06cc \u0646\u062e\u0648\u0627\u0647\u062f \u0645\u0627\u0646\u062f."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u00ab\u0648\u0644\u06cc \u0643\u0648\u0647 \u0645\u0642\u062f\u0633 \u0645\u0646 \u062f\u0631 \u0627\u0648\u0631\u0634\u0644\u064a\u0645\u060c \u067e\u0646\u0627\u0647\u06af\u0627\u0647 \u0648 \u0645\u062d\u0644 \u0646\u062c\u0627\u062a \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \u0627\u0633\u0631\u0627\u0626\u064a\u0644 \u0633\u0631\u0632\u0645\u064a\u0646 \u062e\u0648\u062f \u0631\u0627 \u062f\u0648\u0628\u0627\u0631\u0647 \u062a\u0635\u0631\u0641 \u062e\u0648\u0627\u0647\u062f \u0646\u0645\u0648\u062f"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0648 \u0645\u0627\u0646\u0646\u062f \u0622\u062a\u0634\u060c \u0627\u062f\u0648\u0645 \u0631\u0627 \u062e\u0648\u0627\u0647\u062f \u0633\u0648\u0632\u0627\u0646\u062f \u0628\u0647 \u0637\u0648\u0631\u06cc \u0643\u0647 \u0627\u0632 \u0627\u062f\u0648\u0645 \u0643\u0633\u06cc \u0628\u0627\u0642\u06cc \u0646\u062e\u0648\u0627\u0647\u062f \u0645\u0627\u0646\u062f.\u00bb \u0627\u064a\u0646 \u0631\u0627 \u062e\u062f\u0627\u0648\u0646\u062f \u0645\u06cc\u200c\u0641\u0631\u0645\u0627\u064a\u062f."
