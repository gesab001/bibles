    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u123d\u121b\u130d\u120c\u12cd\u1364\n\u1260\u12a5\u12cd\u1290\u1275 \u1208\u121d\u12c8\u12f5\u12f0\u12cd \u1208\u12cd\u12f5 \u12c8\u12f3\u1304 \u1208\u130b\u12ed\u12ee\u1235\u1364"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u12c8\u12f3\u1305 \u1206\u12ed\u1364 \u1290\u134d\u1235\u1205 \u1260\u1218\u120d\u12ab\u121d \u1201\u1294\u1273 \u120b\u12ed \u12a5\u1295\u12f3\u1208\u127d \u1201\u1209 \u1218\u120d\u12ab\u121d \u1324\u1295\u1290\u1275 \u12a5\u1295\u12f2\u1296\u122d\u1205\u1293 \u1260\u1290\u1308\u122d \u1201\u1209 \u12a5\u1295\u12f2\u1233\u12ab\u120d\u1205 \u12a5\u1338\u120d\u12eb\u1208\u1201\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1208\u12a5\u12cd\u1290\u1275 \u1273\u121b\u129d \u12a5\u1295\u12f0 \u1206\u1295\u1205\u1293 \u12e8\u121d\u1275\u1218\u120b\u1208\u1230\u12cd\u121d \u1260\u12a5\u12cd\u1290\u1275 \u12a5\u1295\u12f0 \u1206\u1290 \u12a0\u1295\u12f3\u1295\u12f5 \u12c8\u1295\u12f5\u121e\u127d \u1218\u1325\u1270\u12cd \u1235\u1208 \u12a0\u1295\u1270 \u1232\u1218\u1230\u12ad\u1229\u120d\u129d \u12a5\u1305\u130d \u12f0\u1235 \u12a0\u1208\u129d\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u120d\u1306\u127c \u1260\u12a5\u12cd\u1290\u1275 \u12e8\u121a\u1218\u120b\u1208\u1231 \u1218\u1206\u1293\u1278\u12cd\u1295 \u12a8\u1218\u1235\u121b\u1275 \u12e8\u121a\u1260\u120d\u1325 \u12f0\u1235\u1273 \u12e8\u1208\u129d\u121d\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u12c8\u12f3\u1305 \u1206\u12ed\u1364 \u1208\u12a0\u1295\u1270 \u12a5\u1295\u130d\u12f6\u127d \u1262\u1206\u1291\u121d \u12a5\u1295\u12b3\u1363 \u1208\u12c8\u1295\u12f5\u121e\u127d \u1260\u121d\u1273\u12f0\u122d\u1308\u12cd \u1290\u1308\u122d \u1201\u1209 \u1273\u121b\u129d \u1290\u1205\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u12a5\u1290\u122d\u1231 \u12a0\u1295\u1270 \u1235\u120b\u1208\u1205 \u134d\u1245\u122d \u1260\u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1295 \u134a\u1275 \u1218\u1235\u12ad\u1228\u12cb\u120d\u1364 \u1260\u1218\u1295\u1308\u12f3\u1278\u12cd\u121d \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u12f0\u1235 \u1260\u121a\u12eb\u1230\u129d \u1201\u1294\u1273 \u1265\u1275\u1228\u12f3\u1278\u12cd \u1218\u120d\u12ab\u121d \u1273\u12f0\u122d\u130b\u1208\u1205\u1364"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u1235\u1208 \u1235\u1219 \u12e8\u12c8\u1321\u1275 \u12a8\u12a0\u1215\u12db\u1265 \u121d\u1295\u121d \u1233\u12ed\u1240\u1260\u1209 \u1290\u12cd\u1293\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u12a5\u1295\u130d\u12f2\u1205 \u12a0\u1265\u1228\u1295 \u1208\u12a5\u12cd\u1290\u1275 \u12a5\u1295\u12f5\u1295\u1220\u122b \u12a5\u1295\u12f2\u1205 \u12eb\u1209\u1275\u1295 \u1230\u12ce\u127d \u120d\u1293\u1235\u1270\u1293\u130d\u12f5 \u12ed\u1308\u1263\u1293\u120d\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u1208\u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1292\u1271 \u133d\u134c \u1290\u1260\u122d\u1364 \u1290\u1308\u122d \u130d\u1295 \u1218\u122a \u1218\u1206\u1295 \u12e8\u121a\u12c8\u12f5\u12f0\u12cd \u12f2\u12ee\u1325\u122b\u1322\u1235 \u12a0\u12ed\u1240\u1260\u1208\u1295\u121d\u1362"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u1235\u1208\u12da\u1205 \u12a5\u1294 \u12a8\u1218\u1323\u1201 \u12a5\u122d\u1231 \u1260\u12ad\u1349 \u1243\u120b\u1275 \u1235\u121b\u127d\u1295\u1295 \u1208\u121b\u1325\u134b\u1275 \u12e8\u121a\u12eb\u12f0\u122d\u1308\u12cd\u1295 \u1201\u1209 \u12ed\u134b \u12a0\u12c8\u1323\u1208\u1201\u1364 \u12ed\u1205\u121d \u12a0\u120d\u1260\u1243 \u1265\u120e\u1275 \u12c8\u1295\u12f5\u121e\u127d\u1295 \u12a0\u12ed\u1240\u1260\u120d\u121d\u1364 \u120a\u1240\u1260\u120f\u1278\u12cd \u12e8\u121a\u1348\u120d\u1309\u1275\u1295 \u12ed\u12a8\u1208\u12ad\u120b\u1278\u12cb\u120d\u1364 \u12a8\u1264\u1270 \u12ad\u122d\u1235\u1272\u12eb\u1295\u121d \u12eb\u1235\u12c8\u1323\u1278\u12cb\u120d\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u12c8\u12f3\u1305 \u1206\u12ed\u1364 \u1218\u120d\u12ab\u121d \u12e8\u1206\u1290\u12cd\u1295 \u12a5\u1295\u1302 \u12ad\u1349\u12cd\u1295 \u12a0\u1275\u121d\u1230\u120d\u1362 \u1218\u120d\u12ab\u121d \u12e8\u1206\u1290\u12cd\u1295 \u12e8\u121a\u12eb\u12f0\u122d\u130d \u1201\u1209 \u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1290\u12cd\u1362 \u12ad\u1349 \u12e8\u121a\u12eb\u12f0\u122d\u130d \u130d\u1295 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u12a0\u120b\u12e8\u12cd\u121d\u1362"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u1208\u12f5\u121c\u1325\u122e\u1235\u1363 \u1230\u12cd \u1201\u1209 \u12ed\u1218\u1230\u12ad\u122d\u1208\u1273\u120d\u1364 \u12a5\u12cd\u1290\u1275 \u122b\u1237\u121d \u1275\u1218\u1230\u12ad\u122d\u1208\u1273\u1208\u127d\u1364 \u12a5\u129b \u12f0\u130d\u121e \u12a5\u1295\u1218\u1230\u12ad\u122d\u1208\u1273\u1208\u1295\u1364 \u12a5\u1293\u1295\u1270\u121d \u121d\u1235\u12ad\u122d\u1290\u1273\u127d\u1295 \u12a5\u12cd\u1290\u1275 \u12a5\u1295\u12f0 \u1206\u1290 \u1273\u12cd\u1243\u120b\u127d\u1201\u1362"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u12e8\u121d\u133d\u134d\u120d\u1205 \u1265\u12d9 \u1290\u1308\u122d \u1290\u1260\u1228\u129d\u1364 \u12ed\u1201\u1295 \u12a5\u1295\u1302 \u1260\u1240\u1208\u121d\u1293 \u1260\u1265\u122d\u12d5 \u12a5\u1295\u12f2\u1206\u1295 \u12a0\u120d\u1348\u120d\u130d\u121d\u1362"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u1260\u1245\u122d\u1261 \u120b\u12ed\u1205 \u1270\u1235\u134b \u12a0\u12f0\u122d\u130b\u1208\u1201\u1364 \u134a\u1275 \u1208\u134a\u1275\u121d \u12a5\u1295\u1290\u130b\u1308\u122b\u1208\u1295\u1362"
                }
            }
        }
    }
}