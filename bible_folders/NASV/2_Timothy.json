        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u1260\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u1243\u120d \u1260\u1270\u1308\u1263\u12cd \u12e8\u1215\u12ed\u12c8\u1275 \u1270\u1235\u134b \u1218\u1220\u1228\u1275\u1363 \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1348\u1243\u12f5 \u12e8\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u1210\u12cb\u122d\u12eb \u12e8\u1206\u1290 \u1333\u12cd\u120e\u1235\u1364"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u1208\u1270\u12c8\u12f3\u1301 \u120d\u1304\u1363 \u1208\u1322\u121e\u1274\u12ce\u1235\u1364\n\u1338\u130b\u1363 \u121d\u1215\u1228\u1275\u1293 \u1230\u120b\u121d \u12a8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12a0\u1265\u1363 \u12a8\u130c\u1273\u127d\u1295\u121d \u12a8\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u12ed\u1201\u1295\u1362"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u1233\u120b\u124b\u122d\u1325 \u120c\u120a\u1275\u1293 \u1240\u1295 \u1260\u1338\u120e\u1274 \u1235\u1208 \u12a0\u1295\u1270 \u1233\u1235\u1265\u1363 \u1240\u12f0\u121d\u1275 \u12a0\u1263\u1276\u127d \u12a5\u1295\u12f3\u12f0\u1228\u1309\u1275 \u1260\u1295\u1339\u1215 \u1285\u120a\u1293 \u12e8\u121b\u1218\u120d\u12a8\u12cd\u1295 \u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u12a0\u1218\u1230\u130d\u1293\u1208\u1201\u1362"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u12a5\u1295\u1263\u1205\u1295 \u12a5\u12eb\u1235\u1273\u12c8\u1235\u1201 \u1260\u12f0\u1235\u1273 \u12a5\u121e\u120b \u12d8\u1295\u12f5 \u120b\u12ed\u1205 \u12a5\u1293\u134d\u1243\u1208\u1201\u1362"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u130d\u1265\u12dd\u1290\u1275 \u12e8\u120c\u1208\u1260\u1275 \u12a5\u121d\u1290\u1275\u1205\u1295 \u12a0\u1235\u1273\u12cd\u1233\u1208\u1201\u1364 \u12ed\u1205 \u12a5\u121d\u1290\u1275 \u1240\u12f5\u121e \u1260\u12a0\u12eb\u1275\u1205 \u1260\u120e\u12ed\u12f5 \u12a5\u1295\u12f2\u1201\u121d \u1260\u12a5\u1293\u1275\u1205 \u1260\u12a4\u12cd\u1295\u1244 \u12d8\u1295\u12f5 \u1290\u1260\u1228\u1364 \u12a0\u1201\u1295 \u12f0\u130d\u121e \u1260\u12a0\u1295\u1270 \u12a5\u1295\u12f3\u1208 \u1270\u1228\u12f5\u127b\u1208\u1201\u1362"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u1235\u1208\u12da\u1205 \u1260\u12a5\u1306\u127c \u1218\u132b\u1295 \u12e8\u1270\u1240\u1260\u120d\u12b8\u12cd\u1295 \u1260\u12a0\u1295\u1270 \u12cd\u1235\u1325 \u12eb\u1208\u12cd\u1295 \u12e8\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d\u1295 \u1235\u1326\u1273 \u12a5\u1295\u12f5\u1273\u1240\u1323\u1325\u120d \u12a0\u1233\u1235\u1265\u1203\u1208\u1201\u1362"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u12e8\u1280\u12ed\u120d\u1293 \u12e8\u134d\u1245\u122d\u1363 \u122b\u1235\u1295\u121d \u12e8\u1218\u130d\u12db\u1275 \u1218\u1295\u1348\u1235 \u12a5\u1295\u1302 \u12e8\u134d\u122d\u1200\u1275 \u1218\u1295\u1348\u1235 \u12a0\u120d\u1230\u1320\u1295\u121d\u1293\u1362"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u12a5\u1295\u130d\u12f2\u1205\u1363 \u1235\u1208 \u130c\u1273\u127d\u1295 \u1208\u1218\u1218\u1235\u12a8\u122d \u12c8\u12ed\u121d \u12e8\u12a5\u122d\u1231 \u12a5\u1235\u1228\u129b \u1260\u1206\u1295\u1201\u1275 \u1260\u12a5\u1294 \u12a0\u1275\u1348\u122d\u1364 \u1290\u1308\u122d \u130d\u1295 \u1260\u12a5\u130d\u12da\u12a0\u1265\u1214\u122d \u1280\u12ed\u120d \u1235\u1208 \u12c8\u1295\u130c\u120d \u12a8\u12a5\u1294 \u130b\u122d \u1218\u12a8\u122b\u1295 \u1270\u1240\u1260\u120d\u1362"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u12a5\u129b \u1235\u120b\u12f0\u1228\u130d\u1290\u12cd \u12a0\u1295\u12f3\u127d \u1290\u1308\u122d \u1233\u12ed\u1206\u1295\u1363 \u12a8\u12d5\u1245\u12f1\u1293 \u12a8\u1338\u130b\u12cd \u12e8\u1270\u1290\u1223 \u12eb\u12f3\u1290\u1295\u1363 \u12c8\u12f0 \u1245\u12f1\u1235 \u1215\u12ed\u12c8\u1275\u121d \u12e8\u1320\u122b\u1295 \u12a5\u122d\u1231 \u1290\u12cd\u1362 \u12ed\u1205\u121d \u1338\u130b \u12a8\u12d8\u120b\u1208\u121d \u12d8\u1218\u1293\u1275 \u1260\u134a\u1275 \u1260\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u1270\u1230\u1320\u1295\u1364"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u12a0\u1201\u1295 \u130d\u1295 \u121e\u1275\u1295 \u12f0\u121d\u1235\u1236 \u1260\u12c8\u1295\u130c\u120d \u12a0\u121b\u12ab\u12ed\u1290\u1275 \u1215\u12ed\u12c8\u1275\u1295\u1293 \u12a2\u1218\u12cb\u1272\u1290\u1275\u1295 \u12c8\u12f0 \u1265\u122d\u1203\u1295 \u1263\u12c8\u1323\u12cd \u1260\u1218\u12f5\u1280\u1292\u1273\u127d\u1295 \u1260\u12ad\u122d\u1235\u1276\u1235 \u12a2\u12e8\u1231\u1235 \u1218\u121d\u1323\u1275 \u1270\u1308\u120d\u1327\u120d\u1362"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u12a5\u1294\u121d \u1208\u12da\u1205 \u12c8\u1295\u130c\u120d \u12e8\u121d\u1225\u122b\u127d \u1290\u130b\u122a\u1293 \u1210\u12cb\u122d\u12eb\u1363 \u12a0\u1235\u1270\u121b\u122a\u121d \u1206\u129c \u1270\u123e\u121d\u1201\u1362"
