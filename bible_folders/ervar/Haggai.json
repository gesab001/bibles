                "13": {
                    "verse_nr": "13",
                    "verse": "\u062b\u064f\u0645\u0651\u064e \u0642\u0627\u0644\u064e \u062d\u064e\u062c\u0651\u064e\u064a: \u00ab\u0625\u0646\u0652 \u0644\u064e\u0645\u064e\u0633\u064e \u0625\u0646\u0633\u0627\u0646\u064c \u0646\u064e\u062c\u0650\u0633\u064c\u060c\u0634\u064e\u064a\u0626\u0627\u064b \u0645\u0650\u0646\u0652 \u0647\u064e\u0630\u0650\u0647\u0650 \u0627\u0644\u0623\u0634\u064a\u0627\u0621\u0650\u060c \u0641\u064e\u0647\u064e\u0644\u0652 \u062a\u064e\u062a\u064e\u0646\u064e\u062c\u0651\u064e\u0633\u064f\u061f\u00bb \u0641\u064e\u0623\u062c\u0627\u0628\u064e \u0627\u0644\u0643\u064e\u0647\u064e\u0646\u064e\u0629\u064f: \u00ab\u0646\u064e\u0639\u064e\u0645\u0652 \u062a\u064e\u062a\u064e\u0646\u064e\u062c\u0651\u064e\u0633\u064f.\u00bb"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0641\u064e\u0642\u0627\u0644\u064e \u062d\u064e\u062c\u0651\u064e\u064a: \u00ab\u2039\u0647\u064e\u0630\u0627 \u064a\u064e\u0646\u0637\u064e\u0628\u0650\u0642\u064f \u0639\u064e\u0644\u064e\u0649 \u0647\u064e\u0630\u0627 \u0627\u0644\u0634\u0651\u064e\u0639\u0628\u0650 \u0648\u064e\u0639\u064e\u0644\u064e\u0649 \u0647\u064e\u0630\u0650\u0647\u0650 \u0627\u0644\u0623\u064f\u0645\u0651\u064e\u0629\u0650 \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u0623\u0645\u0627\u0645\u0650\u064a\u060c\u203a \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f. \u2039\u0648\u064e\u0643\u064e\u0630\u064e\u0644\u0650\u0643\u064e \u0639\u064e\u0644\u064e\u0649 \u0643\u064f\u0644\u0651\u0650 \u0645\u0627 \u064a\u064e\u0639\u0652\u0645\u064e\u0644\u064f\u0648\u0646\u064e\u0647\u064f \u0648\u064e\u064a\u064f\u0646\u062a\u0650\u062c\u064f\u0648\u0646\u064e\u0647\u064f\u060c \u0648\u064e\u0643\u064f\u0644\u0651\u064f \u0645\u0627 \u064a\u064f\u0642\u064e\u0631\u0651\u0650\u0628\u064f\u0648\u0646\u064e\u0647\u064f \u0625\u0644\u064e\u064a\u0651\u064e \u0646\u064e\u062c\u0650\u0633\u064c."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u00ab\u2039\u0648\u064e\u0627\u0644\u0622\u0646\u064e \u062a\u064e\u0623\u0645\u0651\u064e\u0644\u0648\u0627 \u0645\u0650\u0646\u0652 \u0647\u064e\u0630\u0627 \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0641\u064e\u0635\u0627\u0639\u0650\u062f\u0627\u064b: \u0642\u064e\u0628\u0644\u064e \u0623\u0646\u0652 \u064a\u064f\u0648\u0636\u064e\u0639\u064e \u062d\u064e\u062c\u064e\u0631\u064c \u0639\u064e\u0644\u064e\u0649 \u062d\u064e\u062c\u064e\u0631\u064d \u0641\u0650\u064a \u0647\u064e\u064a\u0643\u064e\u0644\u0650 \u0627\u0644\u0644\u0647\u0650\u060c"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0643\u064e\u064a\u0641\u064e \u0643\u0627\u0646\u064e \u062d\u0627\u0644\u064f\u0643\u064f\u0645\u0652\u061f \u0643\u0627\u0646\u064e \u0623\u062d\u064e\u062f\u064f\u0643\u064f\u0645\u0652 \u064a\u064e\u0623\u0652\u062a\u0650\u064a \u0625\u0644\u064e\u0649 \u0643\u064e\u0648\u0652\u0645\u064e\u0629\u064d \u0645\u0650\u0646\u064e \u0627\u0644\u062d\u064f\u0628\u0648\u0628\u0650 \u0645\u064f\u062a\u064e\u0648\u064e\u0642\u0651\u0650\u0639\u0627\u064b \u0639\u0650\u0634\u0652\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0643\u064a\u0627\u0644\u0627\u064b\u060c \u0641\u064e\u0644\u0627 \u064a\u064e\u062c\u0650\u062f\u064f \u0633\u0650\u0648\u064e\u0649 \u0639\u064e\u0634\u0652\u0631\u064e\u0629\u064d. \u0623\u0648\u0652 \u064a\u064e\u0623\u0652\u062a\u0650\u064a \u0625\u0644\u064e\u0649 \u062d\u064e\u0648\u0652\u0636\u0650 \u0645\u0650\u0639\u0652\u0635\u064e\u0631\u064e\u0629\u0650 \u0627\u0644\u0646\u0651\u064e\u0628\u064a\u0630\u0650 \u0644\u0650\u064a\u064e\u063a\u0631\u0650\u0641\u064e \u062e\u064e\u0645\u0633\u0650\u064a\u0646\u064e \u0645\u0650\u0643\u064a\u0627\u0644\u0627\u064b\u060c \u0641\u064e\u0644\u0627 \u064a\u064e\u062c\u0650\u062f\u064f \u0633\u0650\u0648\u064e\u0649 \u0639\u0650\u0634\u0652\u0631\u0650\u064a\u0646\u064e."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0636\u064e\u0631\u064e\u0628\u0652\u062a\u064f\u0643\u064f\u0645\u0652 \u0648\u064e\u0636\u064e\u0631\u064e\u0628\u0652\u062a\u064f \u0643\u064f\u0644\u0651\u064e \u0645\u0627 \u0639\u064e\u0645\u0650\u0644\u0652\u062a\u064f\u0645\u064f\u0648\u0647\u064f \u0628\u0650\u0627\u0644\u0623\u0648\u0628\u0650\u0626\u064e\u0629\u0650 \u0648\u064e\u0627\u0644\u0639\u064e\u0641\u064e\u0646\u0650 \u0648\u064e\u0627\u0644\u0628\u064e\u0631\u064e\u062f\u0650. \u0644\u064e\u0643\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u0652 \u0644\u064e\u0645\u0652 \u062a\u064e\u0644\u062a\u064e\u0641\u0650\u062a\u064f\u0648\u0627 \u0625\u0644\u064e\u064a\u0651\u064e\u060c\u203a \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u00ab\u062a\u064e\u0623\u0645\u0651\u064e\u0644\u064f\u0648\u0627 \u0628\u0650\u0647\u064e\u0630\u0627 \u0645\u0650\u0646\u064e \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0641\u064e\u0635\u0627\u0639\u0650\u062f\u0627\u064b. \u0645\u0650\u0646\u064e \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0627\u0644\u0631\u0651\u0627\u0628\u0650\u0639\u0650 \u0648\u064e\u0627\u0644\u0639\u0650\u0634\u0652\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u064e \u0627\u0644\u0634\u0651\u064e\u0647\u0652\u0631\u0650 \u0627\u0644\u062a\u0651\u0627\u0633\u0650\u0639\u0650\u060c \u0645\u0650\u0646\u064e \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0648\u064f\u0636\u0650\u0639\u064e \u0641\u0650\u064a\u0647\u0650 \u0623\u0633\u0627\u0633\u064f \u0628\u064e\u064a\u062a\u0650 \u0627\u0644\u0644\u0647\u0650!"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0623\u0645\u0627 \u062a\u064e\u0632\u0627\u0644\u064f \u0647\u064f\u0646\u0627\u0643\u064e \u0628\u064f\u0630\u064f\u0648\u0631\u064c \u0641\u0650\u064a \u0627\u0644\u0645\u064e\u062e\u0627\u0632\u0650\u0646\u0650\u061f \u0623\u0645\u0627 \u062a\u064e\u0632\u0627\u0644\u064f \u0627\u0644\u0643\u064f\u0631\u064f\u0648\u0645\u064f \u0648\u064e\u0623\u0634\u0652\u062c\u0627\u0631\u064f \u0627\u0644\u062a\u0651\u0650\u064a\u0646\u0650 \u0648\u064e\u0627\u0644\u0631\u0651\u064f\u0645\u0651\u0627\u0646\u0650 \u0648\u064e\u0627\u0644\u0632\u0651\u064e\u064a\u0652\u062a\u064f\u0648\u0646\u0650 \u062c\u064e\u0631\u0652\u062f\u0627\u0621\u064e \u0628\u0650\u0644\u0627 \u062b\u064e\u0645\u064e\u0631\u064d\u061f \u0644\u064e\u0643\u0650\u0646\u0651\u0650\u064a \u0645\u0650\u0646\u0652 \u0647\u064e\u0630\u0627 \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0641\u064e\u0635\u0627\u0639\u0650\u062f\u0627\u064b\u060c \u0633\u064e\u0623\u064f\u0628\u0627\u0631\u0650\u0643\u064f\u0643\u064f\u0645\u0652.\u00bb"
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0632\u064e\u0631\u064f\u0628\u0651\u0627\u0628\u0650\u0644\u064f \u062e\u0627\u062a\u064e\u0645\u064c \u0641\u064a \u0625\u0635\u0628\u0650\u0639 \u0627\u0644\u0644\u0647 - \u0648\u064e\u062a\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e \u0627\u0644\u0644\u0647\u064f \u062b\u0627\u0646\u0650\u064a\u064e\u0629\u064b \u0625\u0644\u064e\u0649 \u062d\u064e\u062c\u0651\u064e\u064a \u0641\u0650\u064a \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0627\u0644\u0631\u0651\u0627\u0628\u0650\u0639\u0650 \u0648\u064e\u0627\u0644\u0639\u0650\u0634\u0652\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u064e \u0627\u0644\u0634\u0651\u064e\u0647\u0652\u0631\u0650\u060c \u0641\u064e\u0642\u0627\u0644\u064e:"
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u00ab\u062a\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u0652 \u0625\u0644\u064e\u0649 \u0632\u064e\u0631\u064f\u0628\u0651\u0627\u0628\u0650\u0644\u064e\u060c \u0648\u0627\u0644\u0650\u064a \u064a\u064e\u0647\u064f\u0648\u0630\u0627\u060c \u0641\u064e\u0642\u064f\u0644\u0652: \u2039\u0633\u064e\u0623\u064f\u0632\u064e\u0644\u0652\u0632\u0650\u0644\u064f \u0627\u0644\u0633\u0651\u064e\u0645\u0627\u0648\u0627\u062a\u0650 \u0648\u064e\u0627\u0644\u0623\u0631\u0652\u0636\u064e."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u0633\u064e\u0623\u0642\u0644\u0650\u0628\u064f \u0627\u0644\u062d\u064f\u0643\u064f\u0648\u0645\u0627\u062a\u0650 \u0648\u064e\u0623\u064f\u062f\u064e\u0645\u0651\u0650\u0631\u064f \u0642\u064f\u0648\u0651\u064e\u0629\u064e \u0627\u0644\u0645\u064e\u0645\u0627\u0644\u0650\u0643\u0650 \u0627\u0644\u0623\u062c\u0646\u064e\u0628\u0650\u064a\u0651\u064e\u0629\u0650. \u0633\u064e\u0623\u0642\u0644\u0650\u0628\u064f \u0627\u0644\u0645\u064e\u0631\u0643\u064e\u0628\u0627\u062a\u0650 \u0648\u064e\u0631\u0627\u0643\u0650\u0628\u0650\u064a\u0647\u0627\u060c \u0648\u064e\u0627\u0644\u062e\u064f\u064a\u064f\u0648\u0644\u064e \u0648\u064e\u0641\u064f\u0631\u0633\u0627\u0646\u0650\u0647\u0627. \u0633\u064e\u064a\u064e\u0633\u0642\u064f\u0637\u064f\u0648\u0646\u064e \u0643\u064f\u0644\u0651\u064f \u0648\u0627\u062d\u0650\u062f\u064d \u0628\u0650\u0633\u064e\u064a\u0641\u0650 \u0631\u064e\u0641\u0650\u064a\u0642\u0650\u0647\u0650."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u0641\u0650\u064a \u0630\u064e\u0644\u0650\u0643\u064e \u0627\u0644\u0648\u064e\u0642\u062a\u0650\u060c\u203a \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f\u060c \u2039\u0633\u064e\u0622\u062e\u064f\u0630\u064f\u0643\u064e \u064a\u0627 \u062e\u0627\u062f\u0650\u0645\u0650\u064a \u0632\u064e\u0631\u064f\u0628\u0651\u0627\u0628\u0650\u0644\u064e \u0628\u0652\u0646\u064e \u0634\u064e\u0623\u0644\u0652\u062a\u064e\u0626\u0650\u064a\u0644\u064e \u0648\u064e\u0633\u064e\u0623\u062c\u0639\u064e\u0644\u064f\u0643\u064e \u0643\u064e\u062e\u0627\u062a\u064e\u0645\u064d \u0641\u0650\u064a \u0625\u0635\u0628\u0650\u0639\u0650\u064a. \u0644\u0650\u0623\u0646\u0651\u0650\u064a \u0627\u062e\u062a\u064e\u0631\u0652\u062a\u064f\u0643\u064e\u060c\u203a\u00bb \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f."
                }
            }
        }
