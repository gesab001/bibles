                "9": {
                    "verse_nr": "9",
                    "verse": "\u0648\u064e\u0645\u064e\u062c\u0652\u062f\u064f \u0627\u0644\u0628\u064e\u064a\u062a\u0650 \u0627\u0644\u062b\u0651\u0627\u0646\u0650\u064a \u0633\u064e\u064a\u064e\u0643\u064f\u0648\u0646\u064f \u0623\u0639\u0638\u064e\u0645\u064e \u0645\u0650\u0646\u0652 \u0645\u064e\u062c\u0652\u062f\u0650 \u0627\u0644\u0628\u064e\u064a\u062a\u0650 \u0627\u0644\u0623\u0648\u0651\u064e\u0644\u0650\u060c \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f. \u0648\u064e\u0641\u0650\u064a \u0647\u064e\u0630\u0627 \u0627\u0644\u0645\u064e\u0643\u0627\u0646\u0650 \u0633\u064e\u0623\u0645\u0652\u0646\u064e\u062d\u064f \u0627\u0644\u0633\u0651\u064e\u0644\u0627\u0645\u064e\u060c \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f.\u203a\u00bb"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0628\u064e\u0631\u064e\u0643\u0627\u062a\u064f \u0627\u0644\u0644\u0647 - \u0648\u064e\u0641\u0650\u064a \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0627\u0644\u0631\u0651\u0627\u0628\u0650\u0639\u0650 \u0648\u064e\u0627\u0644\u0639\u0650\u0634\u0652\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u064e \u0627\u0644\u0634\u0651\u064e\u0647\u0652\u0631\u0650 \u0627\u0644\u062a\u0651\u0627\u0633\u0650\u0639\u0650 \u0645\u0650\u0646\u064e \u0627\u0644\u0633\u0651\u064e\u0646\u064e\u0629\u0650 \u0627\u0644\u062b\u0651\u0627\u0646\u0650\u064a\u064e\u0629\u0650 \u0644\u0650\u062d\u064f\u0643\u0652\u0645\u0650 \u0627\u0644\u0645\u064e\u0644\u0650\u0643\u0650 \u062f\u0627\u0631\u064a\u064f\u0648\u0633\u064e\u060c \u062a\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e \u0627\u0644\u0644\u0647\u064f \u0625\u0644\u064e\u0649 \u062d\u064e\u062c\u0651\u064e\u064a \u0627\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650 \u0642\u0627\u0626\u0644\u0627\u064b:"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u00ab\u0647\u064e\u0630\u0627 \u0647\u064f\u0648\u064e \u0645\u0627 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0647\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f: \u2039\u0627\u0633\u0652\u0623\u0644\u0650 \u0627\u0644\u0643\u064e\u0647\u064e\u0646\u064e\u0629\u064e \u0639\u064e\u0646\u0652 \u062d\u064f\u0643\u0652\u0645\u064d \u0634\u064e\u0631\u0652\u0639\u0650\u064a\u0651\u064d \u0648\u064e\u0642\u064f\u0644\u0652 \u0644\u064e\u0647\u064f\u0645\u0652:"
