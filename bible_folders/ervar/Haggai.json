        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u062a\u064e\u0634\u062c\u0650\u064a\u0639\u064f \u0627\u0644\u0644\u0647\u0650 \u0644\u0650\u0644\u0634\u0651\u064e\u0639\u0628 - \u0648\u064e\u0641\u0650\u064a \u0627\u0644\u064a\u064e\u0648\u0652\u0645\u0650 \u0627\u0644\u062d\u0627\u062f\u0650\u064a \u0648\u064e\u0627\u0644\u0639\u0650\u0634\u0652\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u064e \u0627\u0644\u0634\u0651\u064e\u0647\u0652\u0631\u0650 \u0627\u0644\u0633\u0651\u0627\u0628\u0650\u0639\u0650\u060c \u0641\u0650\u064a \u0627\u0644\u0633\u0651\u064e\u0646\u064e\u0629\u0650 \u0627\u0644\u062b\u0651\u0627\u0646\u0650\u064a\u064e\u0629\u0650 \u0645\u0650\u0646\u0652 \u062d\u064f\u0643\u0652\u0645\u0650 \u0627\u0644\u0645\u064e\u0644\u0650\u0643\u0650 \u062f\u0627\u0631\u0652\u064a\u064f\u0648\u0633\u064e\u060c \u062a\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e \u0627\u0644\u0644\u0647\u064f \u0639\u064e\u0644\u064e\u0649 \u0641\u064e\u0645\u0650 \u062d\u064e\u062c\u0651\u064e\u064a \u0627\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650 \u0641\u064e\u0642\u0627\u0644\u064e:"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u00ab\u0642\u064f\u0644\u0652 \u0644\u0650\u0632\u064e\u0631\u064f\u0628\u0651\u0627\u0628\u0650\u0644\u064e \u0628\u0652\u0646\u0650 \u0634\u064e\u0623\u0644\u0652\u062a\u064e\u0626\u0650\u064a\u0644\u064e\u060c \u0648\u0627\u0644\u0650\u064a \u064a\u064e\u0647\u064f\u0648\u0630\u0627\u060c \u0648\u064e\u0644\u0650\u064a\u064e\u0634\u064f\u0648\u0639\u064e \u0628\u0652\u0646\u0650 \u064a\u064e\u0647\u064f\u0648\u0635\u0627\u062f\u0627\u0642\u064e \u0631\u064e\u0626\u0650\u064a\u0633\u0650 \u0627\u0644\u0643\u064e\u0647\u064e\u0646\u064e\u0629\u0650\u060c \u0648\u064e\u0644\u0650\u0628\u064e\u0642\u0650\u064a\u0651\u064e\u0629\u0650 \u0627\u0644\u0634\u0651\u064e\u0639\u0628\u0650:"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u2039\u0645\u064e\u0646\u0652 \u0645\u0650\u0646\u0652\u0643\u064f\u0645\u0652 \u0623\u064a\u0651\u064e\u0647\u0627 \u0627\u0644\u0646\u0651\u0627\u062c\u064f\u0648\u0646\u064e \u0631\u064e\u0623\u0649 \u0647\u064e\u0630\u0627 \u0627\u0644\u0647\u064e\u064a\u0643\u064e\u0644\u064e \u0641\u0650\u064a \u0645\u064e\u062c\u062f\u0650\u0647\u0650 \u0627\u0644\u0623\u0648\u0651\u064e\u0644\u0650\u061f \u0648\u064e\u0643\u064e\u064a\u0641\u064e \u062a\u064e\u0631\u064e\u0648\u0652\u0646\u064e\u0647\u064f \u0627\u0644\u0622\u0646\u064e\u061f \u0623\u0644\u0627 \u064a\u064e\u0628\u0652\u062f\u064f\u0648 \u0643\u064e\u0644\u0627 \u0634\u064e\u064a\u0621\u064d \u0628\u0650\u0627\u0644\u0646\u0651\u0650\u0633\u0652\u0628\u064e\u0629\u0650 \u0644\u064e\u0643\u064f\u0645\u0652\u061f"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0644\u064e\u0643\u0650\u0646\u0652 \u062a\u064e\u0634\u064e\u062f\u0651\u064e\u062f\u0652 \u064a\u0627 \u0632\u064e\u0631\u064f\u0628\u0651\u0627\u0628\u0650\u0644\u064f\u060c \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f. \u062a\u064e\u0642\u064e\u0648\u0651\u064e \u064a\u0627 \u0631\u064e\u0626\u0650\u064a\u0633\u064e \u0627\u0644\u0643\u064e\u0647\u064e\u0646\u064e\u0629\u0650 \u064a\u064e\u0634\u064f\u0648\u0639\u064e \u0628\u0652\u0646\u064f \u064a\u064e\u0647\u064f\u0648\u0635\u0627\u062f\u0627\u0642\u064e\u060c \u0648\u064e\u062a\u064e\u0642\u064e\u0648\u0651\u064f\u0648\u0627 \u064a\u0627 \u0628\u064e\u0642\u0650\u064a\u0651\u064e\u0629\u064e \u0633\u064f\u0643\u0651\u0627\u0646\u0650 \u0627\u0644\u0623\u0631\u0652\u0636\u0650\u060c \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f\u060c \u0648\u064e\u0627\u0639\u0652\u0645\u064e\u0644\u064f\u0648\u0627 \u0644\u0650\u0623\u0646\u0651\u0650\u064a \u0645\u064e\u0639\u064e\u0643\u064f\u0645\u0652 \u062c\u064e\u0645\u0650\u064a\u0639\u0627\u064b\u060c\u203a \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u00ab\u2039\u0647\u064e\u0630\u0627 \u0647\u064f\u0648\u064e \u0627\u0644\u0639\u064e\u0647\u0652\u062f\u064f \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0642\u064e\u0637\u064e\u0639\u0652\u062a\u064f\u0647\u064c \u0645\u064e\u0639\u064e\u0643\u064f\u0645\u0652 \u062d\u0650\u064a\u0646\u064e \u062e\u064e\u0631\u064e\u062c\u0652\u062a\u064f\u0645\u0652 \u0645\u0650\u0646\u0652 \u0645\u0650\u0635\u0652\u0631\u064e: \u0631\u064f\u0648\u062d\u0650\u064a \u0633\u064e\u064a\u064e\u0628\u0652\u0642\u064e\u0649 \u062f\u0627\u0626\u0650\u0645\u0627\u064b \u0641\u0650\u064a \u0648\u064e\u0633\u064e\u0637\u0650\u0643\u064f\u0645\u0652. \u0641\u064e\u0644\u0627 \u062a\u064e\u062e\u0627\u0641\u064f\u0648\u0627."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0641\u064e\u0647\u064e\u0630\u0627 \u0647\u064f\u0648\u064e \u0645\u0627 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0647\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f: \u0628\u064e\u0639\u0652\u062f\u064e \u0641\u064e\u062a\u0652\u0631\u064e\u0629\u064d \u0642\u064e\u0635\u0650\u064a\u0631\u064e\u0629\u064d \u0633\u064e\u0623\u064f\u0632\u064e\u0644\u0652\u0632\u0650\u0644\u064f \u0627\u0644\u0633\u0651\u064e\u0645\u0627\u0648\u0627\u062a\u0650 \u0648\u064e\u0627\u0644\u0623\u0631\u0652\u0636\u064e \u0648\u064e\u0627\u0644\u0628\u064e\u062d\u0631\u064e \u0648\u064e\u0627\u0644\u064a\u0627\u0628\u0650\u0633\u064e\u0629\u064e \u062b\u0627\u0646\u0650\u064a\u064e\u0629\u064b."
