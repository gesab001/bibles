{
    "version": "ervar",
    "book_name": "Nahum",
    "book_nr": "34",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0647\u064e\u0630\u0627 \u0625\u0639\u0644\u0627\u0646\u064c \u0646\u064e\u0628\u064e\u0648\u0650\u064a\u0651\u064c \u0639\u064e\u0646\u0652 \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649. \u0643\u0650\u062a\u0627\u0628\u064f \u0631\u064f\u0624\u0652\u064a\u0627 \u0646\u0627\u062d\u064f\u0648\u0645\u064e \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0645\u0650\u0646\u0652 \u0645\u064e\u062f\u064a\u0646\u064e\u0629\u0650 \u0623\u0644\u0652\u0642\u064f\u0648\u0634\u064e."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u063a\u064e\u0636\u064e\u0628\u064f \u0627\u0644\u0644\u0647\u0650 \u0639\u064e\u0644\u064e\u0649 \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 - \u0627\u0644\u0644\u0647\u064f \u0625\u0644\u064e\u0647\u064c \u063a\u064e\u064a\u064f\u0648\u0631\u064c \u064a\u064e\u0646\u062a\u064e\u0642\u0650\u0645\u064f. \u0627\u0644\u0644\u0647\u064f \u064a\u064e\u0646\u062a\u064e\u0642\u0650\u0645\u064f \u0648\u064e\u064a\u064e\u0633\u0652\u062e\u064e\u0637\u064f. \u0627\u0644\u0644\u0647\u064f \u064a\u064e\u0646\u062a\u064e\u0642\u0650\u0645\u064f \u0645\u0650\u0646\u0652 \u0645\u064f\u0642\u0627\u0648\u0650\u0645\u0650\u064a\u0647\u0650\u060c \u0648\u064e\u064a\u064e\u063a\u0636\u064e\u0628\u064f \u0645\u0650\u0646\u0652 \u0623\u0639\u062f\u0627\u0626\u0650\u0647\u0650."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0627\u0644\u0644\u0647\u064f \u0635\u064e\u0628\u064f\u0648\u0631\u064c \u0648\u064e\u0644\u064e\u0643\u0650\u0646\u0651\u064e\u0647\u064f \u0642\u064e\u0648\u0650\u064a\u0651\u064c \u062c\u0650\u062f\u0651\u0627\u064b. \u0648\u064e\u0644\u0627 \u064a\u064f\u0628\u064e\u0631\u0651\u0650\u0626\u064f \u0627\u0644\u0645\u064f\u062e\u0637\u0650\u0626\u0650\u064a\u0646\u064e. \u062d\u0650\u064a\u0646\u064e \u064a\u064e\u062a\u064e\u062d\u064e\u0631\u0651\u064e\u0643\u064f\u060c \u0641\u064e\u0627\u0644\u0632\u0651\u064e\u0648\u0627\u0628\u0650\u0639\u064f \u0648\u064e\u0627\u0644\u0639\u064e\u0648\u0627\u0635\u0650\u0641\u064f \u062a\u064e\u062a\u0628\u064e\u0639\u064f\u0647\u064f \u0641\u0650\u064a \u0633\u064e\u064a\u0631\u0650\u0647\u0650\u060c \u0648\u064e\u0627\u0644\u063a\u064f\u064a\u064f\u0648\u0645\u064f \u0647\u0650\u064a\u064e \u0627\u0644\u063a\u064f\u0628\u0627\u0631\u064f \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u062a\u064f\u062b\u0650\u064a\u0631\u064f\u0647\u064f \u0642\u064e\u062f\u064e\u0645\u0627\u0647\u064f."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u064a\u064e\u0646\u062a\u064e\u0647\u0650\u0631\u064f \u0627\u0644\u0628\u064e\u062d\u0631\u064e \u0641\u064e\u064a\u064e\u062c\u0650\u0641\u0651\u064f\u060c \u0648\u064e\u064a\u064f\u062c\u064e\u0641\u0651\u0650\u0641\u064f \u0643\u064f\u0644\u0651\u064e \u0627\u0644\u0623\u0646\u0647\u0627\u0631\u0650. \u0623\u0631\u0627\u0636\u0650\u064a \u0628\u0627\u0634\u0627\u0646\u064e \u0648\u064e\u0627\u0644\u0643\u064e\u0631\u0652\u0645\u064e\u0644\u0650 \u0627\u0644\u062e\u064e\u0635\u0652\u0628\u064e\u0629\u064f \u062a\u064e\u062c\u0650\u0641\u0651\u064f\u060c \u0648\u064e\u0646\u064e\u0628\u0627\u062a\u0627\u062a\u064f \u0644\u064f\u0628\u0646\u0627\u0646\u064e \u062a\u064e\u0630\u0652\u0628\u064f\u0644\u064f."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0627\u0644\u062c\u0650\u0628\u0627\u0644\u064f \u062a\u064e\u0631\u062a\u064e\u062c\u0650\u0641\u064f \u062e\u064e\u0648\u0641\u0627\u064b \u0645\u0650\u0646\u0647\u064f\u060c \u0648\u064e\u0627\u0644\u062a\u0651\u0650\u0644\u0627\u0644\u064f \u062a\u064e\u0630\u064f\u0648\u0628\u064f. \u062a\u064e\u0631\u0652\u062a\u064e\u062c\u0650\u0641\u064f \u0627\u0644\u0623\u0631\u0652\u0636\u064f \u0628\u0650\u062e\u064e\u0648\u0641\u064d \u0623\u0645\u0627\u0645\u064e\u0647\u064f\u060c \u0627\u0644\u0645\u064e\u0633\u0643\u064f\u0648\u0646\u064e\u0629\u064f \u0648\u064e\u0643\u064f\u0644\u0651\u064f \u0627\u0644\u0633\u0651\u0627\u0643\u0650\u0646\u0650\u064a\u0646\u064e \u0641\u0650\u064a\u0647\u0627."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0645\u064e\u0646\u0652 \u064a\u064f\u0645\u0643\u0650\u0646\u064f\u0647\u064f \u0627\u0644\u0648\u064f\u0642\u064f\u0648\u0641\u064f \u0623\u0645\u0627\u0645\u064e\u0647\u064f \u062d\u0650\u064a\u0646\u064e \u064a\u064e\u063a\u064e\u0636\u064e\u0628\u064f\u061f \u0645\u064e\u0646\u0652 \u064a\u064e\u0633\u062a\u064e\u0637\u0650\u064a\u0639\u064f \u0627\u062d\u062a\u0650\u0645\u0627\u0644\u064e \u063a\u064e\u0636\u064e\u0628\u0650\u0647\u0650 \u0627\u0644\u0634\u0651\u064e\u062f\u0650\u064a\u062f\u0650\u061f \u064a\u064e\u0646\u0633\u064e\u0643\u0650\u0628\u064f \u063a\u064e\u0636\u064e\u0628\u064f\u0647\u064f \u0643\u064e\u0646\u0627\u0631\u064d\u060c \u0641\u064e\u062a\u064e\u062a\u064e\u0634\u064e\u0642\u0651\u064e\u0642\u064e \u0645\u0650\u0646\u0652\u0647\u064f \u0627\u0644\u0635\u0651\u064f\u062e\u064f\u0648\u0631\u064f."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0627\u0644\u0644\u0647\u064f \u0635\u0627\u0644\u0650\u062d\u064c\u060c \u0648\u064e\u0647\u064f\u0648\u064e \u0645\u064e\u0644\u062c\u064e\u0623\u064c \u0641\u0650\u064a \u0648\u064e\u0642\u062a\u0650 \u0627\u0644\u0636\u0651\u0650\u064a\u0642\u0650. \u0648\u064e\u064a\u064e\u0647\u062a\u064e\u0645\u0651\u064f \u0628\u0650\u0627\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0644\u062c\u064e\u0623\u064f\u0648\u0646\u064e \u0625\u0644\u064e\u064a\u0647\u0650."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0644\u064e\u0643\u0650\u0646\u0651\u064e\u0647\u064f \u0628\u0650\u0637\u064f\u0648\u0641\u0627\u0646\u064d \u0639\u064e\u0638\u0650\u064a\u0645\u064d \u064a\u064e\u0642\u0636\u0650\u064a \u0639\u064e\u0644\u064e\u0649 \u0645\u064f\u0642\u0627\u0648\u0650\u0645\u0650\u064a\u0647\u0650. \u064a\u064f\u0637\u0627\u0631\u0650\u062f\u064f \u0623\u0639\u062f\u0627\u0621\u064e\u0647\u064f \u0625\u0644\u064e\u0649 \u0627\u0644\u0638\u0651\u064f\u0644\u0645\u064e\u0629\u0650."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0628\u0650\u0645\u0627\u0630\u0627 \u062a\u064f\u062e\u064e\u0637\u0651\u0650\u0637\u064f\u0648\u0646\u064e \u0636\u0650\u062f\u0651\u064e \u0627\u0644\u0644\u0647\u0650\u061f \u0633\u064e\u064a\u064e\u0642\u0636\u0650\u064a \u0639\u064e\u0644\u064e\u064a\u0647\u0650\u0645\u0652\u060c \u0648\u064e\u0644\u064e\u0646\u0652 \u064a\u064e\u0642\u064f\u0648\u0645\u064e \u0627\u0644\u0636\u0651\u0650\u064a\u0642\u064f \u062b\u0627\u0646\u0650\u064a\u064e\u0629\u064b."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0645\u064e\u0639\u064e \u0623\u0646\u0651\u064e\u0647\u064f\u0645\u0652 \u0645\u0650\u062b\u0644\u064f \u0634\u064e\u0648\u0643\u064d \u0645\u064f\u062a\u064e\u0634\u0627\u0628\u0650\u0643\u064d\u060c \u0648\u064e\u0645\u0650\u062b\u0644\u064f \u0633\u064f\u0643\u0627\u0631\u0649 \u0628\u0650\u0627\u0644\u062e\u064e\u0645\u0631\u0650\u060c \u0641\u064e\u0633\u064e\u062a\u064e\u0644\u0652\u062a\u064e\u0647\u0650\u0645\u064f\u0647\u064f\u0645\u064f \u0627\u0644\u0646\u0651\u0627\u0631\u064f \u0643\u064e\u0642\u064e\u0634\u0651\u064d \u064a\u0627\u0628\u0650\u0633\u064d."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0633\u064e\u064a\u064e\u062e\u0631\u064f\u062c\u064f \u0645\u0650\u0646\u0652\u0643\u0650 \u0645\u064e\u0646\u0652 \u064a\u064f\u062e\u064e\u0637\u0651\u064e\u0637\u064f \u0628\u0650\u0627\u0644\u0634\u0651\u064e\u0631\u0651\u0650 \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u0644\u0647\u0650. \u0625\u0646\u0651\u064e\u0647\u064f \u0644\u064e\u0645\u064f\u0634\u064a\u0631\u064c \u062f\u064e\u0646\u064a\u0621\u064c!"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0647\u064e\u0643\u064e\u0630\u0627 \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f: \u00ab\u062d\u064e\u062a\u0651\u064e\u0649 \u0648\u064e\u0625\u0646\u0652 \u0643\u0627\u0646\u064f\u0648\u0627 \u0623\u0642\u0648\u0650\u064a\u0627\u0621\u064e \u0648\u064e\u0643\u064e\u062b\u0650\u064a\u0631\u0650\u064a\u0646\u064e\u060c \u0633\u064e\u064a\u064e\u0633\u0642\u064f\u0637\u064f\u0648\u0646\u064e \u0648\u064e\u064a\u064e\u0632\u064f\u0648\u0644\u064f\u0648\u0646\u064e. \u0645\u064e\u0639\u064e \u0623\u0646\u0651\u064e\u0646\u0650\u064a \u0623\u0630\u0644\u064e\u0644\u0652\u062a\u064f\u0643\u0650\u060c \u0644\u064e\u0643\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0646\u0652 \u0623\u064f\u0630\u0650\u0644\u0651\u064e\u0643\u0650 \u062b\u0627\u0646\u0650\u064a\u064e\u0629\u064b."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0648\u064e\u0627\u0644\u0622\u0646\u064e\u060c \u0633\u064e\u0623\u0643\u0633\u0650\u0631\u064f \u0646\u0650\u064a\u0631\u064e\u0643\u0650 \u0639\u064e\u0646\u0652\u0643\u0650\u060c \u0648\u064e\u0633\u064e\u0623\u0646\u0632\u064e\u0639\u064f \u0633\u064e\u0644\u0627\u0633\u0650\u0644\u064e\u0643\u0650.\u00bb"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0623\u0635\u062f\u064e\u0631\u064e \u0627\u0644\u0644\u0647\u064f \u0623\u0645\u0631\u0627\u064b \u0636\u0650\u062f\u0651\u064e\u0643\u064e \u064a\u0627 \u0645\u064e\u0644\u0650\u0643\u064e \u0623\u0634\u0651\u064f\u0648\u0631\u064e: \u00ab\u0644\u064e\u0646\u0652 \u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0644\u064e\u0643\u064e \u0646\u064e\u0633\u0644\u064c \u0628\u064e\u0639\u0652\u062f\u064f. \u0633\u064e\u0623\u064f\u0632\u0650\u064a\u0644\u064f \u0643\u064f\u0644\u0651\u064e \u0635\u064e\u0646\u064e\u0645\u064d \u0648\u064e\u062a\u0650\u0645\u062b\u0627\u0644\u064d \u0645\u064e\u0633\u0628\u064f\u0648\u0643\u064d \u0645\u0650\u0646\u0652 \u0647\u064e\u064a\u0643\u064e\u0644\u0650\u0643\u064e\u060c \u0648\u064e\u0633\u064e\u0623\u064f\u062c\u064e\u0647\u0651\u0650\u0632\u064f \u0642\u064e\u0628\u0631\u064e\u0643\u064e\u060c \u0644\u0650\u0623\u0646\u0651\u064e\u0643\u064e \u062d\u064e\u0642\u0650\u064a\u0631\u064c \u0648\u064e\u0642\u064e\u0644\u0650\u064a\u0644\u064f \u0627\u0644\u0642\u0650\u064a\u0645\u064e\u0629\u0650.\u00bb"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0647\u064f\u0646\u0627\u0643\u064e \u0631\u064e\u0633\u064f\u0648\u0644\u064c \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u062c\u0650\u0628\u0627\u0644\u0650 \u064a\u064e\u062d\u0652\u0645\u0650\u0644\u064f \u0628\u0650\u0634\u0627\u0631\u064e\u0629\u064b\u060c \u064a\u064f\u0639\u0644\u0650\u0646\u064f \u0627\u0644\u0633\u0651\u064e\u0644\u0627\u0645\u064e. \u0627\u062d\u062a\u064e\u0641\u0650\u0644\u0650\u064a \u0628\u0650\u0623\u0639\u064a\u0627\u062f\u0650\u0643\u0650 \u064a\u0627 \u064a\u064e\u0647\u064f\u0648\u0630\u0627. \u0623\u0648\u0652\u0641\u0650\u064a \u0627\u0644\u0646\u0651\u064f\u0630\u064f\u0648\u0631\u064e \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u062a\u064e\u0639\u064e\u0647\u0651\u064e\u062f\u0652\u062a\u0650 \u0628\u0650\u0647\u0627 \u0625\u0644\u064e\u0649 \u0627\u0644\u0644\u0647\u0650. \u0644\u064e\u0646\u0652 \u064a\u062c\u062a\u0627\u062d\u064e\u0643\u0650 \u0627\u0644\u062f\u0651\u064e\u0646\u0650\u064a\u0621\u064f \u0645\u064e\u0631\u0651\u064e\u0629\u064b \u0623\u064f\u062e\u0652\u0631\u064e\u0649\u060c \u0628\u064e\u0644\u0652 \u0633\u064e\u064a\u064f\u0647\u0632\u064e\u0645\u064f \u062a\u064e\u0645\u0627\u0645\u0627\u064b."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u062a\u064e\u062f\u0645\u0650\u064a\u0631\u064f \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 - \u0642\u064e\u062f\u0652 \u062e\u064e\u0631\u064e\u062c\u064e \u0645\u064f\u0628\u064e\u062f\u0651\u0650\u062f\u064f \u0627\u0644\u0634\u0651\u064f\u0639\u064f\u0648\u0628\u0650 \u0644\u0650\u064a\u064f\u0647\u0627\u062c\u0650\u0645\u064e\u0643\u064e \u064a\u0627 \u0645\u064e\u0644\u0650\u0643\u064e \u0623\u0634\u0651\u064f\u0648\u0631\u064e. \u0641\u064e\u0627\u062d\u0652\u0631\u064f\u0633\u0650 \u0627\u0644\u0623\u0645\u0627\u0643\u0650\u0646\u064e \u0627\u0644\u0645\u064f\u062d\u064e\u0635\u0651\u064e\u0646\u064e\u0629\u064e\u060c \u0631\u0627\u0642\u0650\u0628\u0650 \u0627\u0644\u0637\u0651\u064e\u0631\u0650\u064a\u0642\u064e. \u0623\u0639\u0650\u062f\u0651\u064e \u062a\u064e\u062c\u0647\u0650\u064a\u0632\u0627\u062a\u0650 \u0627\u0644\u0645\u064e\u0639\u0631\u064e\u0643\u064e\u0629\u0650\u060c \u062c\u064e\u0647\u0651\u0650\u0632\u0652 \u0646\u064e\u0641\u0633\u064e\u0643\u064e \u0628\u0650\u0642\u064f\u0648\u0651\u064e\u0629\u064d \u0639\u064e\u0638\u0650\u064a\u0645\u064e\u0629\u064d."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0644\u0650\u0623\u0646\u0651\u064e \u0627\u0644\u0644\u0647\u064e \u0633\u064e\u064a\u064e\u0631\u064f\u062f\u0651\u064f \u0645\u064e\u062c\u062f\u064e \u064a\u064e\u0639\u0642\u064f\u0648\u0628\u064e\u060c \u0644\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0643\u064e\u0645\u064e\u062c\u062f\u0650 \u0625\u0633\u0652\u0631\u0627\u0626\u0650\u064a\u0644\u064e. \u0641\u064e\u0642\u064e\u062f\u0652 \u0623\u062e\u0631\u064e\u0628\u064e\u0647\u064f\u0645\u064f \u0627\u0644\u0645\u064f\u062e\u064e\u0631\u0651\u0650\u0628\u064f\u0648\u0646\u064e\u060c \u0648\u064e\u0642\u064e\u062f\u0652 \u0623\u062a\u0644\u064e\u0641\u064f\u0648\u0627 \u0643\u064f\u0631\u064f\u0648\u0645\u064e \u0625\u0633\u0652\u0631\u0627\u0626\u0650\u064a\u0644\u064e."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u062a\u064f\u0631\u064f\u0648\u0633\u064f \u0645\u062d\u0627\u0631\u0650\u0628\u064a\u0647\u0650 \u062d\u064e\u0645\u0631\u0627\u0621\u064f\u060c \u0648\u064e\u062b\u0650\u064a\u0627\u0628\u064f \u062c\u064f\u0646\u064f\u0648\u062f\u0650\u0647\u0650 \u0642\u064f\u0631\u0645\u064f\u0632\u0650\u064a\u0651\u064e\u0629\u064c. \u0645\u064e\u0639\u062f\u0650\u0646\u064f \u0627\u0644\u0645\u064e\u0631\u0652\u0643\u064e\u0628\u0629\u0650 \u064a\u064e\u0644\u0645\u064e\u0639\u064f \u0643\u064e\u0627\u0644\u0646\u0651\u0627\u0631\u0650\u060c \u0641\u0650\u064a \u064a\u064e\u0648\u0645\u0650 \u0627\u0633\u062a\u0650\u0639\u062f\u0627\u062f\u0650\u0647\u0650 \u0644\u0650\u0644\u0645\u064e\u0639\u0631\u064e\u0643\u064e\u0629\u0650\u060c \u0648\u064e\u0627\u0644\u0631\u0651\u0650\u0645\u0627\u062d\u064f \u0645\u064f\u0647\u062a\u064e\u0632\u0651\u064e\u0629\u064c."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u062a\u064e\u0646\u062f\u064e\u0641\u0650\u0639\u064f \u0627\u0644\u0645\u064e\u0631\u0643\u064e\u0628\u0627\u062a\u064f \u0628\u0650\u0639\u064f\u0646\u0652\u0641\u064d \u0641\u0650\u064a \u0627\u0644\u0634\u0651\u064e\u0648\u0627\u0631\u0650\u0639\u0650\u060c \u062a\u064e\u062a\u064e\u0633\u0627\u0628\u064e\u0642\u064f \u0645\u064e\u0639\u0627\u064b \u0641\u0650\u064a \u0627\u0644\u0633\u0651\u0627\u062d\u0627\u062a\u0650. \u064a\u064e\u0628\u062f\u064f\u0648\u0646\u064e \u0645\u0650\u062b\u0644\u064e \u0645\u064e\u0634\u0627\u0639\u0650\u0644\u064e. \u064a\u064e\u0646\u062f\u064e\u0641\u0650\u0639\u064f\u0648\u0646\u064e \u0643\u064e\u0627\u0644\u0628\u064e\u0631\u0642\u0650."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u064a\u064f\u0639\u0637\u0650\u064a \u0627\u0644\u0645\u064f\u0628\u064e\u062f\u0651\u0650\u062f\u064f \u0623\u0648\u0627\u0645\u0650\u0631\u064e \u0644\u0650\u0642\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u060c \u0641\u064e\u064a\u064e\u062a\u064e\u0639\u064e\u062b\u0651\u064e\u0631\u064f\u0648\u0646\u064e \u0625\u0630\u0652 \u064a\u064e\u062a\u064e\u0642\u064e\u062f\u0651\u064e\u0645\u064f\u0648\u0646\u064e. \u064a\u064f\u0633\u0631\u0650\u0639\u064f\u0648\u0646\u064e \u0625\u0644\u064e\u0649 \u0627\u0644\u0633\u0651\u064f\u0648\u0631\u0650\u060c \u0648\u064e\u064a\u064e\u0646\u0652\u0635\u0650\u0628\u064f\u0648\u0646\u064e \u0628\u064f\u0631\u064f\u0648\u062c\u064e \u0627\u0644\u062d\u0650\u0635\u0627\u0631\u0650."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0627\u0646\u0641\u064e\u062a\u064e\u062d\u064e\u062a\u0652 \u0623\u0628\u0648\u0627\u0628\u064f \u0627\u0644\u0641\u064e\u064a\u064e\u0636\u0627\u0646\u0650\u060c \u0648\u064e\u0627\u0646\u0652\u0647\u0627\u0631\u064e \u0642\u064e\u0635\u0652\u0631\u064f \u0627\u0644\u0645\u064e\u0644\u0650\u0643\u0650."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u062a\u064f\u062c\u064e\u0631\u0651\u064e\u062f\u064f \u0627\u0644\u0645\u064e\u0644\u0650\u0643\u064e\u0629\u064f \u0648\u064e\u062a\u064f\u0633\u0652\u0628\u064e\u0649\u060c \u0648\u064e\u062a\u064f\u0624\u0652\u062e\u064e\u0630\u064f \u062c\u064e\u0648\u0627\u0631\u0650\u064a\u0647\u0627 \u0628\u064e\u0639\u0650\u064a\u062f\u0627\u064b. \u064a\u064e\u0642\u0631\u064e\u0639\u0652\u0646\u064e \u0639\u064e\u0644\u064e\u0649 \u0635\u064f\u062f\u064f\u0648\u0631\u0650\u0647\u0650\u0646\u0651\u064e\u060c \u0648\u064e\u064a\u064e\u062a\u064e\u0646\u064e\u0647\u0651\u064e\u062f\u0652\u0646\u064e \u062a\u064e\u0646\u064e\u0647\u0651\u064f\u062f\u0627\u064b \u0643\u064e\u0647\u064e\u062f\u0650\u064a\u0644\u0650 \u0627\u0644\u062d\u064e\u0645\u0627\u0645\u0650."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 \u0645\u0650\u062b\u0644\u064e \u0628\u0650\u0631\u0652\u0643\u064e\u0629\u0650 \u0645\u0627\u0621\u064d \u064a\u064e\u0631\u0652\u0634\u064e\u062d\u064f \u0645\u0627\u0624\u064f\u0647\u0627 \u0645\u0650\u0646\u0652\u0647\u0627 \u0628\u0650\u0633\u064f\u0631\u0639\u064e\u0629\u064d. \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0642\u0627\u062f\u064e\u062a\u064f\u0647\u0627: \u00ab\u062a\u064e\u0648\u064e\u0642\u0651\u064e\u0641\u0652! \u062a\u064e\u0648\u064e\u0642\u0651\u064e\u0641\u0652!\u00bb \u0648\u064e\u0644\u064e\u0643\u0650\u0646\u0652 \u0644\u0627 \u064a\u064e\u0644\u0652\u062a\u064e\u0641\u0650\u062a\u064f \u0623\u062d\u064e\u062f\u064c."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0627\u0646\u0647\u064e\u0628\u064f\u0648\u0627 \u0627\u0644\u0641\u0650\u0636\u0651\u064e\u0629\u064e! \u0627\u0646\u0647\u064e\u0628\u064f\u0648\u0627 \u0627\u0644\u0630\u0651\u064e\u0647\u064e\u0628\u0650! \u0644\u0627 \u0646\u0650\u0647\u0627\u064a\u064e\u0629\u064e \u0644\u0650\u0644\u0643\u064f\u0646\u064f\u0648\u0632\u0650 \u0645\u0650\u0646\u0652 \u0643\u064e\u062b\u0631\u064e\u062a\u0650\u0647\u0627\u060c \u0648\u064e\u0641\u0650\u064a\u0647\u0627 \u0627\u0644\u0643\u064e\u062b\u0650\u064a\u0631\u064f \u0645\u0650\u0646\u064e \u0627\u0644\u0623\u0634\u064a\u0627\u0621\u0650 \u0627\u0644\u0646\u0651\u064e\u0641\u0650\u064a\u0633\u064e\u0629\u0650."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0641\u064e\u0631\u0627\u063a\u064c \u0648\u064e\u062f\u064e\u0645\u0627\u0631\u064c \u0648\u064e\u062e\u064e\u0631\u0627\u0628\u064c! \u0630\u0627\u0628\u064e\u062a\u0652 \u0642\u064f\u0644\u064f\u0648\u0628\u064c \u0648\u064e\u0627\u0631\u062a\u064e\u0639\u064e\u0634\u064e\u062a\u0652 \u0623\u0631\u062c\u064f\u0644\u064c \u0645\u0650\u0646\u064e \u0627\u0644\u062e\u064e\u0648\u0641\u0650. \u0627\u0647\u062a\u0651\u0632\u0651\u064e\u062a\u0650 \u0627\u0644\u0623\u0628\u062f\u0627\u0646\u064f \u0648\u064e\u0634\u064e\u062d\u064e\u0628\u064e\u062a\u0650 \u0627\u0644\u0648\u064f\u062c\u064f\u0648\u0647\u064f \u062c\u064e\u0645\u064a\u0639\u0627\u064b. \u0648\u064e\u0642\u064e\u062f\u0650 \u0627\u0628\u064a\u064e\u0636\u0651\u064e\u062a\u0652 \u0648\u064f\u062c\u064f\u0648\u0647\u064f\u0647\u064f\u0645\u0652 \u0645\u0650\u0646\u064e \u0627\u0644\u062e\u064e\u0648\u0641\u0650."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0623\u064a\u0646\u064e \u0645\u064e\u0633\u0652\u0643\u0650\u0646\u064f \u0627\u0644\u0623\u064f\u0633\u064f\u0648\u062f\u0650\u060c \u0648\u064e\u0639\u064e\u0631\u0650\u064a\u0646\u064f \u0627\u0644\u0623\u0634\u0628\u0627\u0644\u0650\u061f \u0623\u064a\u0646\u064e \u0627\u0644\u0645\u064e\u0643\u0627\u0646\u064f \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0630\u064e\u0647\u064e\u0628\u064e \u0625\u0644\u064e\u064a\u0647\u0650 \u0627\u0644\u0623\u0633\u064e\u062f\u064f \u0648\u064e\u0627\u0644\u0644\u0651\u064e\u0628\u064f\u0624\u064e\u0629\u064f\u060c \u062d\u064e\u064a\u062b\u064f \u0644\u0627 \u064a\u064e\u062e\u0634\u064e\u0649 \u062c\u064e\u0631\u0652\u0648\u064f \u0627\u0644\u0623\u0633\u064e\u062f\u0650 \u0623\u0630\u064e\u0649\u061f"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u064a\u064e\u062c\u0650\u062f\u064f \u0627\u0644\u0623\u0633\u064e\u062f\u064f \u0637\u064e\u0639\u0627\u0645\u0627\u064b \u0643\u064e\u062b\u0650\u064a\u0631\u0627\u064b \u0644\u0650\u062c\u0650\u0631\u0627\u0626\u0650\u0647\u0650\u060c \u0648\u064e\u064a\u064e\u0630\u0628\u064e\u062d\u064f \u0644\u0650\u0644\u064e\u0628\u064f\u0624\u0627\u062a\u0650\u0647\u0650. \u064a\u064e\u0645\u0644\u0623\u064f \u0645\u064e\u063a\u0627\u0631\u064e\u062a\u064e\u0647\u064f \u0628\u0650\u0627\u0644\u0641\u064e\u0631\u0627\u0626\u0650\u0633\u0650\u060c \u0648\u064e\u0643\u064f\u0647\u064f\u0648\u0641\u064e\u0647\u064f \u0628\u0650\u0627\u0644\u0651\u064e\u0644\u064e\u062d\u0645\u0650 \u0627\u0644\u0645\u064f\u0645\u064e\u0632\u0651\u064e\u0642\u0650."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064f: \u00ab\u0623\u0646\u0627 \u0636\u0650\u062f\u0651\u064f\u0643\u0650. \u0633\u064e\u0623\u064f\u062d\u0631\u0650\u0642\u064f \u0645\u0631\u0643\u0628\u0627\u062a\u0650\u0643\u0650 \u062d\u064e\u062a\u0651\u064e\u0649 \u0644\u0627 \u064a\u064e\u0628\u0642\u064e\u0649 \u0633\u0650\u0648\u064e\u0649 \u0627\u0644\u062f\u0651\u064f\u062e\u0627\u0646\u0650\u060c \u0648\u064e\u0633\u064e\u064a\u064f\u0642\u062a\u064e\u0644\u064f \u0623\u0634\u0628\u0627\u0644\u064f\u0643\u0650 \u0641\u0650\u064a \u0627\u0644\u0645\u064e\u0639\u0631\u064e\u0643\u064e\u0629\u0650. \u0633\u064e\u0623\u064f\u062e\u0631\u0650\u062c\u064f \u0641\u064e\u0631\u0627\u0626\u0650\u0633\u064e\u0643\u0650 \u0645\u0650\u0646\u064e \u0627\u0644\u0623\u0631\u0652\u0636\u0650\u060c \u0648\u064e\u0635\u064e\u0648\u062a\u064f \u0631\u064f\u0633\u064f\u0644\u0650\u0643\u0650 \u0644\u064e\u0646\u0652 \u064a\u064f\u0633\u0645\u064e\u0639\u064e \u062b\u0627\u0646\u064a\u0629\u064b.\u00bb"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0623\u062e\u0628\u0627\u0631\u064c \u0633\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u064c \u0644\u0650\u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 - \u0648\u064e\u064a\u0644\u064c \u0644\u064e\u0643\u0650 \u064a\u0627 \u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u064e \u0627\u0644\u0642\u064e\u062a\u064e\u0644\u064e\u0629\u0650\u060c \u0627\u0644\u0645\u064e\u0644\u0650\u064a\u0626\u0650\u0629\u064e \u0628\u0650\u0627\u0644\u0643\u064e\u0630\u0650\u0628\u0650\u060c \u0627\u0644\u0645\u064e\u0644\u0650\u064a\u0626\u0650\u0629\u0650 \u0628\u0650\u0627\u0644\u063a\u064e\u0646\u0627\u0626\u0650\u0645\u0650\u060c \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u0644\u0627 \u062a\u064e\u062e\u0644\u064f\u0648 \u0645\u0650\u0646\u064e \u0627\u0644\u0641\u064e\u0631\u0627\u0626\u0650\u0633\u0650."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0635\u064e\u0648\u062a\u064f \u0636\u064e\u0631\u0628\u0627\u062a\u0650 \u0633\u064e\u0648\u0637\u064d\u060c \u0648\u064e\u0636\u064e\u062c\u0650\u064a\u062c\u064f \u062f\u064e\u0648\u0627\u0644\u0650\u064a\u0628\u064e\u060c \u0648\u064e\u0635\u064e\u0648\u062a\u064f \u062e\u064f\u064a\u064f\u0648\u0644\u064d \u062a\u064e\u062c\u0631\u0650\u064a \u0648\u064e\u0645\u064e\u0631\u0643\u064e\u0628\u0627\u062a\u064d \u062a\u064e\u062a\u064e\u0642\u0627\u0641\u064e\u0632\u064f."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0627\u0644\u062c\u064e\u064a\u0627\u062f\u064f \u0645\u064f\u0646\u062f\u064e\u0641\u0650\u0639\u064e\u0629\u064c\u060c \u0648\u064e\u0627\u0644\u0633\u0651\u064e\u064a\u0641\u064f \u064a\u064e\u0644\u0645\u064e\u0639\u064f\u060c \u0627\u0644\u0631\u0651\u064f\u0645\u062d\u064f \u064a\u064f\u0628\u0631\u0650\u0642\u064f. \u0623\u0643\u0648\u0627\u0645\u064c \u0645\u0650\u0646\u064e \u0627\u0644\u0642\u064f\u062a\u0644\u064e\u0649\u060c \u0623\u0643\u062f\u0627\u0633\u064c \u0645\u0650\u0646\u064e \u0627\u0644\u062c\u064f\u062b\u064e\u062b\u0650 \u0628\u0650\u0644\u0627 \u062d\u064f\u062f\u064f\u0648\u062f\u064d. \u0625\u0646\u0651\u064e\u0647\u064f\u0645\u0652 \u064a\u064e\u062a\u064e\u0639\u064e\u062b\u0651\u064e\u0631\u0648\u0646\u064e \u0628\u0650\u0627\u0644\u062c\u064f\u062b\u064e\u062b\u0650!"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0628\u0650\u0633\u064e\u0628\u064e\u0628\u0650 \u0627\u0644\u0632\u0651\u0650\u0646\u064e\u0649 \u0627\u0644\u0643\u064e\u062b\u0650\u064a\u0631\u0650 \u0644\u0650\u0644\u0632\u0651\u0627\u0646\u0650\u064a\u064e\u0629\u0650\u060c \u0627\u0644\u0633\u0651\u0627\u062d\u0650\u0631\u064e\u0629\u0650 \u0627\u0644\u062c\u064e\u0645\u0650\u064a\u0644\u064e\u0629\u0650 \u0627\u0644\u0641\u0627\u062a\u0650\u0646\u064e\u0629\u0650\u060c \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u062a\u064e\u0633\u062a\u064e\u0639\u0628\u0650\u062f\u064f \u0623\u064f\u0645\u064e\u0645\u0627\u064b \u0643\u0627\u0645\u0650\u0644\u064e\u0629\u064b \u0628\u0650\u0637\u064f\u0631\u064f\u0642\u0650\u0647\u0627 \u0627\u0644\u062e\u0627\u062f\u0650\u0639\u064e\u0629\u0650\u060c \u0648\u064e\u0639\u064e\u0634\u0627\u0626\u0650\u0631\u064e \u0643\u0627\u0645\u0650\u0644\u064e\u0629\u064b \u0628\u0650\u0623\u0633\u062d\u0627\u0631\u0650\u0647\u0627\u060c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0641\u064e\u0625\u0646\u0651\u064e \u0627\u0644\u0644\u0647\u064e \u0627\u0644\u0642\u064e\u062f\u0650\u064a\u0631\u064e \u064a\u064e\u0642\u064f\u0648\u0644\u064f: \u00ab\u0623\u0646\u0627 \u0636\u0650\u062f\u0651\u064f\u0643\u0650\u060c \u0648\u064e\u0633\u064e\u0623\u0631\u0641\u064e\u0639\u064f \u0623\u0637\u0631\u0627\u0641\u064e \u062b\u064e\u0648\u0628\u0650\u0643\u0650 \u0625\u0644\u064e\u0649 \u0648\u064e\u062c\u0647\u0650\u0643\u0650\u060c \u0648\u064e\u0633\u064e\u0623\u064f\u0631\u0650\u064a \u0627\u0644\u0623\u064f\u0645\u064e\u0645\u064e \u062c\u064e\u0633\u064e\u062f\u064e\u0643\u0650 \u0639\u0627\u0631\u0650\u064a\u0627\u064b\u060c \u0648\u064e\u0623\u064f\u0631\u0650\u064a \u0627\u0644\u0645\u064e\u0645\u0627\u0644\u0650\u0643\u064e \u062e\u0650\u0632\u0652\u064a\u064e\u0643\u0650."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0633\u064e\u0623\u0631\u0645\u064a\u0643\u0650 \u0628\u0650\u0627\u0644\u0646\u0651\u0650\u0641\u0627\u064a\u0627\u062a\u0650\u060c \u0648\u064e\u0633\u064e\u0623\u064f\u0639\u0627\u0645\u0650\u0644\u064f\u0643\u0650 \u0628\u0650\u0627\u062d\u062a\u0650\u0642\u0627\u0631\u064d\u060c \u0648\u064e\u0633\u064e\u0623\u064f\u0634\u064e\u0647\u0651\u0650\u0631\u064f \u0628\u0650\u0643\u0650 \u0623\u0645\u0627\u0645\u064e \u0627\u0644\u062c\u064e\u0645\u0650\u064a\u0639\u0650."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u062d\u064e\u064a\u0646\u064e\u0626\u0650\u0630\u064d \u0633\u064e\u064a\u064e\u0647\u0631\u064f\u0628\u064f \u0645\u0650\u0646\u0643\u0650 \u0643\u064f\u0644\u0651\u064f \u0645\u064e\u0646\u0652 \u064a\u064e\u0631\u0627\u0643\u0650\u060c \u0648\u064e\u0633\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u062c\u064e\u0645\u0650\u064a\u0639\u064f: \u2039\u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 \u062e\u064e\u0631\u0650\u0628\u064e\u0629\u064c\u060c \u0641\u064e\u0645\u064e\u0646\u0652 \u0633\u064e\u064a\u064e\u062d\u0632\u064e\u0646\u064f \u0639\u064e\u0644\u064e\u064a\u0647\u0627\u061f\u203a \u0644\u064e\u0646\u0652 \u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0647\u064f\u0646\u0627\u0643\u064e \u0645\u064e\u0646\u0652 \u064a\u064f\u0634\u0641\u0650\u0642\u064f \u0639\u064e\u0644\u064e\u064a\u0643\u0650.\u00bb"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0647\u064e\u0644\u0652 \u0623\u0646\u062a\u0650 \u0623\u0641\u0636\u064e\u0644\u064f \u0645\u0650\u0646\u0652 \u0637\u0650\u064a\u0628\u064e\u0629\u064e \u0627\u0644\u0642\u0627\u0626\u0650\u0645\u064e\u0629\u0650 \u0628\u064e\u064a\u0646\u064e \u062c\u064e\u062f\u0627\u0648\u0650\u0644\u0650 \u0646\u064e\u0647\u0652\u0631\u0650\u0627\u0644\u0646\u0651\u0650\u064a\u0644\u0650\u060c \u0627\u0644\u0645\u064f\u062d\u0627\u0637\u064e\u0629\u0650 \u0628\u0650\u0627\u0644\u0645\u0627\u0621\u0650\u061f \u0642\u064e\u062f\u0652 \u0643\u0627\u0646\u064e \u0627\u0644\u0628\u064e\u062d\u0652\u0631\u064f \u0644\u064e\u0647\u0627 \u062d\u0650\u0635\u0652\u0646\u0627\u064b\u060c \u0648\u064e\u0627\u0644\u0645\u0627\u0621\u064f \u0633\u064f\u0648\u0631\u0627\u064b \u0645\u0650\u0646\u0652 \u062d\u064e\u0648\u0644\u0650\u0647\u0627."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0643\u064f\u0648\u0634\u064f \u0648\u064e\u0645\u0650\u0635\u0652\u0631\u064f \u0623\u0639\u0637\u064e\u064a\u0627\u0647\u0627 \u0642\u064f\u0648\u0651\u064e\u0629\u064b \u0639\u064e\u0638\u0650\u064a\u0645\u064e\u0629\u064b. \u0643\u0627\u0646\u064e\u062a\u0650 \u0641\u064f\u0648\u0637\u064f \u0648\u064e\u0644\u0650\u064a\u0628\u064a\u0627 \u0645\u0650\u0646\u0652 \u062d\u064f\u0644\u064e\u0641\u0627\u0626\u0650\u0647\u0627 \u0648\u064e\u062f\u0627\u0639\u0650\u0645\u0650\u064a\u0647\u0627."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0648\u064e\u0645\u064e\u0639\u064e \u0647\u064e\u0630\u0627 \u0646\u064f\u0641\u0650\u064a\u064e\u062a\u0652 \u0648\u064e\u0633\u064f\u0628\u0650\u064a\u064e\u062a\u0652. \u062d\u064e\u062a\u0651\u064e\u0649 \u0623\u0637\u0641\u0627\u0644\u064f\u0647\u0627 \u062d\u064f\u0637\u0651\u0650\u0645\u064f\u0648\u0627 \u0641\u0650\u064a \u0632\u0627\u0648\u0650\u064a\u064e\u0629\u0650 \u0643\u064f\u0644\u0651\u0650 \u0634\u0627\u0631\u0650\u0639\u064d. \u0627\u064f\u0644\u0642\u0650\u064a\u064e\u062a\u0650 \u0627\u0644\u0642\u064f\u0631\u0639\u064e\u0629\u064f \u0639\u064e\u0644\u064e\u0649 \u0623\u0634\u0631\u0627\u0641\u0650\u0647\u0627\u060c \u0648\u064e\u0643\u064f\u0644\u0651\u064f \u0648\u064f\u062c\u064e\u0647\u0627\u0626\u0650\u0647\u0627 \u0642\u064f\u064a\u0651\u0650\u062f\u064f\u0648\u0627 \u0628\u0650\u0627\u0644\u0633\u0651\u064e\u0644\u0627\u0633\u0650\u0644\u0650."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u062d\u064e\u062a\u0651\u064e\u0649 \u0623\u0646\u062a\u0650 \u0633\u064e\u062a\u064f\u0635\u0628\u0650\u062d\u0650\u064a\u0646\u064e \u0643\u064e\u0633\u064e\u0643\u0631\u064e\u0649\u060c \u0648\u064e\u0633\u064e\u062a\u064f\u062d\u0627\u0648\u0650\u0644\u0650\u064a\u0646\u064e \u0627\u0644\u0627\u062e\u062a\u0650\u0628\u0627\u0621\u064e. \u062d\u064e\u062a\u0651\u064e\u0649 \u0623\u0646\u062a\u0650 \u0633\u064e\u062a\u064e\u0628\u062d\u064e\u062b\u0650\u064a\u0646\u064e \u0639\u064e\u0646\u0652 \u0645\u064e\u0644\u0627\u0630\u064d \u0645\u0650\u0646\u064e \u0627\u0644\u0639\u064e\u062f\u064f\u0648\u0651\u0650."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0633\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064f \u0643\u064f\u0644\u0651\u064f \u062d\u064f\u0635\u064f\u0648\u0646\u0650\u0643\u0650 \u0643\u064e\u0623\u0634\u062c\u0627\u0631\u0650 \u062a\u0650\u064a\u0646\u064d \u0645\u064f\u062d\u064e\u0645\u0651\u064e\u0644\u064e\u0629\u064d \u0628\u0650\u0623\u0641\u0636\u064e\u0644\u0650 \u062b\u0650\u0645\u0627\u0631\u064d\u060c \u0625\u0646\u0652 \u0647\u064f\u0632\u0651\u064e\u062a\u0652 \u064a\u064e\u062a\u064e\u0633\u0627\u0642\u064e\u0637\u064f \u062b\u064e\u0645\u064e\u0631\u064f\u0647\u0627 \u0641\u0650\u064a \u0641\u064e\u0645\u0650 \u0627\u0644\u0622\u0643\u0650\u0644\u0650."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u064a\u0627 \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649\u060c \u0633\u064e\u064a\u064e\u0628\u062f\u064f\u0648 \u0634\u064e\u0639\u0628\u064f\u0643\u0650 \u0641\u0650\u064a\u0643\u0650 \u0643\u064e\u0627\u0644\u0646\u0651\u0650\u0633\u0627\u0621\u0650! \u0623\u0628\u0648\u0627\u0628\u064f \u0623\u0631\u0652\u0636\u0650\u0643\u0650 \u0645\u064e\u0641\u062a\u064f\u0648\u062d\u064e\u0629\u064c \u062a\u064e\u0645\u0627\u0645\u0627\u064b \u0644\u0650\u0623\u0639\u062f\u0627\u0626\u0650\u0643\u0650. \u0627\u0644\u0646\u0651\u0627\u0631\u064f \u0627\u0644\u062a\u064e\u0647\u064e\u0645\u064e\u062a\u0652 \u0623\u0642\u0641\u0627\u0644\u064e\u0647\u0627."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0627\u062c\u0645\u064e\u0639\u0650\u064a \u0645\u0627\u0621\u064b \u0645\u0650\u0646\u0652 \u0623\u062c\u0644\u0650 \u0623\u064a\u0651\u0627\u0645\u0650 \u0627\u0644\u062d\u0650\u0635\u0627\u0631\u0650. \u0642\u064e\u0648\u0651\u0650\u064a \u062a\u064e\u062d\u0635\u0650\u064a\u0646\u0627\u062a\u0650\u0643\u0650. \u0623\u062c\u0628\u0650\u0644\u0650\u064a \u0627\u0644\u0637\u0651\u0650\u064a\u0646\u064e \u0648\u064e\u0627\u0644\u0631\u0651\u064e\u0645\u0644\u064e\u060c \u0648\u064e\u062c\u064e\u0647\u0651\u0650\u0632\u0650\u064a \u0642\u064e\u0648\u0627\u0644\u0650\u0628\u064e \u0627\u0644\u0644\u0651\u0650\u0628\u0652\u0646\u0650."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0633\u064e\u062a\u064e\u0644\u062a\u064e\u0647\u0650\u0645\u064f\u0643\u0650 \u0627\u0644\u0646\u0651\u0627\u0631\u064f\u060c \u0648\u064e\u0633\u064e\u064a\u064e\u0642\u0637\u064e\u0639\u064f\u0643\u0650 \u0627\u0644\u0633\u0651\u064e\u064a\u0641\u064f. \u0633\u064e\u062a\u064e\u0623\u0643\u064f\u0644\u064f\u0643\u0650 \u0627\u0644\u0646\u0651\u0627\u0631\u064f \u0643\u064e\u0627\u0644\u062c\u064e\u0631\u0627\u062f\u0650. \u062a\u064e\u0643\u0627\u062b\u064e\u0631\u0650\u064a \u0643\u064e\u0627\u0644\u062c\u064e\u0631\u0627\u062f\u0650\u060c \u0648\u064e\u0627\u0632\u062f\u0627\u062f\u064e\u064a \u0643\u064e\u0627\u0644\u062c\u064e\u0646\u0627\u062f\u0650\u0628\u0650!"
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0643\u064e\u062b\u0651\u0650\u0631\u0650\u064a \u062a\u064f\u062c\u0651\u0627\u0631\u064e\u0643\u0650 \u0643\u064e\u0646\u064f\u062c\u064f\u0648\u0645\u0650 \u0627\u0644\u0633\u0651\u064e\u0645\u0627\u0621\u0650. \u0625\u0646\u0651\u064e\u0647\u064f\u0645\u0652 \u0643\u064e\u0627\u0644\u062c\u064e\u0631\u0627\u062f\u0650 \u0627\u0644\u0651\u0630\u064a \u064a\u064e\u0644\u0652\u062a\u064e\u0647\u0650\u0645\u064f \u0643\u064f\u0644\u0651\u064e \u0645\u0627 \u0647\u064f\u0648\u064e \u0623\u0645\u0627\u0645\u064e\u0647\u064f\u060c \u0648\u064e\u0628\u064e\u0639\u0652\u062f\u064e \u0630\u064e\u0644\u0650\u0643\u064e \u064a\u064e\u0637\u0650\u064a\u0631\u064f."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u062d\u064f\u0631\u0651\u0627\u0633\u064f\u0643\u0650 \u0643\u064e\u0627\u0644\u062c\u064e\u0631\u0627\u062f\u0650\u060c \u0648\u0642\u0627\u062f\u062a\u064f\u0643\u0650 \u0643\u064e\u0623\u0633\u0652\u0631\u0627\u0628\u0650 \u0627\u0644\u062c\u0631\u0627\u062f\u0650 \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u062a\u064e\u0633\u0643\u064f\u0646\u064f \u0641\u0650\u064a \u0627\u0644\u062c\u064f\u062f\u0631\u0627\u0646\u0650 \u0641\u0650\u064a \u064a\u064e\u0648\u0645\u064d \u0628\u0627\u0631\u0650\u062f\u064d. \u0644\u064e\u0643\u0650\u0646\u0652 \u062d\u0650\u064a\u0646\u064e \u062a\u064f\u0634\u0652\u0631\u0650\u0642\u064f \u0627\u0644\u0634\u0651\u064e\u0645\u0633\u064f\u060c \u0641\u064e\u0625\u0646\u0651\u064e\u0647\u0627 \u062a\u064e\u0637\u0650\u064a\u0631\u064f\u060c \u0648\u064e\u0644\u0627 \u064a\u064e\u0639\u0631\u0650\u0641\u064f \u0623\u062d\u064e\u062f\u064c \u0625\u0644\u064e\u0649 \u0623\u064a\u0646\u064e \u062a\u064e\u0630\u0652\u0647\u064e\u0628\u064f."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u064a\u0627 \u0645\u064e\u0644\u0650\u0643\u064e \u0623\u0634\u0651\u064f\u0648\u0631\u064e\u060c \u0631\u064f\u0639\u0627\u062a\u064f\u0643\u064e \u0646\u064e\u0639\u0650\u0633\u064f\u0648\u0627 \u0648\u064e\u0646\u0627\u0645\u064f\u0648\u0627! \u0642\u0627\u062f\u064e\u062a\u064f\u0643\u064e \u0627\u0633\u062a\u064e\u0644\u0652\u0642\u064f\u0648\u0627 \u0644\u0650\u0642\u064e\u0636\u0627\u0621\u0650 \u0627\u0644\u0644\u0651\u064e\u064a\u0644\u0650. \u0634\u064e\u0639\u0628\u064f\u0643\u064e \u0645\u064f\u0634\u064e\u062a\u0651\u064e\u062a\u064c \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u062a\u0651\u0650\u0644\u0627\u0644\u0650\u060c \u0648\u064e\u0644\u064e\u064a\u0633\u064e \u0645\u064e\u0646\u0652 \u064a\u064e\u062c\u0645\u064e\u0639\u064f\u0647\u064f."
                },
