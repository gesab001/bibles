                "1": {
                    "verse_nr": "1",
                    "verse": "\u0635\u064e\u0644\u0651\u064f\u0648\u0627 \u0645\u0650\u0646\u0652 \u0623\u062c\u0644\u0650\u0646\u0627 - \u0623\u062e\u0650\u064a\u0631\u0627\u064b \u0623\u064a\u0651\u064f\u0647\u0627 \u0627\u0644\u0625\u062e\u0648\u064e\u0629\u064f\u060c \u0646\u064e\u0637\u0644\u064f\u0628\u064f \u0645\u0650\u0646\u0643\u064f\u0645\u0652 \u0623\u0646\u0652 \u062a\u064f\u0635\u064e\u0644\u0651\u064f\u0648\u0627 \u0645\u0650\u0646\u0652 \u0623\u062c\u0644\u0650\u0646\u0627\u060c \u0644\u0650\u0643\u064e\u064a \u062a\u064e\u0646\u062a\u064e\u0634\u0650\u0631\u064e \u0631\u0650\u0633\u0627\u0644\u064e\u0629\u064f \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650 \u0628\u0650\u0633\u064f\u0631\u0639\u064e\u0629\u064d \u0648\u064e\u062a\u064e\u062a\u064e\u0645\u064e\u062c\u0651\u064e\u062f\u064e\u060c \u0643\u064e\u0645\u0627 \u062d\u064e\u062f\u064e\u062b\u064e \u0639\u0650\u0646\u062f\u064e\u0643\u064f\u0645\u0652."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0648\u064e\u0635\u064e\u0644\u0651\u064f\u0648\u0627 \u0623\u0646\u0652 \u064a\u064f\u0646\u0642\u0650\u0630\u064e\u0646\u0627 \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u064f \u0645\u0650\u0646\u064e \u0627\u0644\u0645\u064f\u0646\u062d\u064e\u0631\u0650\u0641\u0650\u064a\u0646\u064e \u0627\u0644\u0623\u0634\u0631\u0627\u0631\u0650. \u0641\u064e\u0644\u064e\u064a\u0633\u064e \u0643\u064f\u0644\u0651\u064f \u0627\u0644\u0646\u0651\u0627\u0633\u0650 \u064a\u064f\u0624\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650\u060c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0644\u064e\u0643\u0650\u0646\u0651\u064e \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u064e \u0623\u0645\u0650\u064a\u0646\u064c \u062f\u0627\u0626\u0650\u0645\u0627\u064b\u060c \u0648\u064e\u0647\u064f\u0648\u064e \u0633\u064e\u064a\u064f\u0642\u064e\u0648\u0651\u0650\u064a\u0643\u064f\u0645\u0652 \u0648\u064e\u064a\u064e\u062d\u0631\u064f\u0633\u064f\u0643\u064f\u0645\u0652 \u0645\u0650\u0646\u064e \u0627\u0644\u0634\u0651\u0650\u0631\u0651\u0650\u064a\u0652\u0631\u0650."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0646\u064e\u062d\u0646\u064f \u0648\u0627\u062b\u0650\u0642\u064f\u0648\u0646\u064e \u0628\u0650\u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650 \u0628\u0650\u0634\u064e\u0623\u0646\u0650\u0643\u064f\u0645\u0652\u060c \u0648\u064e\u0645\u064f\u062a\u064e\u0623\u0643\u0651\u0650\u062f\u064f\u0648\u0646\u064e \u0623\u0646\u0651\u064e\u0643\u064f\u0645\u0652 \u062a\u064e\u0639\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0648\u064e\u0633\u064e\u062a\u064e\u0639\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0628\u0650\u0645\u0627 \u0623\u0648\u0635\u064e\u064a\u0646\u0627\u0643\u064f\u0645\u0652."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0641\u064e\u0644\u064e\u064a\u062a\u064e \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u064e \u064a\u064f\u0648\u064e\u062c\u0651\u0650\u0647\u064f \u0642\u064f\u0644\u064f\u0648\u0628\u064e\u0643\u064f\u0645\u0652 \u0625\u0644\u064e\u0649 \u0645\u064e\u062d\u064e\u0628\u0651\u064e\u0629\u0650 \u0627\u0644\u0644\u0647\u0650 \u0648\u064e\u0625\u0644\u064e\u0649 \u0635\u064e\u0628\u0631\u0650 \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u062d\u0650."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0623\u0647\u064e\u0645\u0651\u0650\u064a\u0651\u064e\u0629\u064f \u0627\u0644\u0639\u064e\u0645\u064e\u0644 - \u0648\u064e\u0627\u0644\u0622\u0646\u064e \u0646\u064f\u0648\u0635\u0650\u064a\u0643\u064f\u0645\u0652\u060c \u0623\u064a\u0651\u064f\u0647\u0627 \u0627\u0644\u0625\u062e\u0648\u064e\u0629\u064f \u0628\u0650\u0627\u0633\u0652\u0645\u0650 \u0631\u064e\u0628\u0651\u0650\u0646\u0627 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u062d\u0650 \u0623\u0646\u0652 \u062a\u064e\u062a\u064e\u062c\u064e\u0646\u0651\u064e\u0628\u064f\u0648\u0627 \u0643\u064f\u0644\u0651\u064e \u0623\u062e\u064d \u064a\u064e\u062d\u064a\u0627 \u062d\u064e\u064a\u0627\u0629\u064e \u0627\u0644\u0643\u064e\u0633\u064e\u0644\u0650\u060c \u0648\u064e\u0644\u064e\u064a\u0633\u064e \u062d\u064e\u0633\u064e\u0628\u064e \u0627\u0644\u062a\u0651\u064e\u0642\u0644\u0650\u064a\u062f\u0650 \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0623\u062e\u064e\u0630\u064e\u0647\u064f \u0639\u064e\u0646\u0651\u0627."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0623\u0642\u064f\u0648\u0644\u064f \u0647\u064e\u0630\u0627 \u0644\u0650\u0623\u0646\u0651\u064e\u0643\u064f\u0645\u0652 \u062a\u064e\u0639\u0631\u0650\u0641\u064f\u0648\u0646\u064e \u0643\u064e\u064a\u0641\u064e \u064a\u064e\u0646\u0628\u064e\u063a\u0650\u064a \u0623\u0646\u0652 \u062a\u064e\u0642\u062a\u064e\u062f\u064f\u0648\u0627 \u0628\u0650\u0646\u0627. \u0641\u064e\u062d\u0650\u064a\u0646\u064e \u0639\u0650\u0634\u0646\u0627 \u0628\u064e\u064a\u0646\u064e\u0643\u064f\u0645\u0652 \u0644\u064e\u0645\u0652 \u0646\u064e\u0643\u064f\u0646\u0652 \u0643\u064e\u0633\u0627\u0644\u064e\u0649."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0644\u064e\u0645\u0652 \u0646\u064e\u0623\u0643\u064f\u0644\u0652 \u0637\u064e\u0639\u0627\u0645\u0627\u064b \u0645\u0650\u0646\u0652 \u0639\u0650\u0646\u062f\u0650 \u0623\u062d\u064e\u062f\u064d \u062f\u064f\u0648\u0646\u064e \u0645\u064f\u0642\u0627\u0628\u0650\u0644\u064d\u060c \u0628\u064e\u0644\u0652 \u0639\u064e\u0645\u0650\u0644\u0646\u0627 \u0648\u064e\u062a\u064e\u0639\u0650\u0628\u0646\u0627 \u0644\u064e\u064a\u0644\u0627\u064b \u0648\u064e\u0646\u064e\u0647\u0627\u0631\u0627\u064b \u0644\u0650\u0626\u064e\u0644\u0651\u0627 \u0646\u064e\u0643\u064f\u0648\u0646\u064e \u0639\u0650\u0628\u0626\u0650\u0627\u064e \u0639\u064e\u0644\u064e\u0649 \u0623\u062d\u064e\u062f\u064d \u0645\u0650\u0646\u0643\u064f\u0645\u0652."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0648\u064e\u0647\u064e\u0630\u0627 \u0644\u0627 \u064a\u064e\u0639\u0646\u0650\u064a \u0623\u0646\u0651\u064e\u0647\u064f \u0644\u0627 \u062d\u064e\u0642\u0651\u064e \u0644\u064e\u0646\u0627 \u0641\u0650\u064a \u0637\u064e\u0644\u064e\u0628\u0650 \u062f\u064e\u0639\u0645\u064d \u0645\u0650\u0646\u0643\u064f\u0645\u0652\u060c \u0644\u064e\u0643\u0650\u0646\u0651\u064e\u0646\u0627 \u0639\u064e\u0645\u0650\u0644\u0652\u0646\u0627 \u0628\u0650\u0623\u064a\u062f\u064a\u0646\u0627 \u0644\u0650\u0643\u064e\u064a \u0646\u064e\u0636\u0631\u0650\u0628\u064e \u0644\u064e\u0643\u064f\u0645\u0652 \u0645\u0650\u062b\u0627\u0644\u0627\u064b \u0641\u064e\u062a\u064e\u0642\u062a\u064e\u062f\u064f\u0648\u0627 \u0628\u0650\u0646\u0627."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0641\u064e\u0644\u064e\u0645\u0651\u0627 \u0643\u064f\u0646\u0651\u0627 \u0639\u0650\u0646\u062f\u064e\u0643\u064f\u0645\u0652\u060c \u0648\u064e\u0636\u064e\u0639\u0646\u0627 \u0644\u064e\u0643\u064f\u0645\u064f \u0627\u0644\u0642\u0627\u0639\u0650\u062f\u064e\u0629\u064e \u0627\u0644\u062a\u0651\u0627\u0644\u0650\u064a\u064e\u0629\u064e: \u00ab\u0625\u0646\u0652 \u0643\u0627\u0646\u064e \u0623\u062d\u064e\u062f\u064c \u064a\u064e\u0631\u0641\u064f\u0636\u064f \u0623\u0646\u0652 \u064a\u064e\u0639\u0645\u064e\u0644\u064e\u060c \u0641\u064e\u0644\u0627 \u064a\u064e\u062d\u0650\u0642\u0651\u064f \u0644\u064e\u0647\u064f \u0623\u0646\u0652 \u064a\u064e\u0623\u0643\u064f\u0644\u064e.\u00bb"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0646\u064e\u0642\u064f\u0648\u0644\u064f \u0647\u064e\u0630\u0627 \u0644\u0650\u0623\u0646\u0651\u064e\u0646\u0627 \u0646\u064e\u0633\u0645\u064e\u0639\u064f \u0623\u0646\u0651\u064e \u0628\u064e\u0639\u0636\u0627\u064b \u0645\u0650\u0646\u0643\u064f\u0645\u0652 \u064a\u064e\u062d\u064a\u064e\u0648\u0646\u064e \u062d\u064e\u064a\u0627\u0629\u064e \u0627\u0644\u0643\u064e\u0633\u064e\u0644\u0650 \u0648\u064e\u0644\u0627 \u064a\u064e\u0646\u0634\u064e\u063a\u0650\u0644\u064f\u0648\u0646\u064e \u0628\u0650\u0623\u0639\u0645\u0627\u0644\u0650\u0647\u0650\u0645\u0652\u060c \u0628\u064e\u0644\u0652 \u064a\u064e\u062c\u0631\u064f\u0648\u0646\u064e \u0647\u064f\u0646\u0627 \u0648\u064e\u0647\u064f\u0646\u0627\u0643\u064e \u0628\u0650\u0644\u0627 \u0647\u064e\u062f\u064e\u0641\u064d."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0641\u064e\u0646\u064e\u062d\u0646\u064f \u0646\u064e\u0623\u0645\u064f\u0631\u064f \u0645\u0650\u062b\u0644\u064e \u0647\u064e\u0624\u064f\u0644\u0627\u0621\u0650 \u0627\u0644\u0623\u0634\u062e\u0627\u0635\u064e \u0648\u064e\u0646\u064e\u062d\u064f\u062b\u0651\u064f\u0647\u064f\u0645\u0652 \u0641\u0650\u064a \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u062d\u0650 \u0623\u0646\u0652 \u064a\u064e\u0634\u062a\u064e\u063a\u0650\u0644\u064f\u0648\u0627 \u0628\u0650\u0647\u064f\u062f\u064f\u0648\u0621\u064d\u060c \u0648\u064e\u0623\u0646\u0652 \u064a\u064e\u0643\u0633\u0650\u0628\u064f\u0648\u0627 \u062e\u064f\u0628\u0632\u064e\u0647\u064f\u0645\u0652 \u0628\u0650\u062a\u064e\u0639\u064e\u0628\u0650\u0647\u0650\u0645\u0652."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0623\u0645\u0651\u0627 \u0623\u0646\u062a\u064f\u0645\u0652 \u0623\u064a\u0651\u064f\u0647\u0627 \u0627\u0644\u0625\u062e\u0648\u064e\u0629\u064f\u060c \u0641\u064e\u0644\u0627 \u062a\u064e\u0645\u064e\u0644\u0651\u064f\u0648\u0627 \u0645\u0650\u0646\u0652 \u0639\u064e\u0645\u064e\u0644\u0650 \u0627\u0644\u062e\u064e\u064a\u0631\u0650."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0648\u064e\u0625\u0630\u0627 \u0643\u0627\u0646\u064e \u0623\u062d\u064e\u062f\u064c \u0644\u0627 \u064a\u064f\u0637\u0650\u064a\u0639\u064f \u062a\u064e\u0639\u0644\u0650\u064a\u0645\u064e\u0646\u0627 \u0627\u0644\u0648\u0627\u0631\u0650\u062f\u0650 \u0641\u0650\u064a \u0647\u064e\u0630\u0650\u0647\u0650 \u0627\u0644\u0631\u0651\u0650\u0633\u0627\u0644\u064e\u0629\u0650\u060c \u0641\u064e\u0644\u0652\u064a\u064e\u0643\u064f\u0646\u0652 \u0645\u064e\u0639\u0631\u064f\u0648\u0641\u0627\u064b \u0644\u064e\u062f\u064e\u064a\u0643\u064f\u0645\u0652. \u0648\u064e\u0644\u0627 \u062a\u064f\u062e\u0627\u0644\u0650\u0637\u064f\u0648\u0647\u064f\u060c \u0644\u0650\u0643\u064e\u064a \u064a\u064e\u062e\u062c\u064e\u0644\u064e \u0645\u0650\u0646\u0652 \u0646\u064e\u0641\u0633\u0650\u0647\u0650."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0644\u064e\u0643\u0650\u0646\u0652 \u0644\u0627 \u062a\u064f\u0639\u0627\u0645\u0650\u0644\u064f\u0648\u0647\u064f \u0643\u064e\u0639\u064e\u062f\u064f\u0648\u0651\u064d\u060c \u0628\u064e\u0644\u0650 \u0627\u0646\u0635\u064e\u062d\u064f\u0648\u0647\u064f \u0643\u064e\u0623\u062e\u064d."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u062e\u0627\u062a\u0650\u0645\u064e\u0629 - \u0648\u064e\u0627\u0644\u0622\u0646\u064e\u060c \u0644\u0650\u064a\u064f\u0639\u0637\u0650\u0643\u064f\u0645\u0652 \u0631\u064e\u0628\u0651\u064f \u0627\u0644\u0633\u0651\u064e\u0644\u0627\u0645\u0650 \u0646\u064e\u0641\u0633\u064e\u0647\u064f \u0633\u064e\u0644\u0627\u0645\u0627\u064b \u0643\u064f\u0644\u0651\u064e \u062d\u0650\u064a\u0646\u064d\u060c \u0648\u064e\u0645\u0650\u0646\u0652 \u0643\u064f\u0644\u0651\u0650 \u0646\u0627\u062d\u0650\u064a\u064e\u0629\u064d. \u0648\u064e\u0644\u0652\u064a\u064e\u0643\u064f\u0646\u0650 \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u064f \u0645\u064e\u0639\u064e\u0643\u064f\u0645\u0652 \u062c\u064e\u0645\u0650\u064a\u0639\u0627\u064b."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0648\u064e\u0647\u0627 \u0623\u0646\u0627 \u0628\u064f\u0648\u0644\u064f\u0633\u064e\u060c \u0623\u0643\u062a\u064f\u0628\u064f \u0647\u064e\u0630\u0650\u0647\u0650 \u0627\u0644\u062a\u0651\u064e\u062d\u0650\u064a\u0651\u064e\u0629\u064e \u0628\u0650\u062e\u064e\u0637\u0651\u0650 \u064a\u064e\u062f\u0650\u064a. \u0647\u064e\u0643\u064e\u0630\u0627 \u0623\u0643\u062a\u064f\u0628\u064f \u0648\u064e\u0623\u064f\u0648\u064e\u0642\u0651\u0650\u0639\u064f \u0643\u064f\u0644\u0651\u064e \u0631\u0650\u0633\u0627\u0644\u064e\u0629\u064d:"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0644\u0650\u062a\u064e\u0643\u064f\u0646\u0652 \u0646\u0650\u0639\u0645\u064e\u0629\u064f \u0631\u064e\u0628\u0651\u0650\u0646\u0627 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u062d\u0650 \u0645\u064e\u0639\u064e\u0643\u064f\u0645\u0652 \u062c\u064e\u0645\u0650\u064a\u0639\u0627\u064b."
                }
            }
