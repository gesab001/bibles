        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0645\u0650\u0646\u0652 \u0628\u064f\u0648\u0644\u064f\u0633\u064e \u0627\u0644\u0645\u064e\u0633\u062c\u064f\u0648\u0646\u0650 \u0644\u0650\u0623\u062c\u0644\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u0652\u062d\u0650\u060c \u0648\u064e\u0645\u0650\u0646\u0652 \u0623\u062e\u0650\u064a\u0652\u0646\u0627 \u062a\u0650\u064a\u0652\u0645\u064f\u0648\u062b\u0627\u0648\u064f\u0633\u064e\u060c \u0625\u0644\u064e\u0649 \u0641\u0650\u0644\u0650\u064a\u0652\u0645\u064f\u0648\u0646\u064e \u0635\u064e\u062f\u0650\u064a\u0652\u0642\u0650\u0646\u0627 \u0627\u0644\u0645\u064e\u062d\u0628\u064f\u0648\u0628\u0650 \u0648\u064e\u0627\u0644\u0639\u0627\u0645\u0650\u0644\u0650 \u0645\u064e\u0639\u064e\u0646\u0627."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0648\u064e\u0625\u0644\u064e\u0649 \u0627\u0644\u0623\u064f\u062e\u062a\u0650 \u0627\u0644\u0645\u064e\u062d\u0628\u064f\u0648\u0628\u064e\u0629\u0650 \u0623\u0628\u0652\u0641\u0650\u064a\u0651\u064e\u0629\u064e\u060c \u0648\u064e\u0623\u0631\u062e\u0650\u0628\u0651\u064f\u0633\u064e \u0627\u0644\u0645\u064f\u062c\u0627\u0647\u0650\u062f\u0650 \u0645\u064e\u0639\u064e\u0646\u0627\u060c \u0648\u064e\u0625\u0644\u064e\u0649 \u0627\u0644\u0643\u064e\u0646\u0650\u064a\u0652\u0633\u064e\u0629\u0650 \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u062a\u064e\u062c\u062a\u064e\u0645\u0650\u0639\u064f \u0641\u0650\u064a \u0628\u064e\u064a\u0652\u062a\u0650\u0643\u064e."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0644\u0650\u062a\u064e\u0643\u064f\u0646\u0652 \u0644\u064e\u0643\u064f\u0645\u0652 \u0646\u0650\u0639\u0645\u064e\u0629\u064c \u0648\u064e\u0633\u064e\u0644\u0627\u0645\u064c \u0645\u0650\u0646\u064e \u0627\u0644\u0644\u0647\u0650 \u0623\u0628\u0650\u064a\u0652\u0646\u0627 \u0648\u064e\u0645\u0650\u0646\u064e \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u0652\u062d\u0650."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0645\u064e\u062d\u064e\u0628\u0651\u064e\u0629\u064f \u0641\u0650\u0644\u0650\u064a\u0652\u0645\u064f\u0648\u0646\u064e \u0648\u064e\u0625\u064a\u0652\u0645\u0627\u0646\u064f\u0647 - \u0623\u0646\u0627 \u0623\u0634\u0643\u064f\u0631\u064f \u0627\u0644\u0644\u0647\u064e \u0645\u0650\u0646\u0652 \u0623\u062c\u0644\u0650\u0643\u064e \u062f\u0627\u0626\u0650\u0645\u0627\u064b \u0643\u064f\u0644\u0651\u064e\u0645\u0627 \u0630\u064e\u0643\u064e\u0631\u0652\u062a\u064f\u0643\u064e \u0641\u0650\u064a \u0635\u064e\u0644\u064e\u0648\u0627\u062a\u0650\u064a\u060c"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0644\u0650\u0623\u0646\u0651\u064e\u0646\u0650\u064a \u0623\u0633\u0645\u064e\u0639\u064f \u0628\u0650\u0645\u064e\u062d\u064e\u0628\u0651\u064e\u062a\u0650\u0643\u064e \u0648\u064e\u0625\u064a\u0652\u0645\u0627\u0646\u0650\u0643\u064e: \u0625\u064a\u0652\u0645\u0627\u0646\u0650\u0643\u064e \u0628\u0650\u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e\u060c \u0648\u064e\u0645\u064e\u062d\u064e\u0628\u0651\u064e\u062a\u0650\u0643\u064e \u0644\u0650\u062c\u064e\u0645\u0650\u064a\u0639\u0650 \u0634\u064e\u0639\u0628\u0650 \u0627\u0644\u0644\u0647\u0650 \u0627\u0644\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u0650."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0648\u064e\u0623\u064f\u0635\u064e\u0644\u0651\u0650\u064a \u0623\u0646\u0652 \u064a\u064e\u0642\u064f\u0648\u062f\u064e\u0643\u064e \u0625\u064a\u0652\u0645\u0627\u0646\u064f\u0643\u064e \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0646\u064e\u0634\u062a\u064e\u0631\u0650\u0643\u064f \u0641\u0650\u064a\u0652\u0647\u0650 \u0645\u064e\u0639\u0627\u064b\u060c \u0625\u0644\u064e\u0649 \u0623\u0646\u0652 \u062a\u064e\u0641\u0647\u064e\u0645\u064e \u0643\u064f\u0644\u0651\u064e \u0627\u0644\u0623\u0634\u064a\u0627\u0621\u0650 \u0627\u0644\u0635\u0651\u0627\u0644\u0650\u062d\u064e\u0629\u0650 \u0627\u0644\u0651\u064e\u062a\u0650\u064a \u0646\u064e\u0645\u062a\u064e\u0644\u0650\u0643\u064f\u0647\u0627 \u0641\u0650\u064a \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u0652\u062d\u0650."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0644\u064e\u0642\u064e\u062f\u0652 \u0634\u064e\u0639\u064e\u0631\u0652\u062a\u064f \u0628\u0650\u0641\u064e\u0631\u064e\u062d\u064d \u0648\u064e\u062a\u064e\u0634\u062c\u0650\u064a\u0652\u0639\u064d \u0639\u064e\u0638\u0650\u064a\u0645\u064e\u064a\u0646\u0650 \u0628\u0650\u0633\u064e\u0628\u064e\u0628\u0650 \u0645\u064e\u062d\u064e\u0628\u0651\u064e\u062a\u0650\u0643\u064e\u060c \u0644\u0650\u0623\u0646\u0651\u064e \u0642\u064f\u0644\u064f\u0648\u0628\u064e \u0627\u0644\u0645\u064f\u0624\u0645\u0650\u0646\u064a\u0646\u064e \u0627\u0644\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u064a\u0646\u064e \u0642\u064e\u062f\u0650 \u0627\u0646\u062a\u064e\u0639\u064e\u0634\u064e\u062a\u0652 \u0628\u0650\u0643\u064e \u0623\u064a\u0651\u064f\u0647\u0627 \u0627\u0644\u0623\u062e\u064f."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0627\u0642\u0628\u064e\u0644\u0652 \u0623\u064f\u0646\u0650\u0633\u0650\u064a\u0652\u0645\u064f\u0633\u064e \u0643\u064e\u0623\u062e - \u0644\u0650\u0630\u064e\u0644\u0650\u0643\u064e\u060c \u0648\u064e\u0645\u064e\u0639\u0652 \u0623\u0646\u0651\u064e\u0646\u0650\u064a \u0623\u0645\u062a\u064e\u0644\u0650\u0643\u064f \u0627\u0644\u062c\u064f\u0631\u0623\u0629\u064e \u0627\u0644\u0643\u0627\u0645\u0650\u0644\u064e\u0629\u064e \u0641\u0650\u064a \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u0652\u062d\u0650 \u0644\u0650\u0643\u064e\u064a \u0622\u0645\u064f\u0631\u064e\u0643\u064e \u0628\u0650\u0623\u0646\u0652 \u062a\u064e\u0641\u0652\u0639\u064e\u0644\u064e \u0645\u0627 \u0647\u064f\u0648\u064e \u0645\u064f\u0646\u0627\u0633\u0650\u0628\u064c\u060c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0625\u0644\u0651\u0627 \u0623\u0646\u0651\u064e\u0646\u0650\u064a \u0623\u064f\u0641\u064e\u0636\u0651\u0650\u0644\u064f \u0623\u0646\u0652 \u0623\u0637\u0644\u064f\u0628\u0650 \u0645\u0650\u0646\u0652\u0643\u064e \u0628\u0650\u0631\u0650\u0641\u0652\u0642\u064d \u0639\u064e\u0644\u064e\u0649 \u0623\u0633\u0627\u0633\u0650 \u0627\u0644\u0645\u064e\u062d\u064e\u0628\u0651\u064e\u0629\u0650. \u0641\u064e\u0647\u0627 \u0623\u0646\u0627 \u0628\u064f\u0648\u0644\u064f\u0633\u064f \u0627\u0644\u0631\u0651\u064e\u062c\u064f\u0644\u064f \u0627\u0644\u0643\u064e\u0628\u0650\u064a\u0652\u0631\u064f \u0641\u0650\u064a \u0627\u0644\u0633\u0651\u0650\u0646\u0651\u0650\u060c \u0648\u064e\u0633\u064e\u062c\u064a\u0646\u064f \u062e\u0650\u062f\u0645\u064e\u0629\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0645\u064e\u0633\u0650\u064a\u0652\u062d\u0650\u060c"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0623\u0643\u062a\u064f\u0628\u064f \u0625\u0644\u064e\u064a\u0652\u0643\u064e \u0628\u0650\u062e\u064f\u0635\u064f\u0648\u0635\u0650 \u0627\u0628\u0652\u0646\u0650\u064a \u0623\u064f\u0646\u0650\u0633\u0650\u064a\u0652\u0645\u064f\u0633\u064e \u0627\u0644\u0651\u064e\u0630\u0650\u064a \u0648\u064e\u0644\u064e\u062f\u0652\u062a\u064f\u0647\u064f \u0648\u064e\u0623\u0646\u0627 \u0641\u0650\u064a \u0627\u0644\u0633\u0651\u0650\u062c\u0646\u0650."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0641\u064e\u0647\u0648\u064e \u0644\u064e\u0645\u0652 \u064a\u064e\u0643\u064f\u0646\u0652 \u0645\u0650\u0646\u0652 \u0642\u064e\u0628\u0644\u064f \u0646\u0627\u0641\u0650\u0639\u0627\u064b \u0644\u064e\u0643\u064e\u060c \u0623\u0645\u0651\u0627 \u0627\u0644\u0622\u0646\u064e \u0641\u064e\u0647\u064f\u0648\u064e \u0646\u0627\u0641\u0650\u0639\u064c \u0644\u064e\u064a\u0652\u0633\u064e \u0644\u064e\u0643\u064e \u0641\u064e\u0642\u064e\u0637\u0652\u060c \u0628\u064e\u0644\u0652 \u0644\u0650\u064a \u0623\u064a\u0636\u0627\u064b."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0648\u064e\u0647\u0627 \u0623\u0646\u0627 \u0623\u064f\u0631\u0633\u0650\u0644\u064f\u0647\u064f \u0625\u0644\u064e\u064a\u0652\u0643\u064e \u062b\u0627\u0646\u0650\u064a\u064e\u0629\u064b \u0648\u064e\u0645\u064e\u0639\u064e\u0647\u064f \u0623\u064f\u0631\u0633\u0650\u0644\u064f \u0642\u064e\u0644\u0652\u0628\u0650\u064a \u0625\u0644\u064e\u064a\u0652\u0643\u064e."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0623\u0646\u0627 \u0623\u0648\u064e\u062f\u0651\u064f \u0623\u0646\u0652 \u0623\u064f\u0628\u0642\u0650\u064a\u064e\u0647\u064f \u0647\u064f\u0646\u0627 \u0645\u064e\u0639\u0650\u064a \u0644\u0650\u0643\u064e\u064a \u064a\u064f\u0633\u0627\u0639\u0650\u062f\u064e\u0646\u0650\u064a \u0648\u064e\u0623\u0646\u0627 \u0645\u064f\u0642\u064e\u064a\u0651\u064e\u062f\u064c \u0628\u0650\u0633\u064e\u0628\u064e\u0628\u0650 \u0646\u064e\u0634\u0652\u0631\u0650 \u0627\u0644\u0628\u0650\u0634\u0627\u0631\u064e\u0629\u0650."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0644\u064e\u0643\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0645\u0652 \u0623\u0631\u063a\u064e\u0628\u0652 \u0623\u0646\u0652 \u0623\u0641\u0639\u064e\u0644\u064e \u0634\u064e\u064a\u0626\u0627\u064b \u0645\u0650\u0646\u0652 \u062f\u064f\u0648\u0646\u0650 \u0645\u064f\u0648\u0627\u0641\u064e\u0642\u064e\u062a\u0650\u0643\u064e\u060c \u0648\u064e\u0647\u064e\u0643\u064e\u0630\u0627 \u0644\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064f \u0645\u064f\u0636\u0637\u064e\u0631\u0651\u0627\u064b \u0644\u0650\u0639\u064e\u0645\u064e\u0644\u0650 \u0645\u0627 \u0647\u064f\u0648\u064e \u0635\u064e\u0648\u0627\u0628\u064c\u060c \u0628\u064e\u0644\u0652 \u062a\u064e\u0639\u0645\u064e\u0644\u064f\u0647\u064f \u0628\u0650\u0625\u0631\u0627\u062f\u064e\u062a\u0650\u0643\u064e \u0627\u0644\u062d\u064f\u0631\u0651\u064e\u0629\u0650."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0631\u064f\u0628\u0651\u064e\u0645\u0627 \u062a\u064e\u0631\u064e\u0643\u064e\u0643\u064e \u0623\u064f\u0646\u0650\u0633\u0650\u064a\u0652\u0645\u064f\u0633\u064f \u0644\u0650\u0648\u064e\u0642\u062a\u064d \u0642\u064e\u0635\u0650\u064a\u0652\u0631\u064d\u060c \u0644\u0650\u0643\u064e\u064a \u062a\u064e\u0633\u062a\u064e\u0631\u0650\u062f\u0651\u064e\u0647\u064f \u0627\u0644\u0622\u0646\u064e \u0625\u0644\u064e\u0649 \u0627\u0644\u0623\u0628\u064e\u062f\u0650."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0644\u064e\u0643\u0650\u0646\u0652 \u0644\u064e\u064a\u0652\u0633\u064e \u0628\u0627\u0639\u062a\u0650\u0628\u0627\u0631\u0650\u0647\u0650 \u0639\u064e\u0628\u0652\u062f\u0627\u064b \u0641\u0650\u064a\u0645\u0627 \u0628\u064e\u0639\u0652\u062f\u064f\u060c \u0628\u064e\u0644\u0652 \u0623\u0641\u0636\u064e\u0644\u064e \u0645\u0650\u0646\u0652 \u0639\u064e\u0628\u0652\u062f\u064d\u060c \u0623\u064a\u0652 \u0623\u062e\u0627\u064b \u0645\u064e\u062d\u0628\u064f\u0648\u0628\u0627\u064b. \u0623\u0646\u0627 \u0623\u064f\u062d\u0650\u0628\u0651\u064f\u0647\u064f \u0643\u064e\u062b\u0650\u064a\u0652\u0631\u0627\u064b\u060c \u0644\u064e\u0643\u0650\u0646\u0651\u064e\u0643\u064e \u062a\u064f\u062d\u0650\u0628\u0651\u064f\u0647\u064f \u0623\u0643\u062b\u064e\u0631\u064e \u062c\u0650\u062f\u0651\u0627\u064b\u060c \u0644\u0627 \u0643\u064e\u0625\u0646\u0633\u0627\u0646\u064d \u0641\u064e\u0642\u064e\u0637\u0652 \u0628\u064e\u0644\u0652 \u0643\u064e\u0623\u062e\u064d \u0641\u0650\u064a \u0627\u0644\u0631\u0651\u064e\u0628\u0651\u0650."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0641\u064e\u0625\u0646\u0652 \u0643\u064f\u0646\u062a\u064e \u062a\u064e\u0639\u062a\u064e\u0628\u0650\u0631\u064f\u0646\u0650\u064a \u0634\u064e\u0631\u0650\u064a\u0643\u0627\u064b \u0644\u064e\u0643\u064e \u062d\u064e\u0642\u0651\u0627\u064b\u060c \u0623\u0631\u062c\u064f\u0648 \u0623\u0646\u0652 \u062a\u064f\u0631\u064e\u062d\u0651\u0650\u0628\u064e \u0628\u0650\u0647\u0650 \u0643\u064e\u0645\u0627 \u0644\u064e\u0648\u0652 \u0643\u064f\u0646\u062a\u064e \u0633\u064e\u062a\u064f\u0631\u064e\u062d\u0651\u0650\u0628\u064f \u0628\u0650\u064a."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0648\u064e\u0625\u0646\u0652 \u0643\u0627\u0646\u064e \u0623\u062e\u0637\u064e\u0623 \u0625\u0644\u064e\u064a\u0652\u0643\u064e\u060c \u0623\u0648\u0652 \u0643\u0627\u0646\u064e \u0645\u064e\u062f\u064a\u064f\u0648\u0646\u0627\u064b \u0644\u064e\u0643\u064e \u0628\u0650\u0634\u064e\u064a\u0621\u064d\u060c \u0641\u0627\u062d\u0652\u0633\u0650\u0628\u0652 \u062f\u064e\u064a\u0646\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u0651\u064e \u0623\u0646\u0627."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0623\u0646\u0627 \u0628\u064f\u0648\u0644\u064f\u0633\u064f\u060c \u0623\u0643\u062a\u064f\u0628\u064f \u0625\u0644\u064e\u064a\u0652\u0643\u064e \u0647\u064e\u0630\u0627 \u0628\u0650\u062e\u064e\u0637\u0651\u0650 \u064a\u064e\u062f\u0650\u064a: \u0623\u0646\u0627 \u0633\u064e\u0623\u0648\u0641\u064a \u062f\u064e\u064a\u0646\u064e\u0647\u064f. \u0648\u064e\u0644\u0627 \u062f\u0627\u0639\u0650\u064a\u064e \u0644\u0650\u0623\u0646\u0652 \u0623\u064f\u0630\u064e\u0643\u0651\u0650\u0631\u064e\u0643\u064e \u0628\u0650\u0623\u0646\u0651\u064e\u0643\u064e \u0623\u0646\u062a\u064e \u0645\u064e\u062f\u064a\u064f\u0648\u0646\u064c \u0644\u0650\u064a \u0628\u0650\u0646\u064e\u0641\u0633\u0650\u0643\u064e!"
                },
                "20": {
