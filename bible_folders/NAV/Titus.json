            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0639\u0645\u0644 \u0645\u0627 \u0647\u0648 \u0635\u0627\u0644\u062d - \u0630\u064e\u0643\u0650\u0651\u0631\u0650 \u0627\u0644\u0652\u0645\u064f\u0624\u0652\u0645\u0650\u0646\u0650\u064a\u0646\u064e \u0623\u064e\u0646\u0652 \u064a\u064e\u062e\u0652\u0636\u064e\u0639\u064f\u0648\u0627 \u0644\u0650\u0644\u0652\u062d\u064f\u0643\u064e\u0651\u0627\u0645\u0650 \u0648\u064e\u0627\u0644\u0633\u064f\u0651\u0644\u064f\u0637\u064e\u0627\u062a\u0650\u060c \u0648\u064e\u064a\u064f\u0637\u0650\u064a\u0639\u064f\u0648\u0627 \u0627\u0644\u0652\u0642\u064e\u0627\u0646\u064f\u0648\u0646\u064e\u060c \u0648\u064e\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627 \u0645\u064f\u0633\u0652\u062a\u064e\u0639\u0650\u062f\u0650\u0651\u064a\u0646\u064e \u0644\u0650\u0643\u064f\u0644\u0650\u0651 \u0639\u064e\u0645\u064e\u0644\u064d \u0635\u064e\u0627\u0644\u0650\u062d\u064d\u060c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0648\u064e\u0644\u0627 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0627 \u0633\u064f\u0648\u0621\u0627\u064b \u0641\u0650\u064a \u0623\u064e\u062d\u064e\u062f\u064d\u060c \u0648\u064e\u0644\u0627 \u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627 \u0645\u064f\u062e\u064e\u0627\u0635\u0650\u0645\u0650\u064a\u0646\u064e\u060c \u0628\u064e\u0644\u0652 \u0644\u064f\u0637\u064e\u0641\u064e\u0627\u0621\u064e \u064a\u064f\u0639\u064e\u0627\u0645\u0650\u0644\u064f\u0648\u0646\u064e \u0627\u0644\u0652\u062c\u064e\u0645\u0650\u064a\u0639\u064e \u0628\u0650\u0648\u064e\u062f\u064e\u0627\u0639\u064e\u0629\u064d \u062a\u064e\u0627\u0645\u064e\u0651\u0629\u064d."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0641\u064e\u0625\u0650\u0646\u064e\u0651\u0646\u064e\u0627 \u0646\u064e\u062d\u0652\u0646\u064f \u0623\u064e\u064a\u0652\u0636\u0627\u064b \u0643\u064f\u0646\u064e\u0651\u0627 \u0641\u0650\u064a \u0627\u0644\u0652\u0645\u064e\u0627\u0636\u0650\u064a \u062c\u064f\u0647\u064e\u0651\u0627\u0644\u0627\u064b\u060c \u063a\u064e\u064a\u0652\u0631\u064e \u0645\u064f\u0637\u0650\u064a\u0639\u0650\u064a\u0646\u064e\u060c \u062a\u064e\u0627\u0626\u0650\u0647\u0650\u064a\u0646\u064e \u0641\u0650\u064a \u0627\u0644\u0636\u064e\u0651\u0644\u0627\u0644\u0650\u060c \u0639\u064e\u0628\u0650\u064a\u062f\u0627\u064b \u064a\u064e\u062e\u0652\u062f\u0650\u0645\u064f\u0648\u0646\u064e \u0627\u0644\u0634\u064e\u0651\u0647\u064e\u0648\u064e\u0627\u062a\u0650 \u0648\u064e\u0627\u0644\u0644\u064e\u0651\u0630\u064e\u0651\u0627\u062a\u0650 \u0627\u0644\u0652\u0645\u064f\u062e\u0652\u062a\u064e\u0644\u0650\u0641\u064e\u0629\u064e\u060c \u0646\u064e\u0639\u0650\u064a\u0634\u064f \u0641\u0650\u064a \u0627\u0644\u0652\u062e\u064f\u0628\u0652\u062b\u0650 \u0648\u064e\u0627\u0644\u0652\u062d\u064e\u0633\u064e\u062f\u0650\u060c \u0645\u064e\u0643\u0652\u0631\u064f\u0648\u0647\u0650\u064a\u0646\u064e\u060c \u0648\u064e\u0643\u064e\u0627\u0631\u0650\u0647\u0650\u064a\u0646\u064e \u0628\u064e\u0639\u0652\u0636\u064f\u0646\u064e\u0627 \u0644\u0650\u0628\u064e\u0639\u0652\u0636\u064d."
