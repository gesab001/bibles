        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0645\u0650\u0646\u0652 \u0628\u064f\u0648\u0644\u064f\u0633\u064e\u060c \u0639\u064e\u0628\u0652\u062f\u0650 \u0627\u0644\u0644\u0647\u0650 \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0627\u0644\u0652\u0645\u064e\u0633\u0650\u064a\u062d\u0650 \u0641\u0650\u064a \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0625\u0650\u064a\u0645\u064e\u0627\u0646\u0650 \u0645\u064e\u0646\u0650 \u0627\u062e\u0652\u062a\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u064f \u0627\u0644\u0644\u0647\u064f\u060c \u0648\u064e\u0645\u064e\u0639\u0652\u0631\u0650\u0641\u064e\u062a\u0650\u0647\u0650\u0645\u0652 \u0644\u0650\u0644\u0652\u062d\u064e\u0642\u0650\u0651 \u0627\u0644\u0652\u0645\u064f\u0648\u064e\u0627\u0641\u0650\u0642\u0650 \u0644\u0650\u0644\u062a\u064e\u0651\u0642\u0652\u0648\u064e\u0649\u060c"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0641\u0650\u064a \u0631\u064e\u062c\u064e\u0627\u0621\u0650 \u0627\u0644\u0652\u062d\u064e\u064a\u064e\u0627\u0629\u0650 \u0627\u0644\u0623\u064e\u0628\u064e\u062f\u0650\u064a\u064e\u0651\u0629\u0650\u060c \u0627\u0644\u064e\u0651\u062a\u0650\u064a \u0648\u064e\u0639\u064e\u062f\u064e \u0628\u0650\u0647\u0627 \u0627\u0644\u0644\u0647\u064f \u0627\u0644\u0652\u0645\u064f\u0646\u064e\u0632\u064e\u0651\u0647\u064f \u0639\u064e\u0646\u0650 \u0627\u0644\u0652\u0643\u064e\u0630\u0650\u0628\u0650\u060c \u0645\u0650\u0646\u0652 \u0642\u064e\u0628\u0652\u0644\u0650 \u0623\u064e\u0632\u0652\u0645\u0650\u0646\u064e\u0629\u0650 \u0627\u0644\u0623\u064e\u0632\u064e\u0644\u0650\u060c"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u062b\u064f\u0645\u064e\u0651 \u0628\u064e\u064a\u064e\u0651\u0646\u064e \u0643\u064e\u0644\u0650\u0645\u064e\u062a\u064e\u0647\u064f \u0641\u0650\u064a \u0623\u064e\u0648\u064e\u0627\u0646\u0650\u0647\u064e\u0627 \u0627\u0644\u0652\u0645\u064f\u0639\u064e\u064a\u064e\u0651\u0646\u0650: \u0628\u0650\u0627\u0644\u0652\u0628\u0650\u0634\u064e\u0627\u0631\u064e\u0629\u0650 \u0627\u0644\u064e\u0651\u062a\u0650\u064a \u0648\u064f\u0636\u0650\u0639\u064e\u062a\u0652 \u0623\u064e\u0645\u064e\u0627\u0646\u064e\u0629\u064b \u0628\u064e\u064a\u0652\u0646\u064e \u064a\u064e\u062f\u064e\u064a\u064e\u0651 \u0628\u0650\u0645\u064f\u0648\u062c\u0650\u0628\u0650 \u0623\u064e\u0645\u0652\u0631\u0650 \u0645\u064f\u062e\u064e\u0644\u0650\u0651\u0635\u0650\u0646\u064e\u0627 \u0627\u0644\u0644\u0647\u0650."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0625\u0650\u0644\u064e\u0649 \u062a\u0650\u064a\u0637\u064f\u0633\u064e\u060c \u0648\u064e\u0644\u064e\u062f\u0650\u064a \u0627\u0644\u0652\u062d\u064e\u0642\u0650\u064a\u0642\u0650\u064a\u0650\u0651 \u0628\u0650\u0627\u0644\u0646\u0650\u0651\u0633\u0652\u0628\u064e\u0629\u0650 \u0625\u0650\u0644\u064e\u0649 \u0627\u0644\u0625\u0650\u064a\u0645\u064e\u0627\u0646\u0650 \u0627\u0644\u0652\u0645\u064f\u0634\u0652\u062a\u064e\u0631\u064e\u0643\u0650 \u0628\u064e\u064a\u0652\u0646\u064e\u0646\u064e\u0627. \u0644\u0650\u062a\u064e\u0643\u064f\u0646\u0652 \u0644\u064e\u0643\u064e \u0627\u0644\u0646\u0650\u0651\u0639\u0652\u0645\u064e\u0629\u064f \u0648\u064e\u0627\u0644\u0633\u064e\u0651\u0644\u0627\u0645\u064f \u0645\u0650\u0646\u064e \u0627\u0644\u0644\u0647\u0650 \u0627\u0644\u0622\u0628\u0650\u060c \u0648\u064e\u0627\u0644\u0652\u0645\u064e\u0633\u0650\u064a\u062d\u0650 \u064a\u064e\u0633\u064f\u0648\u0639\u064e \u0645\u064f\u062e\u064e\u0644\u0650\u0651\u0635\u0650\u0646\u064e\u0627!"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0625\u0642\u0627\u0645\u0629 \u0634\u064a\u0648\u062e \u0635\u0627\u0644\u062d\u064a\u0646 - \u062a\u064e\u0631\u064e\u0643\u0652\u062a\u064f\u0643\u064e \u0641\u0650\u064a \u062c\u064e\u0632\u0650\u064a\u0631\u064e\u0629\u0650 \u0643\u0650\u0631\u0650\u064a\u062a\u064e \u0644\u0650\u0643\u064e\u064a\u0652 \u062a\u064f\u0643\u064e\u0645\u0650\u0651\u0644\u064e \u062a\u064e\u0631\u0652\u062a\u0650\u064a\u0628\u064e \u0627\u0644\u0623\u064f\u0645\u064f\u0648\u0631\u0650 \u0627\u0644\u0652\u0628\u064e\u0627\u0642\u0650\u064a\u064e\u0629\u0650\u060c \u0648\u064e\u062a\u064f\u0642\u0650\u064a\u0645\u064e \u0634\u064f\u064a\u064f\u0648\u062e\u0627\u064b \u0641\u0650\u064a \u0643\u064f\u0644\u0650\u0651 \u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u064d\u060c \u0645\u0650\u062b\u0652\u0644\u064e\u0645\u064e\u0627 \u0623\u064e\u0645\u064e\u0631\u0652\u062a\u064f\u0643\u064e\u061b"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0639\u064e\u0644\u064e\u0649 \u0623\u064e\u0646\u0652 \u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0627\u0644\u0652\u0648\u064e\u0627\u062d\u0650\u062f\u064f \u0645\u0650\u0646\u0652\u0647\u064f\u0645\u0652 \u0628\u064e\u0631\u0650\u064a\u0626\u0627\u064b \u0645\u0650\u0646\u0652 \u0643\u064f\u0644\u0650\u0651 \u062a\u064f\u0647\u0652\u0645\u064e\u0629\u064d\u060c \u0632\u064e\u0648\u0652\u062c\u0627\u064b \u0644\u0627\u0645\u0652\u0631\u064e\u0623\u064e\u0629\u064d \u0648\u064e\u0627\u062d\u0650\u062f\u064e\u0629\u064d\u060c \u0623\u064e\u0628\u0627\u064b \u0644\u0623\u064e\u0648\u0652\u0644\u0627\u062f\u064d \u0645\u064f\u0624\u0652\u0645\u0650\u0646\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u062a\u064e\u0651\u0647\u064e\u0645\u064f\u0648\u0646\u064e \u0628\u0650\u0627\u0644\u0652\u062e\u064e\u0644\u0627\u0639\u064e\u0629\u0650 \u0648\u064e\u0627\u0644\u062a\u064e\u0651\u0645\u064e\u0631\u064f\u0651\u062f\u0650."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0648\u064e\u0630\u064e\u0644\u0650\u0643\u064e \u0644\u0623\u064e\u0646\u064e\u0651 \u0627\u0644\u0631\u064e\u0651\u0627\u0639\u0650\u064a\u064e \u064a\u064e\u062c\u0650\u0628\u064f \u0623\u064e\u0646\u0652 \u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0628\u064e\u0631\u0650\u064a\u0626\u0627\u064b \u0645\u0650\u0646\u0652 \u0643\u064f\u0644\u0650\u0651 \u062a\u064f\u0647\u0652\u0645\u064e\u0629\u064d \u0628\u0650\u0627\u0639\u0652\u062a\u0650\u0628\u064e\u0627\u0631\u0650\u0647\u0650 \u0648\u064e\u0643\u0650\u064a\u0644\u0627\u064b \u0644\u0650\u0644\u0647\u0650\u060c \u0644\u064e\u0627 \u0645\u064f\u0639\u0652\u062c\u064e\u0628\u0627\u064b \u0628\u0650\u0646\u064e\u0641\u0652\u0633\u0650\u0647\u0650 \u0648\u064e\u0644\u0627 \u062d\u064e\u0627\u062f\u064e\u0651 \u0627\u0644\u0637\u064e\u0651\u0628\u0652\u0639\u0650\u060c \u0648\u064e\u0644\u0627 \u0645\u064f\u062f\u0652\u0645\u0650\u0646\u064e \u0627\u0644\u0652\u062e\u064e\u0645\u0652\u0631\u0650\u060c \u0648\u064e\u0644\u0627 \u0639\u064e\u0646\u0650\u064a\u0641\u0627\u064b\u060c \u0648\u064e\u0644\u0627 \u0633\u064e\u0627\u0639\u0650\u064a\u0627\u064b \u0625\u0650\u0644\u064e\u0649 \u0627\u0644\u0652\u0645\u064e\u0643\u0652\u0633\u064e\u0628\u0650 \u0627\u0644\u0652\u062e\u064e\u0633\u0650\u064a\u0633\u0650\u061b"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0628\u064e\u0644\u0652 \u0645\u0650\u0636\u0652\u064a\u064e\u0627\u0641\u0627\u064b\u060c \u0645\u064f\u062d\u0650\u0628\u0651\u0627\u064b \u0644\u0650\u0644\u0635\u064e\u0651\u0644\u0627\u062d\u0650\u060c \u0631\u064e\u0632\u0650\u064a\u0646\u0627\u064b\u060c \u0628\u064e\u0627\u0631\u0651\u0627\u064b\u060c \u062a\u064e\u0642\u0650\u064a\u064b\u0651\u0627\u060c \u0636\u064e\u0627\u0628\u0650\u0637\u0627\u064b \u0646\u064e\u0641\u0652\u0633\u064e\u0647\u064f\u060c"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0645\u064f\u0644\u0652\u062a\u064e\u0635\u0650\u0642\u0627\u064b \u0628\u0650\u0627\u0644\u0652\u0643\u064e\u0644\u0650\u0645\u064e\u0629\u0650 \u0627\u0644\u0635\u064e\u0651\u0627\u062f\u0650\u0642\u064e\u0629\u0650 \u0627\u0644\u0652\u0645\u064f\u0648\u064e\u0627\u0641\u0650\u0642\u064e\u0629\u0650 \u0644\u0650\u0644\u062a\u064e\u0651\u0639\u0652\u0644\u0650\u064a\u0645\u0650\u060c \u0644\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0642\u064e\u0627\u062f\u0650\u0631\u0627\u064b \u0639\u064e\u0644\u064e\u0649 \u062a\u064e\u0634\u0652\u062c\u0650\u064a\u0639\u0650 \u0627\u0644\u0652\u0645\u064f\u0624\u0652\u0645\u0650\u0646\u0650\u064a\u0650\u0646\u064e \u0628\u0650\u0627\u0644\u062a\u064e\u0651\u0639\u0652\u0644\u0650\u064a\u0645\u0650 \u0627\u0644\u0635\u064e\u0651\u062d\u0650\u064a\u062d\u0650 \u0648\u064e\u0639\u064e\u0644\u064e\u0649 \u0625\u0650\u0641\u0652\u062d\u064e\u0627\u0645\u0650 \u0627\u0644\u0652\u0645\u064f\u0639\u064e\u0627\u0631\u0650\u0636\u0650\u064a\u0646\u064e."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u062a\u0648\u0628\u064a\u062e \u0645\u0646 \u0644\u0627 \u064a\u0641\u0639\u0644\u0648\u0646 \u0635\u0644\u0627\u062d\u0627\u064b - \u0641\u064e\u0625\u0650\u0646\u064e\u0651 \u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e \u0643\u064e\u062b\u0650\u064a\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u0652 \u0645\u064f\u0639\u064e\u0644\u0650\u0651\u0645\u0650\u064a \u0627\u0644\u0652\u0628\u064e\u0627\u0637\u0650\u0644\u0650 \u0627\u0644\u0652\u0645\u064f\u062a\u064e\u0645\u064e\u0631\u0650\u0651\u062f\u0650\u064a\u0646\u064e \u0648\u064e\u062e\u064e\u0627\u062f\u0650\u0639\u0650\u064a \u0639\u064f\u0642\u064f\u0648\u0644\u0650 \u0627\u0644\u0646\u064e\u0651\u0627\u0633\u0650\u060c \u0648\u064e\u0628\u0650\u062e\u064e\u0627\u0635\u064e\u0651\u0629\u064d \u0627\u0644\u064e\u0651\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u0652 \u0623\u064e\u0647\u0652\u0644\u0650 \u0627\u0644\u0652\u062e\u0650\u062a\u064e\u0627\u0646\u0650."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0647\u064e\u0624\u064f\u0644\u0627\u0621\u0650 \u064a\u064e\u062c\u0650\u0628\u064f \u0623\u064e\u0646\u0652 \u062a\u064f\u0633\u064e\u062f\u064e\u0651 \u0623\u064e\u0641\u0652\u0648\u064e\u0627\u0647\u064f\u0647\u064f\u0645\u0652: \u0641\u064e\u0647\u064f\u0645\u0652 \u064a\u064f\u062e\u0652\u0631\u0650\u0628\u064f\u0648\u0646\u064e \u0628\u064f\u064a\u064f\u0648\u062a\u0627\u064b \u0628\u0650\u062c\u064f\u0645\u0652\u0644\u064e\u062a\u0650\u0647\u064e\u0627\u060c \u0625\u0650\u0630\u0652 \u064a\u064f\u0639\u064e\u0644\u0650\u0651\u0645\u064f\u0648\u0646\u064e \u062a\u064e\u0639\u064e\u0627\u0644\u0650\u064a\u0645\u064e \u064a\u064e\u062c\u0650\u0628\u064f \u0623\u064e\u0644\u0651\u0627 \u062a\u064f\u0639\u064e\u0644\u064e\u0651\u0645\u064e\u060c \u0641\u0650\u064a \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0645\u064e\u0643\u0652\u0633\u064e\u0628\u064d \u062e\u064e\u0633\u0650\u064a\u0633\u064d."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0648\u064e\u0642\u064e\u062f\u0652 \u0642\u064e\u0627\u0644\u064e \u0648\u064e\u0627\u062d\u0650\u062f\u064c \u0645\u0650\u0646\u0652\u0647\u064f\u0645\u0652\u060c \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u0650\u0646\u0652\u062f\u064e\u0647\u064f\u0645\u0652 \u0646\u064e\u0628\u0650\u064a\u064c\u0651 \u062e\u064e\u0627\u0635\u064c\u0651 \u0628\u0650\u0647\u0650\u0645\u0652: \u00ab\u0623\u064e\u0647\u0652\u0644\u064f \u0643\u0650\u0631\u0650\u064a\u062a\u064e \u062f\u064e\u0627\u0626\u0650\u0645\u0627\u064b \u0643\u064e\u0630\u064e\u0651\u0627\u0628\u064f\u0648\u0646\u064e\u060c \u0648\u064f\u062d\u064f\u0648\u0634\u064c \u0634\u064e\u0631\u0650\u0633\u064e\u0629\u064c\u060c \u0646\u064e\u0647\u0650\u0645\u064f\u0648\u0646\u064e \u0643\u064f\u0633\u064e\u0627\u0644\u064e\u0649\u00bb."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0648\u064e\u0647\u064e\u0630\u0650\u0647\u0650 \u0634\u064e\u0647\u064e\u0627\u062f\u064e\u0629\u064f \u0635\u0650\u062f\u0652\u0642\u064d. \u0644\u0650\u0630\u0644\u0650\u0643\u064e \u0643\u064f\u0646\u0652 \u0645\u064f\u062a\u064e\u0634\u064e\u062f\u0650\u0651\u062f\u0627\u064b \u0641\u0650\u064a \u062a\u064e\u0648\u0652\u0628\u0650\u064a\u062e\u0650\u0647\u0650\u0645\u0652\u060c \u0644\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627 \u0623\u064e\u0635\u0650\u062d\u064e\u0651\u0627\u0621\u064e \u0641\u0650\u064a \u0627\u0644\u0625\u0650\u064a\u0645\u064e\u0627\u0646\u0650\u060c"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0644\u064e\u0627 \u064a\u064f\u062f\u0650\u064a\u0631\u064f\u0648\u0646\u064e \u0639\u064f\u0642\u064f\u0648\u0644\u064e\u0647\u064f\u0645\u0652 \u0625\u0650\u0644\u064e\u0649 \u062e\u064f\u0631\u064e\u0627\u0641\u064e\u0627\u062a\u064d \u064a\u064e\u0647\u064f\u0648\u062f\u0650\u064a\u064e\u0651\u0629\u064d \u0648\u064e\u0648\u064e\u0635\u064e\u0627\u064a\u064e\u0627 \u0623\u064f\u0646\u064e\u0627\u0633\u064d \u062a\u064e\u062d\u064e\u0648\u064e\u0651\u0644\u064f\u0648\u0627 \u0639\u064e\u0646\u0650 \u0627\u0644\u0652\u062d\u064e\u0642\u0650\u0651 \u0628\u064e\u0639\u0650\u064a\u062f\u0627\u064b."
                },
                "15": {
