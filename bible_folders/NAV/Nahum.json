    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u063a\u0636\u0628 \u0627\u0644\u0644\u0647 \u0639\u0644\u0649 \u0646\u064a\u0646\u0648\u0649 - \u0648\u064e\u062d\u0652\u064a\u064c \u0628\u0650\u0634\u064e\u0623\u0652\u0646\u0650 \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649\u060c \u0643\u064e\u0645\u064e\u0627 \u0648\u064e\u0631\u064e\u062f\u064e \u0641\u0650\u064a \u0643\u0650\u062a\u064e\u0627\u0628\u0650 \u0631\u064f\u0624\u0652\u064a\u064e\u0627 \u0646\u064e\u0627\u062d\u064f\u0648\u0645\u064e \u0627\u0644\u0623\u064e\u0644\u0652\u0642\u064f\u0648\u0634\u0650\u064a\u0650\u0651."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651 \u0625\u0650\u0644\u064e\u0647\u064c \u063a\u064e\u064a\u064f\u0648\u0631\u064c \u0648\u064e\u0645\u064f\u0646\u0652\u062a\u064e\u0642\u0650\u0645\u064c. \u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651 \u0645\u064f\u0646\u0652\u062a\u064e\u0642\u0650\u0645\u064c \u0648\u064e\u0633\u064e\u0627\u062e\u0650\u0637\u064c. \u064a\u064e\u0646\u0652\u062a\u064e\u0642\u0650\u0645\u064f \u0645\u0650\u0646\u0652 \u0623\u064e\u0639\u0652\u062f\u064e\u0627\u0626\u0650\u0647\u0650\u060c \u0648\u064e\u064a\u064f\u0636\u0652\u0645\u0650\u0631\u064f \u0627\u0644\u0652\u063a\u064e\u0636\u064e\u0628\u064e \u0644\u0650\u062e\u064f\u0635\u064f\u0648\u0645\u0650\u0647\u0650."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651 \u0628\u064e\u0637\u0650\u064a\u0621\u064c \u0641\u0650\u064a \u063a\u064e\u0636\u064e\u0628\u0650\u0647\u0650 \u0648\u064e\u0639\u064e\u0638\u0650\u064a\u0645\u064f \u0627\u0644\u0652\u0639\u0650\u0632\u064e\u0651\u0629\u0650\u060c \u0625\u0650\u0646\u064e\u0651\u0645\u064e\u0627 \u0644\u064e\u0627 \u064a\u064f\u0628\u0652\u0631\u0650\u0626\u064f \u0627\u0644\u0652\u062e\u064e\u0627\u0637\u0650\u0626\u064e \u0627\u0644\u0652\u0628\u064e\u062a\u064e\u0651\u0629\u064e. \u0637\u064e\u0631\u0650\u064a\u0642\u064f \u0627\u0644\u0631\u064e\u0651\u0628\u0650\u0651 \u0641\u0650\u064a \u0627\u0644\u0632\u064e\u0651\u0648\u0652\u0628\u064e\u0639\u064e\u0629\u0650 \u0648\u064e\u0627\u0644\u0652\u0639\u064e\u0627\u0635\u0650\u0641\u064e\u0629\u0650\u060c \u0648\u064e\u0627\u0644\u0652\u063a\u064e\u0645\u064e\u0627\u0645\u064f \u063a\u064f\u0628\u064e\u0627\u0631\u064f \u0642\u064e\u062f\u064e\u0645\u064e\u064a\u0652\u0647\u0650."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u064a\u064e\u0632\u0652\u062c\u064f\u0631\u064f \u0627\u0644\u0652\u0628\u064e\u062d\u0652\u0631\u064e \u0641\u064e\u064a\u064f\u062c\u064e\u0641\u0650\u0651\u0641\u064f\u0647\u064f. \u064a\u064f\u0646\u0652\u0636\u0650\u0628\u064f \u062c\u064e\u0645\u0650\u064a\u0639\u064e \u0627\u0644\u0623\u064e\u0646\u0652\u0647\u064e\u0627\u0631\u0650\u060c \u0641\u064e\u062a\u064e\u0630\u0652\u0648\u0650\u064a \u0645\u064e\u0631\u064e\u0627\u0639\u0650\u064a \u0628\u064e\u0627\u0634\u064e\u0627\u0646\u064e \u0648\u064e\u0627\u0644\u0652\u0643\u064e\u0631\u0652\u0645\u064e\u0644\u0650\u060c \u0648\u064e\u064a\u064e\u0630\u0652\u0628\u064f\u0644\u064f \u0632\u064e\u0647\u0652\u0631\u064f \u0644\u064f\u0628\u0652\u0646\u064e\u0627\u0646\u064e."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u062a\u064e\u062a\u064e\u0632\u064e\u0644\u0652\u0632\u064e\u0644\u064f \u0627\u0644\u0652\u062c\u0650\u0628\u064e\u0627\u0644\u064f \u0623\u064e\u0645\u064e\u0627\u0645\u064e\u0647\u064f\u060c \u0648\u064e\u062a\u064e\u0630\u064f\u0648\u0628\u064f \u0627\u0644\u062a\u0650\u0651\u0644\u0627\u0644\u064f\u060c \u0648\u064e\u062a\u064e\u062a\u064e\u0635\u064e\u062f\u064e\u0651\u0639\u064f \u0627\u0644\u0623\u064e\u0631\u0652\u0636\u064f \u0641\u0650\u064a \u062d\u064e\u0636\u0652\u0631\u064e\u062a\u0650\u0647\u0650 \u0648\u064e\u0627\u0644\u0652\u0645\u064e\u0633\u0652\u0643\u064f\u0648\u0646\u064e\u0629\u064f \u0648\u064e\u0627\u0644\u0633\u064e\u0651\u0627\u0643\u0650\u0646\u064f\u0648\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0645\u064e\u0646\u0652 \u064a\u064e\u0635\u0652\u0645\u0650\u062f\u064f \u0623\u064e\u0645\u064e\u0627\u0645\u064e \u0633\u064e\u062e\u064e\u0637\u0650\u0647\u0650\u061f \u0645\u064e\u0646\u0652 \u064a\u064e\u062a\u064e\u062d\u064e\u0645\u064e\u0651\u0644\u064f \u0641\u064e\u0631\u0652\u0637\u064e \u0627\u0636\u0652\u0637\u0650\u0631\u064e\u0627\u0645\u0650 \u063a\u064e\u0636\u064e\u0628\u0650\u0647\u0650\u061f \u064a\u064e\u0646\u0652\u0635\u064e\u0628\u064f\u0651 \u063a\u064e\u0636\u064e\u0628\u064f\u0647\u064f \u0643\u064e\u0627\u0644\u0646\u064e\u0651\u0627\u0631\u0650 \u0648\u064e\u062a\u064e\u0646\u0652\u062d\u064e\u0644\u064f\u0651 \u062a\u064e\u062d\u0652\u062a\u064e \u0648\u064e\u0637\u0652\u0623\u064e\u062a\u0650\u0647\u0650 \u0627\u0644\u0635\u064f\u0651\u062e\u064f\u0648\u0631\u064f."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651 \u0635\u064e\u0627\u0644\u0650\u062d\u064c\u060c \u062d\u0650\u0635\u0652\u0646\u064c \u0641\u0650\u064a \u064a\u064e\u0648\u0652\u0645\u0650 \u0627\u0644\u0636\u0650\u0651\u064a\u0642\u0650\u060c \u0648\u064e\u064a\u064e\u0639\u0652\u0631\u0650\u0641\u064f \u0627\u0644\u0652\u0645\u064f\u0639\u0652\u062a\u064e\u0635\u0650\u0645\u0650\u064a\u0646\u064e \u0628\u0650\u0647\u0650."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0648\u064e\u0644\u064e\u0643\u0650\u0646\u064e\u0651\u0647\u064f \u0628\u0650\u0637\u064f\u0648\u0641\u064e\u0627\u0646\u064d \u0637\u064e\u0627\u0645\u064d \u064a\u064f\u062e\u0652\u0641\u0650\u064a \u0645\u064e\u0639\u064e\u0627\u0644\u0650\u0645\u064e \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649\u060c \u0648\u064e\u062a\u064f\u062f\u0652\u0631\u0650\u0643\u064f \u0627\u0644\u0638\u064f\u0651\u0644\u0652\u0645\u064e\u0629\u064f \u0623\u064e\u0639\u0652\u062f\u064e\u0627\u0621\u064e\u0647\u064f."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0644\u0650\u0645\u064e\u0627\u0630\u064e\u0627 \u062a\u064e\u062a\u064e\u0622\u0645\u064e\u0631\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u0631\u064e\u0651\u0628\u0650\u0651\u061f \u0625\u0650\u0646\u064e\u0651\u0647\u064f \u064a\u064e\u0642\u0652\u0636\u0650\u064a \u0639\u064e\u0644\u064e\u0649 \u0645\u064f\u0624\u064e\u0627\u0645\u064e\u0631\u064e\u062a\u0650\u0643\u064f\u0645\u0652\u060c \u0648\u064e\u064a\u064f\u0641\u0652\u0646\u0650\u064a\u0643\u064f\u0645\u0652 \u0628\u0650\u0636\u064e\u0631\u0652\u0628\u064e\u0629\u064d \u0648\u064e\u0627\u062d\u0650\u062f\u064e\u0629\u064d."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0648\u064e\u062a\u064e\u0644\u0652\u062a\u064e\u0647\u0650\u0645\u064f\u0647\u064f\u0645\u064f \u0627\u0644\u0646\u064e\u0651\u0627\u0631\u064f \u0643\u064e\u0645\u064e\u0627 \u062a\u064e\u0644\u0652\u062a\u064e\u0647\u0650\u0645\u064f \u0634\u064e\u062c\u064e\u0631\u064e\u0629\u064e \u0639\u064f\u0644\u064e\u0651\u064a\u0652\u0642\u064d \u0643\u064e\u062b\u0650\u064a\u0641\u064e\u0629\u064b \u0623\u064e\u0648\u0652 \u0633\u064f\u0643\u064e\u0627\u0631\u064e\u0649 \u0645\u064f\u062a\u064e\u0631\u064e\u0646\u0650\u0651\u062d\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u0652 \u062e\u064e\u0645\u0652\u0631\u0650\u0647\u0650\u0645\u0652 \u0623\u064e\u0648\u0652 \u062d\u0650\u0632\u0652\u0645\u064e\u0629\u064e \u0642\u064e\u0634\u064d\u0651 \u062c\u064e\u0627\u0641\u064e\u0651\u0629\u064b."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0645\u0650\u0646\u0652\u0643\u0650 \u062e\u064e\u0631\u064e\u062c\u064e \u064a\u064e\u0627 \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 \u0645\u064e\u0646\u0652 \u062a\u064e\u0622\u0645\u064e\u0631\u064e \u0628\u0650\u0627\u0644\u0634\u064e\u0651\u0631\u0650\u0651 \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u0631\u064e\u0651\u0628\u0650\u0651\u060c \u0648\u064e\u0627\u0644\u0652\u0645\u064f\u0634\u0650\u064a\u0631\u064f \u0628\u0650\u0627\u0644\u0633\u064f\u0651\u0648\u0621\u0650."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0648\u064e\u0647\u064e\u0630\u064e\u0627 \u0645\u064e\u0627 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0647\u064f \u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651: \u0645\u064e\u0639\u064e \u0623\u064e\u0646\u064e\u0651\u0643\u064f\u0645\u0652 \u0623\u064e\u0642\u0652\u0648\u0650\u064a\u064e\u0627\u0621\u064f \u0648\u064e\u0643\u064e\u062b\u0650\u064a\u0631\u064f\u0648\u0646\u064e \u0641\u064e\u0625\u0650\u0646\u064e\u0651\u0643\u064f\u0645\u0652 \u062a\u064f\u0633\u0652\u062a\u064e\u0623\u0652\u0635\u064e\u0644\u064f\u0648\u0646\u064e \u0648\u064e\u062a\u064e\u0641\u0652\u0646\u064e\u0648\u0652\u0646\u064e. \u0623\u064e\u0645\u064e\u0651\u0627 \u0623\u064e\u0646\u0652\u062a\u064f\u0645\u0652 \u064a\u064e\u0627 \u0634\u064e\u0639\u0652\u0628\u0650\u064a \u0641\u064e\u0642\u064e\u062f\u0652 \u0639\u064e\u0627\u0642\u064e\u0628\u0652\u062a\u064f\u0643\u064f\u0645\u0652 \u0623\u064e\u0634\u064e\u062f\u064e\u0651 \u0639\u0650\u0642\u064e\u0627\u0628\u064d \u0648\u064e\u0644\u064e\u0646\u0652 \u0623\u064f\u0646\u0652\u0632\u0650\u0644\u064e \u0628\u0650\u0643\u064f\u0645\u064f \u0627\u0644\u0652\u0648\u064e\u064a\u0652\u0644\u0627\u062a\u0650 \u062b\u064e\u0627\u0646\u0650\u064a\u064e\u0629\u064b."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0628\u064e\u0644\u0652 \u0623\u064f\u062d\u064e\u0637\u0650\u0651\u0645\u064f \u0627\u0644\u0622\u0646\u064e \u0646\u0650\u064a\u0631\u064e \u0623\u064e\u0634\u064f\u0651\u0648\u0631\u064e \u0639\u064e\u0646\u0652\u0643\u064f\u0645\u0652\u060c \u0648\u064e\u0623\u064e\u0643\u0652\u0633\u0650\u0631\u064f \u0623\u064e\u063a\u0652\u0644\u0627\u0644\u064e\u0643\u064f\u0645\u0652."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0648\u064e\u0647\u064e\u0627 \u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651 \u0642\u064e\u062f\u0652 \u0623\u064e\u0635\u0652\u062f\u064e\u0631\u064e \u0642\u064e\u0636\u064e\u0627\u0621\u064e\u0647\u064f \u0628\u0650\u0634\u064e\u0623\u0652\u0646\u0650\u0643\u064e \u064a\u064e\u0627 \u0623\u064e\u0634\u064f\u0651\u0648\u0631\u064f: \u0644\u064e\u0646\u0652 \u062a\u064e\u0628\u0652\u0642\u064e\u0649 \u0644\u064e\u0643\u064e \u0630\u064f\u0631\u0650\u0651\u064a\u064e\u0651\u0629\u064c \u062a\u064e\u062d\u0652\u0645\u0650\u0644\u064f \u0627\u0633\u0652\u0645\u064e\u0643\u064e. \u0648\u064e\u0623\u064e\u0633\u0652\u062a\u064e\u0623\u0652\u0635\u0650\u0644\u064f \u0645\u0650\u0646\u0652 \u0647\u064e\u064a\u0652\u0643\u064e\u0644\u0650 \u0622\u0644\u0647\u064e\u062a\u0650\u0643\u064e \u0645\u064e\u0646\u0652\u062d\u064f\u0648\u062a\u064e\u0627\u062a\u0650\u0643\u064e \u0648\u064e\u0645\u064e\u0633\u0652\u0628\u064f\u0648\u0643\u064e\u0627\u062a\u0650\u0643\u064e\u060c \u0648\u064e\u0623\u064e\u062c\u0652\u0639\u064e\u0644\u064f\u0647\u064f \u0642\u064e\u0628\u0652\u0631\u064e\u0643\u064e\u060c \u0644\u0623\u064e\u0646\u064e\u0651\u0643\u064e \u0635\u0650\u0631\u0652\u062a\u064e \u0646\u064e\u062c\u0650\u0633\u0627\u064b."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0647\u064f\u0648\u064e\u0630\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u0652\u062c\u0650\u0628\u064e\u0627\u0644\u0650 (\u062a\u064e\u0633\u0650\u064a\u0631\u064f) \u0642\u064e\u062f\u064e\u0645\u064e\u0627 \u0627\u0644\u0652\u0645\u064f\u0628\u064e\u0634\u0650\u0651\u0631\u0650 \u062d\u064e\u0627\u0645\u0650\u0644\u0650 \u0627\u0644\u0623\u064e\u062e\u0652\u0628\u064e\u0627\u0631\u0650 \u0627\u0644\u0633\u064e\u0651\u0627\u0631\u064e\u0651\u0629\u0650\u060c \u0627\u0644\u064e\u0651\u0630\u0650\u064a \u064a\u064f\u0639\u0652\u0644\u0650\u0646\u064f \u0627\u0644\u0633\u064e\u0651\u0644\u0627\u0645\u064e. \u0641\u064e\u064a\u064e\u0627 \u064a\u064e\u0647\u064f\u0648\u0630\u064e\u0627 \u0648\u064e\u0627\u0638\u0650\u0628\u0652 \u0639\u064e\u0644\u064e\u0649 \u0627\u0644\u0627\u062d\u0652\u062a\u0650\u0641\u064e\u0627\u0644\u0650 \u0628\u0650\u0623\u064e\u0639\u0652\u064a\u064e\u0627\u062f\u0650\u0643\u064e \u0648\u064e\u0623\u064e\u0648\u0652\u0641\u0650 \u0646\u064f\u0630\u064f\u0648\u0631\u064e\u0643\u064e \u0644\u0623\u064e\u0646\u064e\u0651\u0647\u064f \u0644\u064e\u0646\u0652 \u064a\u064f\u0647\u064e\u0627\u062c\u0650\u0645\u064e\u0643\u064e \u0627\u0644\u0634\u0650\u0651\u0631\u0650\u0651\u064a\u0631\u064f \u0645\u0650\u0646\u0652 \u0628\u064e\u0639\u0652\u062f\u064f\u060c \u0625\u0650\u0630\u0652 \u0642\u064e\u062f\u0650 \u0627\u0646\u0652\u0642\u064e\u0631\u064e\u0636\u064e \u062a\u064e\u0645\u064e\u0627\u0645\u0627\u064b."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0646\u064a\u0646\u0648\u0649 \u0633\u0642\u0648\u0637 - \u0642\u064e\u062f\u0652 \u0632\u064e\u062d\u064e\u0641\u064e \u0639\u064e\u0644\u064e\u064a\u0652\u0643\u0650 \u0627\u0644\u0652\u0645\u064f\u0647\u064e\u0627\u062c\u0650\u0645\u064f \u064a\u064e\u0627 \u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649\u060c \u0641\u064e\u0627\u062d\u0652\u0631\u064f\u0633\u0650\u064a \u0627\u0644\u0652\u062d\u0650\u0635\u0652\u0646\u064e \u0648\u064e\u0631\u064e\u0627\u0642\u0650\u0628\u0650\u064a \u0627\u0644\u0637\u064e\u0651\u0631\u0650\u064a\u0642\u064e\u060c \u0645\u064e\u0646\u0650\u0651\u0639\u0650\u064a \u0623\u064e\u0633\u0652\u0648\u064e\u0627\u0631\u064e\u0643\u0650\u060c \u0648\u064e\u062c\u064e\u0646\u0650\u0651\u062f\u0650\u064a \u0643\u064f\u0644\u064e\u0651 \u0642\u064f\u0648\u064e\u0651\u062a\u0650\u0643\u0650."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0644\u0623\u064e\u0646\u064e\u0651 \u0627\u0644\u0631\u064e\u0651\u0628\u064e\u0651 \u064a\u064f\u0639\u0650\u064a\u062f\u064f \u0628\u064e\u0647\u064e\u0627\u0621\u064e \u064a\u064e\u0639\u0652\u0642\u064f\u0648\u0628\u064e \u0648\u064e\u0645\u064e\u062c\u0652\u062f\u064e \u0625\u0650\u0633\u0652\u0631\u064e\u0627\u0626\u0650\u064a\u0644\u064e\u060c \u0644\u0623\u064e\u0646\u064e\u0651 \u0627\u0644\u0646\u064e\u0651\u0627\u0647\u0650\u0628\u0650\u064a\u0646\u064e \u0633\u064e\u0644\u064e\u0628\u064f\u0648\u0647\u064f\u0645\u0652 \u0648\u064e\u0623\u064e\u062a\u0652\u0644\u064e\u0641\u064f\u0648\u0627 \u0643\u064f\u0631\u064f\u0648\u0645\u064e\u0647\u064f\u0645\u0652."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u062a\u064f\u0631\u064f\u0648\u0633\u064f \u0623\u064e\u0628\u0652\u0637\u064e\u0627\u0644\u0650\u0647\u0650 \u0645\u064f\u062e\u064e\u0636\u064e\u0651\u0628\u064e\u0629\u064c \u0628\u0650\u0627\u0644\u0627\u062d\u0652\u0645\u0650\u0631\u064e\u0627\u0631\u0650\u060c \u0648\u064e\u062c\u064f\u0646\u064f\u0648\u062f\u064f\u0647\u064f \u0627\u0644\u0652\u0645\u064f\u062d\u064e\u0627\u0631\u0650\u0628\u064f\u0648\u0646\u064e \u0645\u064f\u0633\u064e\u0631\u0652\u0628\u064e\u0644\u064f\u0648\u0646\u064e \u0628\u0650\u0627\u0644\u0652\u0642\u0650\u0631\u0652\u0645\u0650\u0632\u0650. \u064a\u064e\u0628\u0652\u0631\u064f\u0642\u064f \u0641\u064f\u0648\u0644\u0627\u0630\u064f \u0627\u0644\u0652\u0645\u064e\u0631\u0652\u0643\u064e\u0628\u064e\u0627\u062a\u0650 \u0641\u0650\u064a \u064a\u064e\u0648\u0652\u0645\u0650 \u062a\u064e\u0623\u064e\u0647\u064f\u0651\u0628\u0650\u0647\u064e\u0627\u060c \u0648\u064e\u062a\u064e\u062a\u064e\u0628\u064e\u062e\u0652\u062a\u064e\u0631\u064f \u062c\u0650\u064a\u064e\u0627\u062f\u064f\u0647\u064e\u0627."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u062a\u064e\u062a\u064e\u0631\u064e\u0627\u0643\u064e\u0636\u064f \u0627\u0644\u0652\u0645\u064e\u0631\u0652\u0643\u064e\u0628\u064e\u0627\u062a\u064f \u0628\u0650\u0639\u064f\u0646\u0652\u0641\u064d \u0641\u0650\u064a \u0627\u0644\u0634\u064e\u0651\u0648\u064e\u0627\u0631\u0650\u0639\u0650\u060c \u0648\u064e\u0639\u064e\u0628\u0652\u0631\u064e \u0627\u0644\u0633\u064e\u0651\u0627\u062d\u064e\u0627\u062a\u0650 \u062a\u064e\u0645\u0652\u0631\u064f\u0642\u064f \u0643\u064e\u0627\u0644\u0652\u0628\u064e\u0631\u0652\u0642\u0650 \u0648\u064e\u0645\u064e\u0646\u0652\u0638\u064e\u0631\u064f\u0647\u064e\u0627 \u0643\u064e\u0627\u0644\u0652\u0645\u064e\u0634\u064e\u0627\u0639\u0650\u0644\u0650 \u0627\u0644\u0652\u0645\u064f\u062a\u064e\u0648\u064e\u0647\u0650\u0651\u062c\u064e\u0629\u0650."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u064a\u064e\u0633\u0652\u062a\u064e\u062f\u0652\u0639\u0650\u064a \u0627\u0644\u0652\u0645\u064e\u0644\u0650\u0643\u064f \u0636\u064f\u0628\u064e\u0651\u0627\u0637\u064e\u0647\u064f\u060c \u0641\u064e\u064a\u064f\u0633\u0652\u0631\u0650\u0639\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u064e\u064a\u0652\u0647\u0650 \u0645\u064f\u062a\u064e\u0639\u064e\u062b\u0650\u0651\u0631\u0650\u064a\u0646\u064e \u0641\u0650\u064a \u062e\u064f\u0637\u064e\u0627\u0647\u064f\u0645\u0652\u060c \u064a\u064f\u0647\u064e\u0631\u0652\u0648\u0650\u0644\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u064e\u0649 \u0633\u064f\u0648\u0631\u0650 \u0627\u0644\u0652\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650 \u0648\u064e\u0642\u064e\u062f\u0652 \u0623\u064f\u0642\u0650\u064a\u0645\u064e\u062a\u0650 \u0627\u0644\u0652\u0645\u064e\u062a\u064e\u0627\u0631\u0650\u064a\u0633\u064f."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0642\u064e\u062f\u0650 \u0627\u0646\u0652\u0641\u064e\u062a\u064e\u062d\u064e\u062a\u0652 \u0628\u064e\u0648\u064e\u0651\u0627\u0628\u064e\u0627\u062a\u064f \u0627\u0644\u0623\u064e\u0646\u0652\u0647\u064e\u0627\u0631\u0650\u060c \u0648\u064e\u0627\u0646\u0652\u0647\u064e\u0627\u0631\u064e \u0627\u0644\u0652\u0642\u064e\u0635\u0652\u0631\u064f \u0623\u064e\u0645\u064e\u0627\u0645\u064e \u062c\u064e\u062d\u064e\u0627\u0641\u0650\u0644\u0650 \u0627\u0644\u0623\u064e\u0639\u0652\u062f\u064e\u0627\u0621\u0650."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0623\u064e\u0635\u0652\u0628\u064e\u062d\u064e\u062a\u0652 \u0633\u064e\u064a\u0650\u0651\u062f\u064e\u0629\u064f \u0627\u0644\u0652\u0642\u064e\u0635\u0652\u0631\u0650 \u0639\u064e\u0627\u0631\u0650\u064a\u064e\u0629\u064b \u0645\u064e\u0633\u064f\u0648\u0642\u064e\u0629\u064b \u0625\u0650\u0644\u064e\u0649 \u0627\u0644\u0623\u064e\u0633\u0652\u0631\u0650\u060c \u0648\u064e\u0634\u064e\u0631\u064e\u0639\u064e\u062a\u0652 \u062c\u064e\u0648\u064e\u0627\u0631\u0650\u064a\u0647\u064e\u0627 \u064a\u064e\u0646\u064f\u062d\u0652\u0646\u064e \u0643\u064e\u0646\u064e\u0648\u0652\u062d\u0650 \u0627\u0644\u0652\u062d\u064e\u0645\u064e\u0627\u0645\u0650 \u0648\u064e\u064a\u064e\u0636\u0652\u0631\u0650\u0628\u0652\u0646\u064e \u0635\u064f\u062f\u064f\u0648\u0631\u064e\u0647\u064f\u0646\u064e\u0651."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0646\u0650\u064a\u0646\u064e\u0648\u064e\u0649 \u0643\u064e\u0628\u0650\u0631\u0652\u0643\u064e\u0629\u064d \u0646\u064e\u0636\u064e\u0628\u064e\u062a\u0652 \u0645\u0650\u064a\u064e\u0627\u0647\u064f\u0647\u064e\u0627\u060c \u0625\u0650\u0630\u0652 \u0641\u064e\u0631\u064e\u0651 \u0623\u064e\u0647\u0652\u0644\u064f\u0647\u064e\u0627. \u0648\u064e\u062a\u064e\u0639\u0652\u0644\u064f\u0648 \u0627\u0644\u0635\u064e\u0651\u0631\u0652\u062e\u064e\u0629\u064f: \u00ab\u0642\u0650\u0641\u064f\u0648\u0627\u060c \u0642\u0650\u0641\u064f\u0648\u0627\u00bb. \u0648\u064e\u0644\u0627 \u0645\u0650\u0646\u0652 \u0645\u064f\u062c\u0650\u064a\u0628\u064d \u064a\u064e\u0644\u0652\u062a\u064e\u0641\u0650\u062a\u064f."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0627\u0646\u0652\u0647\u064e\u0628\u064f\u0648\u0627 \u0627\u0644\u0652\u0641\u0650\u0636\u064e\u0651\u0629\u064e\u060c \u0627\u0646\u0652\u0647\u064e\u0628\u064f\u0648\u0627 \u0627\u0644\u0630\u064e\u0651\u0647\u064e\u0628\u064e. \u0644\u064e\u0627 \u0646\u0650\u0647\u064e\u0627\u064a\u064e\u0629\u064e \u0644\u0650\u0643\u064f\u0646\u064f\u0648\u0632\u0650\u0647\u064e\u0627 \u0623\u064e\u0648\u0652 \u0644\u0650\u0646\u064e\u0641\u064e\u0627\u0626\u0650\u0633\u0650 \u062b\u064e\u0631\u0652\u0648\u064e\u062a\u0650\u0647\u064e\u0627."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0623\u064e\u0636\u0652\u062d\u064e\u062a\u0652 \u0645\u064f\u0648\u062d\u0650\u0634\u064e\u0629\u064b \u062e\u064e\u0627\u0648\u0650\u064a\u064e\u0629\u064b \u062c\u064e\u0631\u0652\u062f\u064e\u0627\u0621\u064e\u060c \u0630\u064e\u0627\u0628\u064e\u062a\u0652 \u0645\u0650\u0646\u0652\u0647\u064e\u0627 \u0627\u0644\u0652\u0642\u064f\u0644\u064f\u0648\u0628\u064f \u0631\u064f\u0639\u0652\u0628\u0627\u064b \u0648\u064e\u0627\u0646\u0652\u062d\u064e\u0644\u064e\u0651\u062a\u0652 \u0645\u0650\u0646\u0652\u0647\u064e\u0627 \u0627\u0644\u0631\u064f\u0651\u0643\u064e\u0628\u064f\u060c \u0637\u064e\u063a\u064e\u0649 \u0639\u064e\u0644\u064e\u064a\u0652\u0647\u064e\u0627 \u0627\u0644\u0623\u064e\u0644\u064e\u0645\u064f \u0648\u064e\u0639\u064e\u0644\u0627 \u0648\u064f\u062c\u064f\u0648\u0647\u064e \u0623\u064e\u0647\u0652\u0644\u0650\u0647\u064e\u0627 \u0627\u0644\u0634\u064f\u0651\u062d\u064f\u0648\u0628\u064f."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0623\u064e\u064a\u0652\u0646\u064e \u0646\u0650\u064a\u0646\u064e\u0648\u0649 \u0639\u064e\u0631\u0650\u064a\u0646\u064f \u0627\u0644\u0623\u064f\u0633\u064f\u0648\u062f\u0650 \u0648\u064e\u0645\u0652\u0631\u0652\u062a\u064e\u0639\u064f \u0627\u0644\u0623\u064e\u0634\u0652\u0628\u064e\u0627\u0644\u0650 \u062d\u064e\u064a\u0652\u062b\u064f \u064a\u064e\u0633\u0652\u0631\u064e\u062d\u064f \u0627\u0644\u0623\u064e\u0633\u064e\u062f\u064f \u0648\u064e\u0627\u0644\u0644\u064e\u0651\u0628\u064f\u0648\u0621\u064e\u0629\u064f \u0648\u064e\u0627\u0644\u0623\u064e\u0634\u0652\u0628\u064e\u0627\u0644\u064f \u0645\u0650\u0646\u0652 \u063a\u064e\u064a\u0652\u0631\u0650 \u0625\u0650\u0632\u0652\u0639\u064e\u0627\u062c\u064d\u061f"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0642\u064e\u062f\u0650 \u0627\u0641\u0652\u062a\u064e\u0631\u064e\u0633\u064e \u0627\u0644\u0623\u064e\u0633\u064e\u062f\u064f \u0645\u064e\u0627 \u064a\u064e\u0643\u0652\u0641\u0650\u064a \u0644\u0625\u0650\u0639\u064e\u0627\u0644\u064e\u0629\u0650 \u0623\u064e\u0634\u0652\u0628\u064e\u0627\u0644\u0650\u0647\u0650 \u0648\u064e\u062e\u064e\u0646\u064e\u0642\u064e \u0641\u064e\u0631\u0650\u064a\u0633\u064e\u0629\u064b \u0644\u0650\u0644\u064e\u0628\u064f\u0648\u0621\u064e\u0627\u062a\u0650\u0647\u0650. \u0642\u064e\u062f\u0652 \u0645\u064e\u0644\u0623\u064e \u0639\u064e\u0631\u0650\u064a\u0646\u064e\u0647\u064f \u0628\u0650\u0642\u064e\u0646\u064e\u0627\u0626\u0650\u0635\u0650\u0647\u0650 \u0648\u064e\u0643\u064e\u0647\u0652\u0641\u064e\u0647\u064f \u0628\u0650\u0623\u064e\u0634\u0652\u0644\u0627\u0626\u0650\u0647\u0650\u0645\u0652."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0647\u064e\u0627 \u0623\u064e\u0646\u064e\u0627 \u0623\u064f\u0642\u064e\u0627\u0648\u0650\u0645\u064f\u0643\u0650. \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0627\u0644\u0631\u064e\u0651\u0628\u064f\u0651 \u0627\u0644\u0652\u0642\u064e\u062f\u0650\u064a\u0631\u064f. \u0641\u064e\u0623\u064f\u062d\u0652\u0631\u0650\u0642\u064f \u0645\u064e\u0631\u0652\u0643\u064e\u0628\u064e\u0627\u062a\u0650\u0643\u0650 \u0641\u064e\u062a\u064f\u0635\u0652\u0628\u0650\u062d\u064f \u062f\u064f\u062e\u064e\u0627\u0646\u0627\u064b\u060c \u0648\u064e\u064a\u064e\u0644\u0652\u062a\u064e\u0647\u0650\u0645\u064f \u0627\u0644\u0633\u064e\u0651\u064a\u0652\u0641\u064f \u0644\u064f\u062d\u064f\u0648\u0645\u064e \u0623\u064e\u0628\u0652\u0646\u064e\u0627\u0626\u0650\u0643\u0650\u060c \u0648\u064e\u0623\u064e\u0633\u0652\u062a\u064e\u0623\u0652\u0635\u0650\u0644\u064f \u0645\u0650\u0646\u064e \u0627\u0644\u0623\u064e\u0631\u0652\u0636\u0650 \u063a\u064e\u0646\u064e\u0627\u0626\u0650\u0645\u064e\u0643\u0650 \u0648\u064e\u0644\u064e\u0646\u0652 \u064a\u064e\u062a\u064e\u0631\u064e\u062f\u064e\u0651\u062f\u064e \u0641\u0650\u064a \u0645\u064e\u0627 \u0628\u064e\u0639\u0652\u062f\u064f \u0635\u064e\u0648\u0652\u062a\u064f \u0645\u064e\u0646\u0652\u062f\u064f\u0648\u0628\u0650\u064a\u0643\u0650."
                }
