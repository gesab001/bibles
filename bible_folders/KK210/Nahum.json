        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0411\u04b1\u043b \u2014 \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u043d\u0456\u04a3 \u041d\u0438\u043d\u0435\u0432\u0438 \u0436\u0430\u0439\u044b\u043d\u0434\u0430\u0493\u044b \u0430\u0443\u044b\u0440 \u0445\u0430\u0431\u0430\u0440\u044b, \u0435\u043b\u049b\u043e\u0441\u0442\u044b\u049b \u041d\u0430\u049b\u04b1\u043c \u043a\u04e9\u0440\u0433\u0435\u043d \u043a\u04e9\u0440\u0456\u043d\u0456\u0441\u0442\u0435\u0440 \u0442\u0443\u0440\u0430\u043b\u044b \u0436\u0430\u0437\u0431\u0430."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u2014 \u04e8\u0437\u0456\u043d\u0435\u043d \u0431\u0430\u0441\u049b\u0430 \u0442\u04d9\u04a3\u0456\u0440\u043b\u0435\u0440\u0456\u04a3\u043d\u0456\u04a3 \u0431\u043e\u043b\u0443\u044b\u043d\u0430 \u0435\u0448 \u0442\u04e9\u0437\u0431\u0435\u0439\u0442\u0456\u043d \u04d9\u0440\u0456 \u0430\u0440\u0430\u043c \u0456\u0441\u0442\u0456\u04a3 \u049b\u0430\u0440\u044b\u043c\u0442\u0430\u0441\u044b\u043d \u049b\u0430\u0439\u0442\u0430\u0440\u0430\u0442\u044b\u043d \u049a\u04b1\u0434\u0430\u0439. \u041e\u043b \u0437\u04b1\u043b\u044b\u043c\u0434\u044b\u049b\u0442\u0430\u0440\u0434\u044b \u0436\u0430\u0437\u0430\u043b\u0430\u043f, \u043e\u043b\u0430\u0440\u0493\u0430 \u0430\u0448\u0443\u043b\u0430\u043d\u0430\u0434\u044b. \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u0436\u0430\u0443\u043b\u0430\u0440\u044b\u043d\u0430 \u043b\u0430\u0439\u044b\u049b\u0442\u044b \u0436\u0430\u0437\u0430\u043b\u0430\u0440\u044b\u043d \u0442\u0430\u0440\u0442\u049b\u044b\u0437\u044b\u043f, \u0434\u04b1\u0448\u043f\u0430\u043d\u0434\u0430\u0440\u044b\u043d\u044b\u04a3 \u049b\u044b\u043b\u043c\u044b\u0441\u0442\u0430\u0440\u044b\u043d \u0435\u0441\u0456\u043d\u0435\u043d \u0448\u044b\u0493\u0430\u0440\u043c\u0430\u0439\u0434\u044b."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u049b\u0430\u04bb\u0430\u0440\u043b\u0430\u043d\u0443\u0493\u0430 \u0430\u0441\u044b\u049b\u043f\u0430\u0439\u0434\u044b, \u041e\u043d\u044b\u04a3 \u049b\u04b1\u0434\u0456\u0440\u0435\u0442\u0456 \u0435\u0440\u0435\u043d \u0437\u043e\u0440. \u041e\u043b \u0430\u0439\u044b\u043f\u0442\u044b\u043d\u044b \u0436\u0430\u0437\u0430\u0441\u044b\u0437 \u049b\u0430\u043b\u0434\u044b\u0440\u043c\u0430\u0439\u0434\u044b. \u0422\u04d9\u04a3\u0456\u0440 \u0418\u0435\u043d\u0456\u04a3 \u0436\u04af\u0440\u0433\u0435\u043d \u0436\u043e\u043b\u044b \u049b\u04b1\u0439\u044b\u043d \u043c\u0435\u043d \u0434\u0430\u0443\u044b\u043b\u0434\u0430\u043d \u04e9\u0442\u0435\u0434\u0456, \u0431\u04b1\u043b\u0442\u0442\u0430\u0440 \u041e\u043d\u044b\u04a3 \u0430\u044f\u0493\u044b\u043d\u044b\u04a3 \u0431\u0430\u0441\u049b\u0430\u043d \u0448\u0430\u04a3\u044b\u043d\u0430 \u04b1\u049b\u0441\u0430\u0439\u0434\u044b."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u0442\u0435\u04a3\u0456\u0437\u0433\u0435 \u0442\u044b\u0439\u044b\u043c \u0441\u0430\u043b\u044b\u043f, \u043e\u043d\u044b \u049b\u04b1\u0440\u0493\u0430\u0442\u0430\u0434\u044b, \u04e9\u0437\u0435\u043d\u0434\u0435\u0440\u0433\u0435 \u0431\u04b1\u0439\u044b\u0440\u0493\u0430\u043d\u0434\u0430 \u0441\u043e\u043b\u0430\u0440\u0434\u044b\u04a3 \u0431\u0430\u0440\u043b\u044b\u0493\u044b \u043a\u0435\u0443\u0456\u043f \u049b\u0430\u043b\u0430\u0434\u044b, \u0411\u0430\u0448\u0430\u043d \u043c\u0435\u043d \u049a\u0430\u0440\u043c\u0438\u043b \u0442\u0430\u0443\u043b\u0430\u0440\u044b\u043d\u0434\u0430\u0493\u044b \u0436\u0430\u0439\u044b\u043b\u044b\u043c\u0434\u0430\u0440 \u043a\u04af\u0439\u0456\u043f \u043a\u0435\u0442\u0435\u0434\u0456, \u041b\u0438\u0432\u0430\u043d\u043d\u044b\u04a3 \u049b\u0430\u0443\u043b\u0430\u0493\u0430\u043d \u043e\u0440\u043c\u0430\u043d\u0434\u0430\u0440\u044b \u0441\u043e\u043b\u044b\u043f \u049b\u0443\u0440\u0430\u043f \u049b\u0430\u043b\u0430\u0434\u044b."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0422\u0430\u0443\u043b\u0430\u0440 \u041e\u043d\u044b\u04a3 \u0430\u043b\u0434\u044b\u043d\u0434\u0430 \u0441\u0456\u043b\u043a\u0456\u043d\u0456\u043f, \u0442\u04e9\u0431\u0435\u043b\u0435\u0440 \u0431\u0430\u043b\u049b\u044b\u043f \u043a\u0435\u0442\u0435\u0434\u0456. \u0416\u0435\u0440 \u0431\u0435\u0442\u0456, \u0431\u04af\u043a\u0456\u043b \u04d9\u043b\u0435\u043c \u0436\u04d9\u043d\u0435 \u043e\u043d\u0434\u0430 \u0442\u04b1\u0440\u0430\u0442\u044b\u043d\u0434\u0430\u0440\u0434\u044b\u04a3 \u0431\u0430\u0440\u043b\u044b\u0493\u044b \u041e\u043d\u044b\u04a3 \u0430\u043b\u0434\u044b\u043d\u0434\u0430 \u0434\u0456\u0440\u0456\u043b\u0434\u0435\u0439\u0434\u0456."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u043d\u0456\u04a3 \u0430\u0448\u0443\u044b\u043d\u0430 \u043a\u0456\u043c \u0448\u044b\u0434\u0430\u0439 \u0430\u043b\u0430\u0440 \u0435\u043a\u0435\u043d? \u041e\u043d\u044b\u04a3 \u043b\u0430\u0443\u043b\u0430\u0493\u0430\u043d \u049b\u0430\u04bb\u0430\u0440\u044b\u043d\u0430 \u043a\u0456\u043c \u0442\u04e9\u0442\u0435\u043f \u0431\u0435\u0440\u0435 \u0430\u043b\u0430\u0434\u044b? \u041e\u043d\u044b\u04a3 \u0430\u0448\u0443-\u044b\u0437\u0430\u0441\u044b \u043e\u0442 \u0441\u0438\u044f\u049b\u0442\u044b \u0442\u04e9\u0433\u0456\u043b\u0456\u043f, \u0436\u0430\u0440\u0442\u0430\u0441\u0442\u0430\u0440 \u0434\u0430 \u041e\u043d\u044b\u04a3 \u0430\u043b\u0434\u044b\u043d\u0434\u0430 \u0442\u0430\u043b\u049b\u0430\u043d\u0434\u0430\u043b\u044b\u043f \u04af\u0433\u0456\u0442\u0456\u043b\u0435\u0434\u0456."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u0456\u0437\u0433\u0456, \u0431\u0430\u0441\u044b\u04a3\u0430 \u049b\u0438\u044b\u043d\u0434\u044b\u049b \u0442\u04af\u0441\u043a\u0435\u043d \u043a\u0435\u0437\u0434\u0435 \u041e\u043b \u043f\u0430\u043d\u0430\u043b\u0430\u0439\u0442\u044b\u043d \u049b\u043e\u0440\u0493\u0430\u043d\u044b\u04a3 \u0431\u043e\u043b\u0430\u0434\u044b. \u04e8\u0437\u0456\u043d\u0435 \u0441\u0435\u043d\u0456\u043c \u0430\u0440\u0442\u049b\u0430\u043d\u0434\u0430\u0440\u0434\u044b \u041e\u043b \u0436\u0430\u049b\u044b\u043d\u043d\u0430\u043d \u0442\u0430\u043d\u044b\u043f \u0431\u0456\u043b\u0435\u0434\u0456."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0411\u0456\u0440\u0430\u049b \u0436\u0430\u0443 \u049b\u0430\u043b\u0430\u0441\u044b\u043d \u0418\u0435\u043c\u0456\u0437 \u0493\u0430\u043b\u0430\u043c\u0430\u0442 \u0442\u043e\u043f\u0430\u043d \u0441\u0443\u043c\u0435\u043d \u0456\u0440\u0433\u0435\u0442\u0430\u0441\u044b\u043d\u0430 \u0434\u0435\u0439\u0456\u043d \u049b\u04b1\u0440\u0442\u0430\u0434\u044b, \u049b\u0430\u0440\u0430 \u0442\u04af\u043d\u0435\u043a \u041e\u043d\u044b\u04a3 \u0436\u0430\u0443\u043b\u0430\u0440\u044b\u043d \u0431\u0430\u0441\u0430\u0434\u044b."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0421\u0435\u043d\u0434\u0435\u0440 \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u0433\u0435 \u049b\u0430\u0440\u0441\u044b \u043d\u0435 \u04b1\u0439\u044b\u043c\u0434\u0430\u0441\u0442\u044b\u0440\u043c\u0430\u049b\u0441\u044b\u04a3\u0434\u0430\u0440? \u041e\u043b \u0441\u043e\u043b\u0430\u0440\u044b\u04a3\u0434\u044b \u0431\u0456\u0440\u0436\u043e\u043b\u0430 \u049b\u04b1\u0440\u0442\u0430\u0434\u044b. \u041e\u0441\u044b \u043f\u04d9\u043b\u0435 \u0435\u043a\u0456\u043d\u0448\u0456 \u0440\u0435\u0442 \u0431\u0430\u0441\u0442\u0430\u0440\u044b\u04a3\u0430 \u0442\u04af\u0441\u043f\u0435\u0439\u0434\u0456."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u041e\u043d\u044b\u04a3 \u0436\u0430\u0443\u043b\u0430\u0440\u044b \u0442\u0456\u043a\u0435\u043d\u0434\u0456 \u0431\u04b1\u0442\u0430\u0493\u0430 \u0448\u044b\u0440\u043c\u0430\u043b\u044b\u043f, \u043c\u0430\u0441 \u0430\u0434\u0430\u043c\u0434\u0430\u0439 \u0442\u04d9\u043b\u0442\u0456\u0440\u0435\u043a\u0442\u0435\u043f \u0436\u04af\u0440\u0435\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0434\u044b. \u041e\u043b\u0430\u0440 \u04e9\u0440\u0442\u043a\u0435 \u0436\u0430\u043d\u0493\u0430\u043d \u049b\u04b1\u0440\u0493\u0430\u049b \u0441\u0430\u0431\u0430\u043d\u0434\u0430\u0439 \u043c\u04af\u043b\u0434\u0435\u043c \u049b\u04b1\u0440\u044b\u043f \u043a\u0435\u0442\u0435\u0434\u0456."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u0433\u0435 \u049b\u0430\u0440\u0441\u044b \u0436\u0430\u043c\u0430\u043d\u0434\u044b\u049b \u043e\u0439\u043b\u0430\u043f, \u0437\u04b1\u043b\u044b\u043c \u0456\u0441 \u0436\u043e\u0441\u043f\u0430\u0440\u043b\u0430\u0439\u0442\u044b\u043d \u0431\u0456\u0440\u0435\u0443 \u0441\u0435\u043d\u0435\u043d \u0448\u044b\u0493\u044b\u043f \u0430\u0442\u0442\u0430\u043d\u0434\u044b (\u0443\u0430, \u041d\u0438\u043d\u0435\u0432\u0438)."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u043c\u044b\u043d\u0430\u043d\u044b \u0430\u0439\u0442\u0430\u0434\u044b: \u00ab\u041e\u043b\u0430\u0440\u0434\u044b\u04a3 \u043a\u04af\u0448\u0456 \u0437\u043e\u0440, \u0441\u0430\u043d\u044b \u04e9\u0442\u0435 \u043a\u04e9\u043f \u0431\u043e\u043b\u0493\u0430\u043d\u043c\u0435\u043d, \u0430\u0441\u0441\u0443\u0440\u043b\u044b\u049b\u0442\u0430\u0440 \u0436\u043e\u0439\u044b\u043b\u044b\u043f, \u049b\u04b1\u0440\u044b\u043f \u043a\u0435\u0442\u0435\u0434\u0456. \u0423\u0430, \u0445\u0430\u043b\u049b\u044b\u043c, \u041c\u0435\u043d \u0441\u0435\u043d\u0456 \u0430\u0443\u044b\u0440 \u0430\u043f\u0430\u0442\u049b\u0430 \u04b1\u0448\u044b\u0440\u0430\u0442\u049b\u0430\u043d \u0435\u0434\u0456\u043c, \u0430\u043b\u0430\u0439\u0434\u0430 \u049b\u0430\u0439\u0442\u0430\u0434\u0430\u043d \u0441\u043e\u043b\u0430\u0439 \u0456\u0441\u0442\u0435\u043c\u0435\u0439\u043c\u0456\u043d."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u041c\u0435\u043d \u0435\u043d\u0434\u0456 \u0410\u0441\u0441\u0443\u0440\u0434\u044b\u04a3 \u0441\u0430\u043b\u0493\u0430\u043d \u043c\u043e\u0439\u044b\u043d\u0442\u04b1\u0440\u044b\u0493\u044b\u043d \u0438\u044b\u0493\u044b\u04a3\u043d\u0430\u043d \u0430\u043b\u044b\u043f \u0442\u0430\u0441\u0442\u0430\u043f, \u043a\u0456\u0441\u0435\u043d\u0434\u0435\u0440\u0456\u04a3\u0434\u0456 \u04af\u0437\u0456\u043f \u0431\u043e\u0441\u0430\u0442\u0430\u043c\u044b\u043d\u00bb."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u0441\u0435\u043d \u0442\u0443\u0440\u0430\u043b\u044b \u043c\u044b\u043d\u0430\u0434\u0430\u0439 \u0431\u04b1\u0439\u0440\u044b\u049b \u0431\u0435\u0440\u0434\u0456, \u0443\u0430, \u0410\u0441\u0441\u0443\u0440 \u043f\u0430\u0442\u0448\u0430\u0441\u044b: \u00ab\u0421\u0435\u043d\u0456\u04a3 \u0441\u043e\u04a3\u044b\u04a3\u043d\u0430\u043d \u04b1\u0440\u043f\u0430\u049b \u049b\u0430\u043b\u043c\u0430\u0439, \u0430\u0442\u044b\u04a3 \u04e9\u0448\u0435\u0434\u0456. \u0422\u04d9\u04a3\u0456\u0440\u0441\u044b\u043c\u0430\u049b\u0442\u0430\u0440\u044b\u04a3\u043d\u044b\u04a3 \u0493\u0438\u0431\u0430\u0434\u0430\u0442\u0445\u0430\u043d\u0430\u043b\u0430\u0440\u044b\u043d\u0434\u0430\u0493\u044b \u049b\u0430\u0448\u0430\u043b\u0493\u0430\u043d \u0431\u0435\u0439\u043d\u0435\u043b\u0435\u0440 \u043c\u0435\u043d \u049b\u04b1\u0439\u044b\u043b\u0493\u0430\u043d \u043c\u04af\u0441\u0456\u043d\u0434\u0435\u0440\u0434\u0456 \u049b\u0438\u0440\u0430\u0442\u0430\u043c\u044b\u043d. \u041c\u0435\u043d \u049b\u0430\u0431\u0456\u0440\u0456\u04a3\u0434\u0456 \u043e\u0439\u0440\u0430\u043d\u0434\u0430\u0442\u049b\u044b\u0437\u0430\u043c\u044b\u043d, \u0441\u0435\u0431\u0435\u0431\u0456 \u0441\u0435\u043d \u0436\u0435\u043a \u043a\u04e9\u0440\u0456\u043d\u0456\u0448\u0442\u0456\u0441\u0456\u04a3\u00bb."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u049a\u0430\u0440\u0430\u04a3\u0434\u0430\u0440, \u0456\u0437\u0433\u0456 \u0445\u0430\u0431\u0430\u0440 \u0436\u0435\u0442\u043a\u0456\u0437\u0443\u0448\u0456 \u0431\u0456\u0440\u0435\u0443 \u0442\u0430\u0443\u0434\u0430\u043d \u0430\u0441\u044b\u043f \u043a\u0435\u043b\u0435 \u0436\u0430\u0442\u044b\u0440! \u041e\u043b \u0442\u044b\u043d\u044b\u0448\u0442\u044b\u049b\u0442\u044b \u0436\u0430\u0440\u0438\u044f\u043b\u0430\u043f: \u00ab\u0423\u0430, \u042f\u04bb\u0443\u0434\u0430 \u0435\u043b\u0456, \u043c\u0435\u0439\u0440\u0430\u043c\u0434\u0430\u0440\u044b\u04a3\u0434\u044b \u0442\u043e\u0439\u043b\u0430\u043f, \u049a\u04b1\u0434\u0430\u0439\u0493\u0430 \u0431\u0435\u0440\u0433\u0435\u043d \u0443\u04d9\u0434\u0435\u043b\u0435\u0440\u0456\u04a3\u0434\u0456 \u043e\u0440\u044b\u043d\u0434\u0430! \u0421\u043e\u043b \u0441\u04b1\u043c\u044b\u0440\u0430\u0439\u043b\u0430\u0440 \u0435\u043d\u0434\u0456 \u0441\u0430\u0493\u0430\u043d \u0431\u0430\u0441\u0430-\u043a\u04e9\u043a\u0442\u0435\u043f \u043a\u0456\u0440\u043c\u0435\u0439\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0434\u044b! \u049a\u0430\u0439\u0442\u0430, \u043e\u043b\u0430\u0440\u0434\u044b\u04a3 \u04e9\u0437\u0434\u0435\u0440\u0456 \u0442\u043e\u043b\u044b\u0493\u044b\u043c\u0435\u043d \u049b\u04b1\u0440\u044b\u043f \u043a\u0435\u0442\u0435\u0434\u0456!\u00bb \u2014 \u0434\u0435\u043f \u043f\u0430\u0448 \u0435\u0442\u0435\u0434\u0456."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0423\u0430, \u041d\u0438\u043d\u0435\u0432\u0438 \u0445\u0430\u043b\u049b\u044b, \u0441\u0435\u043d\u0456 \u0448\u0430\u0448\u044b\u0440\u0430\u0442\u0430\u0442\u044b\u043d \u0431\u0430\u0441\u049b\u044b\u043d\u0448\u044b \u0441\u0430\u0493\u0430\u043d \u049b\u0430\u0440\u0441\u044b \u0430\u0442\u0442\u0430\u043d\u044b\u043f \u043a\u0435\u043b\u0435 \u0436\u0430\u0442\u044b\u0440! \u049a\u0430\u043c\u0430\u043b\u044b\u04a3\u0434\u044b \u043a\u04af\u0437\u0435\u0442\u0456\u043f, \u0436\u043e\u043b\u0434\u0430\u0440\u044b\u04a3\u0434\u044b \u049b\u0430\u0434\u0430\u0493\u0430\u043b\u0430! \u0411\u0435\u043b\u0456\u04a3\u0434\u0456 \u0431\u0435\u043a\u0435\u043c \u0431\u0443\u044b\u043f, \u0431\u0430\u0440 \u043a\u04af\u0448-\u049b\u0430\u0439\u0440\u0430\u0442\u044b\u04a3\u0434\u044b \u0436\u0438\u043d\u0430\u049b\u0442\u0430!"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u042f\u04bb\u0443\u0434\u0430\u043d\u044b\u04a3 \u0434\u0430, \u0421\u043e\u043b\u0442\u04af\u0441\u0442\u0456\u043a \u0418\u0441\u0440\u0430\u0438\u043b\u0434\u0456\u04a3 \u0434\u0435 \u0441\u0430\u043b\u0442\u0430\u043d\u0430\u0442\u044b\u043d \u049b\u0430\u0439\u0442\u0430\u0434\u0430\u043d \u049b\u0430\u043b\u043f\u044b\u043d\u0430 \u043a\u0435\u043b\u0442\u0456\u0440\u0435\u0434\u0456. \u04e8\u0439\u0442\u043a\u0435\u043d\u0456 \u049b\u04b1\u0440\u0442\u0443\u0448\u044b \u0436\u0430\u0443\u043b\u0430\u0440\u044b \u043e\u043b\u0430\u0440\u0434\u044b \u0442\u0430\u043b\u049b\u0430\u043d\u0434\u0430\u043f, \u0431\u0430\u0443-\u0431\u0430\u049b\u0448\u0430\u043b\u0430\u0440\u044b\u043d \u0434\u0430 \u0436\u0435\u0440\u043c\u0435\u043d-\u0436\u0435\u043a\u0441\u0435\u043d \u049b\u044b\u043b\u0493\u0430\u043d \u0431\u043e\u043b\u0430\u0442\u044b\u043d."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "(\u041d\u0438\u043d\u0435\u0432\u0438\u0434\u0456 \u0448\u0430\u0431\u0443\u044b\u043b\u0434\u0430\u0493\u0430\u043d) \u0436\u0430\u0441\u0430\u049b\u0448\u044b\u043b\u0430\u0440\u0434\u044b\u04a3 \u049b\u0430\u043b\u049b\u0430\u043d\u0434\u0430\u0440\u044b \u049b\u0430\u043d \u049b\u044b\u0437\u044b\u043b \u0442\u04af\u0441\u0442\u0456, \u0441\u043e\u043b \u043c\u044b\u049b\u0442\u044b \u0436\u0430\u0443\u044b\u043d\u0433\u0435\u0440\u043b\u0435\u0440 \u049b\u044b\u0440\u043c\u044b\u0437\u044b \u0448\u0430\u043f\u0430\u043d \u0436\u0430\u043c\u044b\u043b\u0493\u0430\u043d. \u0421\u043e\u0493\u044b\u0441 \u043a\u04af\u0439\u043c\u0435\u043b\u0435\u0440\u0456 \u0448\u0430\u0439\u049b\u0430\u0441\u049b\u0430 \u043a\u0435\u043b\u0435 \u0436\u0430\u0442\u049b\u0430\u043d\u0434\u0430 \u0442\u0435\u043c\u0456\u0440\u043b\u0435\u0440\u0456 \u043e\u0442\u0442\u0430\u0439 \u0436\u0430\u043b\u0442-\u0436\u04b1\u043b\u0442 \u0435\u0442\u0435\u0434\u0456. \u0421\u0430\u0440\u0431\u0430\u0437\u0434\u0430\u0440 \u049b\u0430\u0440\u0430\u0493\u0430\u0439 \u043d\u0430\u0439\u0437\u0430\u043b\u0430\u0440\u044b\u043d \u0441\u0456\u043b\u0442\u0435\u043b\u0435\u043f \u0448\u0430\u0439\u049b\u0430\u0441\u049b\u0430 \u0434\u0430\u0439\u044b\u043d \u0442\u04b1\u0440."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0421\u043e\u0493\u044b\u0441 \u043a\u04af\u0439\u043c\u0435\u043b\u0435\u0440\u0456 \u049b\u0430\u043b\u0430\u043d\u044b\u04a3 \u043a\u04e9\u0448\u0435\u043b\u0435\u0440\u0456\u043c\u0435\u043d \u049b\u04b1\u0439\u044b\u043d\u0434\u0430\u0439 \u0436\u04af\u0439\u0442\u043a\u0456\u043f, \u0430\u043b\u0430\u04a3\u0434\u0430\u0440\u043c\u0435\u043d \u0437\u044b\u043c\u044b\u0440\u0430\u043f, \u04d9\u0440\u043b\u0456-\u0431\u0435\u0440\u043b\u0456 \u0448\u0430\u043f\u049b\u044b\u043b\u0430\u0439\u0434\u044b. \u041e\u043b\u0430\u0440 \u0430\u043b\u0430\u0443\u0434\u0430\u0439 \u0436\u0430\u043b\u044b\u043d\u0434\u0430\u043f, \u043d\u0430\u0439\u0437\u0430\u0493\u0430\u0439\u0434\u0430\u0439 \u0436\u0430\u0440\u049b\u044b\u043b\u0434\u0430\u043f \u0437\u0443\u044b\u043b\u0434\u0430\u0439\u0434\u044b."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u049a\u043e\u043b\u0431\u0430\u0441\u0448\u044b \u04e9\u0437\u0456\u043d\u0456\u04a3 \u0442\u0430\u04a3\u0434\u0430\u0443\u043b\u044b \u0436\u0430\u0441\u0430\u049b\u0448\u044b\u043b\u0430\u0440\u044b\u043d\u0430 \u0431\u04b1\u0439\u0440\u044b\u049b \u0431\u0435\u0440\u0435\u0434\u0456. \u041e\u043b\u0430\u0440 \u0436\u043e\u043b \u0431\u043e\u0439\u044b\u043d\u0434\u0430 \u0441\u04af\u0440\u0456\u043d\u0433\u0435\u043d\u043c\u0435\u043d \u049b\u0430\u043b\u0430\u043d\u044b\u04a3 \u049b\u0430\u0431\u044b\u0440\u0493\u0430\u0441\u044b\u043d\u0430 \u049b\u0430\u0440\u0430\u0439 \u0430\u0441\u044b\u0493\u044b\u043f, \u0441\u043e\u043d\u0434\u0430 \u0434\u04d9\u0443 \u049b\u043e\u0440\u0493\u0430\u043d\u044b\u0448 \u049b\u0430\u043b\u049b\u0430\u043d\u0434\u0430\u0440\u044b\u043d \u043e\u0440\u043d\u0430\u0442\u044b\u043f \u049b\u043e\u044f\u0434\u044b."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u049a\u044b\u0440\u0430\u0442\u0442\u0430\u0493\u044b \u04e9\u0437\u0435\u043d \u049b\u0430\u049b\u043f\u0430\u043b\u0430\u0440\u044b \u0430\u0448\u044b\u043b\u044b\u043f, \u0442\u043e\u043f\u0430\u043d \u0441\u0443 \u043f\u0430\u0442\u0448\u0430 \u0441\u0430\u0440\u0430\u0439\u044b\u043d \u0442\u0430\u043b\u049b\u0430\u043d\u0434\u0430\u0439\u0434\u044b."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u041d\u0438\u043d\u0435\u0432\u0438 \u0445\u0430\u043b\u049b\u044b \u0430\u0439\u0434\u0430\u0443\u0493\u0430 \u043a\u0435\u0442\u0443\u0456 \u043a\u0435\u0440\u0435\u043a \u0434\u0435\u043f \u0448\u0435\u0448\u0456\u043b\u0456\u043f \u049b\u043e\u0439\u044b\u043b\u0493\u0430\u043d. \u041e\u043d\u044b\u04a3 \u043a\u04af\u04a3 \u049b\u044b\u0437\u0434\u0430\u0440\u044b \u043a\u04e9\u0433\u0435\u0440\u0448\u0456\u043d\u0434\u0435\u0439 \u043a\u04af\u04a3\u0456\u0440\u0435\u043d\u0456\u043f, \u049b\u0430\u0439\u0493\u044b\u0434\u0430\u043d \u043a\u04e9\u043a\u0456\u0440\u0435\u043a\u0442\u0435\u0440\u0456\u043d \u0441\u043e\u049b\u049b\u044b\u043b\u0430\u0439\u0434\u044b."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0415\u0436\u0435\u043b\u0434\u0435\u043d \u041d\u0438\u043d\u0435\u0432\u0438 \u0441\u0443\u0493\u0430 \u0442\u043e\u043b\u044b \u0442\u043e\u0493\u0430\u043d\u0493\u0430 \u04b1\u049b\u0441\u0430\u0439\u0442\u044b\u043d, \u0431\u0456\u0440\u0430\u049b \u049b\u0430\u0437\u0456\u0440 \u0441\u0443\u044b \u0442\u0430\u0443\u0441\u044b\u043b\u044b\u043f \u0431\u0430\u0440\u0430 \u0436\u0430\u0442\u049b\u0430\u043d\u0434\u0430\u0439, \u049b\u0430\u043b\u0430 \u0445\u0430\u043b\u049b\u044b \u049b\u0430\u0448\u044b\u043f \u043a\u0435\u0442\u0443\u0434\u0435. \u00ab\u0422\u043e\u049b\u0442\u0430, \u0442\u043e\u049b\u0442\u0430!\u00bb \u0434\u0435\u0433\u0435\u043d \u0431\u04b1\u0439\u0440\u044b\u049b \u0431\u0435\u0440\u0456\u043b\u0441\u0435 \u0434\u0435, \u0435\u0448\u043a\u0456\u043c \u0430\u0440\u0442\u044b\u043d\u0430 \u0431\u04b1\u0440\u044b\u043b\u043c\u0430\u0439\u0434\u044b."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0416\u0430\u0443: \u00ab\u041a\u04af\u043c\u0456\u0441\u0442\u0456 \u043e\u043b\u0436\u0430\u043b\u0430\u04a3\u0434\u0430\u0440! \u0410\u043b\u0442\u044b\u043d\u0434\u044b \u0434\u0430 \u04d9\u043a\u0435\u0442\u0456\u04a3\u0434\u0435\u0440!\u00bb \u2014 \u0434\u0435\u043f \u0448\u0430\u049b\u044b\u0440\u0430\u0434\u044b. \u049a\u0430\u043b\u0430 \u049b\u0430\u0437\u044b\u043d\u0430\u043b\u0430\u0440\u044b\u043d\u0434\u0430\u0493\u044b \u049b\u044b\u043c\u0431\u0430\u0442 \u0431\u04b1\u0439\u044b\u043c\u0434\u0430\u0440 \u0441\u0430\u043d\u0441\u044b\u0437 \u043a\u04e9\u043f!"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u041d\u0438\u043d\u0435\u0432\u0438 \u0442\u043e\u043d\u0430\u043b\u044b\u043f, \u049b\u0438\u0440\u0430\u0442\u044b\u043b\u044b\u043f, \u049b\u0430\u04a3\u044b\u0440\u0430\u043f \u0431\u043e\u0441 \u049b\u0430\u043b\u0430\u0434\u044b. \u0410\u0434\u0430\u043c\u0434\u0430\u0440 \u0436\u04af\u0440\u0435\u043a\u0441\u0456\u043d\u0456\u043f, \u0442\u0456\u0437\u0435\u043b\u0435\u0440\u0456 \u0434\u0456\u0440\u0456\u043b\u0434\u0435\u043f, \u0434\u0435\u043d\u0435\u043b\u0435\u0440\u0456 \u049b\u0430\u043b\u0442\u044b\u043b\u0434\u0430\u043f, \u0431\u0430\u0440\u043b\u044b\u0493\u044b\u043d\u044b\u04a3 \u0431\u0435\u0442-\u0436\u04af\u0437\u0434\u0435\u0440\u0456 \u0430\u043f\u043f\u0430\u049b \u049b\u0443\u0434\u0430\u0439 \u0431\u043e\u043b\u0430\u0434\u044b."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0410\u0440\u044b\u0441\u0442\u0430\u043d\u043d\u044b\u04a3 \u0430\u043f\u0430\u043d\u044b\u043d\u0434\u0430\u0439 \u0431\u043e\u043b\u0493\u0430\u043d \u049b\u0430\u043b\u0430 \u0435\u043d\u0434\u0456 \u049b\u0430\u0439\u0434\u0430 \u0435\u043a\u0435\u043d? \u0415\u0440\u043a\u0435\u043a, \u04b1\u0440\u0493\u0430\u0448\u044b \u0430\u0440\u044b\u0441\u0442\u0430\u043d\u0434\u0430\u0440 \u0456\u0441\u043f\u0435\u0442\u0442\u0456 \u0431\u0430\u043b\u0430\u043b\u0430\u0440\u044b\u043d \u0430\u0437\u044b\u049b\u0442\u0430\u043d\u0434\u044b\u0440\u044b\u043f, \u0435\u0448\u0442\u0435\u04a3\u0435\u0434\u0435\u043d \u049b\u0430\u0439\u043c\u044b\u049b\u043f\u0430\u0439 \u0448\u0430\u0440\u043b\u0430\u043f, \u0436\u0435\u0440 \u0431\u0435\u0442\u0456\u043d \u043a\u0435\u0437\u0456\u043f \u0436\u04af\u0440\u0433\u0435\u043d \u049b\u0430\u0442\u044b\u0433\u0435\u0437 \u0445\u0430\u043b\u044b\u049b \u049b\u0430\u0439\u0434\u0430 \u0435\u043d\u0434\u0456?"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0410\u0440\u044b\u0441\u0442\u0430\u043d \u04e9\u0437\u0456\u043d\u0456\u04a3 \u043a\u04af\u0448\u0456\u043a\u0442\u0435\u0440\u0456\u043d\u0435 \u0436\u0435\u043c\u0442\u0456\u043a\u0442\u0435\u0440\u0456\u043d \u043f\u0430\u0440\u0448\u0430\u043b\u0430\u043f \u0436\u0435\u0442\u043a\u0456\u043b\u0456\u043a\u0442\u0456 \u0435\u0442\u0456\u043f \u04d9\u043a\u0435\u043b\u0456\u043f, \u04b1\u0440\u0493\u0430\u0448\u044b\u0441\u044b\u043d \u0434\u0430 \u049b\u0430\u043c\u0442\u0430\u043c\u0430\u0441\u044b\u0437 \u0435\u0442\u0435\u0442\u0456\u043d. \u041e\u043b \u0430\u043f\u0430\u043d\u044b\u043d \u043e\u043b\u0436\u0430\u0441\u044b \u043c\u0435\u043d \u043f\u0430\u0440\u0448\u0430\u043b\u0430\u043d\u0493\u0430\u043d \u0436\u0435\u043c\u0442\u0456\u043a\u043a\u0435 \u0442\u043e\u043b\u0442\u044b\u0440\u0430\u0442\u044b\u043d."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u00ab\u041c\u0435\u043d \u0441\u0430\u0493\u0430\u043d \u049b\u0430\u0440\u0441\u044b\u043c\u044b\u043d, \u2014 \u0434\u0435\u043f \u0435\u0441\u043a\u0435\u0440\u0442\u0435\u0434\u0456 \u04d8\u043b\u0435\u043c\u043d\u0456\u04a3 \u0418\u0435\u0441\u0456. \u2014 \u041c\u0435\u043d \u0441\u0435\u043d\u0456\u04a3 \u0441\u043e\u0493\u044b\u0441 \u043a\u04af\u0439\u043c\u0435\u043b\u0435\u0440\u0456\u04a3\u0434\u0456 \u04e9\u0440\u0442\u0435\u0439\u043c\u0456\u043d, \u0441\u0435\u043c\u0441\u0435\u0440 \u0436\u0430\u0441 \u0430\u0440\u044b\u0441\u0442\u0430\u043d\u0434\u0430\u0440\u044b\u04a3\u0434\u044b \u043c\u0435\u0440\u0442 \u049b\u044b\u043b\u0430\u0434\u044b. \u0421\u0430\u0493\u0430\u043d \u0435\u043d\u0434\u0456 \u0436\u0435\u0440 \u0431\u0435\u0442\u0456\u043d \u043e\u043b\u0436\u0430\u043b\u0430\u0443\u0493\u0430 \u0436\u043e\u043b \u0431\u0435\u0440\u043c\u0435\u0439\u043c\u0456\u043d. \u0421\u0435\u043d\u0456\u04a3 \u0445\u0430\u0431\u0430\u0440\u0448\u044b\u043b\u0430\u0440\u044b\u04a3\u043d\u044b\u04a3 \u043f\u0430\u04a3 \u0434\u0430\u0443\u044b\u0441\u0442\u0430\u0440\u044b \u0431\u04b1\u0434\u0430\u043d \u0431\u044b\u043b\u0430\u0439 \u0435\u0441\u0442\u0456\u043b\u043c\u0435\u0439\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0434\u044b\u00bb."
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u049a\u0430\u043d \u0442\u04e9\u0433\u0443\u0433\u0435, \u04e9\u0442\u0456\u0440\u0456\u043a\u043a\u0435 \u0442\u043e\u043b\u044b, \u04b1\u0440\u043b\u044b\u049b \u043f\u0435\u043d \u0442\u043e\u043d\u0430\u0443 \u0442\u043e\u043b\u0430\u0441\u0442\u0430\u043c\u0430\u0439\u0442\u044b\u043d \u049b\u0430\u043b\u0430 \u2014 \u041d\u0438\u043d\u0435\u0432\u0438 \u049b\u0430\u0441\u0456\u0440\u0435\u0442\u043a\u0435 \u049b\u0430\u043b\u0430\u0434\u044b!"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u049a\u0430\u043c\u0448\u044b\u043d\u044b\u04a3 \u0441\u0430\u0440\u0442\u044b\u043b\u044b, \u0434\u04e9\u04a3\u0433\u0435\u043b\u0435\u043a\u0442\u0456\u04a3 \u0442\u0430\u0440\u0441\u044b\u043b\u044b, \u0436\u044b\u043b\u049b\u044b\u043d\u044b\u04a3 \u0434\u04af\u0431\u0456\u0440\u0456, \u0441\u043e\u0493\u044b\u0441 \u043a\u04af\u0439\u043c\u0435\u043b\u0435\u0440\u0456\u043d\u0456\u04a3 \u0434\u04af\u0440\u0441\u0456\u043b\u0456 \u0435\u0441\u0442\u0456\u043b\u0456\u043f \u0442\u04b1\u0440\u0430\u0434\u044b."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0410\u0442\u0442\u044b \u04d9\u0441\u043a\u0435\u0440 \u0430\u043b\u0493\u0430 \u0436\u04af\u0439\u0442\u043a\u0456\u043f, \u0441\u0435\u043c\u0441\u0435\u0440\u043b\u0435\u0440 \u0436\u0430\u0440\u049b\u044b\u043b\u0434\u0430\u043f, \u043d\u0430\u0439\u0437\u0430\u043b\u0430\u0440 \u0436\u0430\u043b\u0442-\u0436\u04b1\u043b\u0442 \u0435\u0442\u0435\u0434\u0456. \u041a\u04e9\u043f \u0430\u0434\u0430\u043c \u049b\u044b\u0440\u044b\u043b\u044b\u043f, \u0441\u0430\u043d\u0441\u044b\u0437 \u04e9\u043b\u0456\u043a\u0442\u0435\u0440 \u04af\u0439\u0456\u043b\u0456\u043f, \u0436\u04b1\u0440\u0442 \u04e9\u043b\u0456\u043a\u0442\u0435\u0440\u0433\u0435 \u0441\u04af\u0440\u0456\u043d\u0435\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0434\u044b."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u041e\u0441\u044b\u043d\u044b\u04a3 \u0431\u04d9\u0440\u0456 \u041d\u0438\u043d\u0435\u0432\u0438\u0434\u0456\u04a3 \u0448\u0435\u043a\u0441\u0456\u0437 \u0430\u0437\u0493\u044b\u043d\u0434\u044b\u0493\u044b \u043c\u0435\u043d \u043e\u043f\u0430\u0441\u044b\u0437\u0434\u044b\u0493\u044b \u04af\u0448\u0456\u043d \u0431\u0430\u0441\u044b\u043d\u0430 \u0442\u04af\u0441\u0435\u0434\u0456. \u0421\u04b1\u043b\u0443 \u0434\u0430 \u0442\u0430\u0440\u0442\u044b\u043c\u0434\u044b \u0431\u043e\u043b\u044b\u043f, \u0441\u0438\u049b\u044b\u0440\u0448\u044b\u043b\u044b\u049b\u0442\u044b \u04d9\u0431\u0434\u0435\u043d \u043c\u0435\u04a3\u0433\u0435\u0440\u0433\u0435\u043d \u0431\u04b1\u043b \u049b\u0430\u043b\u0430 \u0430\u0440\u0430\u043c \u0456\u0441-\u04d9\u0440\u0435\u043a\u0435\u0442\u0456\u043c\u0435\u043d \u0445\u0430\u043b\u044b\u049b\u0442\u0430\u0440\u0434\u044b \u0431\u0430\u0443\u0440\u0430\u043f \u0430\u043b\u0434\u044b \u0434\u0430, \u043a\u04e9\u043f \u04d9\u0443\u043b\u0435\u0442\u0442\u0435\u0440\u0434\u0456 \u0441\u0438\u049b\u044b\u0440\u0448\u044b\u043b\u044b\u0493\u044b\u043c\u0435\u043d \u049b\u04b1\u043b\u0434\u044b\u049b\u049b\u0430 \u0441\u0430\u0442\u0442\u044b."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u00ab\u041c\u0435\u043d \u0441\u0430\u0493\u0430\u043d \u049b\u0430\u0440\u0441\u044b\u043c\u044b\u043d, \u2014 \u0434\u0435\u043f \u0435\u0441\u043a\u0435\u0440\u0442\u0435\u0434\u0456 \u04d8\u043b\u0435\u043c\u043d\u0456\u04a3 \u0418\u0435\u0441\u0456. \u2014 \u0421\u0435\u043d\u0456\u04a3 \u0435\u0442\u0435\u0433\u0456\u04a3\u0434\u0456 \u0431\u0435\u0442\u0456\u04a3\u0435 \u0434\u0435\u0439\u0456\u043d \u0442\u04af\u0440\u0433\u0456\u0437\u0435\u043c\u0456\u043d! \u0425\u0430\u043b\u044b\u049b\u0442\u0430\u0440 \u043c\u0435\u043d \u043f\u0430\u0442\u0448\u0430\u043b\u044b\u049b\u0442\u0430\u0440\u0493\u0430 \u0441\u0435\u043d\u0456\u04a3 \u0436\u0430\u043b\u0430\u04a3\u0430\u0448 \u0442\u04d9\u043d\u0456\u04a3\u0434\u0456 \u0430\u0448\u044b\u043f, \u0430\u0431\u044b\u0440\u043e\u0439\u044b\u04a3\u0434\u044b \u0442\u04e9\u0433\u0435\u043c\u0456\u043d."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u0421\u0435\u043d\u0456 \u043a\u0456\u0440-\u049b\u043e\u049b\u044b\u0441\u043f\u0435\u043d \u0430\u0442\u049b\u044b\u043b\u0430\u043f \u049b\u043e\u0440\u043b\u0430\u0439\u043c\u044b\u043d, \u043a\u0456\u043c \u0435\u043a\u0435\u043d\u0456\u04a3\u0434\u0456 \u0431\u0430\u0440\u0448\u0430 \u0436\u04b1\u0440\u0442\u049b\u0430 \u043f\u0430\u0448 \u0435\u0442\u0435\u043c\u0456\u043d."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u041a\u04e9\u0440\u0433\u0435\u043d\u0434\u0435\u0440\u0434\u0456\u04a3 \u0431\u0430\u0440\u043b\u044b\u0493\u044b \u0441\u0435\u043d\u0435\u043d \u049b\u0430\u0448\u044b\u043f: \u00ab\u041d\u0438\u043d\u0435\u0432\u0438 \u049b\u0438\u0440\u0430\u043f \u049b\u0430\u043b\u0434\u044b, \u0435\u043d\u0434\u0456 \u043a\u0456\u043c \u043e\u0493\u0430\u043d \u043a\u04e9\u04a3\u0456\u043b \u0430\u0439\u0442\u043f\u0430\u049b?\u00bb \u0434\u0435\u0439\u0434\u0456. \u041e\u043d\u044b \u0436\u04b1\u0431\u0430\u0442\u0430\u0442\u044b\u043d \u0435\u0448\u043a\u0456\u043c \u0434\u0435 \u0442\u0430\u0431\u044b\u043b\u043c\u0430\u0439\u0434\u044b\u00bb."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u04d8\u043b\u0434\u0435 \u0441\u0435\u043d \u041d\u0456\u043b \u04e9\u0437\u0435\u043d\u0456\u043d\u0456\u04a3 \u0431\u043e\u0439\u044b\u043d\u0434\u0430 \u043e\u0440\u043d\u0430\u043b\u0430\u0441\u049b\u0430\u043d, \u0430\u0439\u043d\u0430\u043b\u0430\u0441\u044b\u043d \u0441\u0443 \u049b\u043e\u0440\u0448\u0430\u0493\u0430\u043d \u041d\u043e-\u0410\u043c\u043c\u043e\u043d \u049b\u0430\u043b\u0430\u0441\u044b\u043d\u0430\u043d \u0430\u0440\u0442\u044b\u049b\u0441\u044b\u04a3 \u0431\u0430? \u04e8\u0437\u0435\u043d \u0441\u043e\u043b \u049b\u0430\u043b\u0430\u043d\u044b\u04a3 \u049b\u043e\u0440\u0493\u0430\u043d\u044b\u0448\u044b, \u049b\u043e\u0440\u0448\u0430\u0493\u0430\u043d \u0441\u0443\u044b \u043e\u043d\u044b\u04a3 \u049b\u0430\u043c\u0430\u043b\u044b \u0456\u0441\u043f\u0435\u0442\u0442\u0456 \u0431\u043e\u043b\u0493\u0430\u043d \u0435\u0434\u0456."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u049a\u04b1\u0448 \u043f\u0435\u043d \u041c\u044b\u0441\u044b\u0440 \u043e\u043d\u044b\u04a3 \u0448\u0435\u043a\u0441\u0456\u0437 \u049b\u0443\u0430\u0442\u044b, \u041f\u04b1\u0442 \u043f\u0435\u043d \u041b\u0438\u0431\u0438\u044f \u043e\u043d\u044b\u04a3 \u049b\u043e\u043b\u0434\u0430\u0443\u0448\u044b\u043b\u0430\u0440\u044b\u043d\u044b\u04a3 \u049b\u0430\u0442\u0430\u0440\u044b\u043d\u0434\u0430 \u0431\u043e\u043b\u0430\u0442\u044b\u043d."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0411\u0456\u0440\u0430\u049b \u0442\u0430 \u043e\u043d\u044b\u04a3 \u0445\u0430\u043b\u049b\u044b \u0442\u04b1\u0442\u049b\u044b\u043d\u0434\u0430\u043b\u044b\u043f, \u0430\u0439\u0434\u0430\u043b\u044b\u043f \u043a\u0435\u0442\u0442\u0456; \u0441\u04d9\u0431\u0438\u043b\u0435\u0440\u0456 \u04d9\u0440 \u043a\u04e9\u0448\u0435\u043d\u0456\u04a3 \u049b\u0438\u044b\u043b\u044b\u0441\u044b\u043d\u0434\u0430 \u04b1\u0440\u044b\u043b\u044b\u043f \u04e9\u043b\u0442\u0456\u0440\u0456\u043b\u0434\u0456. \u0416\u0430\u0443\u043b\u0430\u043f \u0430\u043b\u0443\u0448\u044b\u043b\u0430\u0440 \u0430\u0431\u044b\u0440\u043e\u0439\u043b\u044b \u0430\u0434\u0430\u043c\u0434\u0430\u0440\u044b\u043d \u0436\u0435\u0440\u0435\u0431\u0435 \u0442\u0430\u0441\u0442\u0430\u043f \u0431\u04e9\u043b\u0456\u0441\u0456\u043f \u0430\u043b\u044b\u043f, \u0431\u0430\u0440\u043b\u044b\u049b \u04b1\u043b\u044b\u049b\u0442\u0430\u0440\u044b\u043d \u043a\u0456\u0441\u0435\u043d\u0434\u0435\u043f \u0430\u0439\u0434\u0430\u043f \u04d9\u043a\u0435\u0442\u0442\u0456."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0410\u043b \u0435\u043d\u0434\u0456, \u041d\u0438\u043d\u0435\u0432\u0438, \u0441\u0435\u043d \u0434\u0435 (\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u043d\u0456\u04a3 \u049b\u0430\u04bb\u0430\u0440\u044b\u043d\u0430\u043d) \u043c\u0430\u0441 \u0431\u043e\u043b\u0493\u0430\u043d\u0434\u0430\u0439 \u0442\u04d9\u043b\u0442\u0456\u0440\u0435\u043a\u0442\u0435\u043f \u0436\u04af\u0440\u0435\u0441\u0456\u04a3! \u0421\u0435\u043d \u0434\u0435 \u0442\u044b\u0493\u044b\u043b\u044b\u043f, \u0436\u0430\u0443\u0434\u0430\u043d \u0431\u0430\u0441 \u0441\u0430\u0443\u0493\u0430\u043b\u0430\u0439\u0442\u044b\u043d \u043e\u0440\u044b\u043d \u0456\u0437\u0434\u0435\u0439\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0441\u044b\u04a3."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u0406\u043d\u0436\u0456\u0440 \u0430\u0493\u0430\u0448\u044b\u043d \u0435\u0440\u0442\u0435 \u0441\u0456\u043b\u043a\u0456\u0433\u0435\u043d\u0434\u0435, \u043f\u0456\u0441\u043a\u0435\u043d \u0436\u0435\u043c\u0456\u0441\u0442\u0435\u0440\u0456 \u0430\u0434\u0430\u043c\u043d\u044b\u04a3 \u0430\u0443\u0437\u044b\u043d\u0430 \u043a\u0435\u043b\u0456\u043f \u0442\u04af\u0441\u0435\u0434\u0456. \u0421\u0435\u043d\u0456\u04a3 \u0431\u0430\u0440\u043b\u044b\u049b \u049b\u0430\u043c\u0430\u043b\u0434\u0430\u0440\u044b\u04a3 \u0434\u0430 \u0434\u04d9\u043b \u0441\u043e\u043b\u0430\u0439 \u0436\u0430\u0443 \u049b\u043e\u043b\u044b\u043d\u0430 \u0442\u04af\u0441\u0435\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0434\u044b."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u04e8\u0437 \u0436\u0430\u0441\u0430\u049b\u0448\u044b\u043b\u0430\u0440\u044b\u04a3\u0430 \u043a\u04e9\u0437 \u0442\u0430\u0441\u0442\u0430\u0448\u044b: \u043e\u043b\u0430\u0440\u0434\u044b\u04a3 \u0431\u0430\u0440\u043b\u044b\u0493\u044b \u0434\u0430 \u04d9\u0439\u0435\u043b \u0441\u0438\u044f\u049b\u0442\u044b \u04d9\u043b\u0441\u0456\u0437 \u0431\u043e\u043b\u0430\u0434\u044b. \u0415\u043b\u0456\u04a3\u043d\u0456\u04a3 \u049b\u0430\u049b\u043f\u0430\u043b\u0430\u0440\u044b \u0436\u0430\u0443\u043b\u0430\u0440\u044b\u04a3\u0430 \u0430\u0439\u049b\u0430\u0440\u0430 \u0430\u0448\u044b\u043b\u0430\u0434\u044b, \u0441\u0435\u0431\u0435\u0431\u0456 \u043e\u043b\u0430\u0440\u0434\u044b\u04a3 \u044b\u0441\u044b\u0440\u043c\u0430\u043b\u0430\u0440\u044b\u043d \u043e\u0442 \u0436\u0430\u043b\u043c\u0430\u043f \u049b\u043e\u044f\u0434\u044b."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u041d\u0438\u043d\u0435\u0432\u0438, \u049b\u043e\u0440\u0448\u0430\u0443\u0493\u0430 \u0442\u04af\u0441\u0435\u0442\u0456\u043d \u043a\u0435\u0437\u0456\u04a3\u0435 \u0434\u0430\u0439\u044b\u043d\u0434\u0430\u043b\u044b\u043f, \u0441\u0443 \u049b\u043e\u0440\u044b\u043d \u0436\u0438\u043d\u0430\u043f \u0430\u043b! \u0411\u0435\u043a\u0456\u043d\u0456\u0441\u0442\u0435\u0440\u0456\u04a3\u0434\u0456 \u043a\u04af\u0448\u0435\u0439\u0442! \u0411\u0430\u043b\u0448\u044b\u049b \u0438\u043b\u0435\u043f, \u04d9\u043a\u0442\u0430\u0441 \u0435\u0440\u0456\u0442\u0456\u043d\u0434\u0456\u0441\u0456\u043d \u0434\u0430\u0439\u044b\u043d\u0434\u0430\u043f, \u043a\u0456\u0440\u043f\u0456\u0448 \u049b\u0430\u0431\u044b\u0440\u0493\u0430\u043d\u044b \u0436\u04e9\u043d\u0434\u0435!"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0410\u043b\u0430\u0439\u0434\u0430 \u043e\u0442 \u0441\u0435\u043d\u0456 \u0436\u0430\u043b\u043c\u0430\u0439\u0434\u044b, \u0441\u0435\u043c\u0441\u0435\u0440 \u0448\u0430\u0443\u044b\u043f \u0442\u04af\u0441\u0456\u0440\u0456\u043f, \u0448\u0435\u0433\u0456\u0440\u0442\u043a\u0435 \u049b\u04b1\u0440\u0442\u044b\u043d\u0434\u0430\u0439 \u0436\u043e\u049b \u049b\u044b\u043b\u0430\u0434\u044b. \u0428\u0435\u0433\u0456\u0440\u0442\u043a\u0435\u0434\u0435\u0439 \u043a\u04e9\u0431\u0435\u0439\u0456\u043f, \u0441\u043e\u043d\u044b\u04a3 \u049b\u04b1\u0440\u0442\u044b\u043d\u0434\u0430\u0439 \u043c\u043e\u043b\u0430\u0439\u044b\u043f \u043a\u04e9\u0440, \u0431\u04d9\u0440\u0456\u0431\u0456\u0440 \u0436\u0430\u0443 \u0441\u0435\u043d\u0456 \u0436\u043e\u044f\u0442\u044b\u043d \u0431\u043e\u043b\u0430\u0434\u044b."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0421\u0435\u043d\u0456\u04a3 \u0441\u0430\u0443\u0434\u0430\u0433\u0435\u0440\u043b\u0435\u0440\u0456\u04a3 \u0430\u0441\u043f\u0430\u043d\u0434\u0430\u0493\u044b \u0436\u04b1\u043b\u0434\u044b\u0437\u0434\u0430\u0440\u0434\u0430\u043d \u0434\u0430 \u043a\u04e9\u0431\u0435\u0439\u0456\u043f \u043a\u0435\u0442\u043a\u0435\u043d, \u0431\u0456\u0440\u0430\u049b \u043e\u043b\u0430\u0440 \u0448\u0435\u0433\u0456\u0440\u0442\u043a\u0435 \u049b\u04b1\u0440\u0442\u044b\u043d\u0434\u0430\u0439 \u0435\u043b\u0434\u0456 \u0442\u044b\u043f-\u0442\u0438\u043f\u044b\u043b \u049b\u044b\u043b\u044b\u043f, \u04b1\u0448\u044b\u043f \u043a\u0435\u0442\u0435\u0434\u0456."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u041d\u04e9\u043a\u0435\u0440\u043b\u0435\u0440\u0456\u04a3 \u043a\u04e9\u043a \u0448\u0435\u0433\u0456\u0440\u0442\u043a\u0435\u043b\u0435\u0440 \u0456\u0441\u043f\u0435\u0442\u0442\u0456, \u043b\u0430\u0443\u0430\u0437\u044b\u043c\u0434\u044b \u0430\u0434\u0430\u043c\u0434\u0430\u0440\u044b\u04a3 \u0448\u0435\u0433\u0456\u0440\u0442\u043a\u0435 \u04af\u0439\u0456\u0440\u043b\u0435\u0440\u0456 \u0441\u0438\u044f\u049b\u0442\u044b. \u0421\u0430\u043b\u049b\u044b\u043d \u043a\u04af\u043d\u0434\u0435\u0440\u0456 \u043e\u043b\u0430\u0440 \u0448\u0430\u0440\u0431\u0430\u049b\u0442\u0430\u0440\u0493\u0430 \u049b\u043e\u043d\u044b\u043f \u043e\u0442\u044b\u0440\u0430\u0434\u044b \u0434\u0430, \u043a\u04af\u043d \u044b\u0441\u044b\u0493\u0430\u043d\u0434\u0430 \u04b1\u0448\u044b\u043f \u043a\u0435\u0442\u0435\u0434\u0456. \u049a\u0430\u0439\u0434\u0430 \u043a\u0435\u0442\u043a\u0435\u043d\u0434\u0435\u0440\u0456\u043d \u0435\u0448\u043a\u0456\u043c \u0431\u0456\u043b\u043c\u0435\u0439\u0434\u0456."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0423\u0430, \u0410\u0441\u0441\u0443\u0440 \u043f\u0430\u0442\u0448\u0430\u0441\u044b, \u0441\u0435\u043d\u0456\u04a3 \u0431\u0430\u049b\u0442\u0430\u0448\u044b\u043b\u0430\u0440\u044b\u04a3 \u043c\u04af\u043b\u0433\u0456\u043f \u043e\u0442\u044b\u0440: \u0441\u043e\u043b \u0434\u0430\u04a3\u049b\u0442\u044b \u04d9\u043c\u0456\u0440\u0448\u0456\u043b\u0435\u0440\u0456\u04a3 \u0434\u0435\u043c\u0430\u043b\u044b\u043f \u0436\u0430\u0442\u044b\u0440! \u0410\u043b \u0445\u0430\u043b\u049b\u044b\u04a3 \u0442\u0430\u0443\u043b\u0430\u0440\u0434\u044b\u04a3 \u0430\u0440\u0430\u0441\u044b\u043d\u0430 \u0448\u0430\u0448\u044b\u0440\u0430\u043f \u043a\u0435\u0442\u043a\u0435\u043d, \u043e\u043b\u0430\u0440\u0434\u044b \u0436\u0438\u043d\u0430\u043f \u04d9\u043a\u0435\u043b\u0435\u0442\u0456\u043d \u0435\u0448\u043a\u0456\u043c \u0436\u043e\u049b."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0421\u0435\u043d\u0456\u04a3 \u043a\u04af\u0439\u0440\u0435\u0443\u0456\u04a3 \u0435\u0448\u043a\u0456\u043c \u0436\u0430\u0437\u0430 \u0430\u043b\u043c\u0430\u0439\u0442\u044b\u043d \u0436\u0430\u0440\u0430 \u0456\u0441\u043f\u0435\u0442\u0442\u0456, \u04e9\u0437\u0456\u04a3 \u049b\u04b1\u043b\u0434\u044b\u0440\u0430\u0493\u0430\u043d \u04e9\u043b\u0456\u043c\u0448\u0456 \u0445\u0430\u043b\u0434\u0435\u0441\u0456\u04a3. \u0411\u0430\u0441\u044b\u04a3\u0430 \u0442\u04af\u0441\u043a\u0435\u043d \u0430\u043f\u0430\u0442 \u0442\u0443\u0440\u0430\u043b\u044b \u0435\u0441\u0442\u0456\u0433\u0435\u043d \u04d9\u0440\u043a\u0456\u043c \u0441\u043e\u043d\u044b \u049b\u043e\u043b \u0448\u0430\u043f\u0430\u043b\u0430\u049b\u0442\u0430\u043f \u049b\u0430\u0431\u044b\u043b\u0434\u0430\u0439\u0434\u044b. \u0421\u0435\u0431\u0435\u0431\u0456 \u0441\u0435\u043d\u0456\u04a3 \u0448\u0435\u0433\u0456 \u0436\u043e\u049b \u049b\u0430\u0442\u0430\u043b\u0434\u044b\u0493\u044b\u04a3\u0434\u044b \u04e9\u0437 \u0431\u0430\u0441\u044b\u043d\u0430\u043d \u04e9\u0442\u043a\u0456\u0437\u0431\u0435\u0433\u0435\u043d \u0435\u0448\u043a\u0456\u043c \u049b\u0430\u043b\u043c\u0430\u0434\u044b."
                }
            }
        }
    }
