                "16": {
                    "verse_nr": "16",
                    "verse": "\u0422\u044b\u043d\u044b\u0448\u0442\u044b\u049b\u0442\u044b\u04a3 \u0431\u0430\u0441\u0442\u0430\u0443\u044b \u2014 \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u043d\u0456\u04a3 \u04e8\u0437\u0456 \u0441\u0435\u043d\u0434\u0435\u0440\u0433\u0435 \u04d9\u0440\u0434\u0430\u0439\u044b\u043c \u049b\u0430\u0439 \u043d\u04d9\u0440\u0441\u0435\u0434\u0435 \u0434\u0435 \u0442\u044b\u043d\u044b\u0448\u0442\u044b\u049b \u0441\u044b\u0439\u043b\u0430\u0439 \u0431\u0435\u0440\u0441\u0456\u043d! \u0418\u0435\u043c\u0456\u0437 \u0431\u0430\u0440\u043b\u044b\u0493\u044b\u04a3\u0430 \u0436\u0430\u0440 \u0431\u043e\u043b\u0493\u0430\u0439!"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0410\u043b \u043c\u0435\u043d, \u041f\u0430\u0443\u044b\u043b, \u043e\u0441\u044b \u0441\u04d9\u043b\u0435\u043c\u0456\u043c\u0434\u0456 \u04e9\u0437 \u049b\u043e\u043b\u044b\u043c\u043c\u0435\u043d \u0436\u0430\u0437\u044b\u043f \u043e\u0442\u044b\u0440\u043c\u044b\u043d. \u0425\u0430\u0442\u0442\u0430\u0440\u044b\u043c\u0434\u044b \u04b1\u0434\u0430\u0439\u044b \u043e\u0441\u044b\u043b\u0430\u0439 \u0430\u044f\u049b\u0442\u0430\u0439\u043c\u044b\u043d, \u043e\u043b\u0430\u0440\u0434\u044b\u04a3 \u043c\u0435\u043d\u0456\u043a\u0456 \u0435\u043a\u0435\u043d\u0456\u043d \u043e\u0441\u044b\u0493\u0430\u043d \u049b\u0430\u0440\u0430\u043f \u0431\u0456\u043b\u0435\u0441\u0456\u04a3\u0434\u0435\u0440."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u041b\u0430\u0439\u044b\u043c \u0418\u0435\u043c\u0456\u0437 \u0418\u0441\u0430 \u041c\u04d9\u0441\u0456\u0445 \u0431\u0430\u0440\u043b\u044b\u0493\u044b\u04a3\u0430 \u0440\u0430\u049b\u044b\u043c\u044b\u043d \u0436\u0430\u0443\u0434\u044b\u0440\u0430 \u043a\u04e9\u0440\u0441\u0456\u043d!"
