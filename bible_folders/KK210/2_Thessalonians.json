        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0410\u043b \u0435\u043d\u0434\u0456, \u0431\u0430\u0443\u044b\u0440\u043b\u0430\u0441\u0442\u0430\u0440, \u0431\u0456\u0437 \u04af\u0448\u0456\u043d \u043c\u0456\u043d\u0430\u0436\u0430\u0442 \u0435\u0442\u0456\u04a3\u0434\u0435\u0440: \u0418\u0435\u043c\u0456\u0437\u0434\u0456\u04a3 \u0406\u0437\u0433\u0456 \u0445\u0430\u0431\u0430\u0440\u044b \u0442\u0443\u0440\u0430 \u0441\u0435\u043d\u0434\u0435\u0440\u0434\u0435\u0433\u0456\u0434\u0435\u0439 \u043a\u04e9\u043f \u0442\u0430\u0440\u0430\u043f, \u049b\u04b1\u0440\u043c\u0435\u0442\u043a\u0435 \u0431\u04e9\u043b\u0435\u043d\u0435 \u0431\u0435\u0440\u0441\u0456\u043d."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0418\u0435\u043c\u0456\u0437 \u0431\u0456\u0437\u0434\u0456 \u049b\u0430\u0440\u0430 \u043d\u0438\u0435\u0442\u0442\u0456, \u0437\u04b1\u043b\u044b\u043c \u0430\u0434\u0430\u043c\u0434\u0430\u0440\u0434\u044b\u04a3 \u0448\u0430\u0431\u0443\u044b\u043b\u044b\u043d\u0430\u043d \u049b\u04b1\u0442\u049b\u0430\u0440\u0430 \u043a\u04e9\u0440\u0441\u0456\u043d. \u04e8\u0439\u0442\u043a\u0435\u043d\u0456 \u0436\u04b1\u0440\u0442\u0442\u044b\u04a3 \u0431\u04d9\u0440\u0456 \u0431\u0456\u0440\u0434\u0435\u0439 \u0441\u0435\u043d\u0443\u0448\u0456 \u0435\u043c\u0435\u0441."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u0410\u043b \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u0441\u0435\u043d\u0456\u043c\u0434\u0456, \u041e\u043b \u0441\u0435\u043d\u0434\u0435\u0440\u0433\u0435 \u043a\u04af\u0448-\u0436\u0456\u0433\u0435\u0440 \u0431\u0435\u0440\u0456\u043f, \u04d9\u0437\u04d9\u0437\u0456\u043b\u0434\u0456\u04a3 \u04d9\u0440\u0435\u043a\u0435\u0442\u0456\u043d\u0435\u043d \u0441\u0430\u049b\u0442\u0430\u0439\u0434\u044b."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u0421\u0435\u043d\u0434\u0435\u0440 \u0418\u0435\u043c\u0456\u0437\u0431\u0435\u043d \u0442\u044b\u0493\u044b\u0437 \u0431\u0430\u0439\u043b\u0430\u043d\u044b\u0441\u0442\u0430 \u0442\u04b1\u0440\u0493\u0430\u043d\u0434\u044b\u049b\u0442\u0430\u043d, \u0431\u0456\u0437 \u04e9\u0437\u0434\u0435\u0440\u0456\u04a3\u0435 \u0431\u04b1\u0439\u044b\u0440\u0493\u0430\u043d\u044b\u043c\u044b\u0437\u0434\u044b \u049b\u0430\u0437\u0456\u0440 \u0434\u0435, \u043a\u0435\u043b\u0435\u0448\u0435\u043a\u0442\u0435 \u0434\u0435 \u043e\u0440\u044b\u043d\u0434\u0430\u0439\u0434\u044b \u0434\u0435\u043f \u0441\u0435\u043d\u0456\u043c \u0430\u0440\u0442\u0430\u043c\u044b\u0437."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0418\u0435\u043c\u0456\u0437 \u0436\u0430\u043d \u0434\u04af\u043d\u0438\u0435\u043b\u0435\u0440\u0456\u04a3\u0434\u0456 \u049a\u04b1\u0434\u0430\u0439\u0434\u044b\u04a3 \u0441\u04af\u0439\u0456\u0441\u043f\u0435\u043d\u0448\u0456\u043b\u0456\u0433\u0456\u043d\u0435 \u0431\u04b1\u0440\u044b\u043f, \u0441\u0435\u043d\u0434\u0435\u0440 \u041c\u04d9\u0441\u0456\u0445\u0442\u0456\u04a3 \u049b\u0430\u0439\u0442\u0430 \u043e\u0440\u0430\u043b\u0443\u044b\u043d \u0441\u0430\u0431\u044b\u0440\u043c\u0435\u043d \u043a\u04af\u0442\u0441\u0456\u043d \u0434\u0435\u043f \u043c\u0456\u043d\u0430\u0436\u0430\u0442 \u0435\u0442\u0435\u043c\u0456\u0437."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u049a\u044b\u043c\u0431\u0430\u0442\u0442\u044b \u0431\u0430\u0443\u044b\u0440\u043b\u0430\u0441\u0442\u0430\u0440, \u0418\u0435\u043c\u0456\u0437 \u0418\u0441\u0430 \u041c\u04d9\u0441\u0456\u0445\u0442\u0456\u04a3 \u0430\u0442\u044b\u043d\u0430\u043d \u0441\u0435\u043d\u0434\u0435\u0440\u0433\u0435 \u0442\u0430\u0493\u044b \u043c\u044b\u043d\u0430\u043d\u044b \u0435\u0441\u043a\u0435\u0440\u0442\u0435\u043c\u0456\u0437: \u0436\u04af\u0433\u0435\u043d\u0441\u0456\u0437 \u0436\u04af\u0440\u0456\u043f, \u0431\u0456\u0437\u0434\u0456\u04a3 \u04e9\u0437\u0434\u0435\u0440\u0456\u04a3\u0435 \u0442\u0430\u043f\u0441\u044b\u0440\u0493\u0430\u043d \u0456\u043b\u0456\u043c\u0456\u043c\u0456\u0437\u0433\u0435 \u0435\u0440\u043c\u0435\u0439\u0442\u0456\u043d\u0434\u0435\u0440\u0434\u0435\u043d \u0431\u043e\u0439\u043b\u0430\u0440\u044b\u04a3\u0434\u044b \u0430\u0443\u043b\u0430\u049b \u04b1\u0441\u0442\u0430\u04a3\u0434\u0430\u0440!"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u0411\u04b1\u043b \u0436\u0435\u0440\u0434\u0435 \u0431\u0456\u0437\u0434\u0435\u043d \u04af\u043b\u0433\u0456 \u0430\u043b\u0443 \u049b\u0430\u0436\u0435\u0442\u0442\u0456\u0433\u0456\u043d \u04e9\u0437\u0434\u0435\u0440\u0456\u04a3 \u0434\u0435 \u0431\u0456\u043b\u0435\u0441\u0456\u04a3\u0434\u0435\u0440. \u0421\u0435\u0431\u0435\u0431\u0456 \u0431\u0456\u0437 \u0441\u0435\u043d\u0434\u0435\u0440\u0434\u0435 \u0431\u043e\u043b\u0493\u0430\u043d\u0434\u0430 \u0436\u04b1\u043c\u044b\u0441\u0441\u044b\u0437, \u0431\u043e\u0441 \u0436\u04af\u0440\u0456\u043f"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0431\u0430\u0441\u049b\u0430\u043d\u044b\u04a3 \u043d\u0430\u043d\u044b\u043d \u0442\u0435\u0433\u0456\u043d \u0436\u0435\u0433\u0435\u043d \u0436\u043e\u049b\u043f\u044b\u0437. \u0415\u0448\u049b\u0430\u0439\u0441\u044b\u04a3\u0430 \u0430\u0443\u044b\u0440\u0442\u043f\u0430\u043b\u044b\u049b \u0442\u04af\u0441\u0456\u0440\u043c\u0435\u0443 \u04af\u0448\u0456\u043d \u043a\u04af\u043d\u0456-\u0442\u04af\u043d\u0456 \u0435\u04a3\u0431\u0435\u043a \u0435\u0442\u0456\u043f, \u0436\u0430\u043d \u0441\u0430\u043b\u0430 \u0431\u0435\u0439\u043d\u0435\u0442\u0442\u0435\u043d\u0456\u043f \u0436\u04b1\u043c\u044b\u0441 \u0456\u0441\u0442\u0435\u0434\u0456\u043a."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u041d\u0430\u043d\u0434\u0430\u0440\u044b\u04a3\u0434\u044b \u0436\u0435\u0443\u0433\u0435 \u049b\u0430\u049b\u044b\u043c\u044b\u0437 \u0431\u0430\u0440 \u0435\u0434\u0456; \u0431\u0456\u0440\u0430\u049b \u0441\u0435\u043d\u0434\u0435\u0440 \u0431\u0456\u0437\u0433\u0435 \u0435\u043b\u0456\u043a\u0442\u0435\u0441\u0456\u043d \u0434\u0435\u043f, \u04af\u043b\u0433\u0456 \u0431\u043e\u043b\u0493\u044b\u043c\u044b\u0437 \u043a\u0435\u043b\u0434\u0456."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0410\u0440\u0430\u043b\u0430\u0440\u044b\u04a3\u0434\u0430 \u0436\u04af\u0440\u0433\u0435\u043d\u0434\u0435 \u00ab\u0415\u04a3\u0431\u0435\u043a \u0435\u0442\u043a\u0456\u0441\u0456 \u043a\u0435\u043b\u043c\u0435\u0433\u0435\u043d \u0442\u0430\u043c\u0430\u049b \u0442\u0430 \u0456\u0448\u0456\u043f-\u0436\u0435\u043c\u0435\u0441\u0456\u043d\u00bb \u0434\u0435\u0433\u0435\u043d \u049b\u0430\u0493\u0438\u0434\u0430\u043d\u044b \u0441\u0430\u043d\u0430\u043b\u0430\u0440\u044b\u04a3\u0430 \u0441\u0456\u04a3\u0456\u0440\u0434\u0456\u043a."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u0410\u043b \u0435\u043d\u0434\u0456 \u043a\u0435\u0439\u0431\u0456\u0440\u0435\u0443\u043b\u0435\u0440\u0456\u04a3\u043d\u0456\u04a3 \u0436\u04af\u0433\u0435\u043d\u0441\u0456\u0437 \u0436\u04af\u0440\u0433\u0435\u043d\u0434\u0456\u043a\u0442\u0435\u0440\u0456\u04a3 \u0442\u0443\u0440\u0430\u043b\u044b \u0435\u0441\u0442\u0456\u0434\u0456\u043a. \u041e\u043b\u0430\u0440 \u0436\u04b1\u043c\u044b\u0441 \u0456\u0441\u0442\u0435\u043c\u0435\u0439, \u0431\u0430\u0441\u049b\u0430\u043b\u0430\u0440\u0434\u044b\u04a3 \u043c\u0430\u0437\u0430\u0441\u044b\u043d \u0430\u043b\u044b\u043f, \u0431\u043e\u0441\u049b\u0430 \u0441\u0430\u043d\u0434\u0430\u043b\u044b\u043f \u0436\u04af\u0440 \u0435\u043a\u0435\u043d."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u041e\u0441\u044b\u043d\u0434\u0430\u0439\u043b\u0430\u0440\u0493\u0430 \u0431\u0456\u0437 \u0442\u044b\u043d\u044b\u0448 \u0435\u04a3\u0431\u0435\u043a \u0435\u0442\u0456\u043f, \u04e9\u0437 \u043d\u0430\u043d\u0434\u0430\u0440\u044b\u043d \u04e9\u0437\u0434\u0435\u0440\u0456 \u0442\u0430\u0443\u044b\u043f \u0436\u0435\u0441\u0456\u043d \u0434\u0435\u043f \u0418\u0435\u043c\u0456\u0437 \u0418\u0441\u0430 \u041c\u04d9\u0441\u0456\u0445\u0442\u0456\u04a3 \u0430\u0442\u044b\u043d\u0430\u043d \u0431\u04b1\u0439\u044b\u0440\u044b\u043f \u0435\u0441\u043a\u0435\u0440\u0442\u0435\u043c\u0456\u0437."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u0410\u043b \u04e9\u0437\u0434\u0435\u0440\u0456\u04a3, \u049b\u044b\u043c\u0431\u0430\u0442\u0442\u044b \u0431\u0430\u0443\u044b\u0440\u043b\u0430\u0441\u0442\u0430\u0440, \u0436\u0430\u049b\u0441\u044b\u043b\u044b\u049b \u0436\u0430\u0441\u0430\u0443\u0434\u0430\u043d \u0436\u0430\u043b\u044b\u049b\u043f\u0430\u0441\u0430\u04a3\u0434\u0430\u0440 \u0435\u043a\u0435\u043d."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u041a\u0456\u043c\u0434\u0435-\u043a\u0456\u043c \u0431\u0456\u0437\u0434\u0456\u04a3 \u043e\u0441\u044b \u0445\u0430\u0442\u0442\u0430 \u0430\u0439\u0442\u044b\u043f \u043e\u0442\u044b\u0440\u0493\u0430\u043d \u043d\u04b1\u0441\u049b\u0430\u0443\u043b\u0430\u0440\u044b\u043c\u044b\u0437\u0493\u0430 \u049b\u04b1\u043b\u0430\u049b \u0430\u0441\u043f\u0430\u0441\u0430, \u0441\u043e\u043d\u044b \u0435\u0441\u0442\u0435\u0440\u0456\u04a3\u0434\u0435 \u0441\u0430\u049b\u0442\u0430\u04a3\u0434\u0430\u0440. \u04e8\u0437 \u049b\u044b\u043b\u044b\u0493\u044b\u043d\u0430 \u04b1\u044f\u043b\u0443\u044b \u04af\u0448\u0456\u043d \u0441\u043e\u043b \u043a\u0456\u0441\u0456\u043c\u0435\u043d \u0430\u0440\u0430\u043b\u0430\u0441\u043f\u0430\u0439 \u049b\u043e\u0439\u044b\u04a3\u0434\u0430\u0440."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0411\u0456\u0440\u0430\u049b \u043e\u0493\u0430\u043d \u0436\u0430\u0443 \u0434\u0435\u043f \u049b\u0430\u0440\u0430\u043c\u0430\u0439, \u0431\u0430\u0443\u044b\u0440\u043b\u0430\u0441 \u0440\u0435\u0442\u0456\u043d\u0434\u0435 \u0442\u04af\u0441\u0456\u043d\u0434\u0456\u0440\u0456\u043f \u0435\u0441\u043a\u0435\u0440\u0442\u0456\u04a3\u0434\u0435\u0440."
