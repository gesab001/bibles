                "16": {
                    "verse_nr": "16",
                    "verse": "\u0421\u0435\u043d \u0434\u0430\u04a3\u049b\u049b\u0430 \u0435\u043c\u0435\u0441, \u043c\u0430\u0441\u049b\u0430\u0440\u0430\u0493\u0430 \u0431\u04e9\u043b\u0435\u043d\u0435\u0442\u0456\u043d \u0431\u043e\u043b\u0430\u0441\u044b\u04a3! \u0415\u043d\u0434\u0456\u0433\u0456 \u043a\u0435\u0437\u0435\u043a \u0441\u0430\u0493\u0430\u043d \u043a\u0435\u043b\u0456\u043f, \u04e9\u0437\u0456\u04a3 \u0434\u0435 \u0456\u0448\u0456\u043f, \u04b1\u044f\u0442\u044b\u04a3 \u0430\u0448\u044b\u043b\u0441\u044b\u043d! \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u043d\u0456\u04a3 \u049b\u04b1\u0434\u0456\u0440\u0435\u0442\u0442\u0456 \u049b\u043e\u043b\u044b\u043d\u0430\u043d \u0448\u044b\u049b\u049b\u0430\u043d, \u041e\u043d\u044b\u04a3 \u049b\u0430\u04bb\u0430\u0440\u044b\u043d\u0430 \u0442\u043e\u043b\u044b \u0442\u043e\u0441\u0442\u0430\u0493\u0430\u043d\u044b\u043d\u0430\u043d \u0456\u0448! \u0421\u043e\u043d\u0434\u0430 \u0437\u043e\u0440 \u0434\u0430\u04a3\u049b\u044b\u04a3 \u043c\u0430\u0441\u049b\u0430\u0440\u0430\u0493\u0430 \u0430\u0439\u043d\u0430\u043b\u0430\u0434\u044b."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0421\u0435\u043d\u0456\u04a3 \u041b\u0438\u0432\u0430\u043d \u043e\u0440\u043c\u0430\u043d\u0434\u0430\u0440\u044b\u043d\u0430 \u0436\u0430\u0441\u0430\u0493\u0430\u043d \u0437\u043e\u0440\u043b\u044b\u0493\u044b\u04a3 \u04e9\u0437 \u0431\u0430\u0441\u044b\u04a3\u0430 \u043a\u0435\u043b\u0456\u043f, \u0430\u04a3\u0434\u0430\u0440\u0434\u044b \u049b\u04b1\u0440\u0442\u049b\u0430\u043d\u044b\u04a3 \u04e9\u0437\u0456\u04a3\u0434\u0456 \u04af\u0440\u0435\u0439\u043b\u0435\u043d\u0434\u0456\u0440\u0435\u0434\u0456. \u0421\u0435\u043d \u0430\u0434\u0430\u043c \u049b\u0430\u043d\u044b\u043d \u0434\u0430 \u0442\u04e9\u0433\u0456\u043f, \u0435\u043b\u0434\u0435\u0440 \u043c\u0435\u043d \u049b\u0430\u043b\u0430\u043b\u0430\u0440\u0434\u044b \u0442\u04b1\u0440\u0493\u044b\u043d\u0434\u0430\u0440\u044b\u043c\u0435\u043d \u0431\u0456\u0440\u0433\u0435 \u049b\u04b1\u0440\u0442\u0442\u044b\u04a3."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u0421\u043e\u043d\u0434\u0430 \u0442\u04d9\u04a3\u0456\u0440 \u043c\u04af\u0441\u0456\u043d\u0456\u043d\u0456\u04a3 \u043a\u0435\u043b\u0442\u0456\u0440\u0435\u0440 \u043f\u0430\u0439\u0434\u0430\u0441\u044b \u043d\u0435? \u041e\u043d\u044b \u043e\u0439\u044b\u043f \u0436\u0430\u0441\u0430\u0493\u0430\u043d \u0436\u0430\u0439 \u043f\u0435\u043d\u0434\u0435 \u0435\u043c\u0435\u0441 \u043f\u0435? \u041d\u0435\u043c\u0435\u0441\u0435 \u049b\u04b1\u0439\u044b\u043b\u044b\u043f \u0436\u0430\u0441\u0430\u043b\u0493\u0430\u043d, \u04e9\u0442\u0456\u0440\u0456\u043a\u0442\u0456 \u04af\u0439\u0440\u0435\u0442\u0435\u0442\u0456\u043d \u043c\u04af\u0441\u0456\u043d\u043d\u0456\u04a3 \u043f\u0430\u0439\u0434\u0430\u0441\u044b \u049b\u0430\u043d\u0434\u0430\u0439? \u041e\u043d\u044b \u0436\u0430\u0441\u0430\u0439\u0442\u044b\u043d \u0430\u0434\u0430\u043c \u0431\u0430\u043b\u0430\u0441\u044b \u04e9\u0437\u0456\u043d\u0456\u04a3 \u049b\u043e\u043b\u044b\u043d\u0430\u043d \u0448\u044b\u049b\u049b\u0430\u043d \u0442\u04d9\u04a3\u0456\u0440\u0441\u044b\u043c\u0430\u049b\u049b\u0430 \u0441\u0435\u043d\u0435\u0434\u0456, \u0431\u0456\u0440\u0430\u049b \u0441\u043e\u043b \u043c\u04af\u0441\u0456\u043d \u0441\u04e9\u0439\u043b\u0435\u0439 \u0430\u043b\u043c\u0430\u0439\u0434\u044b, \u043e\u043b \u04af\u043d\u0441\u0456\u0437 \u043c\u044b\u043b\u049b\u0430\u0443."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0410\u0493\u0430\u0448 \u043c\u04af\u0441\u0456\u043d\u0433\u0435: \u00ab\u0422\u0456\u0440\u0456\u043b!\u00bb, \u043c\u044b\u043b\u049b\u0430\u0443 \u0442\u0430\u0441\u049b\u0430: \u00ab\u041e\u044f\u043d!\u00bb \u0434\u0435\u0439\u0442\u0456\u043d \u049b\u0430\u0441\u0456\u0440\u0435\u0442\u043a\u0435 \u049b\u0430\u043b\u0430\u0434\u044b! \u04e8\u043b\u0456 \u043c\u04af\u0441\u0456\u043d\u0434\u0435\u0440 \u0441\u0435\u043d\u0434\u0435\u0440\u0433\u0435 \u0434\u04b1\u0440\u044b\u0441 \u0436\u043e\u043b\u0434\u044b \u04af\u0439\u0440\u0435\u0442\u0435 \u0430\u043b\u0430 \u043c\u0430? \u0410\u043b\u0442\u044b\u043d, \u043a\u04af\u043c\u0456\u0441\u043f\u0435\u043d \u049b\u0430\u043f\u0442\u0430\u043b\u0493\u0430\u043d \u0431\u043e\u043b\u0441\u0430 \u0434\u0430, \u043e\u043b\u0430\u0440\u0434\u0430 \u0442\u0456\u0440\u0448\u0456\u043b\u0456\u043a \u0436\u043e\u049b."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u0410\u043b \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435 \u04e8\u0437\u0456\u043d\u0456\u04a3 (\u043a\u04e9\u043a\u0442\u0435\u0433\u0456) \u043a\u0438\u0435\u043b\u0456 \u04af\u0439\u0456\u043d\u0434\u0435. \u0411\u04af\u043a\u0456\u043b \u0436\u0435\u0440 \u0431\u0435\u0442\u0456 \u041e\u043d\u044b\u04a3 \u0430\u043b\u0434\u044b\u043d\u0434\u0430 \u04af\u043d\u0434\u0435\u043c\u0435\u0439, \u041e\u043d\u044b \u0442\u0435\u0440\u0435\u04a3 \u049b\u0430\u0441\u0442\u0435\u0440\u043b\u0435\u0439\u0442\u0456\u043d \u0431\u043e\u043b\u0441\u044b\u043d!"
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u0410\u0431\u0431\u0430\u049b\u04b1\u049b \u043f\u0430\u0439\u0493\u0430\u043c\u0431\u0430\u0440\u0434\u044b\u04a3 \u049a\u04b1\u0434\u0430\u0439\u0493\u0430 \u0435\u0442\u043a\u0435\u043d \u043c\u0456\u043d\u0430\u0436\u0430\u0442\u044b; \u0435\u043a\u043f\u0456\u043d\u0434\u0456 \u04d9\u0443\u0435\u043d\u043c\u0435\u043d \u0430\u0439\u0442\u044b\u043b\u0430\u0434\u044b."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u0423\u0430, \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d, \u0421\u0435\u043d\u0456\u04a3 \u0434\u0430\u04a3\u049b\u044b\u04a3 \u0436\u0430\u0439\u043b\u044b \u0435\u0441\u0442\u0456\u0433\u0435\u043d\u043c\u0456\u043d,\n\u0418\u0435\u043c, \u0456\u0441\u0442\u0435\u0440\u0456\u04a3\u0435 \u049b\u0430\u0442\u0442\u044b \u0442\u0430\u04a3\u044b\u0440\u049b\u0430\u043f \u043a\u0435\u043b\u0435\u043c\u0456\u043d.\n\u041e\u0441\u044b \u0437\u0430\u043c\u0430\u043d\u0434\u0430 \u0434\u0430 \u0441\u043e\u043d\u0434\u0430\u0439\u043b\u0430\u0440\u0434\u044b \u0436\u0430\u0441\u0430\u0439 \u0433\u04e9\u0440,\n\u0422\u0456\u0440\u0456 \u043a\u0435\u0437\u0456\u043c\u0456\u0437\u0434\u0435 \u043e\u043b\u0430\u0440\u0434\u044b \u043f\u0430\u0448 \u0435\u0442\u0435 \u0433\u04e9\u0440,\n\u049a\u0430\u04bb\u0430\u0440\u044b\u04a3\u0434\u0430 \u043c\u0435\u0439\u0456\u0440\u0456\u043c\u0456\u04a3\u0434\u0456 \u0435\u0441\u043a\u0435\u0440\u0435 \u0433\u04e9\u0440."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u041a\u0438\u0435\u043b\u0456 \u049a\u04b1\u0434\u0430\u0439 \u0422\u0435\u043c\u0430\u043d\u043d\u0430\u043d, \u041f\u0430\u0440\u0430\u043d \u0442\u0430\u0443\u044b\u043d\u0430\u043d \u043a\u0435\u043b\u0434\u0456.\n\u04b0\u043b\u044b \u0441\u0430\u043b\u0442\u0430\u043d\u0430\u0442\u044b \u041e\u043d\u044b\u04a3 \u0436\u0430\u043f\u0442\u044b \u043a\u04af\u043b\u043b\u0456 \u0430\u0441\u043f\u0430\u043d\u0434\u044b,\n\u041e\u043d\u044b \u0434\u04d9\u0440\u0456\u043f\u0442\u0435\u0433\u0435\u043d \u043c\u0430\u0434\u0430\u049b\u049b\u0430 \u0436\u0435\u0440 \u0431\u0435\u0442\u0456 \u043a\u0435\u043d\u0435\u043b\u0434\u0456."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u049a\u04b1\u0434\u0430\u0439 \u043d\u04b1\u0440\u044b \u0442\u0430\u04a3 \u0448\u0430\u043f\u0430\u0493\u044b\u043d\u0434\u0430\u0439 \u0436\u0430\u0440\u049b\u044b\u0440\u0430\u0434\u044b,\n\u049a\u04b1\u0434\u0456\u0440\u0435\u0442\u0442\u0456 \u049b\u043e\u043b\u044b\u043d\u0430\u043d \u0441\u04d9\u0443\u043b\u0435\u043b\u0435\u0440 \u0436\u0430\u043b\u0442\u044b\u0440\u0430\u0434\u044b."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d \u0418\u0435\u043d\u0456\u04a3 \u0430\u043b\u0434\u044b\u043d\u0434\u0430 \u0430\u043f\u0430\u0442 \u0436\u04af\u0440\u0434\u0456,\n\u0410\u043b \u0441\u043e\u04a3\u044b\u043d\u0430\u043d \u043a\u04af\u0439\u0434\u0456\u0440\u0433\u0456\u0448 \u0456\u043d\u0434\u0435\u0442 \u0442\u0435 \u0435\u0440\u0434\u0456."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u049a\u04b1\u0434\u0430\u0439 \u0442\u04b1\u0440\u0430 \u049b\u0430\u043b\u044b\u043f, \u0436\u0435\u0440\u0434\u0456 \u0441\u0456\u043b\u043a\u0456\u043d\u0434\u0456\u0440\u0434\u0456,\n\u041e\u043b \u049b\u0430\u0440\u0430\u0493\u0430\u043d\u0434\u0430 \u0445\u0430\u043b\u044b\u049b\u0442\u0430\u0440 \u0434\u0456\u0440\u0456\u043b\u0434\u0435\u0434\u0456.\n\u0415\u0436\u0435\u043b\u0433\u0456 \u0442\u0430\u0443\u043b\u0430\u0440 \u043e\u043f\u044b\u0440\u044b\u043b\u044b\u043f \u049b\u04b1\u043b\u0430\u0434\u044b,\n\u0410\u0442\u0430\u043c \u0437\u0430\u043c\u0430\u043d\u0493\u044b \u0442\u04e9\u0431\u0435\u043b\u0435\u0440 \u0448\u04e9\u0433\u0456\u043f \u049b\u0430\u043b\u0434\u044b.\n\u041c\u04d9\u04a3\u0433\u0456\u043b\u0456\u043a \u2014 \u041a\u0438\u0435\u043b\u0456 \u049a\u04b1\u0434\u0430\u0439\u0434\u044b\u04a3 \u0436\u043e\u043b\u0434\u0430\u0440\u044b!"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u041a\u04e9\u0440\u0434\u0456\u043c \u049a\u04b1\u0448\u0430\u043d\u043d\u044b\u04a3 \u0430\u0431\u044b\u0440\u0436\u044b\u0493\u0430\u043d \u0448\u0430\u0442\u044b\u0440\u043b\u0430\u0440\u044b\u043d,\n\u0414\u0456\u0440\u0456\u043b\u0434\u0435\u043f \u049b\u043e\u0440\u044b\u049b\u049b\u0430\u043d \u041c\u0430\u0434\u0438\u044f\u043d\u043d\u044b\u04a3 \u049b\u043e\u0441\u0442\u0430\u0440\u044b\u043d."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u0423\u0430, \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d, \u04e9\u0437\u0435\u043d\u0434\u0435\u0440\u0433\u0435 \u0430\u0448\u0443\u043b\u0430\u043d\u0434\u044b\u04a3 \u0431\u0430,\n\u049a\u0430\u04bb\u0430\u0440\u044b\u04a3 \u0430\u0493\u044b\u043c\u0434\u0430\u0440\u0493\u0430 \u0431\u0430\u0493\u044b\u0442\u0442\u0430\u043b\u0493\u0430\u043d \u0431\u0430?\n\u0421\u0435\u043d \u0430\u0442\u0442\u0430\u0440\u044b\u04a3\u0430 \u043c\u0456\u043d\u0456\u043f, \u0430\u0442\u043e\u0439\u043b\u0430\u0493\u0430\u043d\u0434\u0430\n\u0422\u0435\u04a3\u0456\u0437\u0433\u0435 \u049b\u0430\u0440\u0441\u044b \u049b\u0430\u0442\u0442\u044b \u049b\u0430\u04bb\u0430\u0440\u043b\u0430\u043d\u0434\u044b\u04a3 \u0431\u0430?\n\u0416\u043e\u049b, \u043a\u04af\u0439\u043c\u0435\u043b\u0435\u0440\u0456\u04a3\u0434\u0456 \u0436\u0456\u0431\u0435\u0440\u0434\u0456\u04a3 \u049b\u04b1\u0442\u049b\u0430\u0440\u0443\u0493\u0430!"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u0421\u0435\u043d \u0441\u0430\u0434\u0430\u0493\u044b\u04a3\u0434\u044b \u049b\u044b\u043d\u0430\u0431\u044b\u043d\u0430\u043d \u0448\u044b\u0493\u0430\u0440\u0434\u044b\u04a3,\n\u041e\u0442\u0442\u044b \u0436\u0435\u0431\u0435\u043b\u0435\u0440\u0434\u0456 \u0443\u04d9\u0434\u0435\u04a3\u0435 \u0441\u0430\u0439 \u0434\u0430\u0439\u044b\u043d\u0434\u0430\u0434\u044b\u04a3,\n\u0416\u0435\u0440 \u0431\u0435\u0442\u0456\u043d \u0441\u0430\u0439-\u0430\u04a3\u0493\u0430\u0440\u043c\u0435\u043d \u0431\u04e9\u043b\u0456\u043f \u0442\u0430\u0441\u0442\u0430\u0434\u044b\u04a3."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u0422\u0430\u0443\u043b\u0430\u0440 \u0421\u0435\u043d\u0456 \u043a\u04e9\u0440\u0433\u0435\u043d\u0434\u0435 \u0434\u04af\u0440 \u0441\u0456\u043b\u043a\u0456\u043d\u0434\u0456,\n\u0421\u0443 \u0442\u0430\u0441\u049b\u044b\u043d\u044b \u0430\u0493\u044b\u043f \u04e9\u0442\u0456\u043f \u043b\u0430\u043f \u0431\u0435\u0440\u0434\u0456,\n\u0422\u04af\u043f\u0441\u0456\u0437 \u0442\u04b1\u04a3\u0493\u0438\u044b\u049b \u0433\u04af\u0440\u0456\u043b\u0434\u0435\u043f \u043a\u04af\u0440\u043a\u0456\u0440\u0435\u0434\u0456,\n\u0422\u043e\u043b\u049b\u044b\u043d\u0434\u0430\u0440\u044b \u0431\u0438\u0456\u043a\u043a\u0435 \u043a\u04e9\u0442\u0435\u0440\u0456\u043b\u0434\u0456."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u04b0\u0448\u049b\u0430\u043d \u0436\u0435\u0431\u0435\u043b\u0435\u0440\u0456\u04a3\u043d\u0456\u04a3 \u0436\u0430\u0440\u044b\u0493\u044b\u043d\u0434\u0430,\n\u0416\u0430\u043b\u0442\u044b\u0440\u0430\u0493\u0430\u043d \u043d\u0430\u0439\u0437\u0430\u04a3\u043d\u044b\u04a3 \u0436\u0430\u0440\u049b\u044b\u043b\u044b\u043d\u0434\u0430\n\u0422\u044b\u043f-\u0442\u044b\u043d\u044b\u0448 \u0442\u04b1\u0440\u0430 \u049b\u0430\u043b\u0434\u044b \u043a\u04af\u043d \u0434\u0435, \u0430\u0439 \u0434\u0430."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u049a\u0430\u04bb\u0430\u0440\u043b\u0430\u043d\u0430 \u0436\u0435\u0440 \u0431\u0435\u0442\u0456\u043c\u0435\u043d \u049b\u0430\u0434\u0430\u043c \u0431\u0430\u0441\u0442\u044b\u04a3,\n\u0410\u0448\u0443\u043b\u0430\u043d\u0430 \u0445\u0430\u043b\u044b\u049b\u0442\u0430\u0440\u0434\u044b \u0442\u0430\u043f\u0442\u0430\u0434\u044b\u04a3."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u049a\u04b1\u0442\u049b\u0430\u0440\u0443\u0493\u0430 \u0430\u0442\u0442\u0430\u043d\u0434\u044b\u04a3 \u0421\u0435\u043d \u0445\u0430\u043b\u049b\u044b\u04a3\u0434\u044b\n\u0416\u04d9\u043d\u0435 \u04e8\u0437\u0456\u04a3 \u0442\u0430\u0493\u0430\u0439\u044b\u043d\u0434\u0430\u0493\u0430\u043d \u043f\u0430\u0442\u0448\u0430\u04a3\u0434\u044b.\n\u0417\u04b1\u043b\u044b\u043c \u0435\u043b\u0434\u0456\u04a3 \u04d9\u043c\u0456\u0440\u0448\u0456\u0441\u0456\u043d \u043a\u04af\u0439\u0440\u0435\u0442\u0442\u0456\u04a3,\n\u041e\u043d\u044b \u0442\u043e\u043b\u044b\u049b\u0442\u0430\u0439 \u04b1\u044f\u0442\u049b\u0430 \u049b\u0430\u043b\u0434\u044b\u0440\u0434\u044b\u04a3."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u0416\u0430\u0443 \u049b\u04b1\u0439\u044b\u043d\u0434\u0430\u0439 \u049b\u04b1\u0439\u0493\u044b\u0442\u044b\u043f \u0431\u0456\u0437\u0434\u0456 \u0448\u0430\u0448\u043f\u0430\u049b \u0431\u043e\u043b\u0493\u0430\u043d\u0434\u0430,\n\u0422\u044b\u0493\u044b\u043b\u0493\u0430\u043d \u0431\u0430\u0439\u0493\u04b1\u0441\u0442\u044b \u049b\u0443\u0430\u043d\u0430 \u0436\u0430\u043b\u043c\u0430\u043c\u0430\u049b \u0431\u043e\u043b\u0493\u0430\u043d\u0434\u0430,\n\u049a\u043e\u043b\u0431\u0430\u0441\u0448\u044b\u043d\u044b\u04a3 \u0431\u0430\u0441\u044b\u043d\u0430 \u04e9\u0437 \u043d\u0430\u0439\u0437\u0430\u0441\u044b\u043d \u0441\u04b1\u0493\u044b\u043f \u0430\u043b\u0434\u044b\u04a3"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u0410\u0442\u0442\u0430\u0440\u044b\u04a3\u043d\u044b\u04a3 \u0442\u04b1\u044f\u0493\u044b\u043c\u0435\u043d \u0442\u0435\u04a3\u0456\u0437\u0434\u0456 \u0442\u0430\u043f\u0442\u0430\u0434\u044b\u04a3,\n\u0417\u04d9\u0443\u043b\u0456\u043c \u0441\u0443 \u0438\u0456\u0440\u0456\u043c\u0456 \u0430\u0440\u049b\u044b\u043b\u044b \u0436\u043e\u043b \u0441\u0430\u043b\u0434\u044b\u04a3."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u0415\u0441\u0442\u0456\u0433\u0435\u043d\u0434\u0435 \u0436\u04af\u0440\u0435\u0433\u0456\u043c \u0434\u04af\u0440\u0441\u0456\u043b\u0434\u0435\u0439 \u0441\u043e\u049b\u0442\u044b,\n\u0417\u043e\u0440 \u0433\u04af\u0440\u0456\u043b\u0434\u0435\u043d \u0435\u0440\u0456\u043d\u0434\u0435\u0440\u0456\u043c \u049b\u0430\u043b\u0442\u044b\u0440\u0430\u0434\u044b,\n\u0421\u04af\u0439\u0435\u043a\u0442\u0435\u0440\u0456\u043c \u04d9\u043b\u0441\u0456\u0440\u0435\u043f, \u043a\u04af\u0448\u0442\u0435\u043d \u0430\u0439\u044b\u0440\u044b\u043b\u0434\u044b,\n\u0410\u044f\u049b \u0430\u0441\u0442\u044b\u043c \u049b\u0430\u043b\u0448-\u049b\u0430\u043b\u0448 \u0435\u0442\u0456\u043f \u0448\u0430\u0439\u049b\u0430\u043b\u0434\u044b.\n\u0410\u043b \u0431\u0456\u0437\u0434\u0456 \u0442\u043e\u043d\u0430\u043f, \u0442\u0430\u043b\u0430\u043d-\u0442\u0430\u0440\u0430\u0436\u0493\u0430 \u0441\u0430\u043b\u0493\u0430\u043d\n\u0415\u043b\u0434\u0456\u04a3 \u04e9\u0437\u0456 \u0430\u043f\u0430\u0442\u049b\u0430 \u04b1\u0448\u044b\u0440\u0430\u043f \u0436\u0430\u0442\u049b\u0430\u043d\n\u041a\u04af\u043d\u0434\u0456 \u0448\u044b\u0434\u0430\u043c\u0434\u044b\u043b\u044b\u049b\u043f\u0435\u043d \u0442\u043e\u0441\u0430\u0442\u044b\u043d \u0431\u043e\u043b\u0430\u043c."
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u0406\u043d\u0436\u0456\u0440 \u0430\u0493\u0430\u0448\u0442\u0430\u0440\u044b \u0433\u04af\u043b\u0434\u0435\u043c\u0435\u0441\u0435 \u0434\u0435,\n\u0411\u0430\u049b\u0442\u0430\u0440\u0434\u0430 \u0436\u04af\u0437\u0456\u043c \u0448\u043e\u049b\u0442\u0430\u0440\u044b \u043f\u0456\u0441\u043f\u0435\u0441\u0435 \u0434\u0435,\n\u0417\u04d9\u0439\u0442\u04af\u043d \u0430\u0493\u0430\u0448\u0442\u0430\u0440\u044b \u04e9\u043d\u0456\u043c \u0431\u0435\u0440\u043c\u0435\u0441\u0435 \u0434\u0435,\n\u0415\u0433\u0456\u0441\u0442\u0456\u043a\u0442\u0435\u0440\u0434\u0435\u043d \u0430\u0441\u0442\u044b\u049b \u0436\u0438\u043d\u0430\u043b\u043c\u0430\u0441\u0430 \u0434\u0430,\n\u049a\u043e\u0440\u0430\u0434\u0430 \u049b\u043e\u0439, \u0442\u0430\u0431\u044b\u043d\u0434\u0430 \u043c\u0430\u043b \u049b\u0430\u043b\u043c\u0430\u0441\u0430 \u0434\u0430"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u041c\u0435\u043d \u0431\u04d9\u0440\u0456\u0431\u0456\u0440 \u0416\u0430\u0440\u0430\u0442\u049b\u0430\u043d\u0493\u0430 \u0448\u0430\u0442\u0442\u0430\u043d\u0430\u043c\u044b\u043d,\n\u049a\u04b1\u0442\u049b\u0430\u0440\u0443\u0448\u044b \u049a\u04b1\u0434\u0430\u0439\u044b\u043c\u0430 \u049b\u0443\u0430\u043d\u0430\u043c\u044b\u043d."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u0416\u0430\u0440\u0430\u0442\u0443\u0448\u044b \u0422\u04d9\u04a3\u0456\u0440 \u043a\u04af\u0448-\u049b\u0443\u0430\u0442\u044b\u043c \u0431\u043e\u043b\u0430\u0434\u044b,\n\u0418\u0435\u043c \u043c\u0435\u043d\u0456 \u0431\u04b1\u0493\u044b\u0434\u0430\u0439 \u0436\u04af\u0439\u0440\u0456\u043a \u049b\u044b\u043b\u0430\u0434\u044b,\n\u049a\u044b\u0440\u0430\u0442 \u0436\u043e\u043b\u043c\u0435\u043d \u0436\u04af\u0440\u0443\u0456\u043c\u0435 \u043a\u04af\u0448 \u0434\u0430\u0440\u044b\u0442\u0430\u0434\u044b.\n(\u04d8\u043d-\u043a\u04af\u0439\u0434\u0456\u04a3 \u0436\u0435\u0442\u0435\u043a\u0448\u0456\u0441\u0456\u043d\u0435 \u0430\u0440\u043d\u0430\u043b\u0493\u0430\u043d, \u0456\u0448\u0435\u043a\u0442\u0456 \u0430\u0441\u043f\u0430\u043f\u0442\u0430\u0440\u0434\u044b\u04a3 \u0441\u04af\u0439\u0435\u043c\u0435\u043b\u0434\u0435\u0443\u0456\u043c\u0435\u043d \u043e\u0440\u044b\u043d\u0434\u0430\u043b\u0430\u0434\u044b.)"
