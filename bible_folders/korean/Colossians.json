{
    "version": "korean",
    "book_name": "Colossians",
    "book_nr": "51",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\ud558\ub098\ub2d8\uc758 \ub73b\uc73c\ub85c \ub9d0\ubbf8\uc554\uc544 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\uc758 \uc0ac\ub3c4 \ub41c \ubc14\uc6b8\uacfc \ud615\uc81c \ub514\ubaa8\ub370\ub294\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\uace8\ub85c\uc0c8\uc5d0 \uc788\ub294 \uc131\ub3c4\ub4e4 \uace7 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \uc2e0\uc2e4\ud55c \ud615\uc81c\ub4e4\uc5d0\uac8c \ud3b8\uc9c0\ud558\ub178\ub2c8 \uc6b0\ub9ac \uc544\ubc84\uc9c0 \ud558\ub098\ub2d8\uc73c\ub85c\ubd80\ud130 \uc740\ud61c\uc640 \ud3c9\uac15\uc774 \ub108\ud76c\uc5d0\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\uc6b0\ub9ac\uac00 \ub108\ud76c\ub97c \uc704\ud558\uc5ec \uae30\ub3c4\ud560 \ub54c\ub9c8\ub2e4 \ud558\ub098\ub2d8 \uace7 \uc6b0\ub9ac \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc544\ubc84\uc9c0\uaed8 \uac10\uc0ac\ud558\ub178\ub77c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\uc774\ub294 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218 \uc548\uc5d0 \ub108\ud76c\uc758 \ubbff\uc74c\uacfc \ubaa8\ub4e0 \uc131\ub3c4\uc5d0 \ub300\ud55c \uc0ac\ub791\uc744 \ub4e4\uc74c\uc774\uc694\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\ub108\ud76c\ub97c \uc704\ud558\uc5ec \ud558\ub298\uc5d0 \uc313\uc544 \ub454 \uc18c\ub9dd\uc744 \uc778\ud568\uc774\ub2c8 \uace7 \ub108\ud76c\uac00 \uc804\uc5d0 \ubcf5\uc74c \uc9c4\ub9ac\uc758 \ub9d0\uc500\uc744 \ub4e4\uc740 \uac83\uc774\ub77c\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\uc774 \ubcf5\uc74c\uc774 \uc774\ubbf8 \ub108\ud76c\uc5d0\uac8c \uc774\ub974\ub9e4 \ub108\ud76c\uac00 \ub4e3\uace0 \ucc38\uc73c\ub85c \ud558\ub098\ub2d8\uc758 \uc740\ud61c\ub97c \uae68\ub2ec\uc740 \ub0a0\ubd80\ud130 \ub108\ud76c \uc911\uc5d0\uc11c\uc640 \uac19\uc774 \ub610\ud55c \uc628 \ucc9c\ud558\uc5d0\uc11c\ub3c4 \uc5f4\ub9e4\ub97c \ub9fa\uc5b4 \uc790\ub77c\ub294\ub3c4\ub2e4\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\uc774\uc640 \uac19\uc774 \uc6b0\ub9ac\uc640 \ud568\uaed8 \uc885 \ub41c \uc0ac\ub791\ud558\ub294 \uc5d0\ubc14\ube0c\ub77c\uc5d0\uac8c \ub108\ud76c\uac00 \ubc30\uc6e0\ub098\ub2c8 \uadf8\ub294 \ub108\ud76c\ub97c \uc704\ud558\uc5ec \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc2e0\uc2e4\ud55c \uc77c\uad70\uc774\uc694\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\uc131\ub839 \uc548\uc5d0\uc11c \ub108\ud76c \uc0ac\ub791\uc744 \uc6b0\ub9ac\uc5d0\uac8c \uace0\ud55c \uc790\ub2c8\ub77c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\uc774\ub85c\uc368 \uc6b0\ub9ac\ub3c4 \ub4e3\ub358 \ub0a0\ubd80\ud130 \ub108\ud76c\ub97c \uc704\ud558\uc5ec \uae30\ub3c4\ud558\uae30\ub97c \uadf8\uce58\uc9c0 \uc544\ub2c8\ud558\uace0 \uad6c\ud558\ub178\ub2c8 \ub108\ud76c\ub85c \ud558\uc5ec\uae08 \ubaa8\ub4e0 \uc2e0\ub839\ud55c \uc9c0\ud61c\uc640 \ucd1d\uba85\uc5d0 \ud558\ub098\ub2d8\uc758 \ub73b\uc744 \uc544\ub294 \uac83\uc73c\ub85c \ucc44\uc6b0\uac8c \ud558\uc2dc\uace0\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\uc8fc\uaed8 \ud569\ub2f9\ud788 \ud589\ud558\uc5ec \ubc94\uc0ac\uc5d0 \uae30\uc058\uc2dc\uac8c \ud558\uace0 \ubaa8\ub4e0 \uc120\ud55c \uc77c\uc5d0 \uc5f4\ub9e4\ub97c \ub9fa\uac8c \ud558\uc2dc\uba70 \ud558\ub098\ub2d8\uc744 \uc544\ub294 \uac83\uc5d0 \uc790\ub77c\uac8c \ud558\uc2dc\uace0\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\uadf8 \uc601\uad11\uc758 \ud798\uc744 \uc887\uc544 \ubaa8\ub4e0 \ub2a5\ub825\uc73c\ub85c \ub2a5\ud558\uac8c \ud558\uc2dc\uba70 \uae30\uc068\uc73c\ub85c \ubaa8\ub4e0 \uacac\ub524\uacfc \uc624\ub798 \ucc38\uc74c\uc5d0 \uc774\ub974\uac8c \ud558\uc2dc\uace0\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\uc6b0\ub9ac\ub85c \ud558\uc5ec\uae08 \ube5b \uac00\uc6b4\ub370\uc11c \uc131\ub3c4\uc758 \uae30\uc5c5\uc758 \ubd80\ubd84\uc744 \uc5bb\uae30\uc5d0 \ud569\ub2f9\ud558\uac8c \ud558\uc2e0 \uc544\ubc84\uc9c0\uaed8 \uac10\uc0ac\ud558\uac8c \ud558\uc2dc\uae30\ub97c \uc6d0\ud558\ub178\ub77c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\uadf8\uac00 \uc6b0\ub9ac\ub97c \ud751\uc554\uc758 \uad8c\uc138\uc5d0\uc11c \uac74\uc838\ub0b4\uc0ac \uadf8\uc758 \uc0ac\ub791\uc758 \uc544\ub4e4\uc758 \ub098\ub77c\ub85c \uc62e\uae30\uc168\uc73c\ub2c8\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\uadf8 \uc544\ub4e4 \uc548\uc5d0\uc11c \uc6b0\ub9ac\uac00 \uad6c\uc18d \uace7 \uc8c4 \uc0ac\ud568\uc744 \uc5bb\uc5c8\ub3c4\ub2e4\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\uadf8\ub294 \ubcf4\uc774\uc9c0 \uc544\ub2c8\ud558\uc2dc\ub294 \ud558\ub098\ub2d8\uc758 \ud615\uc0c1\uc774\uc694 \ubaa8\ub4e0 \ucc3d\uc870\ubb3c\ubcf4\ub2e4 \uba3c\uc800 \ub098\uc2e0 \uc790\ub2c8\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\ub9cc\ubb3c\uc774 \uadf8\uc5d0\uac8c \ucc3d\uc870\ub418\ub418 \ud558\ub298\uacfc \ub545\uc5d0\uc11c \ubcf4\uc774\ub294 \uac83\ub4e4\uacfc \ubcf4\uc774\uc9c0 \uc54a\ub294 \uac83\ub4e4\uacfc \ud639\uc740 \ubcf4\uc88c\ub4e4\uc774\ub098 \uc8fc\uad00\ub4e4\uc774\ub098 \uc815\uc0ac\ub4e4\uc774\ub098 \uad8c\uc138\ub4e4\uc774\ub098 \ub9cc\ubb3c\uc774 \ub2e4 \uadf8\ub85c \ub9d0\ubbf8\uc554\uace0 \uadf8\ub97c \uc704\ud558\uc5ec \ucc3d\uc870\ub418\uc5c8\uace0\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\ub610\ud55c \uadf8\uac00 \ub9cc\ubb3c\ubcf4\ub2e4 \uba3c\uc800 \uacc4\uc2dc\uace0 \ub9cc\ubb3c\uc774 \uadf8 \uc548\uc5d0 \ud568\uaed8 \uc130\ub290\ub2c8\ub77c\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\uadf8\ub294 \ubab8\uc778 \uad50\ud68c\uc758 \uba38\ub9ac\ub77c \uadf8\uac00 \uadfc\ubcf8\uc774\uc694 \uc8fd\uc740 \uc790\ub4e4 \uac00\uc6b4\ub370\uc11c \uba3c\uc800 \ub098\uc2e0 \uc790\ub2c8 \uc774\ub294 \uce5c\ud788 \ub9cc\ubb3c\uc758 \uc73c\ub738\uc774 \ub418\ub824 \ud558\uc2ec\uc774\uc694\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\uc544\ubc84\uc9c0\uaed8\uc11c\ub294 \ubaa8\ub4e0 \ucda9\ub9cc\uc73c\ub85c \uc608\uc218 \uc548\uc5d0 \uac70\ud558\uac8c \ud558\uc2dc\uace0\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\uadf8\uc758 \uc2ed\uc790\uac00\uc758 \ud53c\ub85c \ud654\ud3c9\uc744 \uc774\ub8e8\uc0ac \ub9cc\ubb3c \uace7 \ub545\uc5d0 \uc788\ub294 \uac83\ub4e4\uc774\ub098 \ud558\ub298\uc5d0 \uc788\ub294 \uac83\ub4e4\uc744 \uadf8\ub85c \ub9d0\ubbf8\uc554\uc544 \uc790\uae30\uc640 \ud654\ubaa9\ucf00 \ub418\uae30\ub97c \uae30\ubed0\ud558\uc2ec\uc774\ub77c\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\uc804\uc5d0 \uc545\ud55c \ud589\uc2e4\ub85c \uba40\ub9ac \ub5a0\ub098 \ub9c8\uc74c\uc73c\ub85c \uc6d0\uc218\uac00 \ub418\uc5c8\ub358 \ub108\ud76c\ub97c\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\uc774\uc81c\ub294 \uadf8\uc758 \uc721\uccb4\uc758 \uc8fd\uc74c\uc73c\ub85c \ub9d0\ubbf8\uc554\uc544 \ud654\ubaa9\ucf00 \ud558\uc0ac \ub108\ud76c\ub97c \uac70\ub8e9\ud558\uace0 \ud760 \uc5c6\uace0 \ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\ub294 \uc790\ub85c \uadf8 \uc55e\uc5d0 \uc138\uc6b0\uace0\uc790 \ud558\uc168\uc73c\ub2c8\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\ub9cc\uc77c \ub108\ud76c\uac00 \ubbff\uc74c\uc5d0 \uac70\ud558\uace0 \ud130 \uc704\uc5d0 \uad73\uac8c \uc11c\uc11c \ub108\ud76c \ub4e4\uc740 \ubc14 \ubcf5\uc74c\uc758 \uc18c\ub9dd\uc5d0\uc11c \ud754\ub4e4\ub9ac\uc9c0 \uc544\ub2c8\ud558\uba74 \uadf8\ub9ac\ud558\ub9ac\ub77c \uc774 \ubcf5\uc74c\uc740 \ucc9c\ud558 \ub9cc\ubbfc\uc5d0\uac8c \uc804\ud30c\ub41c \ubc14\uc694 \ub098 \ubc14\uc6b8\uc740 \uc774 \ubcf5\uc74c\uc758 \uc77c\uad70\uc774 \ub418\uc5c8\ub178\ub77c\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\ub0b4\uac00 \uc774\uc81c \ub108\ud76c\ub97c \uc704\ud558\uc5ec \ubc1b\ub294 \uad34\ub85c\uc6c0\uc744 \uae30\ubed0\ud558\uace0 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \ub0a8\uc740 \uace0\ub09c\uc744 \uadf8\uc758 \ubab8\ub41c \uad50\ud68c\ub97c \uc704\ud558\uc5ec \ub0b4 \uc721\uccb4\uc5d0 \ucc44\uc6b0\ub178\ub77c\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\ub0b4\uac00 \uad50\ud68c \uc77c\uad70 \ub41c \uac83\uc740 \ud558\ub098\ub2d8\uc774 \ub108\ud76c\ub97c \uc704\ud558\uc5ec \ub0b4\uac8c \uc8fc\uc2e0 \uacbd\ub95c\uc744 \ub530\ub77c \ud558\ub098\ub2d8\uc758 \ub9d0\uc500\uc744 \uc774\ub8e8\ub824 \ud568\uc774\ub2c8\ub77c\n"
                },
                "26": {
                    "verse_nr": 26,
                    "verse": "\uc774 \ube44\ubc00\uc740 \ub9cc\uc138\uc640 \ub9cc\ub300\ub85c\ubd80\ud130 \uc634\uc73c\ub85c \uac10\ucde8\uc5c8\ub358 \uac83\uc778\ub370 \uc774\uc81c\ub294 \uadf8\uc758 \uc131\ub3c4\ub4e4\uc5d0\uac8c \ub098\ud0c0\ub0ac\uace0\n"
                },
                "27": {
                    "verse_nr": 27,
                    "verse": "\ud558\ub098\ub2d8\uc774 \uadf8\ub4e4\ub85c \ud558\uc5ec\uae08 \uc774 \ube44\ubc00\uc758 \uc601\uad11\uc774 \uc774\ubc29\uc778 \uac00\uc6b4\ub370 \uc5b4\ub5bb\uac8c \ud48d\uc131\ud55c \uac83\uc744 \uc54c\uac8c \ud558\ub824 \ud558\uc2ec\uc774\ub77c \uc774 \ube44\ubc00\uc740 \ub108\ud76c \uc548\uc5d0 \uacc4\uc2e0 \uadf8\ub9ac\uc2a4\ub3c4\uc2dc\ub2c8 \uace7 \uc601\uad11\uc758 \uc18c\ub9dd\uc774\ub2c8\ub77c\n"
                },
                "28": {
                    "verse_nr": 28,
                    "verse": "\uc6b0\ub9ac\uac00 \uadf8\ub97c \uc804\ud30c\ud558\uc5ec \uac01 \uc0ac\ub78c\uc744 \uad8c\ud558\uace0 \ubaa8\ub4e0 \uc9c0\ud61c\ub85c \uac01 \uc0ac\ub78c\uc744 \uac00\ub974\uce68\uc740 \uac01 \uc0ac\ub78c\uc744 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \uc644\uc804\ud55c \uc790\ub85c \uc138\uc6b0\ub824 \ud568\uc774\ub2c8\n"
                },
                "29": {
                    "verse_nr": 29,
                    "verse": "\uc774\ub97c \uc704\ud558\uc5ec \ub098\ub3c4 \ub0b4 \uc18d\uc5d0\uc11c \ub2a5\ub825\uc73c\ub85c \uc5ed\uc0ac\ud558\uc2dc\ub294 \uc774\uc758 \uc5ed\uc0ac\ub97c \ub530\ub77c \ud798\uc744 \ub2e4\ud558\uc5ec \uc218\uace0\ud558\ub178\ub77c\n"
                }
            }
        },
        "2": {
            "chapter_nr": 2,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\ub0b4\uac00 \ub108\ud76c\uc640 \ub77c\uc624\ub514\uac8c\uc544\uc5d0 \uc788\ub294 \uc790\ub4e4\uacfc \ubb34\ub987 \ub0b4 \uc721\uc2e0\uc758 \uc5bc\uad74\uc744 \ubcf4\uc9c0 \ubabb\ud55c \uc790\ub4e4\uc744 \uc704\ud558\uc5ec \uc5b4\ub5bb\uac8c \ud798\uc4f0\ub294 \uac83\uc744 \ub108\ud76c\uac00 \uc54c\uae30\ub97c \uc6d0\ud558\ub178\ub2c8\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\uc774\ub294 \uc800\ud76c\ub85c \ub9c8\uc74c\uc5d0 \uc704\uc548\uc744 \ubc1b\uace0 \uc0ac\ub791 \uc548\uc5d0\uc11c \uc5f0\ud569\ud558\uc5ec \uc6d0\ub9cc\ud55c \uc774\ud574\uc758 \ubaa8\ub4e0 \ubd80\uc694\uc5d0 \uc774\ub974\ub7ec \ud558\ub098\ub2d8\uc758 \ube44\ubc00\uc778 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uae68\ub2eb\uac8c \ud558\ub824 \ud568\uc774\ub77c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\uadf8 \uc548\uc5d0\ub294 \uc9c0\ud61c\uc640 \uc9c0\uc2dd\uc758 \ubaa8\ub4e0 \ubcf4\ud654\uac00 \uac10\ucde8\uc5b4 \uc788\ub290\ub2c8\ub77c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\ub0b4\uac00 \uc774\uac83\uc744 \ub9d0\ud568\uc740 \uc544\ubb34\ub3c4 \uacf5\uad50\ud55c \ub9d0\ub85c \ub108\ud76c\ub97c \uc18d\uc774\uc9c0 \ubabb\ud558\uac8c \ud558\ub824 \ud568\uc774\ub2c8\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\uc774\ub294 \ub0b4\uac00 \uc721\uc2e0\uc73c\ub85c\ub294 \ub5a0\ub098 \uc788\uc73c\ub098 \uc2ec\ub839\uc73c\ub85c\ub294 \ub108\ud76c\uc640 \ud568\uaed8 \uc788\uc5b4 \ub108\ud76c\uc758 \uaddc\ubaa8\uc640 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \ubbff\ub294 \ub108\ud76c \ubbff\uc74c\uc758 \uad73\uc740 \uac83\uc744 \uae30\uc058\uac8c \ubd04\uc774\ub77c\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\uac00 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub97c \uc8fc\ub85c \ubc1b\uc558\uc73c\ub2c8 \uadf8 \uc548\uc5d0\uc11c \ud589\ud558\ub418\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\uadf8 \uc548\uc5d0 \ubfcc\ub9ac\ub97c \ubc15\uc73c\uba70 \uc138\uc6c0\uc744 \uc785\uc5b4 \uad50\ud6c8\uc744 \ubc1b\uc740\ub300\ub85c \ubbff\uc74c\uc5d0 \uad73\uac8c \uc11c\uc11c \uac10\uc0ac\ud568\uc744 \ub118\uce58\uac8c \ud558\ub77c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\ub204\uac00 \ucca0\ud559\uacfc \ud5db\ub41c \uc18d\uc784\uc218\ub85c \ub108\ud76c\ub97c \ub178\ub7b5\ud560\uae4c \uc8fc\uc758\ud558\ub77c \uc774\uac83\uc774 \uc0ac\ub78c\uc758 \uc720\uc804\uacfc \uc138\uc0c1\uc758 \ucd08\ub4f1 \ud559\ubb38\uc744 \uc887\uc74c\uc774\uc694 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc887\uc74c\uc774 \uc544\ub2c8\ub2c8\ub77c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\uadf8 \uc548\uc5d0\ub294 \uc2e0\uc131\uc758 \ubaa8\ub4e0 \ucda9\ub9cc\uc774 \uc721\uccb4\ub85c \uac70\ud558\uc2dc\uace0\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\ub108\ud76c\ub3c4 \uadf8 \uc548\uc5d0\uc11c \ucda9\ub9cc\ud558\uc5ec\uc84c\uc73c\ub2c8 \uadf8\ub294 \ubaa8\ub4e0 \uc815\uc0ac\uc640 \uad8c\uc138\uc758 \uba38\ub9ac\uc2dc\ub77c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\ub610 \uadf8 \uc548\uc5d0\uc11c \ub108\ud76c\uac00 \uc190\uc73c\ub85c \ud558\uc9c0 \uc544\ub2c8\ud55c \ud560\ub840\ub97c \ubc1b\uc558\uc73c\ub2c8 \uace7 \uc721\uc801 \ubab8\uc744 \ubc97\ub294 \uac83\uc774\uc694 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \ud560\ub840\ub2c8\ub77c\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\ub108\ud76c\uac00 \uc138\ub840\ub85c \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \uc7a5\uc0ac\ud55c \ubc14 \ub418\uace0 \ub610 \uc8fd\uc740 \uc790\ub4e4 \uac00\uc6b4\ub370\uc11c \uadf8\ub97c \uc77c\uc73c\ud0a4\uc2e0 \ud558\ub098\ub2d8\uc758 \uc5ed\uc0ac\ub97c \ubbff\uc74c\uc73c\ub85c \ub9d0\ubbf8\uc554\uc544 \uadf8 \uc548\uc5d0\uc11c \ud568\uaed8 \uc77c\uc73c\ud0a4\uc2ec\uc744 \ubc1b\uc558\ub290\ub2c8\ub77c\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\ub610 \ub108\ud76c\uc758 \ubc94\uc8c4\uc640 \uc721\uccb4\uc758 \ubb34\ud560\ub840\ub85c \uc8fd\uc5c8\ub358 \ub108\ud76c\ub97c \ud558\ub098\ub2d8\uc774 \uadf8\uc640 \ud568\uaed8 \uc0b4\ub9ac\uc2dc\uace0 \uc6b0\ub9ac\uc5d0\uac8c \ubaa8\ub4e0 \uc8c4\ub97c \uc0ac\ud558\uc2dc\uace0\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\uc6b0\ub9ac\ub97c \uac70\uc2a4\ub9ac\uace0 \uc6b0\ub9ac\ub97c \ub300\uc801\ud558\ub294 \uc758\ubb38\uc5d0 \uc4f4 \uc99d\uc11c\ub97c \ub3c4\ub9d0\ud558\uc2dc\uace0 \uc81c\ud558\uc5ec \ubc84\ub9ac\uc0ac \uc2ed\uc790\uac00\uc5d0 \ubabb \ubc15\uc73c\uc2dc\uace0\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\uc815\uc0ac\uc640 \uad8c\uc138\ub97c \ubc97\uc5b4\ubc84\ub824 \ubc1d\ud788 \ub4dc\ub7ec\ub0b4\uc2dc\uace0 \uc2ed\uc790\uac00\ub85c \uc2b9\ub9ac\ud558\uc168\ub290\ub2c8\ub77c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \uba39\uace0 \ub9c8\uc2dc\ub294 \uac83\uacfc \uc808\uae30\ub098 \uc6d4\uc0ad\uc774\ub098 \uc548\uc2dd\uc77c\uc744 \uc778\ud558\uc5ec \ub204\uad6c\ub4e0\uc9c0 \ub108\ud76c\ub97c \ud3c9\ub860\ud558\uc9c0 \ubabb\ud558\uac8c \ud558\ub77c\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\uc774\uac83\ub4e4\uc740 \uc7a5\ub798 \uc77c\uc758 \uadf8\ub9bc\uc790\uc774\ub098 \ubab8\uc740 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uac83\uc774\ub2c8\ub77c\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\ub204\uad6c\ub4e0\uc9c0 \uc77c\ubd80\ub7ec \uacb8\uc190\ud568\uacfc \ucc9c\uc0ac \uc22d\ubc30\ud568\uc744 \uc778\ud558\uc5ec \ub108\ud76c \uc0c1\uc744 \ube7c\uc557\uc9c0 \ubabb\ud558\uac8c \ud558\ub77c \uc800\uac00 \uadf8 \ubcf8 \uac83\uc744 \uc758\uc9c0\ud558\uc5ec \uadf8 \uc721\uccb4\uc758 \ub9c8\uc74c\uc744 \uc887\uc544 \ud5db\ub418\uc774 \uacfc\uc7a5\ud558\uace0\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\uba38\ub9ac\ub97c \ubd99\ub4e4\uc9c0 \uc544\ub2c8\ud558\ub294\uc9c0\ub77c \uc628 \ubab8\uc774 \uba38\ub9ac\ub85c \ub9d0\ubbf8\uc554\uc544 \ub9c8\ub514\uc640 \ud798\uc904\ub85c \uacf5\uae09\ud568\uc744 \uc5bb\uace0 \uc5f0\ud569\ud558\uc5ec \ud558\ub098\ub2d8\uc774 \uc790\ub77c\uac8c \ud558\uc2ec\uc73c\ub85c \uc790\ub77c\ub290\ub2c8\ub77c\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\ub108\ud76c\uac00 \uc138\uc0c1\uc758 \ucd08\ub4f1 \ud559\ubb38\uc5d0\uc11c \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \uc8fd\uc5c8\uac70\ub4e0 \uc5b4\ucc0c\ud558\uc5ec \uc138\uc0c1\uc5d0 \uc0ac\ub294 \uac83\uacfc \uac19\uc774 \uc758\ubb38\uc5d0 \uc21c\uc885\ud558\ub290\ub0d0 ?\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\uace7 \ubd99\uc7a1\uc9c0\ub3c4 \ub9d0\uace0 \ub9db\ubcf4\uc9c0\ub3c4 \ub9d0\uace0 \ub9cc\uc9c0\uc9c0\ub3c4 \ub9d0\ub77c \ud558\ub294 \uac83\uc774\ub2c8\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "(\uc774 \ubaa8\ub4e0 \uac83\uc740 \uc4f0\ub294 \ub300\ub85c \ubd80\ud328\uc5d0 \ub3cc\uc544\uac00\ub9ac\ub77c) \uc0ac\ub78c\uc758 \uba85\uacfc \uac00\ub974\uce68\uc744 \uc887\ub290\ub0d0\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\uc774\ub7f0 \uac83\ub4e4\uc740 \uc790\uc758\uc801 \uc22d\ubc30\uc640 \uacb8\uc190\uacfc \ubab8\uc744 \uad34\ub86d\uac8c \ud558\ub294 \ub370 \uc9c0\ud61c \uc788\ub294 \ubaa8\uc591\uc774\ub098 \uc624\uc9c1 \uc721\uccb4 \uc887\ub294 \uac83\uc744 \uae08\ud558\ub294 \ub370\ub294 \uc720\uc775\uc774 \uc870\uae08\ub3c4 \uc5c6\ub290\ub2c8\ub77c\n"
                }
            }
        },
        "3": {
            "chapter_nr": 3,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\uac00 \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \ub2e4\uc2dc \uc0b4\ub9ac\uc2ec\uc744 \ubc1b\uc558\uc73c\uba74 \uc704\uc5e3 \uac83\uc744 \ucc3e\uc73c\ub77c ! \uac70\uae30\ub294 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \ud558\ub098\ub2d8 \uc6b0\ud3b8\uc5d0 \uc549\uc544 \uacc4\uc2dc\ub290\ub2c8\ub77c\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\uc704\uc5e3 \uac83\uc744 \uc0dd\uac01\ud558\uace0, \ub545\uc5e3 \uac83\uc744 \uc0dd\uac01\uc9c0 \ub9d0\ub77c !\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\uc774\ub294 \ub108\ud76c\uac00 \uc8fd\uc5c8\uace0 \ub108\ud76c \uc0dd\uba85\uc774 \uadf8\ub9ac\uc2a4\ub3c4\uc640 \ud568\uaed8 \ud558\ub098\ub2d8 \uc548\uc5d0 \uac10\ucde8\uc5c8\uc74c\uc774\ub2c8\ub77c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\uc6b0\ub9ac \uc0dd\uba85\uc774\uc2e0 \uadf8\ub9ac\uc2a4\ub3c4\uaed8\uc11c \ub098\ud0c0\ub098\uc2e4 \uadf8 \ub54c\uc5d0 \ub108\ud76c\ub3c4 \uadf8\uc640 \ud568\uaed8 \uc601\uad11 \uc911\uc5d0 \ub098\ud0c0\ub098\ub9ac\ub77c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \ub545\uc5d0 \uc788\ub294 \uc9c0\uccb4\ub97c \uc8fd\uc774\ub77c \uace7 \uc74c\ub780\uacfc \ubd80\uc815\uacfc \uc0ac\uc695\uacfc \uc545\ud55c \uc815\uc695\uacfc \ud0d0\uc2ec\uc774\ub2c8 \ud0d0\uc2ec\uc740 \uc6b0\uc0c1 \uc22d\ubc30\ub2c8\ub77c\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\uc774\uac83\ub4e4\uc744 \uc778\ud558\uc5ec \ud558\ub098\ub2d8\uc758 \uc9c4\ub178\uac00 \uc784\ud558\ub290\ub2c8\ub77c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\ub108\ud76c\ub3c4 \uc804\uc5d0 \uadf8 \uac00\uc6b4\ub370 \uc0b4 \ub54c\uc5d0\ub294 \uadf8 \uac00\uc6b4\ub370\uc11c \ud589\ud558\uc600\uc73c\ub098\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\uc774\uc81c\ub294 \ub108\ud76c\uac00 \uc774 \ubaa8\ub4e0 \uac83\uc744 \ubc97\uc5b4\ubc84\ub9ac\ub77c \uace7 \ubd84\uacfc \uc545\uc758\uc640 \ud6fc\ubc29\uacfc \ub108\ud76c \uc785\uc758 \ubd80\ub044\ub7ec\uc6b4 \ub9d0\uc774\ub77c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\ub108\ud76c\uac00 \uc11c\ub85c \uac70\uc9d3\ub9d0\uc744 \ub9d0\ub77c \uc61b \uc0ac\ub78c\uacfc \uadf8 \ud589\uc704\ub97c \ubc97\uc5b4\ubc84\ub9ac\uace0\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\uc0c8 \uc0ac\ub78c\uc744 \uc785\uc5c8\uc73c\ub2c8 \uc774\ub294 \uc790\uae30\ub97c \ucc3d\uc870\ud558\uc2e0 \uc790\uc758 \ud615\uc0c1\uc744 \uc887\uc544 \uc9c0\uc2dd\uc5d0\uae4c\uc9c0 \uc0c8\ub86d\uac8c \ud558\uc2ec\uc744 \ubc1b\ub294 \uc790\ub2c8\ub77c\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\uac70\uae30\ub294 \ud5ec\ub77c\uc778\uacfc \uc720\ub300\uc778\uc774\ub098 \ud560\ub840\ub2f9\uacfc \ubb34\ud560\ub840\ub2f9\uc774\ub098 \uc57c\uc778\uc774\ub098 \uc2a4\uad6c\ub514\uc544\uc778\uc774\ub098 \uc885\uc774\ub098 \uc790\uc720\uc778\uc774 \ubd84\ubcc4\uc774 \uc788\uc744 \uc218 \uc5c6\ub098\ub2c8 \uc624\uc9c1 \uadf8\ub9ac\uc2a4\ub3c4\ub294 \ub9cc\uc720\uc2dc\uc694 \ub9cc\uc720 \uc548\uc5d0 \uacc4\uc2dc\ub2c8\ub77c\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\ub294 \ud558\ub098\ub2d8\uc758 \ud0dd\ud558\uc2e0 \uac70\ub8e9\ud558\uace0 \uc0ac\ub791\ud558\uc2e0 \uc790\ucc98\ub7fc \uae0d\ud73c\uacfc \uc790\ube44\uc640 \uacb8\uc190\uacfc \uc628\uc720\uc640 \uc624\ub798 \ucc38\uc74c\uc744 \uc637\uc785\uace0\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\ub204\uac00 \ub258\uac8c \ud610\uc758\uac00 \uc788\uac70\ub4e0 \uc11c\ub85c \uc6a9\ub0a9\ud558\uc5ec \ud53c\ucc28 \uc6a9\uc11c\ud558\ub418 \uc8fc\uaed8\uc11c \ub108\ud76c\ub97c \uc6a9\uc11c\ud558\uc2e0 \uac83\uacfc \uac19\uc774 \ub108\ud76c\ub3c4 \uadf8\ub9ac\ud558\uace0\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\uc774 \ubaa8\ub4e0 \uac83 \uc704\uc5d0 \uc0ac\ub791\uc744 \ub354\ud558\ub77c \uc774\ub294 \uc628\uc804\ud558\uac8c \ub9e4\ub294 \ub760\ub2c8\ub77c\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uc758 \ud3c9\uac15\uc774 \ub108\ud76c \ub9c8\uc74c\uc744 \uc8fc\uc7a5\ud558\uac8c \ud558\ub77c \ud3c9\uac15\uc744 \uc704\ud558\uc5ec \ub108\ud76c\uac00 \ud55c \ubab8\uc73c\ub85c \ubd80\ub974\uc2ec\uc744 \ubc1b\uc558\ub098\ub2c8 \ub610\ud55c \ub108\ud76c\ub294 \uac10\uc0ac\ud558\ub294 \uc790\uac00 \ub418\ub77c\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\uadf8\ub9ac\uc2a4\ub3c4\uc758 \ub9d0\uc500\uc774 \ub108\ud76c \uc18d\uc5d0 \ud48d\uc131\ud788 \uac70\ud558\uc5ec \ubaa8\ub4e0 \uc9c0\ud61c\ub85c \ud53c\ucc28 \uac00\ub974\uce58\uba70 \uad8c\uba74\ud558\uace0 \uc2dc\uc640 \ucc2c\ubbf8\uc640 \uc2e0\ub839\ud55c \ub178\ub798\ub97c \ubd80\ub974\uba70 \ub9c8\uc74c\uc5d0 \uac10\uc0ac\ud568\uc73c\ub85c \ud558\ub098\ub2d8\uc744 \ucc2c\uc591\ud558\uace0\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\ub610 \ubb34\uc5c7\uc744 \ud558\ub4e0\uc9c0 \ub9d0\uc5d0\ub098 \uc77c\uc5d0\ub098 \ub2e4 \uc8fc \uc608\uc218\uc758 \uc774\ub984\uc73c\ub85c \ud558\uace0 \uadf8\ub97c \ud798\uc785\uc5b4 \ud558\ub098\ub2d8 \uc544\ubc84\uc9c0\uaed8 \uac10\uc0ac\ud558\ub77c\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\uc544\ub0b4\ub4e4\uc544 ! \ub0a8\ud3b8\uc5d0\uac8c \ubcf5\uc885\ud558\ub77c \uc774\ub294 \uc8fc \uc548\uc5d0\uc11c \ub9c8\ub545\ud558\ub2c8\ub77c\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\ub0a8\ud3b8\ub4e4\uc544 ! \uc544\ub0b4\ub97c \uc0ac\ub791\ud558\uba70 \uad34\ub86d\uac8c \ud558\uc9c0 \ub9d0\ub77c\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\uc790\ub140\ub4e4\uc544 ! \ubaa8\ub4e0 \uc77c\uc5d0 \ubd80\ubaa8\uc5d0\uac8c \uc21c\uc885\ud558\ub77c \uc774\ub294 \uc8fc \uc548\uc5d0\uc11c \uae30\uc058\uac8c \ud558\ub294 \uac83\uc774\ub2c8\ub77c\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\uc544\ube44\ub4e4\uc544 ! \ub108\ud76c \uc790\ub140\ub97c \uaca9\ub178\ucf00 \ub9d0\uc9c0\ub2c8 \ub099\uc2ec\ud560\uae4c \ud568\uc774\ub77c\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\uc885\ub4e4\uc544 \ubaa8\ub4e0 \uc77c\uc5d0 \uc721\uc2e0\uc758 \uc0c1\uc804\ub4e4\uc5d0\uac8c \uc21c\uc885\ud558\ub418 \uc0ac\ub78c\uc744 \uae30\uc058\uac8c \ud558\ub294 \uc790\uc640 \uac19\uc774 \ub208\uac00\ub9bc\ub9cc \ud558\uc9c0 \ub9d0\uace0 \uc624\uc9c1 \uc8fc\ub97c \ub450\ub824\uc6cc\ud558\uc5ec \uc131\uc2e4\ud55c \ub9c8\uc74c\uc73c\ub85c \ud558\ub77c\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\ubb34\uc2a8 \uc77c\uc744 \ud558\ub4e0\uc9c0 \ub9c8\uc74c\uc744 \ub2e4\ud558\uc5ec \uc8fc\uaed8 \ud558\ub4ef \ud558\uace0 \uc0ac\ub78c\uc5d0\uac8c \ud558\ub4ef \ud558\uc9c0 \ub9d0\ub77c\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\uc774\ub294 \uc720\uc5c5\uc758 \uc0c1\uc744 \uc8fc\uaed8 \ubc1b\uc744 \uc904 \uc54e\uc774\ub2c8 \ub108\ud76c\ub294 \uc8fc \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc12c\uae30\ub290\ub2c8\ub77c\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\ubd88\uc758\ub97c \ud589\ud558\ub294 \uc790\ub294 \ubd88\uc758\uc758 \ubcf4\uc751\uc744 \ubc1b\uc73c\ub9ac\ub2c8 \uc8fc\ub294 \uc678\ubaa8\ub85c \uc0ac\ub78c\uc744 \ucde8\ud558\uc2ec\uc774 \uc5c6\ub290\ub2c8\ub77c\n"
                }
            }
        },
        "4": {
            "chapter_nr": 4,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\uc0c1\uc804\ub4e4\uc544 \uc758\uc640 \uacf5\ud3c9\uc744 \uc885\ub4e4\uc5d0\uac8c \ubca0\ud480\uc9c0\ub2c8 \ub108\ud76c\uc5d0\uac8c\ub3c4 \ud558\ub298\uc5d0 \uc0c1\uc804\uc774 \uacc4\uc2ec\uc744 \uc54c\uc9c0\uc5b4\ub2e4\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\uae30\ub3c4\ub97c \ud56d\uc0c1 \ud798\uc4f0\uace0 \uae30\ub3c4\uc5d0 \uac10\uc0ac\ud568\uc73c\ub85c \uae68\uc5b4 \uc788\uc73c\ub77c\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\ub610\ud55c \uc6b0\ub9ac\ub97c \uc704\ud558\uc5ec \uae30\ub3c4\ud558\ub418 \ud558\ub098\ub2d8\uc774 \uc804\ub3c4\ud560 \ubb38\uc744 \uc6b0\ub9ac\uc5d0\uac8c \uc5f4\uc5b4\uc8fc\uc0ac \uadf8\ub9ac\uc2a4\ub3c4\uc758 \ube44\ubc00\uc744 \ub9d0\ud558\uac8c \ud558\uc2dc\uae30\ub97c \uad6c\ud558\ub77c \ub0b4\uac00 \uc774\uac83\uc744 \uc778\ud558\uc5ec \ub9e4\uc784\uc744 \ub2f9\ud558\uc600\ub178\ub77c\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\uadf8\ub9ac\ud558\uba74 \ub0b4\uac00 \ub9c8\ub545\ud788 \ud560 \ub9d0\ub85c\uc368 \uc774 \ube44\ubc00\uc744 \ub098\ud0c0\ub0b4\ub9ac\ub77c\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\uc678\uc778\uc744 \ud5a5\ud558\uc5ec\uc11c\ub294 \uc9c0\ud61c\ub85c \ud589\ud558\uc5ec \uc138\uc6d4\uc744 \uc544\ub07c\ub77c !\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\ub108\ud76c \ub9d0\uc744 \ud56d\uc0c1 \uc740\ud61c \uac00\uc6b4\ub370\uc11c \uc18c\uae08\uc73c\ub85c \uace0\ub8e8\uac8c \ud568\uac19\uc774 \ud558\ub77c \uadf8\ub9ac\ud558\uba74 \uac01 \uc0ac\ub78c\uc5d0\uac8c \ub9c8\ub545\ud788 \ub300\ub2f5\ud560 \uac83\uc744 \uc54c\ub9ac\ub77c\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\ub450\uae30\uace0\uac00 \ub0b4 \uc0ac\uc815\uc744 \ub2e4 \ub108\ud76c\uc5d0\uac8c \uc54c\uac8c \ud558\ub9ac\ub2c8 \uadf8\ub294 \uc0ac\ub791\uc744 \ubc1b\ub294 \ud615\uc81c\uc694 \uc2e0\uc2e4\ud55c \uc77c\uad70\uc774\uc694 \uc8fc \uc548\uc5d0\uc11c \ud568\uaed8 \ub41c \uc885\uc774\ub77c\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\ub0b4\uac00 \uc800\ub97c \ud2b9\ubcc4\ud788 \ub108\ud76c\uc5d0\uac8c \ubcf4\ub0b8 \uac83\uc740 \ub108\ud76c\ub85c \uc6b0\ub9ac \uc0ac\uc815\uc744 \uc54c\uac8c \ud558\uace0 \ub108\ud76c \ub9c8\uc74c\uc744 \uc704\ub85c\ud558\uac8c \ud558\ub824 \ud568\uc774\ub77c\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\uc2e0\uc2e4\ud558\uace0 \uc0ac\ub791\uc744 \ubc1b\ub294 \ud615\uc81c \uc624\ub124\uc2dc\ubaa8\ub97c \ud568\uaed8 \ubcf4\ub0b4\ub178\ub2c8 \uadf8\ub294 \ub108\ud76c\uc5d0\uac8c\uc11c \uc628 \uc0ac\ub78c\uc774\ub77c \uc800\ud76c\uac00 \uc5ec\uae30 \uc77c\uc744 \ub2e4 \ub108\ud76c\uc5d0\uac8c \uc54c\uac8c \ud558\ub9ac\ub77c\n"
