{
    "version": "CHR",
    "book_name": "2 Timothy",
    "book_nr": "55",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u13a0\u13f4 \u13c9\u13b3 \u13a0\u13a9\u13c5\u13cf\u13db \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13c5\u13e7\u13b5\u13cd\u13d9\u13d4\u13c5 \u13a4\u13c1\u13b3\u13c5\u13af \u13a3\u13cf \u13a4\u13f0\u13b8\u13c5\u13a2, \u13be\u13cd\u13a9\u13ef \u13a0\u13da\u13a2\u13cd\u13db \u13ac\u13c2\u13db \u13a1\u13a9\u13c1\u13d7\u13f1, \u13be\u13cd\u13a9 \u13a6\u13b6\u13c1\u13db \u13e5\u13cc \u13e8\u13d7\u13d3\u13b4\u13b2\u13cd\u13a6;"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u13eb\u13ac\u13f2\u13ea\u13b3\u13cf \u13d7\u13b9\u13d7 \u13ac\u13a8\u13f3\u13a2 \u13a0\u13c7\u13e5; \u13ac\u13e9\u13a6\u13d8\u13ef \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13a0\u13b4 \u13a4\u13d3\u13d9\u13b5\u13e3\u13db \u13a8\u13d2 \u13a0\u13b4 \u13c5\u13e9\u13d9\u13af\u13ef\u13db \u13d5\u13e3\u13a7\u13bf\u13e9\u13d7\u13d9\u13ae\u13cd\u13d7 \u13c5\u13d3\u13f3\u13be\u13b5\u13cd\u13aa\u13b8\u13d4\u13c5\u13af \u13a4\u13c1\u13b3\u13c5\u13af \u13a2\u13a9\u13d9\u13d3 \u13a0\u13b4 \u13a6\u13b6\u13c1\u13db \u13e5\u13cc \u13a2\u13a6\u13e4\u13b5 \u13a4\u13ac\u13eb\u13f3\u13af."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u13e5\u13ef\u13b5\u13a1\u13b5\u13e4 \u13a4\u13c1\u13b3\u13c5\u13af, \u13be\u13cd\u13a9 \u13e5\u13d5\u13e5\u13ef\u13c1\u13b6\u13d7, \u13be\u13cd\u13a9\u13ef \u13d7\u13a9\u13a6\u13f4\u13b5\u13a8 \u13c4\u13be\u13db\u13c1\u13b8\u13a2, \u13e5\u13ac\u13d7\u13ad \u13a6\u13d3\u13ad \u13c2\u13a8\u13d2\u13be \u13a0\u13c6\u13d3\u13c5\u13d9, \u13c2\u13e5\u13f2\u13af\u13cd\u13d7\u13cd\u13ac\u13be, \u13a2\u13a6 \u13a0\u13b4 \u13d2\u13c3\u13f1 \u13ac\u13ef\u13c5\u13d3\u13d7\u13cd\u13ac \u13a6\u13d3\u13d9\u13b5\u13cd\u13d7\u13cd\u13ac\u13a2,"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u13be\u13cd\u13a9 \u13a4\u13e3\u13d8 \u13a0\u13c6\u13da\u13b5\u13cd\u13ac \u13ac\u13aa\u13e9\u13db\u13d7\u13f1, \u13a0\u13b4 \u13a6\u13c5\u13d3\u13d7\u13cd\u13ac \u13d5\u13e3\u13e0\u13f1\u13b8\u13a2 \u13be\u13cd\u13a9\u13c3 \u13a0\u13a9\u13a7\u13b5\u13a2\u13cd\u13d7\u13f1 \u13a4\u13b5\u13ae\u13b5\u13cd\u13d7 \u13a8\u13d2\u13a2,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u13a2\u13f3\u13c3 \u13a6\u13c5\u13d3\u13d3 \u13a4\u13e0\u13be\u13cd\u13d7 \u13c2\u13a8\u13d2\u13be \u13aa\u13af\u13f3\u13d7 \u13a8\u13d2 \u13e5\u13e3\u13ef\u13a0, \u13be\u13cd\u13a9 \u13a2\u13ac\u13f1 \u13e7\u13ef\u13a2 \u13e3\u13b5\u13cf \u13b6\u13cf \u13a0\u13b4 \u13e3\u13c2 \u13f3\u13c2\u13cf, \u13a0\u13b4 \u13c4\u13dc\u13cf\u13db\u13d2\u13be \u13a0\u13a9\u13f0\u13b8\u13ad \u13c2\u13af \u13be\u13cd\u13c9 \u13e3\u13ef\u13a5\u13a2."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13ac\u13ef\u13c5\u13d3\u13d7\u13cd\u13d7\u13ad \u13e3\u13d6\u13b8\u13d7\u13f1 \u13a4\u13c1\u13b3\u13c5\u13a9 \u13e3\u13c1\u13b8\u13af \u13a8\u13d2\u13a2, \u13be\u13cd\u13a9 \u13e5\u13e3\u13ef\u13a0, \u13be\u13cd\u13a9 \u13d5\u13ac\u13ef\u13cf\u13d4\u13db \u13e5\u13c5\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13ad."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u13a4\u13c1\u13b3\u13c5\u13af\u13f0\u13c3 \u13a5\u13dd \u13f1\u13a9\u13c1\u13b8 \u13a0\u13d3\u13c5\u13d9 \u13a4\u13d3\u13cd\u13a6\u13cd\u13d7, \u13a4\u13b5\u13c2\u13a9\u13db\u13cd\u13a9\u13c2 \u13a0\u13b4 \u13a0\u13d3\u13a8\u13f3\u13d7 \u13a8\u13d2 \u13a0\u13b4 \u13a3\u13cd\u13db \u13a0\u13d3\u13c5\u13d6\u13d7 \u13a8\u13d2\u13a2."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13de\u13cd\u13d7 \u13ef\u13d5\u13b0\u13cd\u13a8\u13cd\u13d7 \u13af\u13c3\u13ae\u13cd\u13ac \u13a4\u13ac\u13eb\u13f3\u13af \u13a2\u13a6\u13e4\u13b5\u13a6, \u13a0\u13b4 \u13de\u13cd\u13d7 \u13f1\u13cd\u13c6\u13d5\u13b0\u13cd\u13a8\u13cd\u13d7 \u13a0\u13f4 \u13e5\u13f4\u13a9 \u13be\u13cd\u13a9 \u13a4\u13e4\u13b5\u13a6, \u13e4\u13b3\u13d7\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7\u13cd\u13a9\u13c2 \u13a0\u13a9\u13b5\u13f2\u13ac \u13a3\u13cd\u13db \u13a7\u13c3\u13ae\u13db, \u13e3\u13cd\u13d5\u13b5\u13cd\u13a8\u13cd\u13d7 \u13a4\u13b5\u13c2\u13a9\u13d7 \u13a8\u13d2 \u13a4\u13c1\u13b3\u13c5\u13af,"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13a9\u13cd\u13d5\u13b8\u13db \u13e5\u13a9, \u13a0\u13b4 \u13a2\u13a9\u13ef\u13c5\u13db \u13e5\u13a9 \u13a4\u13e9\u13d4\u13c5\u13af \u13a6\u13b8\u13c9\u13d7 \u13a0\u13d3\u13ef\u13c5\u13d7 \u13a8\u13d2\u13a2, \u13a5\u13dd \u13a0\u13f4 \u13d5\u13a9\u13b8\u13eb\u13cd\u13d3\u13c1\u13b2 \u13f1\u13c5\u13e7\u13b5\u13cd\u13d9\u13d4\u13c5, \u13a4\u13e9\u13d2\u13cd\u13a9\u13c2 \u13a4\u13d3\u13c5\u13d6\u13b8\u13a2 \u13a0\u13b4 \u13ac\u13e9\u13a6\u13d8\u13ef \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13c5\u13e7\u13b5\u13cd\u13d9\u13d4\u13c5, \u13be\u13cd\u13a9 \u13a2\u13a9\u13c1\u13b8\u13af \u13e5\u13a9 \u13c5\u13e7\u13d3\u13b4\u13c5\u13b2\u13be, \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13a8\u13d2 \u13a2\u13f3\u13cd\u13d7,"
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u13aa\u13af\u13c3 \u13a8\u13d2 \u13ac\u13c2\u13a8\u13d2 \u13a2\u13ac\u13c1\u13b8\u13af \u13be\u13cd\u13a9 \u13a4\u13be\u13c4\u13aa\u13e5\u13b8 \u13a2\u13a9\u13cd\u13d5\u13b5\u13cd\u13a9 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db, \u13be\u13cd\u13a9 \u13a4\u13f2\u13cd\u13d4\u13c5\u13af \u13e5\u13a9 \u13a0\u13f2\u13b1\u13af\u13cd\u13d7 \u13a8\u13d2, \u13a0\u13b4 \u13a4\u13be\u13c4\u13aa\u13eb\u13d2\u13af \u13e5\u13a9 \u13ac\u13c2\u13db \u13a0\u13b4 \u13a0\u13f2\u13b1\u13af\u13cd\u13d7 \u13c2\u13a8\u13d2\u13be \u13a8\u13d2, \u13a3\u13cd\u13db \u13a7\u13c3\u13ae\u13db \u13ac\u13d4\u13c5\u13af,"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u13be\u13cd\u13a9 \u13a0\u13f4 \u13a5\u13a9\u13c1\u13e4\u13b8\u13af \u13a6\u13b5\u13e5\u13d9\u13b2\u13cd\u13a9 \u13a0\u13b4 \u13a5\u13a9\u13c5\u13cf\u13db \u13a0\u13b4 \u13d7\u13a6\u13e5\u13f0\u13f2\u13b2\u13cd\u13a9 \u13e7\u13be\u13d3\u13b4\u13c5\u13db \u13f4\u13eb."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u13be\u13cd\u13a9 \u13be\u13cd\u13c9 \u13c5\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13ad \u13af\u13a0 \u13be\u13cd\u13a9 \u13e7\u13d3\u13b4\u13c5\u13db \u13e5\u13a9\u13b5\u13f2\u13a6; \u13a0\u13ce\u13c3 \u13a5\u13dd \u13f1\u13a6\u13d5\u13a3\u13cd\u13a6; \u13e5\u13a6\u13d4\u13ad\u13f0\u13c3 \u13be\u13cd\u13a9 \u13e5\u13e5\u13ef\u13b5\u13cd\u13a6\u13cd\u13d9\u13d4\u13c5, \u13a0\u13b4 \u13a4\u13b5\u13c2\u13a9\u13d7\u13f3 \u13a0\u13c9\u13af\u13f3\u13ad \u13f0\u13b5\u13c9 \u13ac\u13e9\u13cd\u13c6\u13c2\u13aa\u13d9\u13d7 \u13a8\u13d2 \u13be\u13cd\u13a9 \u13e5\u13e5\u13ef\u13a8\u13c5\u13f4 \u13ac\u13c2 \u13be\u13af\u13f3 \u13a2\u13a6 \u13a0\u13cd\u13c6\u13b2\u13ad."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u13d5\u13e3\u13c2\u13f4\u13ce\u13cd\u13d7 \u13be\u13cd\u13a9 \u13d7\u13e3\u13df\u13b6\u13cd\u13d4\u13c5\u13af \u13a3\u13cd\u13db \u13a7\u13c3\u13ae\u13db \u13be\u13cd\u13a9 \u13cd\u13c6\u13db\u13a6\u13c1\u13b8\u13af \u13e5\u13a9, \u13b2\u13d7\u13cd\u13a8\u13cd\u13d7 \u13aa\u13af\u13f3\u13d7 \u13a8\u13d2 \u13a0\u13b4 \u13a0\u13d3\u13a8\u13f3\u13d7 \u13a8\u13d2 \u13be\u13cd\u13a9 \u13a6\u13b6\u13c1\u13db \u13e5\u13cc \u13c5\u13d3\u13f3\u13d3\u13b4\u13c5\u13af."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u13be\u13cd\u13a9 \u13be \u13a3\u13cd\u13db \u13e4\u13e3\u13a8\u13c5\u13f4 \u13e3\u13cd\u13c6\u13c2\u13aa\u13d5\u13cd\u13d7, \u13e3\u13cd\u13d5\u13b5\u13cd\u13a8\u13cd\u13d7 \u13a6\u13b8\u13c9\u13d7\u13f3 \u13a0\u13d3\u13c5\u13d9 \u13be\u13cd\u13a9 \u13e5\u13a9\u13ef\u13a0."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u13af\u13a0 \u13be\u13cd\u13a9 \u13af\u13a6\u13d4\u13ad, \u13c2\u13a6\u13db \u13a1\u13cf\u13f1 \u13a0\u13c1\u13af \u13ac\u13cb\u13d5\u13e8\u13a2, \u13be\u13cd\u13a9 \u13a4\u13be\u13d3\u13d1\u13ef \u13c6\u13e4\u13b3 \u13a0\u13b4 \u13ad\u13b9\u13e5\u13c2."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u13a4\u13ac\u13eb\u13f3\u13af \u13e7\u13c1\u13d7 \u13a8\u13ce\u13cd\u13d7 \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13da\u13d3\u13d8\u13bf\u13a5 \u13a3\u13c1\u13cf\u13c9\u13b3, \u13ef\u13c3\u13f0\u13c3 \u13a0\u13a9\u13a6\u13b5\u13cd\u13d3\u13d7\u13cd\u13ac\u13a9, \u13a0\u13b4 \u13a5\u13dd \u13ef\u13c9\u13d5\u13b0\u13af\u13ce\u13ae \u13a0\u13c6\u13e4\u13b5 \u13e7\u13d3\u13d5\u13d2\u13db;"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u13b6\u13bb\u13cd\u13a9\u13c2 \u13a4\u13ea\u13d9\u13b8 \u13a4\u13b5\u13c2\u13a9\u13db \u13a0\u13a9\u13f2\u13b8\u13a9, \u13a0\u13b4 \u13a0\u13a9\u13e9\u13db\u13b2\u13a9."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u13a4\u13ac\u13eb\u13f3\u13af \u13a4\u13c1\u13d7 \u13a8\u13ce\u13cd\u13d7 \u13a4\u13e9\u13db\u13d7\u13f1 \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13a4\u13ac\u13eb\u13f3\u13af \u13be\u13af\u13f3 \u13a2\u13a6. \u13a0\u13b4 \u13c2\u13a6\u13a5 \u13a0\u13a9\u13cd\u13d5\u13b8\u13af\u13d9\u13b8 \u13a1\u13c8\u13cc \u13a3\u13cf\u13f3 \u13af\u13a6\u13d4\u13ad."
                }
            }
        },
        "2": {
            "chapter_nr": "2",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13c2\u13af \u13a0\u13c7\u13e5 \u13e3\u13b5\u13c2\u13aa\u13af\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7 \u13ac\u13e9\u13a6\u13d8\u13ef \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13be\u13cd\u13a9 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13c5\u13d3\u13f3\u13b5\u13cd\u13aa\u13b8\u13d4\u13c5\u13af."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u13a0\u13b4 \u13be\u13cd\u13a9 \u13cd\u13c6\u13db\u13a6\u13c1\u13b8\u13af \u13e5\u13a9 \u13a4\u13c2\u13e3\u13d8 \u13a0\u13c2\u13a6\u13d4\u13b2\u13a2, \u13be\u13cd\u13a9 \u13d5\u13ad\u13a8\u13c5\u13d7\u13cd\u13a8\u13cd\u13d7 \u13a0\u13c3\u13cd\u13db \u13a0\u13c2\u13cd\u13a6\u13ef \u13be\u13cd\u13a9 \u13f0\u13b5 \u13d7\u13ac\u13e9\u13c1\u13f2\u13d7 \u13a8\u13d2 \u13be\u13cd\u13c9 \u13be \u13ed\u13c5\u13ab\u13db\u13a2."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13c2\u13af \u13b2\u13c2\u13d7\u13f3 \u13a8\u13ce\u13cd\u13d7 \u13af\u13a9\u13b5\u13f2\u13ac\u13a2, \u13be\u13cd\u13a9\u13ef \u13a3\u13cd\u13db \u13a0\u13ef\u13eb\u13cd\u13a9 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13a4\u13e4\u13b5\u13a6."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u13a9\u13b6 \u13d3\u13bf\u13e9 \u13e4\u13aa\u13a2 \u13a5\u13dd \u13ac\u13c5 \u13a4\u13b5\u13cd\u13d5\u13b8\u13d9\u13d7 \u13d5\u13a4\u13b8\u13eb\u13cd\u13d3\u13c1\u13b2 \u13ef\u13da\u13d3\u13b8\u13d7\u13cd\u13aa\u13a2, \u13a4\u13da\u13b5\u13cd\u13aa \u13a0\u13cd\u13a6\u13f0\u13ac\u13cd\u13d3 \u13a3\u13cd\u13db \u13a4\u13f0\u13b8\u13d7 \u13e7\u13b8\u13eb\u13cd\u13d3\u13c1\u13d7\u13f1."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u13a2\u13f3 \u13a0\u13b4 \u13a9\u13b6 \u13a4\u13d3\u13ce\u13aa\u13a9\u13cd\u13d7\u13f1 \u13e3\u13df\u13c2\u13ac\u13c1\u13b0\u13a2, \u13a5\u13dd \u13ef\u13d8\u13cd\u13da\u13b8\u13cd\u13aa\u13a2 \u13ac\u13c2 \u13d7\u13a7\u13bf\u13e9\u13db\u13cd\u13d7 \u13c2\u13ac\u13c5 \u13a2\u13f3\u13db\u13c1\u13b8\u13af \u13e5\u13a8\u13d0\u13a2."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u13d7\u13a6\u13b6\u13a9\u13cd\u13a9 \u13e5\u13da\u13b8\u13eb\u13cd\u13d3\u13c1\u13b0\u13a2 \u13a2\u13ac\u13f1 \u13a4\u13ea\u13b3\u13d7\u13cd\u13d9\u13d7 \u13a8\u13d0 \u13a4\u13be\u13c4\u13aa\u13eb\u13d2\u13af."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u13ad\u13d3\u13c5\u13d3\u13db\u13b5 \u13af\u13a0 \u13e5\u13c2\u13e5\u13ea\u13ad; \u13a4\u13ac\u13eb\u13f3\u13af\u13c3 \u13eb\u13e3\u13b2\u13cf \u13e3\u13a6\u13d9\u13a5\u13af\u13cd\u13d7\u13f1 \u13c2\u13a6\u13a5 \u13e7\u13d3\u13b4\u13c5\u13db."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u13e3\u13c5\u13d6\u13cd\u13d7 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db, \u13d5\u13eb \u13a4\u13ea\u13e5, \u13a4\u13f2\u13b1\u13d2 \u13da\u13b4\u13af\u13cc\u13c5\u13a2, \u13be\u13cd\u13a9\u13ef \u13c2\u13a6\u13ea\u13cd\u13ac \u13a3\u13cd\u13db \u13a7\u13c3\u13ae\u13db \u13a0\u13c6\u13e4\u13b5\u13a6,"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u13be\u13cd\u13a9 \u13be\u13bf \u13e5\u13e5\u13a9\u13b5\u13f2\u13a6 \u13a5\u13c6\u13b8\u13cd\u13d7\u13f1 \u13a2\u13f4\u13db, \u13a4\u13f2 \u13e7\u13b8\u13eb\u13cd\u13d3\u13c1\u13af \u13be\u13cd\u13a9\u13ef\u13a2; \u13a4\u13c1\u13b3\u13c5\u13af\u13cd\u13a9\u13c2 \u13a4\u13e4\u13b5\u13a6 \u13a7\u13c3\u13ae\u13db \u13a5\u13dd \u13a6\u13b8\u13b8\u13af \u13f1\u13a9."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13a4\u13c1\u13b3\u13a9 \u13a8\u13b5\u13ad \u13c2\u13a6\u13db \u13e7\u13d3\u13b4\u13c5\u13db \u13be\u13c6\u13b5\u13cd\u13d3\u13c1\u13b5\u13d9\u13b2\u13a2 \u13a8\u13a6\u13d1\u13f0\u13db \u13c5\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13ad, \u13a0\u13b4 \u13a4\u13c2\u13e9\u13db\u13d7\u13f1 \u13a0\u13b5\u13cd\u13d5\u13b8\u13d9\u13d7 \u13be\u13cd\u13a9 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13c5\u13d3\u13f3\u13b5\u13cd\u13aa\u13b8\u13d4\u13c5\u13af, \u13a0\u13b4 \u13a0\u13b5\u13cd\u13c6\u13d7\u13cd\u13a9 \u13c2\u13a8\u13d2\u13be \u13a6\u13b8\u13c9\u13d7\u13f3 \u13a8\u13d2\u13a2."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u13a4\u13d9\u13af\u13f3\u13af\u13ef \u13a2\u13a6\u13ea\u13db, \u13a2\u13f3\u13f0\u13c3 \u13a2\u13e7\u13b3\u13ad \u13d7\u13a9\u13f2\u13b1\u13d2\u13af \u13a8\u13ce\u13cd\u13d7, \u13a0\u13ce \u13be\u13cd\u13c9 \u13a2\u13e7\u13b3\u13ad \u13d5\u13db\u13c1\u13cd\u13d7;"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u13a2\u13f3\u13c3 \u13d5\u13db\u13c2\u13a1\u13cd\u13d7 \u13a0\u13ce \u13be\u13cd\u13a9 \u13a2\u13e7\u13b3\u13ad \u13a2\u13a9\u13ac\u13eb\u13f3\u13cc\u13d5\u13a8\u13cd\u13d7; \u13a2\u13f3\u13c3 \u13a2\u13d2\u13d3\u13d3\u13f1\u13b8\u13ad \u13be\u13cd\u13c9 \u13a0\u13ce \u13d3\u13a6\u13d3\u13f1\u13b5."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u13a2\u13f3\u13c3 \u13c1\u13d9\u13a2\u13f3\u13c5\u13be \u13a2\u13a8\u13ce\u13cd\u13d7 \u13a0\u13ce \u13c5\u13e9\u13cd\u13d7\u13c9 \u13a6\u13aa\u13af\u13f3\u13d7 \u13a8\u13d2\u13a2; \u13a5\u13dd \u13a4\u13e9\u13d2 \u13f4\u13ac\u13d3\u13d3\u13f1\u13a6."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u13af\u13a0 \u13be\u13cd\u13a9 \u13d5\u13ad\u13c5\u13d3\u13d7\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7, \u13a4\u13b5\u13c2\u13a9\u13db\u13ef \u13d5\u13af\u13c1\u13e4\u13ae\u13cd\u13d7 \u13a4\u13ac\u13eb\u13f3\u13af \u13a0\u13a6\u13d4\u13b2\u13a2, \u13a4\u13be\u13d7\u13d2\u13cd\u13d7\u13f1 \u13c2\u13a8\u13d2\u13be \u13a8\u13d2 \u13d3\u13c2\u13ec\u13c2\u13cd\u13ac\u13a2, \u13aa\u13b1\u13cd\u13d7 \u13ac\u13d9\u13d7 \u13c2\u13a8\u13d2\u13be \u13e5\u13a9, \u13d3\u13c2\u13a6\u13d4\u13b2\u13cd\u13ac\u13c9\u13cd\u13a9\u13c2 \u13ac\u13e9\u13be\u13db\u13d3\u13cd\u13d3\u13c1\u13af."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u13ad\u13df\u13c2\u13ac\u13c1\u13ae\u13cd\u13d7 \u13ac\u13c2\u13a8\u13d2 \u13a2\u13e8\u13c1\u13d7\u13f1 \u13a3\u13cf\u13f3 \u13e3\u13f0\u13b8\u13d2 \u13a4\u13c1\u13b3\u13c5\u13af, \u13d7\u13e3\u13b8\u13eb\u13cd\u13d3\u13c1\u13af \u13a8\u13ce\u13cd\u13d7 \u13a8\u13e3\u13d5\u13b0\u13af\u13cd\u13d7 \u13c2\u13a8\u13d2\u13be \u13a8\u13d2\u13a2, \u13a3\u13cd\u13db \u13d8\u13ef\u13d9\u13ae\u13af \u13a6\u13f0\u13aa\u13a9 \u13c2\u13a8\u13d2\u13be \u13a7\u13c3\u13ae\u13db."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u13a2\u13f4\u13db\u13cd\u13a9\u13c2 \u13c5\u13c1\u13ae\u13cd\u13d7 \u13a6\u13ea\u13e2\u13d7 \u13a8\u13d2 \u13a0\u13b4 \u13c4\u13b5\u13cc\u13b6\u13db\u13be \u13a6\u13ec\u13c2\u13af\u13cd\u13d7 \u13a8\u13d2\u13a2, \u13be\u13cd\u13a9\u13f0\u13c3 \u13a0\u13c2\u13c1\u13c9\u13cd\u13a8\u13cd\u13d7 \u13a4\u13c1\u13b3\u13c5\u13af \u13d7\u13c1\u13b6\u13d9\u13d7 \u13c2\u13a8\u13d2\u13be \u13a8\u13d2\u13a2,"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u13a0\u13b4 \u13a0\u13c2\u13ec\u13c2\u13cd\u13ac \u13a0\u13d3\u13f0\u13cd\u13a8\u13cd\u13d7 \u13be\u13cd\u13a9\u13ef \u13a4\u13aa\u13cf\u13db \u13e3\u13d3\u13f0\u13cd\u13aa\u13a2; \u13be\u13cd\u13a9 \u13c4\u13be\u13cd\u13d7 \u13ad\u13bb\u13c2\u13ef \u13a0\u13b4 \u13c6\u13b5\u13d3,"
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u13be\u13cd\u13a9 \u13da\u13f3\u13aa\u13db \u13a8\u13d2 \u13a4\u13be\u13de\u13d2, \u13af\u13a0 \u13e5\u13be\u13c2\u13ea\u13a0, \u13a0\u13f2\u13b1\u13d2 \u13d7\u13b4\u13af\u13d0\u13d7 \u13a8\u13d2 \u13a6\u13f3\u13b3 \u13a4\u13b6 \u13d0\u13c5; \u13a0\u13b4 \u13d3\u13c2\u13b7\u13c6\u13d7\u13c1\u13ad \u13a2\u13a6\u13db \u13a4\u13c3\u13af\u13f3\u13d2\u13a2."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u13a0\u13ce\u13cd\u13a9\u13c2\u13c3\u13c5 \u13a6\u13ab\u13cd\u13db\u13d7 \u13a4\u13c1\u13b3\u13c5\u13af \u13a4\u13e4\u13b5\u13a6 \u13a4\u13b5\u13c2\u13a9\u13d7\u13f3 \u13c2\u13ac\u13c5; \u13af\u13a0\u13f0\u13c3 \u13c2\u13ac\u13c5 \u13aa\u13ea\u13b8\u13a2, \u13a4\u13ac\u13eb\u13f3\u13af \u13d3\u13a6\u13d4\u13ad \u13be\u13cd\u13a9 \u13e7\u13e4\u13b5\u13a6; \u13a0\u13b4 \u13af\u13a0, \u13a9\u13b6 \u13a7\u13c1\u13a2\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7 \u13a6\u13b6\u13c1\u13db \u13da\u13d9\u13a5 \u13be\u13cd\u13a9 \u13a0\u13d3\u13c5\u13a1\u13ae\u13cd\u13d7 \u13a0\u13cd\u13a6\u13c2."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u13a0\u13ce\u13c3 \u13a1\u13c6 \u13e3\u13d3\u13c1\u13b6 \u13be\u13bf \u13d3\u13b0 \u13d7\u13d6\u13b5\u13d9 \u13a5\u13dd \u13a0\u13d5\u13b8\u13c9 \u13d3\u13b6\u13c2\u13a8 \u13a0\u13b4 \u13a0\u13d5\u13b8\u13a4\u13c1\u13ac \u13a4\u13e9\u13d2 \u13d7\u13aa\u13e2\u13d4\u13c5\u13af \u13f1\u13a8\u13d0\u13a2, \u13be\u13cd\u13c9\u13cd\u13a9\u13c2 \u13a0\u13d3 \u13a0\u13b4 \u13a6\u13d3 \u13d7\u13aa\u13e2\u13d4\u13c5\u13af; \u13a0\u13b4 \u13a2\u13a6\u13db \u13d7\u13a6\u13b8\u13c9\u13d7 \u13a2\u13a6\u13db\u13c3 \u13d7\u13a6\u13b8\u13c9\u13d7 \u13c2\u13a8\u13d2\u13be."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u13a2\u13f3\u13c3 \u13a9\u13b6 \u13a4\u13d3\u13c5\u13a6\u13b8\u13db \u13f1\u13a9 \u13af\u13a0 \u13be\u13cd\u13a9 \u13a8\u13d2\u13a2, \u13be\u13cd\u13a9 \u13be \u13a0\u13d6\u13b5\u13d9 \u13a6\u13b8\u13c9\u13d7 \u13a8\u13ce\u13cd\u13d7, \u13a6\u13b8\u13c9\u13d4\u13c5\u13af, \u13a0\u13b4 \u13f1\u13cd\u13db \u13a2\u13ac\u13c1\u13b8\u13af \u13aa\u13b1\u13cd\u13d7 \u13a4\u13e9\u13d9\u13d7\u13f1 \u13a6\u13c1\u13b3; \u13a0\u13b4 \u13a0\u13db\u13c5\u13a2\u13cd\u13d4\u13c5\u13af \u13be\u13a6\u13a5 \u13a3\u13cd\u13db \u13d7\u13a6\u13b8\u13eb\u13cd\u13d3\u13c1\u13d7 \u13a8\u13d2 \u13a4\u13ac\u13e9\u13b5."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u13a0\u13b4 \u13d5\u13ad\u13d3\u13c5\u13a1\u13ae\u13cd\u13d7 \u13a9\u13b3 \u13d7\u13be\u13db\u13cd\u13a9 \u13a4\u13be\u13da\u13b8\u13c5\u13d7 \u13a8\u13d2\u13a2; \u13af\u13cd\u13d3\u13e9\u13d5\u13a8\u13cd\u13d7\u13c3 \u13da\u13f3\u13aa\u13db \u13a8\u13d2, \u13a0\u13b4 \u13aa\u13af\u13f3\u13d7 \u13a8\u13d2, \u13a0\u13b4 \u13a0\u13d3\u13a8\u13f3\u13d7 \u13a8\u13d2, \u13a0\u13b4 \u13c5\u13e9\u13d9\u13af\u13ef\u13db \u13a8\u13d2\u13a2, \u13a2\u13e3\u13b5\u13aa\u13ce\u13cd\u13d7 \u13be\u13cd\u13a9 \u13be \u13a4\u13ac\u13eb\u13f3\u13af \u13a0\u13be\u13d3\u13d9\u13b5\u13cd\u13d3\u13c1\u13af \u13a4\u13be\u13eb \u13a6\u13d3\u13ad \u13c2\u13a8\u13d2\u13be \u13a0\u13c5\u13d7\u13cd\u13a9."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u13d5\u13ad\u13d3\u13c5\u13a1\u13ae\u13cd\u13d7\u13cd\u13a9\u13c2 \u13c4\u13b5\u13cc\u13b6\u13db\u13be \u13a0\u13b4 \u13c5\u13a6\u13d4\u13b2\u13be \u13da\u13ec\u13c2\u13af\u13cd\u13d7 \u13a8\u13d2\u13a2, \u13af\u13a6\u13d4\u13ad\u13f0\u13c3 \u13d7\u13d8\u13f2\u13cd\u13d7 \u13a8\u13d2 \u13e9\u13b5\u13f0\u13a2\u13b6\u13af\u13b2\u13a2."
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u13a4\u13ac\u13eb\u13f3\u13af\u13f0\u13c3 \u13a4\u13c5\u13cf\u13d3\u13cd\u13d7 \u13a5\u13dd \u13e7\u13d8\u13f2\u13cd\u13d7 \u13f1\u13a9, \u13e7\u13a8\u13f3\u13af\u13cd\u13a9\u13c2 \u13f1\u13a9 \u13be\u13c2\u13a5\u13a2, \u13d7\u13ac\u13e9\u13d5\u13f2\u13d7 \u13f1\u13a9, \u13ac\u13c2\u13d7\u13f3 \u13f1\u13a9,"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u13a4\u13d3\u13c5\u13d8 \u13ac\u13d7\u13cd\u13ac \u13f1\u13a9 \u13d5\u13a8\u13f2\u13b2\u13cd\u13ac \u13ac\u13e9\u13e1\u13d7\u13cd\u13a9; \u13a4\u13da\u13a9 \u13f3\u13e9\u13ad \u13a4\u13c1\u13b3\u13c5\u13af \u13e7\u13c2\u13c1\u13df\u13f4\u13cd\u13d7\u13f1 \u13da\u13be\u13d3\u13c5\u13db\u13a2, \u13a0\u13b4 \u13e7\u13be\u13d3\u13c2\u13b8\u13a2\u13cd\u13d7\u13f1 \u13a4\u13d9\u13af\u13f3\u13af \u13a8\u13d2\u13a2;"
                },
                "26": {
                    "verse_nr": "26",
                    "verse": "\u13a0\u13b4 \u13a4\u13be\u13d3\u13c5\u13d8\u13d0\u13d7\u13f1 \u13a4\u13c5\u13d2 \u13a4\u13be\u13da\u13d3\u13b4\u13cd\u13d7\u13f1 \u13a0\u13cd\u13a9\u13be \u13a4\u13cc\u13db\u13a2, \u13be\u13cd\u13a9 \u13ab\u13f4\u13a9\u13c5\u13af \u13e5\u13a9 \u13a4\u13e9\u13d2\u13c9 \u13a0\u13d3\u13c5\u13d6\u13cd\u13ac \u13a2\u13e7\u13e9\u13c1\u13b8\u13af."
                }
            }
        },
        "3": {
            "chapter_nr": "3",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u13af\u13a0\u13c3 \u13be\u13cd\u13c9 \u13af\u13a6\u13d4\u13ae\u13cd\u13d7, \u13a4\u13b5\u13cd\u13c6 \u13b8\u13d7 \u13a8\u13ce\u13cd\u13d7 \u13a0\u13ce \u13a4\u13d5\u13ef\u13d9\u13d7 \u13a4\u13b5\u13f0\u13a2\u13b6\u13af\u13cd\u13d7 \u13a8\u13ce\u13cd\u13d7."
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u13f4\u13eb\u13f0\u13c3 \u13a4\u13c5\u13d2 \u13a4\u13be\u13d3\u13b8\u13c9\u13d7\u13f3 \u13a8\u13ce\u13cd\u13d7, \u13a0\u13d5\u13b8 \u13a4\u13c2\u13ac\u13a5\u13cd\u13a9, \u13a0\u13be\u13e2\u13c6\u13cd\u13a9, \u13a4\u13be\u13e2\u13c9\u13d7, \u13a4\u13c1\u13b3\u13c5\u13af \u13ac\u13e9\u13c2\u13d0\u13e2\u13d7, \u13e7\u13c2\u13a6\u13f4\u13b5\u13a8 \u13d7\u13c2\u13b3\u13cf\u13d8\u13cd\u13a9, \u13c4\u13be\u13b5\u13ae\u13b5\u13e3\u13db\u13be, \u13a0\u13c2\u13a6\u13d3\u13ad\u13a2,"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u13aa\u13b1\u13cd\u13d7 \u13e7\u13c5\u13be \u13c2\u13da\u13c2\u13a8\u13f3\u13d2\u13be, \u13c4\u13c3\u13af\u13cd\u13d8\u13d2\u13be, \u13a4\u13d0\u13c5 \u13a0\u13be\u13d3\u13c3\u13ae\u13b5\u13d9\u13af, \u13c4\u13be\u13b5\u13cf\u13be\u13af\u13cd\u13db\u13be, \u13a4\u13c2\u13bf\u13b8\u13af, \u13a0\u13c2\u13cd\u13a6\u13a9 \u13a3\u13cd\u13db,"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u13a0\u13c1\u13d3\u13b6\u13c4\u13ae\u13cd\u13a9, \u13a4\u13c2\u13ab\u13cf\u13ef, \u13a4\u13be\u13d3\u13b5\u13cc\u13b3\u13d7, \u13a4\u13c2\u13a8\u13f3\u13af \u13f1\u13cd\u13db \u13a4\u13be\u13d3\u13c5\u13d3\u13d7\u13cd\u13d7\u13cd\u13a9 \u13a4\u13c1\u13b3\u13c5\u13af \u13c4\u13c2\u13a8\u13f3\u13d2\u13be,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u13a4\u13c1\u13b3\u13c5\u13af \u13d7\u13c1\u13b6\u13d9\u13d7 \u13d7\u13e4\u13b5\u13db \u13a4\u13c2\u13af, \u13a0\u13ce\u13c3 \u13a0\u13be\u13d3\u13d9\u13d7 \u13a8\u13d2\u13a2. \u13be\u13cd\u13a9 \u13a2\u13f3\u13be\u13cd\u13d7 \u13d5\u13ad\u13d3\u13c5\u13a1\u13ae\u13cd\u13d7."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u13be\u13cd\u13a9\u13f0\u13c3 \u13af\u13a0 \u13a2\u13f3\u13be\u13cd\u13d7 \u13e5\u13d3\u13c2\u13f4\u13b5\u13d9\u13b0 \u13d3\u13d3\u13c1\u13b8\u13a2 \u13a0\u13b4 \u13e5\u13d3\u13be\u13d8\u13be\u13eb\u13d7\u13cd\u13aa \u13c4\u13be\u13d3\u13c5\u13db\u13be \u13a0\u13c2\u13a8\u13f4 \u13a4\u13c2\u13a7\u13b5\u13e8\u13af \u13a0\u13cd\u13a6\u13be\u13a2, \u13da\u13be\u13d8\u13c2\u13d2 \u13e7\u13d3\u13b4\u13c5\u13db \u13a0\u13da\u13b8\u13d7 \u13a8\u13d2\u13a2,"
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u13be\u13cd\u13a9 \u13c2\u13aa\u13af\u13b8 \u13a0\u13be\u13d5\u13b6\u13c6\u13cd\u13aa\u13a2, \u13a0\u13ce\u13c3 \u13a5\u13dd \u13a2\u13b8\u13af\u13f3 \u13f1\u13e9\u13c2\u13f0\u13a2\u13b6\u13af\u13b0 \u13da\u13f3\u13aa\u13db \u13a0\u13a6\u13d9\u13a5\u13af\u13cd\u13d7 \u13a8\u13d2\u13a2."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u13a0\u13b4 \u13be\u13cd\u13a9\u13ef \u13e4\u13c2 \u13a0\u13b4 \u13e8\u13c8 \u13e3\u13be\u13e1\u13d7\u13cd\u13a8 \u13bc\u13cf \u13be\u13cd\u13a9\u13ef \u13be\u13cd\u13c9 \u13af\u13a0 \u13a0\u13be\u13e1\u13d7 \u13da\u13f3\u13aa\u13db \u13a8\u13d2\u13a2; \u13a0\u13c2\u13cd\u13a6\u13ef \u13a4\u13f2\u13e8\u13af \u13da\u13be\u13d3\u13c5\u13db\u13a2, \u13a0\u13b4 \u13a8\u13e5\u13f2\u13a2\u13ce\u13db \u13be\u13cd\u13a9 \u13aa\u13af\u13f3\u13d7 \u13a8\u13d2 \u13a4\u13ac\u13e9\u13b5."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u13a0\u13ce\u13c3 \u13a5\u13dd \u13a4\u13df \u13a2\u13f4\u13db \u13ac\u13e9\u13c1\u13c5\u13cd\u13d7 \u13f1\u13a9, \u13be\u13c2\u13a6\u13d4\u13bf\u13a5\u13be\u13f0\u13c3 \u13a8\u13d2 \u13ac\u13c2\u13a8\u13d2 \u13c5\u13d3\u13f3\u13be\u13b5\u13cd\u13d3\u13c1\u13b5 \u13be\u13c2\u13a5\u13a2, \u13be\u13cd\u13a9\u13ef \u13be\u13cd\u13c9 \u13e5\u13c4\u13cd\u13d5 \u13be\u13cd\u13a9."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u13c2\u13af\u13cd\u13a9\u13c2 \u13a3\u13cf\u13f3 \u13e3\u13a6\u13d9\u13a5\u13d2 \u13c4\u13cd\u13db \u13d3\u13c6\u13d5\u13f2\u13c5\u13a2, \u13c4\u13cd\u13db \u13a0\u13c6\u13b4\u13c2\u13d9\u13b8\u13a2, \u13c4\u13cd\u13db \u13d3\u13c7\u13aa\u13d4\u13c5 \u13d7\u13c6\u13d3\u13c5\u13db\u13a2, \u13a0\u13c9\u13af\u13f3\u13d2\u13a2, \u13ac\u13c2\u13d7\u13f3 \u13a8\u13d2\u13a2, \u13a0\u13c6\u13d3\u13a8\u13f3\u13af\u13f3 \u13a8\u13d2\u13a2, \u13a4\u13c1\u13b3\u13a9 \u13a8\u13b5\u13cd\u13ac \u13e5\u13a9\u13b5\u13f2\u13ac\u13a2,"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u13a4\u13f2 \u13c5\u13cb\u13c1\u13b5\u13d9\u13b8\u13a2, \u13a0\u13a9\u13a9\u13b5\u13f2\u13e5\u13d9\u13b8\u13a2 \u13a5\u13d8\u13a3\u13a9, \u13a2\u13aa\u13c2\u13ef, \u13b5\u13cd\u13d7--\u13c2\u13a6\u13a5 \u13a4\u13f2 \u13c5\u13cb\u13c1\u13b5\u13d9\u13b8\u13a2; \u13a0\u13ce\u13c3 \u13be\u13cd\u13a9 \u13c2\u13a6\u13db \u13be\u13bf \u13a4\u13ac\u13eb\u13f3\u13af \u13a0\u13ef\u13d3\u13b4\u13d2\u13a9."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u13a0\u13b4 \u13be\u13cd\u13c9 \u13be\u13c2\u13a5 \u13a4\u13be\u13da\u13b5\u13cd\u13a9 \u13a4\u13c1\u13b3\u13c5\u13af \u13d7\u13c1\u13b6\u13d9\u13d7 \u13a8\u13d2 \u13a4\u13c2\u13cd\u13d3\u13e9\u13db\u13cd\u13d7\u13f1 \u13a6\u13b6\u13c1\u13db \u13e5\u13cc \u13a0\u13c3\u13af\u13f3\u13b2\u13cd\u13a9 \u13a2\u13f3\u13be\u13db\u13c1\u13d7 \u13be\u13cd\u13a9 \u13a0\u13ce \u13a4\u13f2 \u13a2\u13a8\u13ac\u13c1\u13d7."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u13a4\u13c2\u13f2\u13cd\u13a9\u13c2 \u13f4\u13eb \u13a4\u13be\u13e0\u13be\u13cd\u13d7 \u13a0\u13c2\u13c1\u13c9\u13a8\u13cd\u13d7 \u13a4\u13c2\u13f2 \u13a8\u13d2\u13a2, \u13d3\u13c2\u13b6\u13be\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7 \u13a0\u13b4 \u13a8\u13e5\u13b6\u13be\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7. \u13cd\u13d7\u13cd\u13a8\u13cd\u13d7 \u13a0\u13b4 \u13a8\u13e5\u13b6\u13be\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u13c2\u13af\u13cd\u13a9\u13c2 \u13d5\u13e3\u13c2\u13f4\u13ce\u13cd\u13d7 \u13c4\u13cd\u13db \u13e3\u13d5\u13b6\u13c6\u13a5\u13a2 \u13a0\u13b4 \u13e6\u13af\u13f3\u13c5\u13a2; \u13af\u13a6\u13d4\u13ad\u13f0\u13c3 \u13be\u13cd\u13a9 \u13af\u13ef\u13d5\u13b6\u13c6\u13a1\u13b8\u13af \u13a8\u13d2\u13a2,"
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u13a0\u13b4 \u13af\u13a6\u13d4\u13ad \u13e3\u13cd\u13d7 \u13a8\u13d2 \u13c5\u13d3\u13ac\u13e9\u13d3\u13b4\u13c5\u13db \u13d5\u13af\u13a6\u13d4\u13b2 \u13d7\u13a6\u13b8\u13c9\u13d7 \u13aa\u13ea\u13b5, \u13be\u13cd\u13a9 \u13f0\u13b5 \u13af\u13a6\u13d4\u13bf\u13a2\u13f3 \u13a2\u13ac\u13e9\u13c1\u13d7, \u13e3\u13b5\u13cd\u13d5\u13b8\u13d9\u13d7 \u13a8\u13d2 \u13eb\u13ac\u13e9\u13b5\u13f1\u13b6\u13af\u13cd\u13d7, \u13a6\u13b6\u13c1\u13db \u13e5\u13cc \u13af\u13f2\u13a2\u13f3\u13d2 \u13a2\u13f3\u13e9\u13c2\u13d0\u13d7."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u13c2\u13a6\u13a5 \u13aa\u13ea\u13b5 \u13a4\u13c1\u13b3\u13c5\u13af \u13a4\u13d3\u13c5\u13d6\u13b8\u13af \u13d7\u13aa\u13ea\u13b3\u13c5\u13af, \u13a0\u13b4 \u13a3\u13cf\u13f3 \u13a4\u13d3\u13d5\u13f2\u13d7\u13f1, \u13a4\u13d3\u13ac\u13cd\u13aa\u13b8\u13d7\u13f1, \u13a4\u13d3\u13aa\u13d7\u13f1 \u13da\u13f3\u13aa\u13db\u13a2, \u13a4\u13d3\u13d5\u13f2\u13d7\u13f1 \u13a3\u13cd\u13db \u13a8\u13d2\u13a2,"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u13be\u13cd\u13a9 \u13a0\u13cd\u13a6\u13ef \u13a4\u13c1\u13b3\u13c5\u13af \u13a4\u13e4\u13b5\u13a6 \u13a4\u13cd\u13c6\u13d7\u13cd\u13d7\u13f1, \u13a4\u13d3\u13c1\u13b5\u13cc\u13db \u13a2\u13f3\u13b5\u13cd\u13d9\u13d7\u13f1 \u13c2\u13a6\u13a5 \u13a3\u13cd\u13db \u13d7\u13a6\u13b8\u13eb\u13cd\u13d3\u13c1\u13d7 \u13a8\u13d2\u13a2."
                }
            }
        },
        "4": {
            "chapter_nr": "4",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13ac\u13c1\u13e4\u13ad \u13a4\u13c1\u13b3\u13c5\u13af \u13a0\u13a6\u13d4\u13b2\u13a2 \u13a0\u13b4 \u13a0\u13a6\u13d4\u13b2 \u13a4\u13ac\u13eb\u13f3\u13af \u13e5\u13cc \u13a6\u13b6\u13c1\u13db, \u13be\u13cd\u13a9 \u13e7\u13ed\u13aa\u13d3\u13c1\u13d7 \u13e5\u13a9 \u13d7\u13c5\u13c3\u13db \u13a0\u13b4 \u13e7\u13c2\u13f2\u13b1\u13d2\u13af \u13be\u13af\u13f3 \u13a6\u13be\u13c4\u13aa\u13e5\u13b8\u13ad \u13a0\u13b4 \u13a4\u13ac\u13eb\u13f3\u13af \u13bf\u13c9 \u13c2\u13a6\u13b5\u13cd\u13d4\u13c5\u13ad,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u13ad\u13b5\u13e5\u13d9\u13b2\u13cd\u13a8\u13cd\u13d7 \u13a7\u13c3\u13ae\u13db, \u13e3\u13b5\u13c1\u13c5\u13d5\u13cd\u13d7\u13c9 \u13f0\u13b5 \u13a8\u13ce\u13cd\u13d7 \u13a0\u13b4 \u13f0\u13b5 \u13c2\u13a8\u13d2\u13be \u13a8\u13ce\u13cd\u13d7, \u13d5\u13af\u13ac\u13cd\u13a6\u13b8\u13a5\u13cd\u13a8\u13cd\u13d7, \u13d5\u13af\u13cd\u13a6\u13a8\u13cd\u13d7, \u13d5\u13af\u13cd\u13d7\u13f0\u13d7\u13cd\u13a8\u13cd\u13d7 \u13b2\u13d7\u13cd\u13a8\u13cd\u13d7 \u13a4\u13e3\u13d8 \u13ac\u13c2\u13db \u13a8\u13d2 \u13a0\u13b4 \u13d7\u13d5\u13f2\u13d7 \u13a8\u13d2\u13a2."
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u13db\u13cd\u13c6\u13b8\u13af\u13f0\u13c3 \u13bf\u13c9 \u13be\u13af\u13f3 \u13a5\u13dd \u13f3\u13be\u13da\u13b5\u13cd-\u13a8\u13cd\u13d7 \u13a4\u13be\u13db\u13d3\u13cd\u13d9\u13d7\u13f1 \u13a3\u13cd\u13db \u13d7\u13d5\u13f2\u13d7 \u13a8\u13d2\u13a2; \u13da\u13c2\u13f2\u13d5\u13cd\u13ac\u13cd\u13a9\u13c2 \u13d7\u13c2\u13b4\u13c2 \u13a0\u13c5\u13d7\u13cd\u13a8\u13cd\u13d7 \u13a4\u13c5\u13d2\u13c9 \u13a4\u13be\u13da\u13b8\u13c5\u13d7 \u13a8\u13d2 \u13d3\u13c2\u13cd\u13d3\u13e9\u13d7\u13d2 \u13d3\u13c2\u13df\u13cf\u13cd\u13a8\u13cd\u13d7 \u13d7\u13be\u13d5\u13f2\u13b2\u13cd\u13a9;"
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u13d7\u13c2\u13b4\u13c2\u13c3 \u13d3\u13c2\u13a6\u13d4\u13b2\u13cd\u13a8\u13cd\u13d7 \u13a0\u13be\u13db\u13a9\u13cd\u13ac \u13da\u13f3\u13aa\u13db\u13a2, \u13a0\u13ce\u13c9\u13c3 \u13a6\u13ec\u13c2\u13af\u13cd\u13d7 \u13a8\u13d2 \u13a2\u13d7\u13e2 \u13e9\u13be\u13a6\u13d4\u13b2\u13cd\u13d7\u13cd\u13a8\u13cd\u13d7."
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u13c2\u13af\u13cd\u13a9\u13c2 \u13ae\u13ef\u13d4\u13af\u13f3 \u13a8\u13ce\u13cd\u13d7 \u13c2\u13a6\u13a5 \u13c2\u13e3\u13db\u13bf\u13d5\u13ac\u13a2, \u13b2\u13c2\u13d7\u13f3 \u13a8\u13ce\u13cd\u13d7 \u13af\u13a9\u13b5\u13f2\u13ac\u13a2, \u13d5\u13e3\u13b8\u13eb\u13cd\u13d3\u13c1\u13ae\u13cd\u13d7 \u13a0\u13b5\u13e5\u13d9\u13c2\u13d9\u13af \u13e7\u13b8\u13eb\u13cd\u13d3\u13c1\u13d7 \u13a8\u13d2\u13a2; \u13f0\u13b5\u13a6\u13ef \u13af\u13cd\u13c6\u13d7\u13cd\u13a8\u13cd\u13d7 \u13d7\u13e3\u13b8\u13eb\u13cd\u13d3\u13c1\u13d7 \u13a8\u13d2\u13a2."
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u13bf\u13c9\u13f0\u13c3 \u13a6\u13d3\u13b5\u13cd\u13aa\u13b8\u13d7\u13ad, \u13a0\u13b4 \u13be\u13af\u13f3 \u13a0\u13c7\u13c5\u13cd\u13d7 \u13a8\u13d2 \u13a4\u13cd\u13c6\u13b8\u13af\u13d7."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u13a0\u13c6\u13b5\u13b8 \u13a3\u13cd\u13db \u13a0\u13b5\u13cd\u13d7 \u13a8\u13d2\u13a2, \u13a0\u13a9\u13cd\u13c6\u13db \u13a6\u13d9\u13a9\u13ef\u13cd\u13ac\u13a2, \u13a0\u13a9\u13cd\u13c6\u13c2\u13aa\u13d4\u13c5 \u13aa\u13af\u13f3\u13d7 \u13a8\u13d2\u13a2,"
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u13bf\u13c9\u13c3 \u13e8\u13c6\u13d3\u13c1\u13b3\u13c1\u13b8 \u13a0\u13b5\u13cd\u13da\u13b6 \u13da\u13f3\u13aa\u13db \u13a2\u13f3\u13be\u13db\u13c1\u13b8\u13af \u13a8\u13a6\u13ab\u13f4\u13d3\u13c1\u13d7, \u13be\u13cd\u13a9 \u13a4\u13ac\u13eb\u13f3\u13af \u13be\u13cd\u13a6\u13c5\u13be \u13d7\u13ab\u13aa\u13d7\u13cd\u13a9 \u13a0\u13d7-\u13c5\u13c1\u13d7 \u13be\u13af\u13f3 \u13a2\u13a6, \u13a5\u13dd \u13a0\u13b4 \u13a0\u13f4\u13c9 \u13a0\u13cb\u13d2, \u13be\u13cd\u13c9\u13cd\u13a9\u13c2 \u13be\u13c2\u13a5 \u13a4\u13be\u13c4\u13aa\u13a2\u13cd\u13d7 \u13a8\u13d2 \u13a3\u13cf\u13f3 \u13a3\u13c2\u13f0\u13b8\u13af."
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u13ad\u13df\u13c2\u13ac\u13c1\u13b8\u13ad \u13c2\u13aa\u13af\u13b8\u13be\u13c9 \u13e3\u13b7\u13af\u13cd\u13d7\u13f1 \u13a0\u13c2 \u13a8\u13d9\u13b2\u13a2."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u13d7\u13b9\u13f0\u13c3 \u13a0\u13cb\u13d5\u13e8, \u13c5\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13ad \u13a4\u13df \u13a4\u13f0\u13b8\u13c5 \u13aa\u13af \u13a8\u13d2\u13a2, \u13a0\u13b4 \u13d5\u13cf\u13b6\u13c2\u13a6 \u13ed\u13b6\u13d2; \u13de\u13cf\u13c2 \u13a8\u13b4\u13cf\u13f1 \u13ed\u13b6\u13d2, \u13d3\u13d3\u13cf \u13d5\u13b5\u13ba\u13cf\u13f1 \u13ed\u13b6\u13d2;"
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u13b7\u13a6 \u13a4\u13e9\u13d2 \u13a3\u13cd\u13d5\u13d9\u13ad; \u13b9\u13a6 \u13d8\u13ef\u13d7\u13c1\u13d2\u13ad, \u13ac\u13a9\u13cd\u13d5\u13b8\u13af\u13d3\u13cd\u13d7\u13f0\u13c3 \u13d3\u13a9\u13b8\u13eb\u13cd\u13d3\u13c1\u13b2\u13a2."
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u13d7\u13e5\u13a6\u13cd\u13a9\u13c2 \u13a1\u13c8\u13cc \u13e5\u13c5\u13d2"
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u13a6\u13cc\u13b4\u13be \u13e0\u13ef \u13e8\u13db\u13a9\u13c2\u13f4\u13a9 \u13a6\u13c6 \u13a6\u13c1\u13b8\u13a2 \u13d6\u13d2 \u13d8\u13c1\u13d2\u13ad, \u13a0\u13b4 \u13aa\u13ea\u13b5 \u13d9\u13d8\u13c1\u13d2\u13ad \u13be\u13cd\u13a9 \u13b0 \u13a4\u13ac\u13eb\u13f3\u13ce\u13cd\u13d7 \u13a6\u13c1\u13a6 \u13d7\u13aa\u13ea\u13b6\u13d7."
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u13a1\u13b5\u13a9 \u13a5\u13e3\u13f1 \u13aa\u13e2\u13c5\u13a5\u13cd\u13a9 \u13a4\u13b6\u13d8 \u13c3\u13d2\u13be \u13be\u13cb\u13c1\u13b8\u13a9. \u13a4\u13ac\u13eb\u13f3\u13af \u13a4\u13ab\u13f4\u13a1\u13d7 \u13a8\u13ce\u13cd\u13d7 \u13be\u13cd\u13a9\u13ef \u13d5\u13a4\u13b8\u13eb\u13cd\u13d3\u13c1\u13b8\u13a2."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u13be\u13cd\u13c9 \u13c2\u13af \u13be\u13cd\u13a9 \u13af\u13f0\u13ef\u13d9\u13e4\u13ae\u13cd\u13d7, \u13a4\u13b5\u13c2\u13a9\u13d7\u13f3\u13f0\u13c3 \u13a4\u13e1\u13d4\u13c5 \u13a3\u13e5\u13ec\u13c2\u13cd\u13ac\u13a2."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u13a2\u13ac\u13f1\u13f1 \u13a0\u13a9\u13ec\u13c2\u13d2 \u13a6\u13b5\u13cd\u13d5\u13b5\u13cd\u13ac \u13a5\u13dd \u13a9\u13b6 \u13be\u13a5 \u13f1\u13a6\u13d9\u13a8\u13a2, \u13be\u13c2\u13a5\u13c9\u13cd\u13a9\u13c2 \u13ac\u13c6\u13d5\u13e8\u13a9; \u13de\u13cd\u13d7 \u13a4\u13c2\u13cd\u13a6\u13c5\u13e8 \u13f1\u13da\u13da\u13aa\u13d4\u13c1\u13cd\u13d7;"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u13a4\u13ac\u13eb\u13f3\u13af\u13cd\u13a9\u13c2\u13c3\u13c5 \u13be\u13a5 \u13a6\u13d9\u13ac\u13a9 \u13a0\u13b4 \u13a0\u13c6\u13b5\u13c2\u13aa\u13af\u13cd\u13d4\u13c5\u13a9; \u13be\u13cd\u13a9 \u13a0\u13f4 \u13a8\u13d2 \u13a0\u13a9\u13cd\u13c6\u13d7\u13cd\u13d7\u13f1 \u13a0\u13f0\u13b8\u13d2\u13a9 \u13a0\u13b5\u13e5\u13d9\u13d7 \u13a8\u13d2\u13a2, \u13a0\u13b4 \u13be\u13c2\u13a5 \u13e7\u13be\u13d3\u13b4\u13c5\u13db \u13f4\u13eb \u13a4\u13be\u13db\u13af\u13d7\u13f1; \u13a0\u13b4 \u13a5\u13a9\u13b4\u13d2\u13a9 \u13e2\u13d3\u13e5 \u13a0\u13b0\u13b5 \u13e5\u13b3\u13a5\u13a2."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u13a0\u13b4 \u13a4\u13ac\u13eb\u13f3\u13af \u13a0\u13ce \u13db\u13c7\u13d3\u13b4\u13cf \u13c2\u13a6\u13a5 \u13a4\u13f2 \u13d7\u13a6\u13b8\u13eb\u13cd\u13d3\u13c1\u13d7 \u13a8\u13d2\u13a2; \u13a0\u13b4 \u13db\u13a9\u13cd\u13c6\u13c2\u13aa\u13d4\u13c2 \u13a6\u13b8\u13b3\u13d7 \u13a1\u13af \u13a4\u13ac\u13eb\u13f3\u13af \u13d7\u13a8\u13d2 \u13eb\u13a6\u13b7\u13a9. \u13be\u13cd\u13a9 \u13a6\u13b8\u13c9\u13d9\u13d7 \u13a8\u13ce\u13cd\u13d7 \u13c2\u13aa\u13af\u13b8 \u13a0\u13b4 \u13c2\u13aa\u13af\u13b8\u13a2, \u13a1\u13ba\u13c5."
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u13a9\u13f2\u13b5\u13b8\u13ad \u13c8\u13cd\u13a6 \u13a0\u13b4 \u13a1\u13c8\u13b3, \u13a0\u13b4 \u13a3\u13c1\u13cf\u13c9\u13b3 \u13da\u13d3\u13d8\u13c5\u13a5\u13a2."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u13a2\u13b3\u13cd\u13d3 \u13aa\u13b4\u13c2\u13d7 \u13d3\u13f3\u13d7\u13a9\u13f4\u13a9, \u13e0\u13c6\u13b9\u13cd\u13a9\u13c2 \u13b9\u13b5\u13d3 \u13d3\u13e5\u13ef\u13a7\u13af\u13f4\u13a9 \u13a4\u13e2\u13a9."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u13ad\u13df\u13c2\u13ac\u13c1\u13b8\u13ad \u13a0\u13c2 \u13e3\u13b7\u13af\u13cd\u13d7\u13f1 \u13a0\u13cf\u13c9 \u13aa\u13b3 \u13c2\u13a8\u13d2\u13be. \u13f3\u13c7\u13b3 \u13eb\u13e3\u13f2\u13b5\u13a6, \u13a0\u13b4 \u13be\u13cd\u13c9 \u13c7\u13d7\u13c2, \u13a0\u13b4 \u13b3\u13be\u13cf, \u13a0\u13b4 \u13e0\u13d7\u13ef, \u13a0\u13b4 \u13be\u13c2\u13a5 \u13a0\u13be\u13b5\u13c5\u13df."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u13a4\u13ac\u13eb\u13f3\u13af \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13d3\u13a7\u13bf\u13e9\u13d7\u13d9\u13ae\u13cd\u13d7 \u13e9\u13d3\u13c5\u13d9. \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13d5\u13e3\u13a7\u13bf\u13e9\u13d7\u13d9\u13ae\u13cd\u13d7. \u13a1\u13ba\u13c5."
                }
            }
        }
    }
}