{
    "version": "CHR",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": "1",
            "chapter": {
                "1": {
                    "verse_nr": "1",
                    "verse": "\u13a0\u13f4, \u13c9\u13b3, \u13e5\u13f4\u13a9 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13a0\u13a9\u13cd\u13db\u13d7\u13cd\u13ac\u13a2, \u13a0\u13b4 \u13d7\u13b9\u13d7 \u13a2\u13a9\u13c5\u13df, \u13eb\u13cd\u13db\u13f2\u13ea\u13b3\u13cf \u13c6\u13b5\u13b9\u13c2 \u13a4\u13e3\u13d8 \u13a2\u13cd\u13db\u13a8\u13f3\u13a2, \u13a0\u13b4 \u13a4\u13e0\u13f1 \u13d7\u13a9\u13b8\u13eb\u13cd\u13d3\u13c1\u13af,"
                },
                "2": {
                    "verse_nr": "2",
                    "verse": "\u13a0\u13b4 \u13a2\u13cd\u13db\u13a8\u13f3\u13a2 \u13a0\u13c8\u13ef, \u13a0\u13b4 \u13a0\u13a9\u13c6, \u13a2\u13e7\u13b3\u13ad \u13a2\u13d7\u13e9\u13eb\u13cd\u13a9, \u13a0\u13b4 \u13e7\u13be\u13c1\u13b6\u13d7\u13a4\u13be\u13d3\u13c8\u13ac \u13af\u13c1\u13b8\u13a2;"
                },
                "3": {
                    "verse_nr": "3",
                    "verse": "\u13ac\u13e9\u13a6\u13d8\u13ef \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13a0\u13b4 \u13c5\u13e9\u13d9\u13af\u13ef\u13db \u13d5\u13e5\u13a7\u13bf\u13e9\u13d7\u13d9\u13ae\u13cd\u13d7 \u13c5\u13d3\u13f3\u13be\u13b5\u13cd\u13aa\u13b8\u13d4\u13c5\u13af \u13a4\u13c1\u13b3\u13c5\u13af \u13a2\u13a9\u13d9\u13d3 \u13a0\u13b4 \u13a4\u13ac\u13eb\u13f3\u13af \u13e5\u13cc \u13a6\u13b6\u13c1\u13db."
                },
                "4": {
                    "verse_nr": "4",
                    "verse": "\u13e5\u13ef\u13b5\u13a1\u13b5\u13e4\u13b0 \u13a0\u13c6\u13c1\u13b3\u13c5\u13af \u13ac\u13c1\u13a2\u13cd\u13d7\u13cd\u13aa \u13c2\u13aa\u13af\u13b8 \u13a6\u13d3\u13d9\u13b5\u13cd\u13d7\u13cd\u13ac\u13a2,"
                },
                "5": {
                    "verse_nr": "5",
                    "verse": "\u13a6\u13db\u13a9\u13cd\u13ac \u13e3\u13d3\u13a8\u13f3\u13d2 \u13a0\u13b4 \u13e6\u13af\u13f3\u13d2\u13a2, \u13be\u13cd\u13a9 \u13e5\u13a8\u13f3\u13ad \u13a4\u13ac\u13eb\u13f3\u13af \u13e5\u13cc, \u13a0\u13b4 \u13be\u13c2\u13a5 \u13a4\u13be\u13d3\u13c5\u13d8 \u13e5\u13d5\u13af\u13a8\u13f3\u13ad;"
                },
                "6": {
                    "verse_nr": "6",
                    "verse": "\u13be\u13cd\u13a9 \u13be \u13e6\u13af\u13f3\u13d2 \u13e4\u13b3\u13d7\u13cd\u13d7\u13cd\u13ac\u13a2 \u13be\u13cd\u13a9 \u13f1\u13c2\u13ac\u13c2\u13d2 \u13ac\u13c2\u13a8\u13d2 \u13f1\u13c2\u13a6\u13b5\u13cd\u13d3 \u13c2\u13a6\u13a5 \u13a3\u13cd\u13db \u13a8\u13d2 \u13a2\u13e5\u13ef\u13a5\u13a2 \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13c5\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13cd\u13ac\u13a2."
                },
                "7": {
                    "verse_nr": "7",
                    "verse": "\u13a4\u13e3\u13d8\u13f0\u13c3 \u13a4\u13b5\u13ae\u13b5\u13cd\u13d7 \u13a3\u13a8\u13ad \u13a0\u13b4 \u13a4\u13a6\u13b5\u13cd\u13d7 \u13a0\u13d3\u13c5\u13d3\u13d7\u13cd\u13d7 \u13a8\u13d2 \u13e3\u13d3\u13a8\u13f3\u13d2 \u13a2\u13f3\u13cd\u13d7, \u13db\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13ad \u13a4\u13be\u13d3\u13c5\u13d8 \u13e7\u13c2\u13be\u13eb \u13d5\u13af\u13a6\u13b5\u13cd\u13d3\u13d5\u13b2\u13a2, \u13a5\u13a9\u13c5\u13df."
                },
                "8": {
                    "verse_nr": "8",
                    "verse": "\u13a4\u13d9\u13af\u13f3\u13af\u13cd\u13a9\u13c2\u13c3\u13c5 \u13a4\u13e3\u13d8 \u13a0\u13a9\u13ad \u13a6\u13b6\u13c1\u13db \u13a0\u13a9\u13c1\u13b8\u13a2, \u13be\u13cd\u13a9 \u13ac\u13c1\u13e4\u13b2 \u13e5\u13da\u13f3\u13aa\u13d7 \u13a2\u13e3\u13db\u13c1\u13d7\u13f1;"
                },
                "9": {
                    "verse_nr": "9",
                    "verse": "\u13a0\u13ce\u13c3 \u13cd\u13a9\u13a8\u13f3\u13af\u13f3 \u13a8\u13d2 \u13c5\u13d7\u13a6\u13b5\u13cd\u13d9\u13d7\u13ad \u13a4\u13df \u13e5\u13f0\u13b8\u13cd\u13a6 \u13ac\u13cd\u13d7\u13f0\u13d9\u13d7\u13f1, \u13c9\u13b3 \u13a4\u13db\u13d0\u13c5\u13af \u13a2\u13ef\u13c6\u13cd\u13d7 \u13e5\u13a9, \u13a0\u13b4 \u13aa\u13af \u13a8\u13d2 \u13a0\u13f4\u13a9, \u13e5\u13cc \u13a6\u13b6\u13c1\u13db \u13a0\u13a9\u13cd\u13db\u13d7\u13cd\u13ac\u13a2."
                },
                "10": {
                    "verse_nr": "10",
                    "verse": "\u13ac\u13cd\u13d7\u13f0\u13d7\u13ad \u13a0\u13c7\u13e5 \u13a3\u13c2\u13cf\u13b9 \u13a4\u13ac\u13e9\u13b5\u13be\u13cd\u13a9 \u13a5\u13c6\u13b8\u13a2\u13db \u13a8\u13d2 \u13a0\u13c6\u13d5\u13c1\u13b8\u13af \u13e5\u13a9."
                },
                "11": {
                    "verse_nr": "11",
                    "verse": "\u13be\u13cd\u13a9 \u13e7\u13e9\u13ab\u13d4\u13c5\u13d2 \u13c2\u13e3\u13cd\u13d5\u13b5\u13cd\u13ac\u13be \u13e5\u13a8\u13d2\u13a9, \u13aa\u13af\u13cd\u13a9\u13c2 \u13e5\u13a9 \u13e3\u13cd\u13d5\u13b5\u13cd\u13a9, \u13a0\u13b4 \u13a0\u13f4 \u13a0\u13a9\u13cd\u13d5\u13b5\u13cd\u13a9;"
                },
                "12": {
                    "verse_nr": "12",
                    "verse": "\u13be\u13cd\u13a9 \u13d4\u13b5\u13c1 \u13a5\u13e5\u13c5\u13b5; \u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13c2\u13af \u13d5\u13af\u13ef\u13d3\u13c2\u13b8\u13e8\u13ad, \u13be\u13cd\u13a9 \u13a0\u13cb\u13d2 \u13a0\u13a9\u13be\u13eb \u13e5\u13a9."
                },
                "13": {
                    "verse_nr": "13",
                    "verse": "\u13be\u13cd\u13a9 \u13a0\u13c6\u13da\u13b5\u13cd\u13ac\u13a9 \u13e5\u13ef\u13a7\u13af\u13ef\u13cd\u13d7\u13f1 \u13a0\u13c2 \u13a8\u13d9\u13b2\u13a2, \u13be\u13cd\u13a9 \u13c2\u13af \u13cd\u13a9\u13cd\u13d5\u13b8\u13af\u13d3\u13cd\u13d7 \u13a8\u13d2 \u13a0\u13a9\u13cd\u13d5\u13b8\u13af\u13d3\u13cd\u13d7\u13f1 \u13a5\u13c6\u13b8\u13a2\u13db \u13a8\u13d2 \u13a3\u13cd\u13db \u13a7\u13c3\u13ae\u13db \u13a0\u13a9\u13cd\u13db\u13d7\u13cd\u13ac\u13a2;"
                },
                "14": {
                    "verse_nr": "14",
                    "verse": "\u13a0\u13ce\u13c3 \u13c2\u13af \u13a3\u13cf\u13f3 \u13c2\u13e3\u13f0\u13b8\u13c5\u13be \u13a5\u13dd \u13ef\u13c6\u13da\u13b8\u13ae \u13aa\u13b1\u13cd\u13d7 \u13a0\u13c6\u13db\u13c1\u13d7\u13f1; \u13be\u13cd\u13a9 \u13a3\u13cd\u13db \u13c2\u13cd\u13c6\u13db\u13c1\u13b2 \u13a3\u13cf\u13f3 \u13e3\u13f0\u13b8\u13c5 \u13ac\u13c2\u13a8\u13d2 \u13a2\u13f3\u13b5\u13cd\u13d9\u13d7\u13f1, \u13a5\u13dd\u13c3 \u13be\u13cd\u13a9 \u13a0\u13ce \u13a2\u13e3\u13db\u13c1\u13d7\u13f1 \u13c2\u13e3\u13b5\u13cd\u13d3\u13c1\u13b2\u13a2."
                },
                "15": {
                    "verse_nr": "15",
                    "verse": "\u13be\u13cd\u13a9\u13f0\u13c3 \u13a2\u13f3\u13cd\u13d7 \u13f1\u13a9 \u13de\u13a6 \u13f3\u13d3\u13c5\u13d2 \u13be\u13cd\u13a9 \u13d8\u13ef\u13d3\u13c2\u13b8\u13a2\u13cd\u13d7 \u13c2\u13aa\u13af\u13b8 \u13a4\u13d5\u13d7\u13f1."
                },
                "16": {
                    "verse_nr": "16",
                    "verse": "\u13a5\u13dd \u13bf\u13c9 \u13a0\u13e5\u13c5\u13f4\u13d3\u13cd\u13d7 \u13be\u13cd\u13a9\u13ef\u13a2, \u13a4\u13df\u13cd\u13a9\u13c2 \u13a6\u13b8\u13c9\u13d7\u13f3 \u13a1\u13cd\u13a6\u13c9 \u13a0\u13e5\u13c5\u13cf\u13d3\u13cd\u13d7, \u13a3\u13c5\u13df \u13a0\u13a8\u13f3\u13af \u13be\u13cd\u13a9\u13ef\u13a2, \u13a0\u13f4 \u13b0 \u13a4\u13ac\u13eb\u13f3\u13ad \u13e5\u13a8\u13f3\u13a2, \u13c2\u13af\u13cd\u13a9\u13c2 \u13c2\u13a6\u13a5 \u13a4\u13df\u13af\u13f3 \u13af\u13a8\u13f3\u13a2, \u13be\u13cd\u13a9 \u13be \u13a4\u13c7\u13d3\u13b5 \u13a8\u13d2 \u13a0\u13b4 \u13a4\u13ac\u13eb\u13f3\u13af \u13d5\u13cd\u13d3\u13c1\u13b6\u13db\u13a2?"
                },
                "17": {
                    "verse_nr": "17",
                    "verse": "\u13be\u13cd\u13a9 \u13a2\u13f3\u13cd\u13d7 \u13a2\u13f3\u13c3 \u13a3\u13ac\u13be\u13d6\u13c9\u13b6\u13af \u13cd\u13c7\u13b5\u13ce\u13ae\u13cd\u13d7, \u13d5\u13af\u13ef\u13d3\u13c2\u13b8\u13e8\u13ad \u13a0\u13f4 \u13e5\u13d5\u13cd\u13c6\u13d3\u13c2\u13b8\u13aa \u13be\u13cd\u13a9\u13ef\u13a2."
                },
                "18": {
                    "verse_nr": "18",
                    "verse": "\u13a2\u13f3\u13c3 \u13a4\u13e3\u13d8\u13c2 \u13c2\u13e3\u13db\u13c1\u13b4\u13cd\u13d7, \u13a0\u13b4 \u13af\u13da\u13a8\u13cd\u13d5, \u13a0\u13f4\u13c9 \u13a0\u13c6\u13ab\u13f4\u13d7 \u13c5\u13c1\u13b8\u13ad;"
                },
                "19": {
                    "verse_nr": "19",
                    "verse": "\u13a0\u13f4 \u13c9\u13b3 \u13a0\u13cb\u13d2 \u13a0\u13c9\u13f0\u13c3 \u13ac\u13d3 \u13aa\u13ea\u13b8\u13a6, \u13a0\u13f4 \u13d3\u13a6\u13ab\u13f4\u13af; \u13a5\u13dd\u13cd\u13a9\u13c2\u13c3\u13c5 \u13e8\u13d2 \u13a8\u13d2 \u13be\u13cd\u13c9 \u13ac\u13da\u13a6 \u13d7\u13cd\u13c6\u13d3\u13f2\u13af\u13ce\u13d7\u13f1 \u13f1\u13ac\u13f2\u13ce\u13ad."
                },
                "20": {
                    "verse_nr": "20",
                    "verse": "\u13a5\u13a5, \u13d7\u13be\u13d3\u13c5\u13df, \u13a4\u13b5\u13ae\u13cd\u13d7 \u13cd\u13c6\u13d3\u13c5\u13d3\u13d7\u13cd\u13d3 \u13a4\u13ac\u13eb\u13f3\u13af \u13a2\u13f3\u13e9\u13c2\u13cc\u13db; \u13aa\u13af\u13b5\u13cd\u13d3\u13d3\u13c9 \u13a0\u13a9\u13be\u13eb \u13a4\u13ac\u13eb\u13f3\u13af \u13d5\u13be\u13c1\u13b6\u13db\u13a2."
                },
                "21": {
                    "verse_nr": "21",
                    "verse": "\u13e6\u13af\u13f3\u13d2 \u13a0\u13c6\u13b5\u13cd\u13a6\u13cd\u13d9\u13db \u13eb\u13ac\u13f2\u13ea\u13b3\u13c1\u13b8\u13a9, \u13e5\u13a6\u13d4\u13b2\u13a9 \u13be\u13cd\u13c9 \u13e3\u13b6\u13d2\u13cd\u13d9\u13d7\u13f1 \u13a8\u13d2 \u13bf\u13db\u13c1\u13b2 \u13c4\u13cd\u13db \u13ac\u13c1\u13e4\u13b2\u13a2."
                },
                "22": {
                    "verse_nr": "22",
                    "verse": "\u13a0\u13b4 \u13be\u13cd\u13c9 \u13cd\u13c6\u13db\u13c5\u13a2\u13cd\u13d3\u13c1\u13b8\u13ad \u13cd\u13a9\u13cd\u13c6\u13c2\u13aa\u13d9\u13d7\u13f1; \u13a4\u13da\u13a9\u13f0\u13c3 \u13a0\u13cb\u13ad \u13a2\u13e3\u13d3\u13d9\u13b5\u13cd\u13d7\u13cd\u13ac \u13a2\u13f3\u13e9\u13c2\u13d0\u13d7\u13f1 \u13d4\u13b5\u13c1 \u13eb\u13e8\u13b7\u13e4\u13d7\u13f1."
                },
                "23": {
                    "verse_nr": "23",
                    "verse": "\u13eb\u13a8\u13e3\u13f2\u13b5\u13a6 \u13a1\u13a6\u13c9, \u13d7\u13e7\u13b3\u13ad \u13a3\u13cd\u13d7\u13f4\u13a9 \u13a6\u13b6\u13c1\u13db \u13e5\u13cc \u13a3\u13a9\u13c2\u13cd\u13db\u13d7\u13cd\u13ac\u13a2;"
                },
                "24": {
                    "verse_nr": "24",
                    "verse": "\u13b9\u13a6, \u13a1\u13b5\u13cd\u13d3\u13a6, \u13d7\u13b9, \u13b7\u13a6, \u13a2\u13e7\u13b3\u13ad \u13e6\u13a9\u13b8\u13eb\u13cd\u13d3\u13c1\u13af"
                },
                "25": {
                    "verse_nr": "25",
                    "verse": "\u13ac\u13e9\u13a6\u13d8\u13ef \u13a4\u13d3\u13d9\u13b5\u13cd\u13d7 \u13a8\u13d2 \u13a2\u13a6\u13e4\u13b5 \u13a4\u13ac\u13eb\u13f3\u13af \u13e5\u13cc \u13a6\u13b6\u13c1\u13cf \u13d4\u13b5 \u13a4\u13db\u13d5\u13a8\u13cd\u13d7 \u13d5\u13e3\u13d3\u13c5\u13db\u13a2 \u13a1\u13ba\u13c5"
                }
            }
        }
    }
}