{
    "version": "arabicsv",
    "book_name": "Philemon",
    "book_nr": "57",
    "book": {
        "1": {
            "chapter_nr": 1,
            "chapter": {
                "1": {
                    "verse_nr": 1,
                    "verse": "\u0628\u0648\u0644\u0633 \u0627\u0633\u064a\u0631 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0648\u062a\u064a\u0645\u0648\u062b\u0627\u0648\u0633 \u0627\u0644\u0627\u062e \u0627\u0644\u0649 \u0641\u0644\u064a\u0645\u0648\u0646 \u0627\u0644\u0645\u062d\u0628\u0648\u0628 \u0648\u0627\u0644\u0639\u0627\u0645\u0644 \u0645\u0639\u0646\u0627\n"
                },
                "2": {
                    "verse_nr": 2,
                    "verse": "\u0648\u0627\u0644\u0649 \u0627\u0628\u0641\u064a\u0651\u0629 \u0627\u0644\u0645\u062d\u0628\u0648\u0628\u0629 \u0648\u0627\u0631\u062e\u0628\u0651\u0633 \u0627\u0644\u0645\u062a\u062c\u0646\u062f \u0645\u0639\u0646\u0627 \u0648\u0627\u0644\u0649 \u0627\u0644\u0643\u0646\u064a\u0633\u0629 \u0627\u0644\u062a\u064a \u0641\u064a \u0628\u064a\u062a\u0643\n"
                },
                "3": {
                    "verse_nr": 3,
                    "verse": "\u0646\u0639\u0645\u0629 \u0644\u0643\u0645 \u0648\u0633\u0644\u0627\u0645 \u0645\u0646 \u0627\u0644\u0644\u0647 \u0627\u0628\u064a\u0646\u0627 \u0648\u0627\u0644\u0631\u0628 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d\n"
                },
                "4": {
                    "verse_nr": 4,
                    "verse": "\u0627\u0634\u0643\u0631 \u0627\u0644\u0647\u064a \u0643\u0644 \u062d\u064a\u0646 \u0630\u0627\u0643\u0631\u0627 \u0627\u064a\u0627\u0643 \u0641\u064a \u0635\u0644\u0648\u0627\u062a\u064a\n"
                },
                "5": {
                    "verse_nr": 5,
                    "verse": "\u0633\u0627\u0645\u0639\u0627 \u0628\u0645\u062d\u0628\u062a\u0643 \u0648\u0627\u0644\u0627\u064a\u0645\u0627\u0646 \u0627\u0644\u0630\u064a \u0644\u0643 \u0646\u062d\u0648 \u0627\u0644\u0631\u0628 \u064a\u0633\u0648\u0639 \u0648\u0644\u062c\u0645\u064a\u0639 \u0627\u0644\u0642\u062f\u064a\u0633\u064a\u0646\n"
                },
                "6": {
                    "verse_nr": 6,
                    "verse": "\u0644\u0643\u064a \u062a\u0643\u0648\u0646 \u0634\u0631\u0643\u0629 \u0627\u064a\u0645\u0627\u0646\u0643 \u0641\u0639\u0651\u0627\u0644\u0629 \u0641\u064a \u0645\u0639\u0631\u0641\u0629 \u0643\u0644 \u0627\u0644\u0635\u0644\u0627\u062d \u0627\u0644\u0630\u064a \u0641\u064a\u0643\u0645 \u0644\u0627\u062c\u0644 \u0627\u0644\u0645\u0633\u064a\u062d \u064a\u0633\u0648\u0639.\n"
                },
                "7": {
                    "verse_nr": 7,
                    "verse": "\u0644\u0627\u0646 \u0644\u0646\u0627 \u0641\u0631\u062d\u0627 \u0643\u062b\u064a\u0631\u0627 \u0648\u062a\u0639\u0632\u064a\u0629 \u0628\u0633\u0628\u0628 \u0645\u062d\u0628\u062a\u0643 \u0644\u0627\u0646 \u0627\u062d\u0634\u0627\u0621 \u0627\u0644\u0642\u062f\u064a\u0633\u064a\u0646 \u0642\u062f \u0627\u0633\u062a\u0631\u0627\u062d\u062a \u0628\u0643 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e\n"
                },
                "8": {
                    "verse_nr": 8,
                    "verse": "\u0644\u0630\u0644\u0643 \u0648\u0627\u0646 \u0643\u0627\u0646 \u0644\u064a \u0628\u0627\u0644\u0645\u0633\u064a\u062d \u062b\u0642\u0629 \u0643\u062b\u064a\u0631\u0629 \u0627\u0646 \u0622\u0645\u0631\u0643 \u0628\u0645\u0627 \u064a\u0644\u064a\u0642\n"
                },
                "9": {
                    "verse_nr": 9,
                    "verse": "\u0645\u0646 \u0627\u062c\u0644 \u0627\u0644\u0645\u062d\u0628\u0629 \u0627\u0637\u0644\u0628 \u0628\u0627\u0644\u062d\u0631\u064a \u0627\u0630 \u0627\u0646\u0627 \u0627\u0646\u0633\u0627\u0646 \u0647\u0643\u0630\u0627 \u0646\u0638\u064a\u0631 \u0628\u0648\u0644\u0633 \u0627\u0644\u0634\u064a\u062e \u0648\u0627\u0644\u0622\u0646 \u0627\u0633\u064a\u0631 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0627\u064a\u0636\u0627\n"
                },
                "10": {
                    "verse_nr": 10,
                    "verse": "\u0627\u0637\u0644\u0628 \u0627\u0644\u064a\u0643 \u0644\u0627\u062c\u0644 \u0627\u0628\u0646\u064a \u0627\u0646\u0633\u064a\u0645\u0633 \u0627\u0644\u0630\u064a \u0648\u0644\u062f\u062a\u0647 \u0641\u064a \u0642\u064a\u0648\u062f\u064a\n"
                },
                "11": {
                    "verse_nr": 11,
                    "verse": "\u0627\u0644\u0630\u064a \u0643\u0627\u0646 \u0642\u0628\u0644\u0627 \u063a\u064a\u0631 \u0646\u0627\u0641\u0639 \u0644\u0643 \u0648\u0644\u0643\u0646\u0647 \u0627\u0644\u0622\u0646 \u0646\u0627\u0641\u0639 \u0644\u0643 \u0648\u0644\u064a\n"
                },
                "12": {
                    "verse_nr": 12,
                    "verse": "\u0627\u0644\u0630\u064a \u0631\u062f\u062f\u062a\u0647. \u0641\u0627\u0642\u0628\u0644\u0647 \u0627\u0644\u0630\u064a \u0647\u0648 \u0627\u062d\u0634\u0627\u0626\u064a.\n"
                },
                "13": {
                    "verse_nr": 13,
                    "verse": "\u0627\u0644\u0630\u064a \u0643\u0646\u062a \u0627\u0634\u0627\u0621 \u0627\u0646 \u0627\u0645\u0633\u0643\u0647 \u0639\u0646\u062f\u064a \u0644\u0643\u064a \u064a\u062e\u062f\u0645\u0646\u064a \u0639\u0648\u0636\u0627 \u0639\u0646\u0643 \u0641\u064a \u0642\u064a\u0648\u062f \u0627\u0644\u0627\u0646\u062c\u064a\u0644\n"
                },
                "14": {
                    "verse_nr": 14,
                    "verse": "\u0648\u0644\u0643\u0646 \u0628\u062f\u0648\u0646 \u0631\u0623\u064a\u0643 \u0644\u0645 \u0627\u0631\u062f \u0627\u0646 \u0627\u0641\u0639\u0644 \u0634\u064a\u0626\u0627 \u0644\u0643\u064a \u0644\u0627 \u064a\u0643\u0648\u0646 \u062e\u064a\u0631\u0643 \u0643\u0627\u0646\u0647 \u0639\u0644\u0649 \u0633\u0628\u064a\u0644 \u0627\u0644\u0627\u0636\u0637\u0631\u0627\u0631 \u0628\u0644 \u0639\u0644\u0649 \u0633\u0628\u064a\u0644 \u0627\u0644\u0627\u062e\u062a\u064a\u0627\u0631.\n"
                },
                "15": {
                    "verse_nr": 15,
                    "verse": "\u0644\u0627\u0646\u0647 \u0631\u0628\u0645\u0627 \u0644\u0627\u062c\u0644 \u0647\u0630\u0627 \u0627\u0641\u062a\u0631\u0642 \u0639\u0646\u0643 \u0627\u0644\u0649 \u0633\u0627\u0639\u0629 \u0644\u0643\u064a \u064a\u0643\u0648\u0646 \u0644\u0643 \u0627\u0644\u0649 \u0627\u0644\u0627\u0628\u062f\n"
                },
                "16": {
                    "verse_nr": 16,
                    "verse": "\u0644\u0627 \u0643\u0639\u0628\u062f \u0641\u064a \u0645\u0627 \u0628\u0639\u062f \u0628\u0644 \u0627\u0641\u0636\u0644 \u0645\u0646 \u0639\u0628\u062f \u0627\u062e\u0627 \u0645\u062d\u0628\u0648\u0628\u0627 \u0648\u0644\u0627 \u0633\u064a\u0645\u0627 \u0627\u0644\u064a\u0651 \u0641\u0643\u0645 \u0628\u0627\u0644\u062d\u0631\u064a \u0627\u0644\u064a\u0643 \u0641\u064a \u0627\u0644\u062c\u0633\u062f \u0648\u0627\u0644\u0631\u0628 \u062c\u0645\u064a\u0639\u0627\n"
                },
                "17": {
                    "verse_nr": 17,
                    "verse": "\u0641\u0627\u0646 \u0643\u0646\u062a \u062a\u062d\u0633\u0628\u0646\u064a \u0634\u0631\u064a\u0643\u0627 \u0641\u0627\u0642\u0628\u0644\u0647 \u0646\u0638\u064a\u0631\u064a.\n"
                },
                "18": {
                    "verse_nr": 18,
                    "verse": "\u062b\u0645 \u0627\u0646 \u0643\u0627\u0646 \u0642\u062f \u0638\u0644\u0645\u0643 \u0628\u0634\u064a\u0621 \u0627\u0648 \u0644\u0643 \u0639\u0644\u064a\u0647 \u062f\u064a\u0646 \u0641\u0627\u062d\u0633\u0628 \u0630\u0644\u0643 \u0639\u0644\u064a\u0651.\n"
                },
                "19": {
                    "verse_nr": 19,
                    "verse": "\u0627\u0646\u0627 \u0628\u0648\u0644\u0633 \u0643\u062a\u0628\u062a \u0628\u064a\u062f\u064a. \u0627\u0646\u0627 \u0627\u0648\u0641\u064a. \u062d\u062a\u0649 \u0644\u0627 \u0627\u0642\u0648\u0644 \u0644\u0643 \u0627\u0646\u0643 \u0645\u062f\u064a\u0648\u0646 \u0644\u064a \u0628\u0646\u0641\u0633\u0643 \u0627\u064a\u0636\u0627.\n"
                },
                "20": {
                    "verse_nr": 20,
                    "verse": "\u0646\u0639\u0645 \u0627\u064a\u0647\u0627 \u0627\u0644\u0627\u062e \u0644\u064a\u0643\u0646 \u0644\u064a \u0641\u0631\u062d \u0628\u0643 \u0641\u064a \u0627\u0644\u0631\u0628. \u0627\u0631\u062d \u0627\u062d\u0634\u0627\u0626\u064a \u0641\u064a \u0627\u0644\u0631\u0628.\n"
                },
                "21": {
                    "verse_nr": 21,
                    "verse": "\u0627\u0630 \u0627\u0646\u0627 \u0648\u0627\u062b\u0642 \u0628\u0627\u0637\u0627\u0639\u062a\u0643 \u0643\u062a\u0628\u062a \u0627\u0644\u064a\u0643 \u0639\u0627\u0644\u0645\u0627 \u0627\u0646\u0643 \u062a\u0641\u0639\u0644 \u0627\u064a\u0636\u0627 \u0627\u0643\u062b\u0631 \u0645\u0645\u0627 \u0627\u0642\u0648\u0644\n"
                },
                "22": {
                    "verse_nr": 22,
                    "verse": "\u0648\u0645\u0639 \u0647\u0630\u0627 \u0627\u0639\u062f\u062f \u0644\u064a \u0627\u064a\u0636\u0627 \u0645\u0646\u0632\u0644\u0627 \u0644\u0627\u0646\u064a \u0627\u0631\u062c\u0648 \u0627\u0646\u0646\u064a \u0628\u0635\u0644\u0648\u0627\u062a\u0643\u0645 \u0633\u0623\u0648\u0647\u0628 \u0644\u0643\u0645\n"
                },
                "23": {
                    "verse_nr": 23,
                    "verse": "\u064a\u0633\u0644\u0645 \u0639\u0644\u064a\u0643 \u0627\u0628\u0641\u0631\u0627\u0633 \u0627\u0644\u0645\u0623\u0633\u0648\u0631 \u0645\u0639\u064a \u0641\u064a \u0627\u0644\u0645\u0633\u064a\u062d \u064a\u0633\u0648\u0639\n"
                },
                "24": {
                    "verse_nr": 24,
                    "verse": "\u0648\u0645\u0631\u0642\u0633 \u0648\u0627\u0631\u0633\u062a\u0631\u062e\u0633 \u0648\u062f\u064a\u0645\u0627\u0633 \u0648\u0644\u0648\u0642\u0627 \u0627\u0644\u0639\u0627\u0645\u0644\u0648\u0646 \u0645\u0639\u064a.\n"
                },
                "25": {
                    "verse_nr": 25,
                    "verse": "\u0646\u0639\u0645\u0629 \u0631\u0628\u0646\u0627 \u064a\u0633\u0648\u0639 \u0627\u0644\u0645\u0633\u064a\u062d \u0645\u0639 \u0631\u0648\u062d\u0643\u0645. \u0622\u0645\u064a\u0646. \u0627\u0644\u0649 \u0641\u0644\u064a\u0645\u0648\u0646 \u0643\u062a\u0628\u062a \u0645\u0646 \u0631\u0648\u0645\u064a\u0629 \u0639\u0644\u0649 \u064a\u062f \u0627\u0646\u0633\u064a\u0645\u0633 \u0627\u0644\u062e\u0627\u062f\u0645\n"
                }
            }
        }
    }
}